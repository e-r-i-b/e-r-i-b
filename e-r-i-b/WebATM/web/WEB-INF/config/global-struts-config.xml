		<form-bean name="ShowBankListATMForm" type="com.rssl.phizic.web.atm.dictionaries.ShowBankListATMForm"/>
        <!-- список видов валютных операций -->
		<form-bean name="ShowOperationCodesForm" type="com.rssl.phizic.web.common.dictionaries.ShowOperationCodesForm"/>

        <form-bean name="ShowRegionsListATMForm" type="com.rssl.phizic.web.atm.dictionaries.ShowRegionsListATMForm"/>
