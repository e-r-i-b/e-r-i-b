        <!--список услуг поставщика-->
        <form-bean name="ListProviderServicesATMForm" type="com.rssl.phizic.web.atm.payments.services.ListProviderServicesATMForm"/>
        <!--популярные платежи-->
        <form-bean name="ListPopularPaymentATMForm" type="com.rssl.phizic.web.atm.payments.services.ListPopularPaymentATMForm"/>
        <!--курсы валют-->
        <form-bean name="GetCurrencyRatesATMForm" type="com.rssl.phizic.web.atm.rates.GetCurrencyRatesATMForm"/>
        <!--Получение информации о валюте счета зачисления-->
        <form-bean name="GetCardCurrencyATMForm" type="com.rssl.phizic.web.atm.cards.GetCardCurrencyATMForm"/>
        <!-- печать документа на базе формы -->
        <form-bean name="ViewPaymentATMForm" type="com.rssl.phizic.web.actions.payments.forms.ViewDocumentForm"/>
        <!-- печать чека исполненного автоплатежа-->
        <form-bean name="PrintAutoSubscriptionPaymentInfoATMForm" type="com.rssl.phizic.web.actions.payments.forms.autosubscription.PrintAutoSubscriptionPaymentInfoForm"/>

        <!-- предложения банка -->
        <form-bean name="BankOffersATMForm" type="com.rssl.phizic.web.atm.common.offer.BankOffersATMForm"/>

        <!--Создание/редактирование шаблона-->
        <form-bean name="EditTemplateAtmForm" type="com.rssl.phizic.web.atm.common.EditTemplateAtmForm"/>
        
        <!-- получение детальной информации по длительному поручению-->
        <form-bean name="ShowLongOfferInfoATMForm"
                   type="com.rssl.phizic.web.atm.longoffers.ShowLongOfferInfoATMForm"/>

        <!-- график длительных поручений -->
        <form-bean name="ShowLongOfferAbstractATMForm"
                   type="com.rssl.phizic.web.atm.longoffers.ShowLongOfferAbstractATMForm"/>

        <!-- получение детальной информации по шинному автоплатежу -->
        <form-bean name="ShowAutoSubscriptionInfoForm"
                   type="com.rssl.phizic.web.actions.payments.forms.autosubscription.ShowAutoSubscriptionInfoForm"/>

        <!-- график шинных автоплатежей -->
        <form-bean name="ShowAutoSubscriptionAbstractMobileForm"
                   type="com.rssl.phizic.web.atm.autosubscriptions.ShowAutoSubscriptionAbstractATMForm"/>

        <!--Список доступных операций в atmAPI-->
        <form-bean name="ListPermissionsATMForm"
                   type="com.rssl.phizic.web.atm.permissions.ListPermissionsATMForm"/>

         <!-- список депозитов -->
        <form-bean name="ListDepositProductsForm"
                   type="com.rssl.phizic.web.common.client.deposits.ListDepositProductsForm"/>

        <!-- подробная информация о депозитном продукте -->
        <form-bean name="DepositDetailsATMForm"
                   type="com.rssl.phizic.web.atm.deposits.DepositDetailsATMForm"/>

        <!-- Определение подвида вклада -->
        <form-bean name="DepositProductTypeInfoForm"
                   type="com.rssl.phizic.web.atm.deposits.DepositProductTypeInfoForm"/>

        <!-- Форма просмотра условий договора по вкладу -->
        <form-bean name="ViewDepositTermsForm"
                   type="com.rssl.phizic.web.ext.sbrf.payments.ViewDepositTermsForm"/>

        <!-- Форма статуса платежа -->
        <form-bean name="PaymentStateForm"
                   type="com.rssl.phizic.web.atm.payments.forms.PaymentStateForm"/>

        <!-- Форма авторизации по ДУЛ -->
        <form-bean name="CheckDULForm"
                   type="com.rssl.phizic.web.atm.ext.sbrf.security.CheckDULForm"/>

        <!--Форма выбора уровня дохода в заявке на кредитную карту-->
        <form-bean name="ListIncomeLevelForm"
                   type="com.rssl.phizic.web.common.client.incomes.ListIncomeLevelForm"/>

        <!-- Список продуктов -->
        <form-bean name="LoanProductListForm"
                   type="com.rssl.phizic.web.common.client.product.LoanProductListForm"/>

        <form-bean name="ListCreditCardOfficeAtmForm"
                   type="com.rssl.phizic.web.atm.cards.ListCreditCardOfficeAtmForm"/>

        <form-bean name="LoyaltyGetPointsForm"
                   type="com.rssl.phizic.web.atm.loyalty.LoyaltyGetPointsForm"/>

        <!-- Форма списка заявок на получение выписки ПФР -->
        <form-bean name="ListPFRClaimATMForm"
                   type="com.rssl.phizic.web.atm.pfr.ListPFRClaimATMForm"/>

        <!-- Форма выписки из пенсионного фонда -->
        <form-bean name="ViewPFRStatementATMForm"
                   type="com.rssl.phizic.web.atm.pfr.ViewPFRStatementATMForm"/>

        <!-- Форма получения временного логина и пароля -->
        <form-bean name="GetRegistrationForm"
                   type="com.rssl.phizic.web.atm.ext.sbrf.security.GetRegistrationForm"/>

        <form-bean name="ViewDocumentForm" type="com.rssl.phizic.web.actions.payments.forms.ViewDocumentForm"/>

        <!--Получение текста согласия на обработку персональных данных-->
        <form-bean name="ShowLoanAgreementForm" type="com.rssl.phizic.web.common.client.offer.ShowLoanAgreementForm"/>

	    <form-bean name="UserServiceInfoForm" type="com.rssl.phizic.web.atm.service.UserServiceInfoForm"/>
    </form-beans>

    <global-exceptions>
        <exception key="error.accessDeny"
                   scope="request"
                   type="java.security.AccessControlException"
                   path="/WEB-INF/jsp/types/accessDenied.jsp"/>

        <exception key="error.external.system"
		           scope="request"
		           type="com.rssl.phizic.common.types.exceptions.InactiveExternalSystemException"
		           handler="com.rssl.phizic.web.actions.InactiveExternalSystemErrorHandler"
                   path="/WEB-INF/jsp/types/error-access-external-system.jsp"/>

		<exception key="error.errorHeader"
		           scope="request"
		           type="java.lang.Exception"
		           handler="com.rssl.phizic.web.actions.GlobalExceptionHandler"/>

        <exception key="error.access"
                   scope="request"
                   type="com.rssl.phizic.business.access.AccessException"
                   handler="com.rssl.phizic.web.actions.AccessExceptionHandler"/>
	</global-exceptions>

	<global-forwards>
        <forward name="GotoUnauthorizedIndex" path="/login.do" redirect="true"/>
        <forward name="GotoAuthorizedIndex" path="/WEB-INF/jsp/login.jsp" redirect="false"/>
        <forward name="ShowEmptyErrorPage" path="/WEB-INF/jsp/emptyErrorPage.jsp" redirect="false"/>
        <forward name="ShowErrorPage" path="/WEB-INF/jsp/errorPage.jsp" redirect="false"/>
	</global-forwards>

    <action-mappings>
        <!-- страница ошибок -->
        <action path="/error" forward="/WEB-INF/jsp/errorPage.jsp"/>

        <action path="/private/payments/makeLongOffer"
                type="com.rssl.phizic.web.atm.common.payments.MakeLongOfferATMAction"
                name="ViewDocumentForm"
                scope="request" validate="false" parameter="unused">
            <forward name="unsupported" path="/WEB-INF/jsp/common/emptyResponse.jsp"/>
        </action>

        <!-- Предложения банка по кредитным продуктам-->
        <action path="/private/loan/loanOffer/show"
                type="com.rssl.phizic.web.atm.common.offer.BankOffersATMAction"
                name="BankOffersATMForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Start" path="/WEB-INF/jsp/common/offer/offerInfo.jsp"/>
        </action>

        <!-- завершение аутентификации -->
        <action path="/postCSALogin" type="com.rssl.phizic.web.atm.ext.sbrf.security.ATMPostCSALoginAction" name="ATMPostCSALoginForm" scope="request" validate="false" parameter="unused">
            <forward name="Show" path="/WEB-INF/jsp/login.jsp"/>
            <forward name="ApiError" path="/WEB-INF/jsp/apiError.jsp"/>
        </action>

        <!-- страница логина -->
        <action path="/login" type="com.rssl.phizic.web.atm.ext.sbrf.security.LoginNamePasswordATMAction" name="LoginForm" scope="request" validate="false" parameter="unused">
            <forward name="Start" path="/WEB-INF/jsp/startlogin.jsp"/>
            <forward name="Show" path="/WEB-INF/jsp/login.jsp"/>
            <forward name="ApiError" path="/WEB-INF/jsp/apiError.jsp"/>
        </action>

        <!-- отправление оповещения клиенту о входе в систему -->
        <action path="/login/notify"
                type="com.rssl.phizic.web.common.client.ext.sbrf.security.UserLoginNotificationAction"
                name="DummyForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Show" path="/WEB-INF/jsp/null.jsp"/>
        </action>

        <!--Выбор договора, по которому будет работать клиент-->
        <action path="/login/chooseAgreement"
                type="com.rssl.phizic.web.atm.ext.sbrf.security.ChooseDepartmentATMAction"
                name="ChooseDepartmentATMForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Show" path="/WEB-INF/jsp/chooseAgreement.jsp"/>
        </action>

        <!-- информация по банковским продуктам -->
		<action path="/private/products/list"
                type="com.rssl.phizic.web.atm.ext.sbrf.accounts.ShowAccountsExtendedAction"
		        name="ShowAccountsForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Show" path="/WEB-INF/jsp/products/list.jsp"/>
		</action>

        <!-- информация по банковским продуктам -->
        <action path="/private/products/newList"
                type="com.rssl.phizic.web.atm.ext.sbrf.accounts.ShowAccountsNewExtendedAction"
                name="ShowAccountsForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Show" path="/WEB-INF/jsp/products/newList.jsp"/>
        </action>

        <!--детальная информация по картам-->
        <action path="/private/cards/info"
                type="com.rssl.phizic.web.atm.ext.sbrf.cards.ShowCardInfoAction"
                name="ShowCardInfoForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Start" path="/WEB-INF/jsp/products/cards/info.jsp"/>
		</action>

        <!--выписка по картам-->
		<action path="/private/cards/abstract"
                type="com.rssl.phizic.web.atm.ext.sbrf.cards.ShowCardAbstractAction"
		        name="ShowCardInfoForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Start" path="/WEB-INF/jsp/products/cards/abstract.jsp"/>
		</action>

        <!--Просмотр детальной информации по вкладу/счету-->
        <action path="/private/accounts/info"
                type="com.rssl.phizic.web.atm.ext.sbrf.accounts.ShowAccountInfoAction"
                name="ShowAccountInfoForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Show" path="/WEB-INF/jsp/products/accounts/info.jsp"/>
        </action>

        <!--выписка по вкладу-->
		<action path="/private/accounts/abstract"
                type="com.rssl.phizic.web.atm.ext.sbrf.accounts.ShowAccountsAbstractAction"
		        name="ShowAccountAbstractForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Show" path="/WEB-INF/jsp/products/accounts/abstract.jsp"/>
		</action>

        <!-- печать выписки по счетам и картам -->
		<action path="/private/accounts/print"
                type="com.rssl.phizic.web.atm.accounts.PrintAccountAbstractATM"
                name="PrintAccountAbstractForm" scope="request" validate="false" parameter="unused">
			<forward name="PrintAbstract" path="/WEB-INF/jsp/products/cards/abstractPrint.jsp"/>
            <forward name="PrintInformation" path="/WEB-INF/jsp/products/cards/abstractPrint.jsp"/>
		</action>

        <!--Просмотр детальной информации по кредиту-->
        <action path="/private/loans/info"
                type="com.rssl.phizic.web.atm.ext.sbrf.loans.ShowLoanInfoAction"
                name="ShowLoanInfoForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Show" path="/WEB-INF/jsp/products/loans/info.jsp"/>
        </action>

        <!-- Кредитный график -->
		<action path="/private/loans/abstract"
                type="com.rssl.phizic.web.atm.ext.sbrf.loans.ShowLoanAbstractAction"
		        name="ShowLoanInfoForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Show" path="/WEB-INF/jsp/products/loans/abstract.jsp"/>
		</action>

        <!--предложения банка по кредитным продуктам-->
        <action path="/private/payments/loan/cardClaim"
                type="com.rssl.phizic.web.common.client.incomes.ListIncomeLevelAction"
                name="ListIncomeLevelForm"
                scope="request"
                validate="false"
                parameter="unused">

            <forward name="Start"     path="/WEB-INF/jsp/private/creditcards/incomes/list.jsp"/>
            <forward name="Next"      redirect="true" path="/private/payments/loan/cardProduct.do"/>
            <forward name="CardOffer" redirect="true" path="/private/payments/loan/cardOffer.do"/>
            <forward name="Claim"     redirect="true" path="/private/payments/payment.do"/>
        </action>

        <action path="/private/payments/incomeLevel"
                type="com.rssl.phizic.web.common.client.incomes.ListIncomeLevelAction"
                name="ListIncomeLevelForm"
                scope="request"
                validate="false"
                parameter="unused">

            <forward name="Start" path="/WEB-INF/jsp/private/creditcards/incomes/list.jsp"/>
            <forward name="Next"      redirect="true" path="/private/payments/loan/cardProduct.do"/>
            <forward name="CardOffer" redirect="true" path="/private/payments/loan/cardOffer.do"/>
            <forward name="Claim"     redirect="true" path="/private/payments/payment.do"/>
        </action>

        <action path="/private/payments/loan/cardProduct"
                type="com.rssl.phizic.web.common.client.product.LoanCardProductAction"
                name="LoanProductListForm"
                scope="request"
                validate="false"
                parameter="unused">

            <forward name="Start" path="/WEB-INF/jsp/private/loans/loan_card_product.jsp"/>
            <forward name="Next"      redirect="true" path="/private/payments/payment.do"/>
            <forward name="CardOffer" redirect="true" path="/private/payments/loan/cardOffer.do"/>
            <forward name="Close"     redirect="true" path="/private/payments/loan/cardProduct.do"/>
        </action>

        <action path="/private/payments/loan/cardOffer"
                type="com.rssl.phizic.web.common.client.offer.LoanCardOfferAction"
                name="LoanProductListForm"
                scope="request"
                validate="false"
                parameter="unused">

            <forward name="Start" path="/WEB-INF/jsp/private/loans/loan_card.jsp"/>
            <forward name="Payment" redirect="true" path="/private/payments/payment.do"/>
            <forward name="Next"    redirect="true" path="/private/payments/payment.do"/>
        </action>

        <!--Заявка на кредит -->
        <!--Инициализация-->
        <action path="/private/payments/loan/claim"
                type="com.rssl.phizic.web.common.client.product.LoanProductAction"
                name="LoanProductListForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Start" path="/WEB-INF/jsp/private/loans/loan_product.jsp"/>
         </action>
        <!--Шаг выбора условий-->
        <action path="/private/payments/loan/loanProduct"
                type="com.rssl.phizic.web.common.client.product.LoanProductAction"
                name="LoanProductListForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Next" path="/private/payments/payment.do" redirect="true"/>
        </action>

        <!--Заявка на предодобренный кредит -->
        <!--Инициализация-->
        <action path="/private/payments/loan/offerClaim"
                type="com.rssl.phizic.web.common.client.offer.LoanOfferAction"
                name="LoanProductListForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Start" path="/WEB-INF/jsp/private/loans/loan_offer.jsp"/>
         </action>
        <!--Шаг выбора условий-->
        <action path="/private/payments/loan/loanOffer"
                type="com.rssl.phizic.web.common.client.offer.LoanOfferAction"
                name="LoanProductListForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Next" path="/private/payments/payment.do" redirect="true"/>
        </action>

        <!--Получение текста согласия на обработку персональных данных-->
        <action path="/private/loans/personal/agreement"
                type="com.rssl.phizic.web.common.client.offer.ShowLoanAgreementAction"
                name="ShowLoanAgreementForm"
                scope="request"
                validate="false"
                parameter="unused">
            <forward name="Start" path="/WEB-INF/jsp/private/loans/loan_agreement.jsp"/>
        </action>

        <!--выписка по ОМС-->
        <action path="/private/ima/abstract"
                type="com.rssl.phizic.web.common.client.ima.IMAccountInfoAction"
                name="IMAccountInfoForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Start" path="/WEB-INF/jsp/products/imaccounts/abstract.jsp"/>
        </action>

        <!-- Список автоплатежей -->
        <action path="/private/regularpayments/list"
                type="com.rssl.phizic.web.common.client.favourite.ListRegularPaymentsAction"
                name="ListPaymentsForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Start" path="/WEB-INF/jsp/private/regularpayments/list.jsp"/>
        </action>

        <!-- Получение графика автоплатежа -->
        <action path="/private/autopayments/abstract"
                type="com.rssl.phizic.web.atm.autopayments.ShowAutoPaymentInfoATMAction"
                name="ShowAutoPaymentInfoATMForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Start" path="/WEB-INF/jsp/private/autopayments/abstract.jsp"/>
        </action>

