        <action path="/private/autopayments/info"
                type="com.rssl.phizic.web.common.client.autopayments.ShowAutoPaymentInfoAction"
                name="ShowAutoPaymentInfoForm"
                scope="request" validate="false" parameter="unused">
            <forward name="Start" path="/WEB-INF/jsp/private/autopayments/info.jsp"/>
