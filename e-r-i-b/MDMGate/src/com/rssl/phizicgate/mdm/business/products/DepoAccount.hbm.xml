<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.rssl.phizicgate.mdm.business.products.DepoAccount" table="MDM_PROFILE_DEPO_ACCOUNTS">
        <id name="id" type="long" column="ID">
            <generator class="native">
                <param name="sequence">S_MDM_PROFILE_DEPO_ACCOUNTS</param>
            </generator>
        </id>

        <property name="profileId"  type="long"     column="PROFILE_ID"     not-null="true" update="false"/>
        <property name="number"     type="string"   column="PRODUCT_NUMBER" not-null="true" update="false"/>
    </class>

</hibernate-mapping>