    <!--Тег для вывода на страницу ошибок валидации, сохраненных в реквесте-->
    <tag>
        <name>messages</name>
        <tag-class>com.rssl.phizic.web.tags.MessagesTag</tag-class>
        <body-content>JSP</body-content>
        <attribute>
            <!--Имя переменной для сохранения текста сообщения-->
            <name>id</name>
            <required>true</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <name>bundle</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <!-- Имя переменной для сохранения поля, в котором произошла ошибка-->
            <name>field</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <attribute>
            <!--ключ для поиска сообщений в реквесте (ключи заданы енумом ActionMessagesKeys)-->
            <name>message</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <!--заголовок блока ошибок(содержится в <title></title> сообщения)-->
            <name>title</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
        <!--Костыльно решение. Требуется переделать-->
        <attribute>
            <!--Нужно ли вытаскивать сообщения из сессии-->
            <name>showSessionMessages</name>
            <required>false</required>
            <rtexprvalue>false</rtexprvalue>
        </attribute>
    </tag>

<!-- Тег для вывода списка операций со скритием под выподающим списком "еще операции" -->
<tag>
    <name>linksList</name>
    <description>Тег для вывода списка операций со скритием под выподающим списком "еще операции"</description>
    <tag-class>com.rssl.phizic.web.tags.LinksListTag</tag-class>
    <attribute>
        <name>num</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
        <description>Количество после которого ссылки скрываются</description>
    </attribute>
    <attribute>
        <name>styleClass</name>
        <required>false</required>
        <rtexprvalue>false</rtexprvalue>
        <description>Имя класса для списка</description>
    </attribute>
    <attribute>
        <name>title</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
        <description>Заголовок списка</description>
    </attribute>
        <attribute>
        <name>titleStyleClass</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
        <description>Имя класса для заголовка списка</description>
    </attribute>
    <attribute>
        <name>listSourceName</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
        <description>Имя ресурса для которого будет строиться список</description>
    </attribute>
    <attribute>
        <name>image</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
        <description>Дополнительная картинка для списка</description>
    </attribute>
</tag>

