        newDate = "00-00-"+tmp;
     elif (kindTerm == "2")
        if (term < 10)
          newDate = "00-0"+term+"-000";
        else
          newDate = "00-"+term+"-000";
        end;
     elif (kindTerm == "3")
        term = term*3;
        if (term < 10)
          newDate = "00-0"+term+"-000";
        else
          newDate = "00-"+term+"-000";
        end;
     elif (kindTerm == "4")
        if (term < 10)
          newDate = "0"+term+"-00-000";
        else
          newDate = term+"-00-000";
        end;
     end;
     return newDate;
  end;
