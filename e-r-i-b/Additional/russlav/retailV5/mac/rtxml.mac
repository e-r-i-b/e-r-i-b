private macro GetDepositList( inXml, outXml )
  var f_sb_dtyp  = Tbfile( "sb_dtyp.dbt",  "r", 0, null, "sbbank.def" );
  var f_currency = TbFile( "currency.dbt", "r", 0, null, "sbbank.def" );
