      cont = ( ( next( of_sbdepdoc )       == true                    ) and
               ( of_sbdepdoc.Referenc      == f_depositr.rec.Referenc ) );
     end;
  end;

  iter = 1;

  while ((iter <= number) and (iter <= data.size))
	returnData[returnData.size()] = CData();
	returnData[iter -1] = data[data.size - iter];
	iter = iter + 1;
  end;	


  return returnData;
end;

