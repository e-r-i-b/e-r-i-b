             (Global_f_pc_rate.rec.Code_Currency == CodeCur                      )  
           )
        if( Global_f_pc_rate.rec.Action != 2 ) // ­¥ ã¤ «¥­ 
          // ¢á¥ ­¨èâïª, ¢áñ ­ è«¨, ¢®§¢à é ¥¬ true,Global_f_pc_rate, ¨ à ¤ã¥¬áï ¦¨§­¨ =) 
          SetParm(4, Global_f_pc_rate);
