      stat = Global_f_pc_rate.getLE();
      while( (stat                                                      ) and
             (Global_f_pc_rate.rec.FNCash        == Global_f_fnc_dtyp.rec.PcAlgBrRefer) and
             (Global_f_pc_rate.rec.FlagCur       == FlagCur                      ) and
             (Global_f_pc_rate.rec.ObjectType    == RTPC_ACCOUNT_TYPE            ) and
