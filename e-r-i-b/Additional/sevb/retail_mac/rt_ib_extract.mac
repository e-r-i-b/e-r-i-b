macro GetSubOper
  var SubOp;
  if ( ( set.value("T_TypeOper") != 0 )  AND  ( set.value("T_ApplType") != 0 ) )
    sop.rec.IsCur    = set.value("T_IsCur");
    sop.rec.Kind     = set.value("T_Type_Account");
