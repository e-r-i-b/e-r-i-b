macro GetQuart

  var
    Q,
    Year,
    Mon;

  DateSplit( doc.rec.Date_Document, null, Mon, Year );

  if ( Mon == 3 )
    Q = 1;
  elif ( Mon == 6 )
    Q = 2;
  elif ( Mon == 9 )
    Q = 3;
  else
    Q = 4;
  end;
  return Q + "-й квартал " + Year + " года";
end;

/*****************************************************************
               Процедуры для работы с xml "вручную"
*****************************************************************/

macro redirectOutputToFile(extFile)
   setoutput(extFile, false);
   open(extFile);
   fileIsOpen = true;
end;

macro closeOutputFile(extFile)
   close(extFile);
   setoutput(null, true);
   fileIsOpen = false;
end;

macro startTag(name)
/* открывающий тэг */
   print("<"+name+">");
end;

macro endTag(name)
/* закрывающий тэг */
   print("</"+name+">");
end;

macro createTagWithData(name, value)
/* тэг с данными */
   startTag(name);
   print(value);
   endTag(name);
end;

macro startXml(extFile)
/* создание нового xml-файла */
   redirectOutputToFile(extFile);
   startTag("response");

   createTagWithData("parentId", ParentId);
   createTagWithData("id", xmlId);
end;

macro endXml(extFile)
/* закрытие xml-файла */
   endTag("response");
   closeOutputFile(extFile);
end;

macro createErrorXml(extFile, mes)
/* xml-файл с ответом-ошибкой */
   if (fileIsOpen)
      closeOutputFile(extFile);
   end;
   startXml(extFile);
   startTag("error_a");

   createTagWithData("code", "ERROR_VALIDATE");
   createTagWithData("message", String(mes));

   endTag("error_a");
   endXml(extFile);
end;

macro processFile(extFile)
/* обработка результата успешного выполнения макроса */
   var outXml = "";
   File extractFile (extFile) txt;
   if (open(extractFile, extFile))
      while (next(extractFile))
         outXml = outXml + extractFile.str;
      end;
      close(extractFile);
   end;
   return outXml;
end;

