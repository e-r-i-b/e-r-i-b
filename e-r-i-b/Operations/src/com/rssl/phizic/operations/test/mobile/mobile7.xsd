                                <xsd:sequence>
                                    <xsd:element name="eventType" type="Field"/>
                                    <xsd:element name="startDate" type="Field"/>
                                    <xsd:element name="amount" type="Field"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="commission" type="Field" minOccurs="0"/>
                        <xsd:element name="commissionCurrency" type="Field" minOccurs="0"/>
                        <xsd:element name="isWithCommission" type="Field" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

