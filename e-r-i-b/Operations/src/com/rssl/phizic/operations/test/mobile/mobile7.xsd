    <xsd:complexType name="InitAirlineReservationPayment">
        <xsd:annotation>
            <xsd:documentation>Оплата брони авиабилетов</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="documentNumber" type="Field"/>
            <xsd:element name="documentDate" type="Field"/>
            <xsd:element name="receiverName" type="Field"/>
            <xsd:element name="fromResource" type="Field"/>
            <xsd:element name="paymentDetails">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="amount" type="Field"/>
                        <xsd:element name="currency" type="Field"/>
                        <xsd:element name="recIdentifier" type="Field"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="reservationInfo">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="reservationId" type="Field"/>
                        <xsd:element name="reservExpirationDate" type="Field" minOccurs="0"/>
                        <xsd:element name="countPassengers" type="Field"/>
                        <xsd:element name="countRoutes" type="Field"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="passengersInfo">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="passenger" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="firstName" type="Field"/>
                                    <xsd:element name="lastName" type="Field"/>
                                    <xsd:element name="type" type="Field"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="routesInfo">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="route" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="departure">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="flight" type="Field"/>
                                                <xsd:element name="dateTime" type="Field"/>
                                                <xsd:element name="location" type="Field" minOccurs="0"/>
                                                <xsd:element name="airport" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="arrival">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="flight" type="Field" minOccurs="0"/>
                                                <xsd:element name="dateTime" type="Field"/>
                                                <xsd:element name="location" type="Field" minOccurs="0"/>
                                                <xsd:element name="airport" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="InitAccountChangeInterestDestinationClaim">
        <xsd:annotation>
            <xsd:documentation>Изменение порядка уплаты процентов по вкладу</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="documentNumber" type="Field"/>
            <xsd:element name="documentDate" type="Field"/>
            <xsd:element name="accountId" type="Field"/>
            <xsd:element name="percentTransferSource" type="Field"/>
            <xsd:element name="percentTransferCardSource" type="Field"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Document">
        <xsd:sequence>
            <xsd:element name="form">
                <xsd:simpleType>
                    <xsd:restriction base="string">
                        <xsd:enumeration value="RurPayJurSB"/>
                        <xsd:enumeration value="JurPayment"/>
                        <xsd:enumeration value="LoanPayment"/>
                        <xsd:enumeration value="LoanPaymentLongOffer"/>
                        <xsd:enumeration value="InternalPayment"/>
						<xsd:enumeration value="InternalPaymentLongOffer"/>
                        <xsd:enumeration value="IMAPayment"/>
                        <xsd:enumeration value="AccountClosingPayment"/>
                        <xsd:enumeration value="BlockingCardClaim"/>
                        <xsd:enumeration value="AccountOpeningClaim"/>
                        <xsd:enumeration value="RurPayment"/>
                        <xsd:enumeration value="RurPaymentLongOffer"/>
                        <xsd:enumeration value="LoanCardProduct"/>
                        <xsd:enumeration value="LoanCardOffer"/>
                        <xsd:enumeration value="LoanProduct"/>
                        <xsd:enumeration value="LoanOffer"/>
                        <xsd:enumeration value="IMAOpeningClaim"/>
                        <xsd:enumeration value="CreateAutoPaymentPayment"/>
                        <xsd:enumeration value="EditAutoPaymentPayment"/>
                        <xsd:enumeration value="RefuseAutoPaymentPayment"/>
                        <xsd:enumeration value="CreateAutoSubscriptionPayment"/>
                        <xsd:enumeration value="EditAutoSubscriptionPayment"/>
                        <xsd:enumeration value="DelayAutoSubscriptionPayment"/>
                        <xsd:enumeration value="RecoveryAutoSubscriptionPayment"/>
                        <xsd:enumeration value="CloseAutoSubscriptionPayment"/>
                        <xsd:enumeration value="RefuseLongOffer"/>
                        <xsd:enumeration value="ExternalProviderPayment"/>
                        <xsd:enumeration value="AirlineReservationPayment"/>
                        <xsd:enumeration value="AccountChangeInterestDestinationClaim"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="status" type="PaymentState"/>
            <xsd:element name="id" type="xsd:integer" minOccurs="0"/>
            <xsd:choice>
                <xsd:element name="RurPayJurSBDocument" type="DocRurPayJurSB"/>
                <xsd:element name="JurPaymentDocument" type="DocJurPayment"/>
                <xsd:element name="LoanPaymentDocument" type="DocLoanPayment"/>
                <xsd:element name="LoanPaymentLongOfferDocument" type="DocLoanPaymentLongOffer"/>
                <xsd:element name="BlockingCardClaimDocument" type="DocBlockingCardClaim"/>
                <xsd:element name="InternalPaymentDocument" type="DocInternalPayment"/>
				<xsd:element name="InternalPaymentLongOfferDocument" type="DocInternalPaymentLongOffer"/>
                <xsd:element name="RurPaymentDocument" type="DocRurPayment"/>
                <xsd:element name="RurPaymentLongOfferDocument" type="DocRurPaymentLongOffer"/>
                <xsd:element name="AccountOpeningClaimDocument" type="DocAccountOpeningClaim"/>
                <xsd:element name="AccountClosingPaymentDocument" type="DocAccountClosingPayment"/>
                <xsd:element name="IMAPaymentDocument" type="DocIMAPayment"/>
                <xsd:element name="IMAOpeningClaimDocument" type="DocIMAOpeningClaim"/>
                <xsd:element name="LoanCardProductDocument" type="DocLoanCardProduct"/>
                <xsd:element name="LoanCardOfferDocument" type="DocLoanCardOffer"/>
                <xsd:element name="LoanProductDocument" type="DocLoanProduct"/>
                <xsd:element name="LoanOfferDocument" type="DocLoanOffer"/>
                <xsd:element name="CreateAutoPaymentDocument" type="DocCreateAutoPaymentPayment"/>
                <xsd:element name="EditAutoPaymentDocument" type="DocEditAutoPaymentPayment"/>
                <xsd:element name="RefuseAutoPaymentDocument" type="DocRefuseAutoPaymentPayment"/>
                <xsd:element name="CreateAutoSubscriptionDocument" type="DocCreateAutoSubscriptionPayment"/>
                <xsd:element name="EditAutoSubscriptionDocument" type="DocEditAutoSubscriptionPayment"/>
                <xsd:element name="DelayAutoSubscriptionDocument" type="AutoSubscriptionPaymentFields"/>
                <xsd:element name="RecoveryAutoSubscriptionDocument" type="AutoSubscriptionPaymentFields"/>
                <xsd:element name="CloseAutoSubscriptionDocument" type="AutoSubscriptionPaymentFields"/>
                <xsd:element name="RefuseLongOfferDocument" type="DocRefuseLongOffer"/>
                <xsd:element name="ExternalProviderPaymentDocument" type="DocExternalProviderPayment"/>
                <xsd:element name="AirlineReservationPaymentDocument" type="DocAirlineReservationPayment"/>
                <xsd:element name="AccountChangeInterestDestinationClaimDocument" type="DocAccountChangeInterestDestinationClaim"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

