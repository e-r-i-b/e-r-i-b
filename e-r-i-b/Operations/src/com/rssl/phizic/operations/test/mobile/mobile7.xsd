                                <xsd:sequence>
                                    <xsd:element name="ticketNumber" type="Field" maxOccurs="unbounded"/>
                                    <xsd:element name="itineraryUrl" type="Field" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="interval" type="Field"/>
                        <xsd:element name="timeout" type="Field"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="Field">
        <xsd:sequence>
            <xsd:element name="name" type="string"/>
            <xsd:element name="title" type="string" minOccurs="0"/>
            <xsd:element name="description" type="string" minOccurs="0"/>
            <xsd:element name="hint" type="string" minOccurs="0"/>
            <xsd:element name="type">
                <xsd:simpleType>
                    <xsd:restriction base="string">
                        <xsd:enumeration value="list"/>
                        <xsd:enumeration value="set"/>
                        <xsd:enumeration value="date"/>
                        <xsd:enumeration value="number"/>
                        <xsd:enumeration value="string"/>
                        <xsd:enumeration value="money"/>
                        <xsd:enumeration value="calendar"/>
                        <xsd:enumeration value="integer"/>
                        <xsd:enumeration value="resource"/>
                        <xsd:enumeration value="boolean"/>
                        <xsd:enumeration value="agreement"/>
                        <xsd:enumeration value="link"/>
                        <xsd:enumeration value="places"/>
                        <xsd:enumeration value="dict"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="subType" type="string" minOccurs="0"/>
            <xsd:element name="maxLength" type="xsd:positiveInteger" minOccurs="0"/>
            <xsd:element name="minLength" type="xsd:positiveInteger" minOccurs="0"/>
            <xsd:element name="required" type="xsd:boolean"/>
            <xsd:element name="editable" type="xsd:boolean"/>
            <xsd:element name="visible" type="xsd:boolean"/>
            <xsd:element name="isSum" type="xsd:boolean" minOccurs="0"/>
            <xsd:choice>
                <xsd:element name="listType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="availableValues" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="valueItem" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="value" type="string"/>
                                                    <xsd:element name="title" type="string"/>
                                                    <xsd:element name="selected" type="xsd:boolean"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="setType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="availableValues" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="valueItem" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="value" type="string"/>
                                                    <xsd:element name="title" type="string"/>
                                                    <xsd:element name="selected" type="xsd:boolean"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="dateType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" type="string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="numberType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" type="xsd:decimal" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="stringType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" minOccurs="0">
                                <xsd:simpleType>
                                    <xsd:restriction base="string"/>
                                </xsd:simpleType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="moneyType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" type="string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="calendarType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" type="string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="integerType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" minOccurs="0">
                                <xsd:simpleType>
                                    <xsd:restriction base="string">
                                        <xsd:pattern value="[\-+]?[0-9]+"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="resourceType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="availableValues" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="valueItem" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="value" type="Resource"/>
                                                    <xsd:element name="selected" type="xsd:boolean"/>
                                                    <xsd:element name="displayedValue" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="currency" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="booleanType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" type="string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="agreementType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" type="string" minOccurs="0"/>
                            <xsd:element name="agreementId" type="string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="linkType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="text" type="string" minOccurs="0"/>
                            <xsd:element name="url" type="string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="placesType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="maxCount" type="string" minOccurs="0"/>
                            <xsd:element name="places" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="place" minOccurs="0" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="number" type="string"/>
                                                    <xsd:element name="state" type="string"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="value" type="string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="dictType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="changed" type="xsd:boolean"/>
            <xsd:element name="dictFields" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="fieldDictType">
                            <xsd:simpleType>
                                <xsd:restriction base="string">
                                    <xsd:enumeration value="phone"/>
                                    <xsd:enumeration value="wallet"/>
                                    <xsd:enumeration value="ourCard"/>
                                    <xsd:enumeration value="ourAccount"/>
                                    <xsd:enumeration value="externalAccount"/>
                                    <xsd:enumeration value="masterCardExternalCard"/>
                                    <xsd:enumeration value="visaExternalCard"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="fieldInfoType">
                            <xsd:simpleType>
                                <xsd:restriction base="string">
                                    <xsd:enumeration value="phone"/>
                                    <xsd:enumeration value="wallet"/>
                                    <xsd:enumeration value="ourCard"/>
                                    <xsd:enumeration value="ourAccount"/>
                                    <xsd:enumeration value="externalAccount"/>
                                    <xsd:enumeration value="masterCardExternalCard"/>
                                    <xsd:enumeration value="visaExternalCard"/>
                                    <xsd:enumeration value="provider"/>
                                    <xsd:enumeration value="service"/>
                                    <xsd:enumeration value="billing"/>
                                    <xsd:enumeration value="surName"/>
                                    <xsd:enumeration value="firstName"/>
                                    <xsd:enumeration value="patrName"/>
                                    <xsd:enumeration value="BIC"/>
                                    <xsd:enumeration value="BankName"/>
                                    <xsd:enumeration value="selfPhone"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="dictTypeFieldName" type="string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="Resource">
        <xsd:restriction base="string">
            <xsd:pattern value="(card|account|loan|im-account)\:(\d{1,})"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="CardState">
        <xsd:restriction base="string">
            <xsd:enumeration value="active"/>
            <xsd:enumeration value="closed"/>
            <xsd:enumeration value="replenishment"/>
            <xsd:enumeration value="ordered"/>
            <xsd:enumeration value="delivery"/>
            <xsd:enumeration value="blocked"/>
            <xsd:enumeration value="changing"/>
            <xsd:enumeration value="unknown"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="TempStatus">
        <xsd:restriction base="string">
            <xsd:enumeration value="active"/>
            <xsd:enumeration value="inactive"/>
            <xsd:enumeration value="error"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="ConfirmType">
        <xsd:sequence>
            <xsd:element name="strategy" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="type">
                            <xsd:simpleType>
                                <xsd:restriction base="string">
                                    <xsd:enumeration value="cardp"/>
                                    <xsd:enumeration value="smsp"/>
                                    <xsd:enumeration value="none"/>
                                    <xsd:enumeration value="deny"/>
                                    <xsd:enumeration value="pushp"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="order" type="xsd:int"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ConfirmInfo">
        <xsd:sequence>
            <xsd:element name="type">
                <xsd:simpleType>
                    <xsd:restriction base="string">
                        <xsd:enumeration value="cardp"/>
                        <xsd:enumeration value="smsp"/>
                        <xsd:enumeration value="pushp"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="cardp" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="passwordNumber" type="string"/>
                        <xsd:element name="cardNumber" type="string"/>
                        <xsd:element name="passwordsLeft" type="xsd:unsignedShort" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="smsp" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="lifeTime" type="xsd:unsignedShort"/>
                        <xsd:element name="attemptsRemain" type="xsd:unsignedByte"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="pushp" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="lifeTime" type="xsd:unsignedShort"/>
                        <xsd:element name="attemptsRemain" type="xsd:unsignedByte"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ConfirmStage">
        <xsd:sequence>
            <xsd:element name="confirmType" type="ConfirmType"/>
            <xsd:element name="confirmInfo" type="ConfirmInfo" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="PaymentState">
        <xsd:restriction base="string">
            <xsd:enumeration value="INITIAL"/>
            <xsd:enumeration value="DRAFT"/>
            <xsd:enumeration value="SAVED"/>
            <xsd:enumeration value="EXECUTED"/>
            <xsd:enumeration value="REFUSED"/>
            <xsd:enumeration value="RECALLED"/>
            <xsd:enumeration value="ERROR"/>
            <xsd:enumeration value="DELAYED_DISPATCH"/>
            <xsd:enumeration value="DISPATCHED"/>
            <xsd:enumeration value="INITIAL"/>
            <xsd:enumeration value="WAIT_CONFIRM"/>
            <xsd:enumeration value="UNKNOW"/>
            <xsd:enumeration value="SUCCESSED"/>
            <xsd:enumeration value="PARTLY_EXECUTED"/>
            <xsd:enumeration value="DRAFTTEMPLATE"/>
            <xsd:enumeration value="SAVED_TEMPLATE"/>
            <xsd:enumeration value="TEMPLATE"/>
            <xsd:enumeration value="WAIT_CONFIRM_TEMPLATE"/>
            <xsd:enumeration value="DELETED"/>
            <xsd:enumeration value="STATEMENT_READY"/>
            <xsd:enumeration value="ADOPTED"/>
            <xsd:enumeration value="INITIAL_LONG_OFFER"/>
            <xsd:enumeration value="OFFLINE_SAVED"/>
            <xsd:enumeration value="OFFLINE_DELAYED"/>
            <xsd:enumeration value="BILLING_CONFIRM_TIMEOUT"/>
            <xsd:enumeration value="BILLING_GATE_CONFIRM_TIMEOUT"/>
            <xsd:enumeration value="ABS_RECALL_TIMEOUT"/>
            <xsd:enumeration value="ABS_GATE_RECALL_TIMEOUT"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="Status">
        <xsd:sequence>
            <xsd:element name="code">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:int">
                        <xsd:pattern value="0|1|2|3|4|8"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="errors" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="error" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="text" type="string"/>
                                    <xsd:element name="elementId" type="string" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="warnings" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="warning" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="text" type="string"/>
                                    <xsd:element name="elementId" type="string" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="TransactionToken">
        <xsd:restriction base="xsd:normalizedString"/>
    </xsd:simpleType>

    <xsd:simpleType name="string">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:whiteSpace value="collapse"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="Money">
        <xsd:sequence>
            <xsd:element name="amount">
                <xsd:simpleType>
                    <xsd:restriction base="string">
                        <xsd:pattern value="(\+|-|)(\d{1,})(\.\d{1,}|)"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="currency">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="code" type="string"/>
                        <xsd:element name="name" type="string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="Amount">
        <xsd:restriction base="string">
            <xsd:pattern value="(\d{1,})(\.\d{1,}|)"/>
        </xsd:restriction>
    </xsd:simpleType>

</xsd:schema>
