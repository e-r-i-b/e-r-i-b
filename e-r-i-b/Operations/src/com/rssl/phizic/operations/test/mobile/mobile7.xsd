                </xsd:complexType>
            </xsd:element>
            <xsd:element name="autoSubParameters">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="name" type="Field"/>
                        <xsd:element name="type" type="Field"/>
                        <xsd:element name="always" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="eventType" type="Field"/>
                                    <xsd:element name="nextPayDate" type="Field" minOccurs="0"/>
                                    <xsd:element name="amount" type="Field"/>
