    <xsd:complexType name="DocRurPayment">
        <xsd:annotation>
            <xsd:documentation>Перевод частному лицу</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="documentNumber" type="Field"/>
            <xsd:element name="documentDate" type="Field"/>
            <xsd:element name="receiverAccount" type="Field" minOccurs="0"/>
            <xsd:element name="receiverPhone" type="Field" minOccurs="0"/>
            <xsd:element name="receiverName" type="Field" minOccurs="0"/>
            <xsd:element name="receiverINN" type="Field" minOccurs="0"/>
            <xsd:element name="receiverAddress" type="Field" minOccurs="0"/>
            <xsd:element name="bankInfo" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="bank" type="Field"/>
                        <xsd:element name="receiverBIC" type="Field"/>
                        <xsd:element name="receiverCorAccount" type="Field"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="ground" type="Field" minOccurs="0"/>
            <xsd:element name="fromResource" type="Field"/>
            <xsd:element name="sellAmount" type="Field" minOccurs="0"/>
            <xsd:element name="sellCurrency" type="Field" minOccurs="0"/>
            <xsd:element name="buyAmount" type="Field" minOccurs="0"/>
