                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="account" type="Field"/>
                                    <xsd:element name="accountCurrency" type="Field"/>
                                    <xsd:element name="INN" type="Field"/>
                                    <xsd:element name="KPP" type="Field"/>
                                    <xsd:element name="bank">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="name" type="Field"/>
