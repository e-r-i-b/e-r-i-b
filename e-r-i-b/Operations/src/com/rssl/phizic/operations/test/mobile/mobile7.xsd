                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="item" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="name" type="string" minOccurs="0"/>
                                    <xsd:element name="description" type="string" minOccurs="0"/>
                                    <xsd:element name="count" type="xsd:integer" minOccurs="0"/>
                                    <xsd:element name="price" type="Money" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
