            <xsd:element name="amount" type="Field"/>
            <xsd:element name="currency" type="Field"/>
            <xsd:element name="recIdentifier" type="Field"/>
            <xsd:element name="itemList" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="item" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="name" type="string" minOccurs="0"/>
                                    <xsd:element name="description" type="string" minOccurs="0"/>
                                    <xsd:element name="count" type="xsd:integer" minOccurs="0"/>
                                    <xsd:element name="price" type="Money" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="refundList" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="refund" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="itemList" minOccurs="0">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="item" maxOccurs="unbounded">
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="name" type="string" minOccurs="0"/>
                                                            <xsd:element name="description" type="string" minOccurs="0"/>
                                                            <xsd:element name="count" type="xsd:integer" minOccurs="0"/>
                                                            <xsd:element name="price" type="Money" minOccurs="0"/>
                                                        </xsd:sequence>
