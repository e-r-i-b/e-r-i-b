                        <xsd:enumeration value="RECEIPT"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="DocRurPaymentLongOffer">
        <xsd:annotation>
            <xsd:documentation>Перевод частному лицу: создание автоплатежа</xsd:documentation>
