                                <xsd:sequence>
                                    <xsd:element name="INN" type="Field"/>
                                    <xsd:element name="account" type="Field"/>
                                    <xsd:element name="bank">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="name" type="Field"/>
                                                <xsd:element name="BIC" type="Field"/>
                                                <xsd:element name="corAccount" type="Field" minOccurs="0"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
