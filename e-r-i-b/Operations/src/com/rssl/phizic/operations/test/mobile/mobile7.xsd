        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="documentNumber" type="Field"/>
            <xsd:element name="documentDate" type="Field"/>
            <xsd:element name="offerType" type="Field"/>
            <xsd:element name="creditCard" type="Field"/>
            <xsd:element name="interestRate" type="Field" minOccurs="0"/>
            <xsd:element name="firstYearPayment" type="Field" minOccurs="0"/>
            <xsd:element name="firstYearPaymentCurrency" type="Field" minOccurs="0"/>
            <xsd:element name="nextYearPayment" type="Field" minOccurs="0"/>
            <xsd:element name="nextYearPaymentCurrency" type="Field" minOccurs="0"/>
            <xsd:element name="amount" type="Field"/>
            <xsd:element name="currency" type="Field"/>
