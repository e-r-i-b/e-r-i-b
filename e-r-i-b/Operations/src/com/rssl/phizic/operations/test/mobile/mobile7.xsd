            <xsd:element name="autoPaymentTotalAmountCurrency" type="Field" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="autoPaymentStartDate" type="Field" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="autoPaymentName" type="Field"/>
