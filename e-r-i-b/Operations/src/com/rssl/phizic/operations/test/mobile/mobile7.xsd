            </xsd:element>
            <xsd:element name="paymentDetails">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="ground" type="Field"/>
                        <xsd:element name="fromResource" type="Field"/>
                        <xsd:element name="sellAmount" type="Field" minOccurs="0"/>
                        <xsd:element name="percent" type="Field" minOccurs="0"/>
                        <xsd:element name="buyAmount" type="Field" minOccurs="0"/>
                        <xsd:element name="buyAmountCurrency" type="Field" minOccurs="0"/>
                        <xsd:element name="commission" type="Field" minOccurs="0"/>
                        <xsd:element name="operationCode" type="Field" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="longOfferDetails">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="startDate" type="Field"/>
                        <xsd:element name="endDate" type="Field"/>
                        <xsd:element name="eventType" type="Field"/>
                        <xsd:element name="firstPaymentDate" type="Field"/>
                        <xsd:element name="priority" type="Field"/>
                        <xsd:element name="sumType" type="Field"/>
                        <xsd:element name="isSumModify" type="Field"/>
                        <xsd:element name="payDayDescription" type="Field" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
