            <xsd:element name="documentDate" type="Field"/>
            <xsd:element name="fromResource" type="Field"/>
            <xsd:element name="toResource" type="Field"/>
            <xsd:element name="course" type="Field" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="standartCourse" type="Field" minOccurs="0"/>
            <xsd:element name="gain" type="Field" minOccurs="0"/>
            <xsd:element name="closingDate" type="Field"/>
            <xsd:element name="chargeOffAmount" type="Field"/>
            <xsd:element name="destinationAmount" type="Field" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="writeDownOperations" type="WriteDownOperations" minOccurs="0"/>
            <xsd:element name="operationCode" type="Field" minOccurs="0" maxOccurs="1"/>
