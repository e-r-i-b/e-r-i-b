                        <xsd:element name="buyAmount" type="Field" minOccurs="0"/>
                        <xsd:element name="percent" type="Field" minOccurs="0"/>
                        <xsd:element name="course" type="Field" minOccurs="0"/>
                        <xsd:element name="standartCourse" type="Field" minOccurs="0"/>
                        <xsd:element name="gain" type="Field" minOccurs="0"/>
                        <xsd:element name="commission" type="Money" minOccurs="0"/>
                        <xsd:element name="operationCode" type="Field" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="longOfferDetails">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="startDate" type="Field"/>
                        <xsd:element name="endDate" type="Field"/>
