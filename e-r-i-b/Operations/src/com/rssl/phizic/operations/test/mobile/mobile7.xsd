            <xsd:element name="currency" type="Field"/>
            <xsd:element name="surName" type="Field"/>
            <xsd:element name="firstName" type="Field"/>
            <xsd:element name="patrName" type="Field"/>
            <xsd:element name="homePhone" type="Field"/>
            <xsd:element name="workPhone" type="Field"/>
