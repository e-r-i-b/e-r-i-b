                        <xsd:element name="updateDate" type="Field" minOccurs="0"/>
                        <xsd:element name="name" type="Field"/>
                        <xsd:element name="type" type="Field"/>
                        <xsd:element name="always" minOccurs="0">
                            <xsd:complexType>
