    <xsd:complexType name="DocAirlineReservationPayment">
        <xsd:annotation>
            <xsd:documentation>Оплата брони авиабилетов</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="documentNumber" type="Field"/>
            <xsd:element name="documentDate" type="Field"/>
            <xsd:element name="receiverName" type="Field"/>
            <xsd:element name="fromResource" type="Field"/>
            <xsd:element name="paymentDetails">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="amount" type="Field"/>
                        <xsd:element name="currency" type="Field"/>
                        <xsd:element name="recIdentifier" type="Field"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="reservationInfo">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="reservationId" type="Field"/>
                        <xsd:element name="reservExpirationDate" type="Field" minOccurs="0"/>
                        <xsd:element name="countPassengers" type="Field"/>
                        <xsd:element name="countRoutes" type="Field"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="passengersInfo">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="passenger" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="firstName" type="Field"/>
                                    <xsd:element name="lastName" type="Field"/>
                                    <xsd:element name="type" type="Field"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="routesInfo">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="route" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="departure">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="flight" type="Field"/>
                                                <xsd:element name="dateTime" type="Field"/>
                                                <xsd:element name="location" type="Field" minOccurs="0"/>
                                                <xsd:element name="airport" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="arrival">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="flight" type="Field" minOccurs="0"/>
                                                <xsd:element name="dateTime" type="Field"/>
                                                <xsd:element name="location" type="Field" minOccurs="0"/>
                                                <xsd:element name="airport" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="ticketsInfo" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="ticketsStatus" type="Field"/>
                        <xsd:element name="ticketsStatusDescription" type="Field"/>
                        <xsd:element name="issueInfo" minOccurs="0">
