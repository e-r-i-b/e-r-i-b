                        <xsd:element name="inn" type="Field" minOccurs="0"/>
                        <xsd:element name="account" type="Field" minOccurs="0"/>
                        <xsd:element name="bank" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="name" type="Field"/>
                                    <xsd:element name="bic" type="Field"/>
                                    <xsd:element name="corAccount" type="Field" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="fromResource" type="Field"/>
