    <xsd:complexType name="DocCreateAutoPaymentPayment">
        <xsd:annotation>
            <xsd:documentation>Создание автоплатежа iqwave</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="receiver">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="description" type="Field" minOccurs="0"/>
                        <xsd:element name="name" type="Field" minOccurs="0"/>
                        <xsd:element name="serviceName" type="Field" minOccurs="0"/>
                        <xsd:element name="bankDetails" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="account" type="Field"/>
                                    <xsd:element name="accountCurrency" type="Field"/>
                                    <xsd:element name="INN" type="Field"/>
                                    <xsd:element name="KPP" type="Field"/>
                                    <xsd:element name="bank">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="name" type="Field"/>
                                                <xsd:element name="BIC" type="Field"/>
                                                <xsd:element name="corAccount" type="Field" minOccurs="0"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="fromResource" type="Field"/>
            <xsd:element name="paymentDetails">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="externalFields" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="field" type="Field" maxOccurs="unbounded"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="operationCode" type="Field" minOccurs="0"/>
                        <xsd:element name="commission" type="Money" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="autoPaymentParameters">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="autoPaymentType" type="Field"/>
                        <xsd:element name="onceInYear" minOccurs="0" maxOccurs="1">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="autoPaymentStartDate" type="Field"/>
                                    <xsd:element name="month" type="Field"/>
                                    <xsd:element name="firstPaymentDate" type="Field" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="onceInQuarter" minOccurs="0" maxOccurs="1">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="autoPaymentStartDate" type="Field"/>
                                    <xsd:element name="months" type="Field"/>
                                    <xsd:element name="firstPaymentDate" type="Field" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="onceInMonth" minOccurs="0" maxOccurs="1">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="autoPaymentStartDate" type="Field"/>
                                    <xsd:element name="firstPaymentDate" type="Field" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="reduseOfBalance" minOccurs="0" maxOccurs="1">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="floorLimit" type="Field"/>
                                    <xsd:element name="autoPaymentTotalAmountLimit" type="Field" minOccurs="0" maxOccurs="1"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="byInvoice" minOccurs="0" maxOccurs="1">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="maxSum" type="Field"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="autoPaymentName" type="Field"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="DocEditAutoPaymentPayment">
        <xsd:annotation>
            <xsd:documentation>Редактирование автоплатежа iqwave</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="receiverName" type="Field"/>
            <xsd:element name="fromResource" type="Field"/>
            <xsd:element name="requisiteName" type="Field"/>
            <xsd:element name="requisite" type="Field"/>
            <xsd:element name="amount" type="Field" minOccurs="0"/>
            <xsd:element name="autoPaymentParameters">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="executionEventType" type="Field"/>
                        <xsd:element name="periodic" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="autoPaymentStartDate" type="Field"/>
                                    <xsd:element name="firstPaymentDate" type="Field"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="reduseOfBalance" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="autoPaymentFloorLimit" type="Field"/>
                                    <xsd:element name="autoPaymentTotalAmountLimit" type="Field" minOccurs="0" maxOccurs="1"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="byInvoice" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="autoPaymentFloorLimit" type="Field"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="autoPaymentName" type="Field"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="DocRefuseAutoPaymentPayment">
        <xsd:annotation>
            <xsd:documentation>Отмена автоплатежа iqwave</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="card" type="Field"/>
            <xsd:element name="receiverName" type="Field"/>
            <xsd:element name="requisite" type="Field"/>
