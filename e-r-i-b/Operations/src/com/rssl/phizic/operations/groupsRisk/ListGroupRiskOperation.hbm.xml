<?xml version="1.0" encoding="windows-1251"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<query name="com.rssl.phizic.operations.groupsRisk.ListGroupRiskOperation.list">
        select groupRisk
        from com.rssl.phizic.business.limits.GroupRisk groupRisk
        order by groupRisk.id
	</query>
</hibernate-mapping>