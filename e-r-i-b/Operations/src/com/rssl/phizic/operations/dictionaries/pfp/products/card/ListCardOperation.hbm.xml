<?xml version="1.0" encoding="windows-1251"?><!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <query name="com.rssl.phizic.operations.dictionaries.pfp.products.card.ListCardOperation.list">
        select card
        from com.rssl.phizic.business.dictionaries.pfp.products.card.Card as card
        order by card.name asc
    </query>
</hibernate-mapping>
