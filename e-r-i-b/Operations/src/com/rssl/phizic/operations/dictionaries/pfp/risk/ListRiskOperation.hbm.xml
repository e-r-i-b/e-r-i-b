<?xml version="1.0" encoding="windows-1251"?><!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <query name="com.rssl.phizic.operations.dictionaries.pfp.risk.ListRiskOperation.list">
        SELECT risk
        FROM com.rssl.phizic.business.dictionaries.pfp.risk.Risk risk
        ORDER BY risk.name asc
    </query>
</hibernate-mapping>
