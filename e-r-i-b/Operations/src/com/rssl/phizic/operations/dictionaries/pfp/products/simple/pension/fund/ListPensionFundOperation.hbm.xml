<?xml version="1.0" encoding="windows-1251"?><!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <query name="com.rssl.phizic.operations.dictionaries.pfp.products.simple.pension.fund.ListPensionFundOperation.list">
        select fund
        from com.rssl.phizic.business.dictionaries.pfp.products.simple.pension.fund.PensionFund as fund
        order by fund.name asc
    </query>
</hibernate-mapping>
