<?xml version="1.0" encoding="windows-1251"?><!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <query name="com.rssl.phizic.operations.dictionaries.regions.GetRegionsListOperation.list">
        select region from com.rssl.phizic.business.dictionaries.regions.Region as region
        order by region.name
    </query>
</hibernate-mapping>