<?xml version="1.0" encoding="windows-1251"?><!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<query name="com.rssl.phizic.operations.loans.offices.ListLoanOfficeOperation.list">
		select office
		from   com.rssl.phizic.gate.dictionaries.officies.LoanOffice office
		order by office.id
	</query>
</hibernate-mapping>
