<!DOCTYPE server PUBLIC "-//JBoss//DTD J2EE Application 1.4//EN"
		"http://www.jboss.org/j2ee/dtd/jboss-service_4_0.dtd">
<server>
	<mbean code="com.rssl.phizgate.common.messaging.retail.jni.RetailJNIStartServiceImpl"
	       name="jboss:service=RetailJNIService">
        <attribute name="Host">@rs-retail-v6r20.remote.host@</attribute>
        <attribute name="Port">@rs-retail-v6r20.remote.port@</attribute>
        <attribute name="Version">@rs-retail-v6r20.version@</attribute>
        <attribute name="Location">@rs-retail-v6r20.location@</attribute>
        <attribute name="Debug">@rs-retail-v6r20.debug@</attribute>
        <attribute name="Remote">@rs-retail-v6r20.remote.mode.on@</attribute>
        <attribute name="Started">true</attribute>        
	</mbean>
</server>