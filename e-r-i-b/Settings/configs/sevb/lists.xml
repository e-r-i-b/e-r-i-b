<?xml version='1.0' encoding='windows-1251'?>

<entity-lists-def>
    <!-- Список счетов клиента -->
    <entity-list-def
		    name="accounts.xml"
		    scope="session"
            className="com.rssl.phizic.business.xslt.lists.AccountListSource"
		    />
    <entity-list-def
		    name="all-accounts.xml"
		    scope="session"
            className="com.rssl.phizic.business.xslt.lists.AccountListSource"
		    >
        <parameter name="filter">com.rssl.phizic.business.resources.external.NullAccountFilter</parameter>
    </entity-list-def>

    <entity-list-def
		    name="rur-accounts.xml"
		    scope="session"
            className="com.rssl.phizic.business.xslt.lists.AccountListSource"
		    >
        <parameter name="filter">com.rssl.phizic.business.resources.external.RURAccountFilter</parameter>
    </entity-list-def>
	<entity-list-def
			name="active-rur-accounts.xml"
			scope="session"
	        className="com.rssl.phizic.business.xslt.lists.AccountListSource"
			>
	    <parameter name="filter">com.rssl.phizic.business.resources.external.ActiveRurAccountFilter</parameter>
	</entity-list-def>
	<entity-list-def
			name="active-foreign-currency-accounts.xml"
			scope="session"
	        className="com.rssl.phizic.business.xslt.lists.AccountListSource"
			>
	    <parameter name="filter">com.rssl.phizic.business.resources.external.ActiveForeignCurrencyAccountFilter</parameter>
	</entity-list-def>
	<entity-list-def
			name="active-accounts.xml"
			scope="session"
	        className="com.rssl.phizic.business.xslt.lists.AccountListSource"
			>
	    <parameter name="filter">com.rssl.phizic.business.resources.external.ActiveAccountFilter</parameter>
	</entity-list-def>

	<entity-list-def
			name="active-debit-accounts.xml"
			scope="session"
	        className="com.rssl.phizic.business.xslt.lists.AccountListSource"
			>
	    <parameter name="filter">com.rssl.phizic.business.resources.external.ActiveDebitAccountFilter</parameter>
	</entity-list-def>

	<entity-list-def
			name="active-debit-rur-accounts.xml"
			scope="session"
	        className="com.rssl.phizic.business.xslt.lists.AccountListSource"
			>
	    <parameter name="filter">com.rssl.phizic.business.resources.external.ActiveDebitRurAccountFilter</parameter>
	</entity-list-def>

	<entity-list-def
			name="active-credit-accounts.xml"
			scope="session"
	        className="com.rssl.phizic.business.xslt.lists.AccountListSource"
			>
	    <parameter name="filter">com.rssl.phizic.business.resources.external.ActiveCreditAccountFilter</parameter>
	</entity-list-def>

    <entity-list-def
		    name="foreign-currency-accounts.xml"
		    scope="session"
            className="com.rssl.phizic.business.xslt.lists.AccountListSource"
		    >
        <parameter name="filter">com.rssl.phizic.business.resources.external.ForeignCurrencyAccountFilter</parameter>
    </entity-list-def>

	<entity-list-def
		    name="foreign-currency-debit-accounts.xml"
		    scope="session"
            className="com.rssl.phizic.business.xslt.lists.AccountListSource"
		    >
        <parameter name="filter">com.rssl.phizic.business.resources.external.ForeignCurrencyDebitAccountFilter</parameter>
    </entity-list-def>

	<entity-list-def
		    name="foreign-currency-credit-accounts.xml"
		    scope="session"
            className="com.rssl.phizic.business.xslt.lists.AccountListSource"
		    >
        <parameter name="filter">com.rssl.phizic.business.resources.external.ForeignCurrencyCreditAccountFilter</parameter>
    </entity-list-def>

	<entity-list-def
			name="active-rur-debit-accounts.xml"
			scope="session"
	        className="com.rssl.phizic.business.xslt.lists.AccountListSource"
			>
	    <parameter name="filter">com.rssl.phizic.business.resources.external.ActiveRurDebitAccountFilter</parameter>
	</entity-list-def>

	<entity-list-def
			name="active-rur-credit-accounts.xml"
			scope="session"
	        className="com.rssl.phizic.business.xslt.lists.AccountListSource"
			>
	    <parameter name="filter">com.rssl.phizic.business.resources.external.ActiveRurCreditAccountFilter</parameter>
	</entity-list-def>

	<!-- Список карт клиента -->
	<entity-list-def
			name="cards.xml"
			scope="session"
			className="com.rssl.phizic.business.xslt.lists.CardListSource"
			/>
	<entity-list-def
			name="active-cards.xml"
			scope="session"
			className="com.rssl.phizic.business.xslt.lists.CardListSource"
		    >
		<parameter name="filter">com.rssl.phizic.business.resources.external.ActiveCardFilter</parameter>
   	</entity-list-def>

    <entity-list-def
		    name="banklist.xml"
		    scope="application"
            className="com.rssl.phizic.business.xslt.lists.BankListSource"
		    />

    <entity-list-def
            name="receivpay.xml"
            scope="session"
            className="com.rssl.phizic.business.ext.sevb.xslt.lists.ReceivListSource"
            >
		<parameter name="filter">com.rssl.phizic.business.resources.external.ActiveReceiverFilter</parameter>	    
    </entity-list-def>
	<!-- Список подразделений ИКФЛ -->
	<entity-list-def
		    name="departments.xml"
		    scope="session"
            className="com.rssl.phizic.business.xslt.lists.DepartmentsListSource"
	/>
	<!-- Список валют СЧЕТОВ, КАРТ-->
	<entity-list-def
		    name="account-card-currency.xml"
		    scope="session"
            className="com.rssl.phizic.business.xslt.lists.AccountCardCurrencyListSource">
		<parameter name="nationCurrency">RUB</parameter>

	</entity-list-def>
    <entity-list-def
			name="status-list.xml"
			scope="application"
	        className="com.rssl.phizic.business.xslt.lists.XmlResourceSource"
			>
		<parameter name="path">com/rssl/phizic/business/payments/forms/status-list.xml</parameter>
	</entity-list-def>

	<entity-list-def
			name="accountInfo.xml"
			scope="request"
	        className="com.rssl.phizic.business.xslt.lists.AccountInfoSource"
			>
	</entity-list-def>

	<entity-list-def
			name="paymentReceiverJurINN.xml"
			scope="request"
	        className="com.rssl.phizic.business.xslt.lists.PaymentReceiverJurSource"
			>
	</entity-list-def>

	<entity-list-def
			name="paymentReceiverJur.xml"
			scope="request"
	        className="com.rssl.phizic.business.xslt.lists.JurPaymentReceiverList"
			>
	</entity-list-def>

	<entity-list-def
            name="currentPerson.xml"
            scope="session"
            className="com.rssl.phizic.business.ext.sevb.xslt.lists.PersonOfficeSource"
            />
	<entity-list-def
            name="currentPersonInfo.xml"
            scope="session"
            className="com.rssl.phizic.business.xslt.lists.PersonInfoSource"
            />
	 <entity-list-def
            name="currentDate.xml"
            scope="session"
            className="com.rssl.phizic.business.xslt.lists.CurrentDateSource"
            />
	<entity-list-def
			name="products.xml"
			scope="application"
		    className="com.rssl.phizic.business.xslt.lists.DepositProductListSource"
			/>
	<!-- Список депозитных продуктов из ретейла -->
	<entity-list-def
			name="getDepositList_a.xml"
			scope="application"
			className="com.rssl.phizic.business.deposits.Retail51DepositSource"
			/>
	<entity-list-def
			name="deposits.xml"
			scope="application"
			className="com.rssl.phizic.business.xslt.lists.DepositListSource"
			/>

	<!-- Список счетов и вкладов клиента-->
	<entity-list-def
			name="accountsDeposits.xml"
			scope="application"
			className="com.rssl.phizic.business.xslt.lists.AccountsDepositsListSource"
			>
		<parameter name="filter">com.rssl.phizic.business.resources.external.ActiveAccountFilter</parameter>
	</entity-list-def>

	<entity-list-def
			name="active-accountsDeposits.xml"
			scope="application"
			className="com.rssl.phizic.business.xslt.lists.AccountsDepositsListSource">
		<parameter name="filter">com.rssl.phizic.business.resources.external.ActiveAccountFilter</parameter>
	</entity-list-def>

	<!-- Получение данных банка из настроек -->
	<entity-list-def
			name="currenntBankConfig.xml"
			scope="application"
			className="com.rssl.phizic.business.xslt.lists.CurrentBankConfigSource"/>

</entity-lists-def>
