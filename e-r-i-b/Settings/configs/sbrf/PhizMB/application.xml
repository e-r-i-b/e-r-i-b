<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE application PUBLIC "-//Sun Microsystems, Inc.//DTD J2EE Application 1.3//EN" "http://java.sun.com/dtd/application_1_3.dtd">
<application>
	<display-name>PhizMB</display-name>

    <module>
        <web>
            <web-uri>AppStarter.war</web-uri>
            <context-root>AppStarter</context-root>
        </web>
    </module>

    <module>
        <web>
            <web-uri>ASFilialListener.war</web-uri>
            <context-root>ASFilialListener</context-root>
        </web>
    </module>

    <module>
        <ejb>ErmbEjbListener.jar</ejb>
    </module>

    <module>
        <ejb>EjbTest.jar</ejb>
    </module>

    <module>
		<web>
			<web-uri>ErmbDictionaryListener.war</web-uri>
			<context-root>ErmbDictionaryListener</context-root>
		</web>
	</module>

    <module>
		<web>
			<web-uri>ErmbScheduler.war</web-uri>
			<context-root>ErmbScheduler</context-root>
		</web>
	</module>

    <module>
		<web>
			<web-uri>WebResources.war</web-uri>
			<context-root>PhizMB-res</context-root>
		</web>
	</module>

    <module>
		<web>
			<web-uri>ERMBListMigrator.war</web-uri>
			<context-root>ERMBListMigrator</context-root>
		</web>
	</module>

    <module>
        <ejb>ErmbMbkListener.jar</ejb>
    </module>

    <module>
        <ejb>MBKP2PListener.jar</ejb>
    </module>

</application>
