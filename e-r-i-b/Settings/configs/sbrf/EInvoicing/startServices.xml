<?xml version="1.0" encoding="windows-1251"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="../../../schemas/startServicies.xsd">
    <service className="com.rssl.phizic.dataaccess.HibernateService" description="Сервис hibernate для базы E-INVOICING">
        <param name="jndiName" description="">
            <value>hibernate/session-factory/PhizIC</value>
        </param>
        <param name="datasourceConfigClass" description="">
            <value>com.rssl.phizic.dataaccess.config.SimpleDataSourceConfig</value>
        </param>
        <param name="hibernateCfg" description="">
            <value>hibernate.cfg.xml</value>
        </param>
        <param name="configResources" description="">
            <value>com/rssl/phizic/einvoicing/einvoicing.cfg.xml</value>
        </param>
    </service>
    <service className="com.rssl.phizic.dataaccess.HibernateService" description="Сервис hibernate для логирования E-INVOICING">
        <param name="jndiName" description="">
            <value>hibernate/session-factory/PhizICLog</value>
        </param>
        <param name="datasourceConfigClass" description="">
            <value>com.rssl.phizic.dataaccess.config.LogDataSourceConfig</value>
        </param>
        <param name="hibernateCfg" description="">
            <value>hibernate.cfg.xml</value>
        </param>
        <param name="configResources" description="">
            <value>logging.cfg.xml</value>
        </param>
    </service>
    <service className="com.rssl.phizic.dataaccess.HibernateService" description="Сервис hibernate для CSAAdmin">
        <param name="jndiName" description="">
            <value>hibernate/session-factory/PhizICCSAAdmin</value>
        </param>
        <param name="datasourceConfigClass" description="">
            <value>com.rssl.phizic.dataaccess.config.CSAAdminDataSourceConfig</value>
        </param>
        <param name="hibernateCfg" description="">
            <value>hibernate.cfg.xml</value>
        </param>
        <param name="configResources" description="">
            <value>com/rssl/phizic/einvoicing/dictionary.cfg.xml</value>
            <value>com/rssl/phizgate/common/providers/provider-properties.cfg.xml</value>
        </param>
    </service>
</services>