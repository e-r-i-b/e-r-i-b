        <serviceName>phiz-gate-iqwave.CardToCard</serviceName>
        <degradationConfig>
            <available>true</available>
            <useMonitoring>true</useMonitoring>

            <timeout>15000</timeout>
            <monitoringCount>150</monitoringCount>
            <monitoringTime>30000</monitoringTime>

