    <xsd:complexType name="ComplexInsuranceProducts">
        <xsd:annotation>
            <xsd:documentation>
               Комплексные страховые продукты
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="description"       minOccurs="1" maxOccurs="1" type="xsd:string"/>
