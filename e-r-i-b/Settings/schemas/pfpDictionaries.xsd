            <xsd:element name="insuranceProducts" minOccurs="1" maxOccurs="1">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="insuranceProduct" minOccurs="1" maxOccurs="unbounded" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="tableParameters"     minOccurs="1" maxOccurs="1" type="TableViewParameters"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ComplexFundInvestmentProducts">
