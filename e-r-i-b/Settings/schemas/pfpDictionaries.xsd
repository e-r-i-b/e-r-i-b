            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="description"         minOccurs="1" maxOccurs="1" type="xsd:string"/>
            <xsd:element name="portfolioParameters" minOccurs="0" maxOccurs="1" type="PortfoliosParameters"/>
            <xsd:element name="account"             minOccurs="1" maxOccurs="1" type="xsd:string"/>
            <xsd:element name="universal"         minOccurs="1" maxOccurs="1" type="YesOrNo"/>
            <xsd:element name="targetGroups"        minOccurs="1" maxOccurs="1">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="targetGroup" minOccurs="1" maxOccurs="4" type="SegmentCodeType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="fundProducts"        minOccurs="1" maxOccurs="1">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="fundProduct" minOccurs="1" maxOccurs="unbounded" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="imaProducts" minOccurs="1" maxOccurs="1">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="imaProduct" minOccurs="1" maxOccurs="unbounded" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="tableParameters"     minOccurs="1" maxOccurs="1" type="TableViewParameters"/>
        </xsd:sequence>
    </xsd:complexType>

</xsd:schema>
