<?xml version="1.0" encoding="utf-8" ?>
<xsd:schema xmlns:jxb="http://java.sun.com/xml/ns/jaxb" xmlns:xd="http://www.w3.org/2001/XMLSchema" jxb:version="1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <xsd:annotation>
        <xsd:appinfo>
			<jxb:schemaBindings>
				<jxb:package name="com.rssl.phizic.operations.test.generated" />
				<jxb:nameXmlTransform>
					<jxb:elementName suffix="Element" />
					<jxb:typeName suffix="Descriptor" />
				</jxb:nameXmlTransform>
			</jxb:schemaBindings>
		</xsd:appinfo>
    </xsd:annotation>

    <xsd:element name="response">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="status" type="Status"/>

                <xsd:choice>

                    <!-- Список шаблонов платежей (Мобильного Банка) -->
                    <xsd:element name="templateList" minOccurs="0">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="card" maxOccurs="unbounded">
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element name="id" type="xsd:int"/>
                                            <xsd:element name="name" type="xsd:string"/>
                                            <xsd:element name="number" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="availableLimit" type="Money" minOccurs="0"/>
                                            <xsd:element name="state" type="CardState"/>
                                            <xsd:element name="templates" minOccurs="0">
                                                <xsd:complexType>
                                                    <xsd:sequence>
                                                        <xsd:element name="template" maxOccurs="unbounded">
                                                            <xsd:complexType>
                                                                <xsd:sequence>
                                                                    <xsd:element name="id" type="xsd:int"/>
                                                                    <xsd:element name="state" type="TempStatus"/>
                                                                    <xsd:element name="error" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                                                                    <xsd:element name="recipient" type="xsd:string"/>
                                                                    <xsd:element name="payer" minOccurs="0">
                                                                        <xsd:complexType>
                                                                            <xsd:sequence>
                                                                                <xsd:element name="name" type="xsd:string"/>
                                                                                <xsd:element name="value" type="xsd:string"/>
                                                                            </xsd:sequence>
                                                                        </xsd:complexType>
                                                                    </xsd:element>
                                                                </xsd:sequence>
                                                            </xsd:complexType>
                                                        </xsd:element>
                                                    </xsd:sequence>
                                                </xsd:complexType>
                                            </xsd:element>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>

                    <!-- Оплата услуги, Перевод организации -->
                    <xsd:sequence>
                        <xsd:element name="transactionToken" type="TransactionToken" minOccurs="0"/>
                        <xsd:choice>
                            <xsd:element name="billingPayments" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="payment" type="BillingPayment" maxOccurs="unbounded"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="document" type="Document" minOccurs="0"/>
                            <xsd:element name="initialData" type="InitialData" minOccurs="0"/>
                            <xsd:element name="incomeStage" type="IncomeStageData" minOccurs="0"/>
                            <xsd:element name="loanCardOfferStage" type="LoanCardOfferStageData" minOccurs="0"/>
                            <xsd:element name="loanCardProductStage" type="LoanCardProductStageData" minOccurs="0"/>
                            <xsd:element name="LoanProductStage" type="LoanProductStageData" minOccurs="0"/>
                            <xsd:element name="LoanOfferStage" type="LoanOfferStageData" minOccurs="0"/>
                        </xsd:choice>
                        <xsd:element name="confirmStage" type="ConfirmStage" minOccurs="0"/>
                        <xsd:element name="autopayable" type="xsd:boolean" minOccurs="0"/>
                    </xsd:sequence>

                </xsd:choice>

            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:complexType name="LoanOfferStageData">
        <xsd:sequence>
            <xsd:element name="option" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="loanId" type="xsd:integer"/>
                        <xsd:element name="name" type="string"/>
                        <xsd:element name="offerSum" type="Money"/>
                        <xsd:element name="rate" type="Amount"/>
                        <xsd:element name="duration" type="xsd:integer"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

     <xsd:complexType name="LoanProductStageData">
        <xsd:sequence>
            <xsd:element name="option" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="loanKindName" type="string" minOccurs="0"/>
                        <xsd:element name="name" type="string"/>
                        <xsd:element name="duration">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="min" type="xsd:integer"  minOccurs="0"/>
                                    <xsd:element name="max" type="xsd:integer"  minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="initialInstalment"  minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="min" type="Amount"  minOccurs="0"/>
                                    <xsd:element name="max" type="Amount"  minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="needSurety" type="xsd:boolean"/>
                        <xsd:element name="conditions">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="condition" maxOccurs="unbounded">
                                       <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="changeDate" type="xsd:integer"/>
                                                <xsd:element name="loanId" type="xsd:integer"/>
                                                <xsd:element name="amount">
                                                    <xsd:complexType>
                                                         <xsd:sequence>
                                                            <xsd:element name="min" type="Money" minOccurs="0"/>
                                                            <xsd:element name="max" minOccurs="0">
                                                                <xsd:complexType>
                                                                    <xsd:sequence>
                                                                        <xsd:element name="maxAmount" type="Money" minOccurs="0"/>
                                                                        <xsd:element name="maxPercent" type="Amount" minOccurs="0"/>
                                                                        <xsd:element name="include" type="xsd:boolean"/>
                                                                    </xsd:sequence>
                                                                </xsd:complexType>
                                                            </xsd:element>
                                                         </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="interestRate" minOccurs="0">
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="min" type="Amount" minOccurs="0"/>
                                                            <xsd:element name="maxPercent" type="Amount" minOccurs="0"/>
                                                            <xsd:element name="max" minOccurs="0">
                                                                <xsd:complexType>
                                                                    <xsd:sequence>
                                                                        <xsd:element name="maxAmount" type="Amount"/>
                                                                        <xsd:element name="include" type="xsd:boolean"/>
                                                                    </xsd:sequence>
                                                                </xsd:complexType>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="additionalTerms" type="string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="IncomeStageData">
        <xsd:sequence>
            <xsd:element name="option" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="id" type="xsd:integer"/>
                        <xsd:element name="minIncome" type="Money" minOccurs="0"/>
                        <xsd:element name="maxIncome" type="Money" minOccurs="0"/>
                        <xsd:element name="maxIncomeInclude" type="xsd:boolean" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LoanCardOfferStageData">
        <xsd:sequence>
            <xsd:element name="option" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="loanId" type="xsd:integer"/>
                        <xsd:element name="offerId" type="string"/>
                        <xsd:element name="name" type="string"/>
                        <xsd:element name="gracePeriodDuration" type="xsd:integer" minOccurs="0"/>
                        <xsd:element name="gracePeriodInterestRate" type="Amount" minOccurs="0"/>
                        <xsd:element name="maxLimit" type="Money"/>
                        <xsd:element name="offerInterestRate" type="Amount"/>
                        <xsd:element name="firstYearPayment" type="Money"/>
                        <xsd:element name="nextYearPayment" type="Money"/>
                        <xsd:element name="terms" type="string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LoanCardProductStageData">
        <xsd:sequence>
            <xsd:element name="option" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="loanId" type="xsd:integer"/>
                        <xsd:element name="changeDate" type="xsd:integer"/>
                        <xsd:element name="name" type="string"/>
                        <xsd:element name="gracePeriod" minOccurs="0">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="duration" type="xsd:integer"/>
                                    <xsd:element name="interestRate" type="Amount"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="creditLimit">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="min" type="Money"/>
                                    <xsd:element name="max" type="Money"/>
                                    <xsd:element name="includeMax" type="xsd:boolean"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="offerInterestRate" type="Amount"/>
                        <xsd:element name="firstYearPayment" type="Money"/>
                        <xsd:element name="nextYearPayment" type="Money"/>
                        <xsd:element name="terms" type="string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="BillingPayment">
        <xsd:sequence>
            <xsd:element name="billing" type="string"/>
            <xsd:element name="service">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="id" type="string"/>
                        <xsd:element name="name" type="string"/>
                        <xsd:element name="img" type="Image" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="provider">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="id" type="string"/>
                        <xsd:element name="name" type="string"/>
                        <xsd:element name="img" type="Image" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="autoPaymentSupported" type="xsd:boolean"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="Image">
        <xsd:sequence>
            <xsd:choice>
                <xsd:element name="staticImage">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="url" type="string"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="dbImage">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="id" type="string"/>
                            <xsd:element name="updated" type="string"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="InitialData">
        <xsd:sequence>
            <xsd:element name="form">
                <xsd:simpleType>
                    <xsd:restriction base="string">
                        <xsd:enumeration value="RurPayJurSB"/>
                        <xsd:enumeration value="InternalPayment"/>
						<xsd:enumeration value="InternalPaymentLongOffer"/>
                        <xsd:enumeration value="LoanPayment"/>
                        <xsd:enumeration value="LoanPaymentLongOffer"/>
                        <xsd:enumeration value="JurPayment"/>
                        <xsd:enumeration value="BlockingCardClaim"/>
                        <xsd:enumeration value="AccountOpeningClaim"/>
                        <xsd:enumeration value="AccountClosingPayment"/>
                        <xsd:enumeration value="RurPayment"/>
                        <xsd:enumeration value="RurPaymentLongOffer"/>
                        <xsd:enumeration value="IMAPayment"/>
                        <xsd:enumeration value="IMAOpeningClaim"/>
                        <xsd:enumeration value="LoanCardOffer"/>
                        <xsd:enumeration value="LoanCardProduct"/>
                        <xsd:enumeration value="LoanProduct"/>
                        <xsd:enumeration value="LoanOffer"/>
                        <xsd:enumeration value="EditAutoPaymentPayment"/>
                        <xsd:enumeration value="RefuseAutoPaymentPayment"/>
                        <xsd:enumeration value="EditAutoSubscriptionPayment"/>
                        <xsd:enumeration value="DelayAutoSubscriptionPayment"/>
                        <xsd:enumeration value="RecoveryAutoSubscriptionPayment"/>
                        <xsd:enumeration value="CloseAutoSubscriptionPayment"/>
                        <xsd:enumeration value="RefuseLongOffer"/>
                        <xsd:enumeration value="LoyaltyProgramRegistrationClaim"/>
                        <xsd:enumeration value="PFRStatementClaim"/>
                        <xsd:enumeration value="CreateMoneyBoxPayment"/>
                        <xsd:enumeration value="EditMoneyBoxClaim"/>
                        <xsd:enumeration value="CreateP2PAutoTransferClaim"/>
                        <xsd:enumeration value="EditP2PAutoTransferClaim"/>
                        <xsd:enumeration value="CloseP2PAutoTransferClaim"/>
                        <xsd:enumeration value="RecoveryP2PAutoTransferClaim"/>
                        <xsd:enumeration value="DelayP2PAutoTransferClaim"/>
                        <xsd:enumeration value="ChangeCreditLimitClaim"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:choice>
                <xsd:element name="RurPayJurSB" type="InitRurPayJurSB"/>
                <xsd:element name="JurPayment" type="InitJurPayment"/>
                <xsd:element name="LoanPayment" type="InitLoanPayment"/>
                <xsd:element name="LoanPaymentLongOffer" type="InitLoanPaymentLongOffer"/>
                <xsd:element name="BlockingCardClaim" type="InitBlockingCardClaim"/>
                <xsd:element name="InternalPayment" type="InitInternalPayment"/>
				<xsd:element name="InternalPaymentLongOffer" type="InitInternalPaymentLongOffer"/>
                <xsd:element name="RurPayment" type="InitRurPayment"/>
                <xsd:element name="RurPaymentLongOffer" type="InitRurPaymentLongOffer"/>
                <xsd:element name="AccountOpeningClaim" type="InitAccountOpeningClaim"/>
                <xsd:element name="AccountClosingPayment" type="InitAccountClosingPayment"/>
                <xsd:element name="IMAPayment" type="InitIMAPayment"/>
                <xsd:element name="IMAOpeningClaim" type="InitIMAOpeningClaim"/>
                <xsd:element name="LoanOffer" type="InitLoanOffer"/>
                <xsd:element name="LoanProduct" type="InitLoanProduct"/>
                <xsd:element name="LoanCardProduct" type="InitLoanCardProduct"/>
                <xsd:element name="LoanCardOffer" type="InitLoanCardOffer"/>
                <xsd:element name="LoyaltyProgramRegistrationClaim" type="InitLoyaltyProgramRegistrationClaim"/>
                <xsd:element name="EditAutoPaymentPayment" type="InitEditAutoPaymentPayment"/>
                <xsd:element name="RefuseAutoPaymentPayment" type="InitRefuseAutoPaymentPayment"/>
                <xsd:element name="EditAutoSubscriptionPayment" type="InitEditAutoSubscriptionPayment"/>
                <xsd:element name="DelayAutoSubscriptionPayment" type="AutoSubscriptionPaymentFields"/>
                <xsd:element name="RecoveryAutoSubscriptionPayment" type="AutoSubscriptionPaymentFields"/>
                <xsd:element name="CloseAutoSubscriptionPayment" type="InitCloseAutoSubscriptionPayment"/>
                <xsd:element name="RefuseLongOffer" type="InitRefuseLongOffer"/>
                <xsd:element name="PFRStatementClaim" type="InitPFRStatementClaim"/>
                <xsd:element name="CreateMoneyBoxPayment" type="CreateMoneyBoxPayment"/>
                <xsd:element name="EditMoneyBoxClaim" type="CreateMoneyBoxPayment"/>
                <xsd:element name="CreateP2PAutoTransferClaim" type="P2PAutoTransferClaimFields"/>
                <xsd:element name="EditP2PAutoTransferClaim" type="P2PAutoTransferClaimFields"/>
                <xsd:element name="CloseP2PAutoTransferClaim" type="P2PAutoTransferClaimFields"/>
                <xsd:element name="RecoveryP2PAutoTransferClaim" type="P2PAutoTransferClaimFields"/>
                <xsd:element name="DelayP2PAutoTransferClaim" type="P2PAutoTransferClaimFields"/>
                <xsd:element name="ChangeCreditLimitClaim" type="ChangeCreditLimitClaimFields"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="InitRurPayJurSB">
        <xsd:annotation>
            <xsd:documentation>Оплата услуг</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="operationUID" type="string"/>
            <xsd:element name="fields">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="field" type="Field" minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="availableFromResources" type="Field"/>
            <xsd:element name="autoPaymentSupported" type="xsd:boolean"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="InitJurPayment">
        <xsd:annotation>
            <xsd:documentation>Перевод организации</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="operationUID" type="string"/>
            <xsd:element name="receiverAccount" type="Field"/>
            <xsd:element name="receiverINN" type="Field"/>
            <xsd:element name="receiverBIC" type="Field"/>
            <xsd:element name="externalReceiver" type="Field" minOccurs="0"/>
            <xsd:element name="availableFromResources" type="Field"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="InitLoanPayment">
        <xsd:annotation>
            <xsd:documentation>Погашение кредита</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="availableFromResources" type="Field"/>
            <xsd:element name="availableLoanResources" type="Field"/>
            <xsd:element name="amount" type="Field"/>
            <xsd:element name="longOfferAllowed" type="xsd:boolean"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="InitLoanPaymentLongOffer">
        <xsd:annotation>
            <xsd:documentation>Погашение кредита: создание автоплатежа</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
                <xsd:element name="loan" type="Field"/>
                <xsd:element name="fromResource" type="Field"/>
                <xsd:element name="longOfferDetails">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="startDate" type="Field"/>
                            <xsd:element name="endDate" type="Field"/>
                            <xsd:element name="eventType" type="Field"/>
                            <xsd:element name="payDay" type="Field"/>
                            <xsd:element name="firstPaymentDate" type="Field"/>
                            <xsd:element name="priority" type="Field"/>
                            <xsd:element name="isSumModify" type="Field"/>
                            <xsd:element name="sumType" type="Field"/>
                            <xsd:element name="fixedSumma">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="amount" type="Field"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="InitBlockingCardClaim">
        <xsd:annotation>
            <xsd:documentation>Блокировка карты</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="card" type="Field"/>
            <xsd:element name="reason" type="Field"/>
            <xsd:element name="fullName" type="Field"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="InitInternalPayment">
        <xsd:annotation>
            <xsd:documentation>Перевод между своими счетами</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="documentNumber" type="Field" minOccurs="0"/>
            <xsd:element name="fromResource" type="Field"/>
            <xsd:element name="toResource" type="Field"/>
            <xsd:element name="buyAmount" type="Field"/>
            <xsd:element name="sellAmount" type="Field"/>
            <xsd:element name="exactAmount" type="Field"/>
            <xsd:element name="operationCode" type="Field" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="longOfferAllowed" type="xsd:boolean"/>
            <xsd:element name="autoTransferAllowed" type="xsd:boolean"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="InitInternalPaymentLongOffer">
        <xsd:annotation>
            <xsd:documentation>Перевод между своими счетами: создание автоплатежа</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="documentNumber" type="Field"/>
            <xsd:element name="documentDate"   type="Field"/>

            <xsd:element name="paymentDetails">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="fromResource"  type="Field"/>
                        <xsd:element name="toResource"    type="Field"/>
                        <xsd:element name="operationCode" type="Field" minOccurs="0"/>
                        <xsd:element name="admissionDate" type="Field" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="longOfferDetails">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="startDate"  		 type="Field"/>
                        <xsd:element name="endDate"    		 type="Field"/>
                        <xsd:element name="firstPaymentDate" type="Field"/>
                        <xsd:element name="fromAccountFields">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="eventType" type="Field"/>
                                    <xsd:element name="periodic">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="payDay"  type="Field"/>
                                                <xsd:element name="sumType" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="byAnyReceipt">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="sumType" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="onRemaind">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="sumType" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="byCapital">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="sumType" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="byPercent">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="sumType" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="fromCardFields">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="eventType" type="Field"/>
                                    <xsd:element name="periodic">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="payDay"  type="Field"/>
                                                <xsd:element name="sumType" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="byAnyReceipt">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="sumType" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="onRemaind">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="sumType" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="bySalary">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="sumType" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="byPension">
                                        <xsd:complexType>
                                            <xsd:sequence>
                                                <xsd:element name="sumType" type="Field"/>
                                            </xsd:sequence>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="priority"    type="Field"/>
                        <xsd:element name="isSumModify" type="Field"/>
                        <xsd:element name="sellAmountFields">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="sellAmount"  type="Field"/>
                                    <xsd:element name="exactAmount" type="Field"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="buyAmountFields">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="buyAmount"   type="Field"/>
                                    <xsd:element name="exactAmount" type="Field"/>
