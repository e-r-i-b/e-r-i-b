    <xsd:complexType name="Field">
        <xsd:sequence>
            <xsd:element name="name" type="string"/>
            <xsd:element name="title" type="string" minOccurs="0"/>
            <xsd:element name="description" type="string" minOccurs="0"/>
            <xsd:element name="hint" type="string" minOccurs="0"/>
            <xsd:element name="type">
                <xsd:simpleType>
                    <xsd:restriction base="string">
                        <xsd:enumeration value="list"/>
                        <xsd:enumeration value="set"/>
                        <xsd:enumeration value="date"/>
                        <xsd:enumeration value="number"/>
                        <xsd:enumeration value="string"/>
                        <xsd:enumeration value="money"/>
                        <xsd:enumeration value="calendar"/>
                        <xsd:enumeration value="integer"/>
                        <xsd:enumeration value="resource"/>
                        <xsd:enumeration value="boolean"/>
                        <xsd:enumeration value="link"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="maxLength" type="xsd:positiveInteger" minOccurs="0"/>
            <xsd:element name="minLength" type="xsd:positiveInteger" minOccurs="0"/>
            <xsd:element name="required" type="xsd:boolean"/>
            <xsd:element name="editable" type="xsd:boolean"/>
            <xsd:element name="visible" type="xsd:boolean"/>
            <xsd:element name="isSum" type="xsd:boolean" minOccurs="0"/>
            <xsd:choice>
                <xsd:element name="listType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="availableValues" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="valueItem" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="value" type="string"/>
                                                    <xsd:element name="title" type="string"/>
                                                    <xsd:element name="selected" type="xsd:boolean"/>
                                                    <xsd:element name="default" type="xsd:boolean" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="setType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="availableValues" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="valueItem" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="value" type="string"/>
                                                    <xsd:element name="title" type="string"/>
                                                    <xsd:element name="selected" type="xsd:boolean"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="dateType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" type="string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="numberType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" type="xsd:decimal" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="stringType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" minOccurs="0">
                                <xsd:simpleType>
                                    <xsd:restriction base="string"/>
                                </xsd:simpleType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="moneyType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" type="string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="calendarType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" type="string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="integerType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" minOccurs="0">
                                <xsd:simpleType>
                                    <xsd:restriction base="string">
                                        <xsd:pattern value="[\-+]?[0-9]+"/>
                                    </xsd:restriction>
                                </xsd:simpleType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="resourceType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="availableValues" minOccurs="0">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="valueItem" minOccurs="0" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element name="value" type="Resource" minOccurs="0"/>
                                                    <xsd:element name="productValue" type="ProductsType" minOccurs="0"/>
                                                    <xsd:element name="selected" type="xsd:boolean"/>
                                                    <xsd:element name="displayedValue" type="xsd:string" minOccurs="0"/>
                                                    <xsd:element name="currency" type="xsd:string" minOccurs="0"/>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="linkType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="text" type="string" minOccurs="0"/>
                            <xsd:element name="url"  type="string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="booleanType">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="value" type="string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </xsd:choice>
            <xsd:element name="changed" type="xsd:boolean"/>
            <xsd:element name="validators" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="validator">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="type">
                                        <xsd:simpleType>
                                            <xsd:restriction base="string">
                                                <xsd:enumeration value="REGEXP"/>
                                            </xsd:restriction>
                                        </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="message" type="string"/>
                                    <xsd:element name="parameter" type="string"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="subTypes" minOccurs="0" maxOccurs="2">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="subType">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="mobile" type="string"/>
                                    <xsd:element name="wallet" type="string"/>
                                    <xsd:element name="risk_field" type="string"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="Resource">
        <xsd:restriction base="string">
            <xsd:pattern value="(card|account|loan)\:(\d{1,})"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="ProductsType">
        <xsd:sequence>
            <xsd:element name="cards" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="status" type="string" minOccurs="0" maxOccurs="1"/>
                        <xsd:element name="card" type="CardProductType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="accounts" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="status" type="string" minOccurs="0" maxOccurs="1"/>
                        <xsd:element name="account" type="AccountType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="loans" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="status" type="string" minOccurs="0" maxOccurs="1"/>
                        <xsd:element name="loan" type="LoanType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="imaccounts" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="status" type="string" minOccurs="0" maxOccurs="1"/>
                        <xsd:element name="imaccount" type="IMAccountType"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CardProductType">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:int"/>
            <xsd:element name="code" type="Resource"/>
            <xsd:element name="name" type="string"/>
            <xsd:element name="description" type="string" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="number" type="string" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="isMain" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="type" type="CardType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="availableLimit" type="Money" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="state" type="CardState"/>
            <xsd:element name="mainCardNumber" type="string" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="additionalCardType" type="string" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="CardType">
       <xsd:restriction base="string">
           <xsd:enumeration value="debit"/>
           <xsd:enumeration value="credit"/>
           <xsd:enumeration value="overdraft"/>
       </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="AccountType">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:int"/>
            <xsd:element name="code" type="Resource"/>
            <xsd:element name="name" type="string"/>
            <xsd:element name="number" type="string" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="balance" type="Money" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="moneyBoxAvailable" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="state" type="AccountState"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="AccountState">
       <xsd:restriction base="string">
           <xsd:enumeration value="OPENED"/>
           <xsd:enumeration value="CLOSED"/>
           <xsd:enumeration value="ARRESTED"/>
           <xsd:enumeration value="LOST_PASSBOOK"/>
       </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="LoanType">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:int"/>
            <xsd:element name="code" type="Resource"/>
            <xsd:element name="name" type="string"/>
            <xsd:element name="amount" type="Money" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="nextPayAmount" type="Money" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="nextPayDate" type="string" minOccurs="0" maxOccurs="1"/>     <!-- что то тут не ладное, тип должен быть datetime -->
            <xsd:element name="state" type="LoanState"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="LoanState">
       <xsd:restriction base="string">
           <xsd:enumeration value="undefined"/>
           <xsd:enumeration value="open"/>
           <xsd:enumeration value="overdue"/>
           <xsd:enumeration value="closed"/>
       </xsd:restriction>
    </xsd:simpleType>


    <xsd:complexType name="IMAccountType">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:int"/>
            <xsd:element name="code" type="Resource"/>
            <xsd:element name="name" type="string"/>
            <xsd:element name="number" type="string" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="openDate" type="string" minOccurs="0" maxOccurs="1"/>        <!-- что то тут не ладное, тип должен быть datetime -->
            <xsd:element name="closeDate" type="string" minOccurs="0" maxOccurs="1"/>       <!-- что то тут не ладное, тип должен быть datetime -->
            <xsd:element name="balance" type="Money" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="currency" type="string" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="agreementNumber" type="string" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="state" type="IMAState"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="IMAState">
       <xsd:restriction base="string">
           <xsd:enumeration value="opened"/>
           <xsd:enumeration value="closed"/>
           <xsd:enumeration value="lost_passbook"/>
       </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="CardState">
        <xsd:restriction base="string">
            <xsd:enumeration value="active"/>
            <xsd:enumeration value="closed"/>
            <xsd:enumeration value="replenishment"/>
            <xsd:enumeration value="ordered"/>
            <xsd:enumeration value="delivery"/>
            <xsd:enumeration value="blocked"/>
            <xsd:enumeration value="changing"/>
            <xsd:enumeration value="unknown"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="TempStatus">
        <xsd:restriction base="string">
            <xsd:enumeration value="active"/>
            <xsd:enumeration value="inactive"/>
            <xsd:enumeration value="error"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="ConfirmType">
        <xsd:sequence>
            <xsd:element name="strategy" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="type">
                            <xsd:simpleType>
                                <xsd:restriction base="string">
                                    <xsd:enumeration value="cardp"/>
                                    <xsd:enumeration value="smsp"/>
                                    <xsd:enumeration value="none"/>
                                </xsd:restriction>
                            </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="order" type="xsd:int"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ConfirmInfo">
        <xsd:sequence>
            <xsd:element name="type">
                <xsd:simpleType>
                    <xsd:restriction base="string">
                        <xsd:enumeration value="cardp"/>
                        <xsd:enumeration value="smsp"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="cardp" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="passwordNumber" type="string"/>
                        <xsd:element name="cardNumber" type="string"/>
                        <xsd:element name="passwordsLeft" type="xsd:unsignedShort" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="smsp" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="lifeTime" type="xsd:unsignedShort"/>
                        <xsd:element name="attemptsRemain" type="xsd:unsignedByte"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ConfirmStage">
        <xsd:sequence>
            <xsd:element name="confirmType" type="ConfirmType"/>
            <xsd:element name="confirmInfo" type="ConfirmInfo" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="PaymentState">
        <xsd:restriction base="string">
            <xsd:enumeration value="INITIAL"/>
            <xsd:enumeration value="DRAFT"/>
            <xsd:enumeration value="SAVED"/>
            <xsd:enumeration value="EXECUTED"/>
            <xsd:enumeration value="REFUSED"/>
            <xsd:enumeration value="RECALLED"/>
            <xsd:enumeration value="ERROR"/>
            <xsd:enumeration value="DELAYED_DISPATCH"/>
            <xsd:enumeration value="DISPATCHED"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="Status">
        <xsd:sequence>
            <xsd:element name="code">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:int">
                        <xsd:pattern value="0|1|2|3|4"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="errors" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="error" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="text" type="string"/>
                                    <xsd:element name="elementId" type="string" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="warnings" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="warning" maxOccurs="unbounded">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="text" type="string"/>
                                    <xsd:element name="elementId" type="string" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="TransactionToken">
        <xsd:restriction base="string"/>
    </xsd:simpleType>

    <xsd:simpleType name="string">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:whiteSpace value="collapse"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="Money">
        <xsd:sequence>
            <xsd:element name="amount">
                <xsd:simpleType>
                    <xsd:restriction base="string">
                        <xsd:pattern value="(\+|-|)(\d{1,})(\.\d{1,}|)"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="currency">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="code" type="string"/>
                        <xsd:element name="name" type="string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="WriteDownOperations">
        <xsd:annotation>
            <xsd:documentation>Микрооперации списания</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="writeDownOperation" type="WriteDownOperation" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="WriteDownOperation">
        <xsd:annotation>
            <xsd:documentation>Микрооперация списания</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="operationName" type="string"/>
            <xsd:element name="curAmnt" type="Amount"/>
            <xsd:element name="turnOver">
                <xsd:simpleType>
                    <xsd:restriction base="string">
                        <xsd:enumeration value="CHARGE"/>
                        <xsd:enumeration value="RECEIPT"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="Amount">
        <xsd:restriction base="string">
            <xsd:pattern value="^(\d{1,})(\.\d{1,}|)"/>
        </xsd:restriction>
    </xsd:simpleType>

</xsd:schema>