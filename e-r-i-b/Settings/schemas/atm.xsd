                </xsd:complexType>
            </xsd:element>
            <xsd:element name="period" type="Field" minOccurs="0"/>
            <xsd:element name="receiverName" type="Field" minOccurs="0"/>
            <xsd:element name="receiverBIC" type="Field" minOccurs="0"/>
            <xsd:element name="receiverINN" type="Field" minOccurs="0"/>
