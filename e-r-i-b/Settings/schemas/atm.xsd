    <xsd:complexType name="DocumentCheck">
        <xsd:sequence>
            <xsd:element name="form">
                <xsd:simpleType>
                    <xsd:restriction base="string">
                        <xsd:enumeration value="RurPayJurSB"/>
                        <xsd:enumeration value="InternalPayment"/>
                        <xsd:enumeration value="JurPayment"/>
                        <xsd:enumeration value="RurPayment"/>
                        <xsd:enumeration value="AccountOpeningClaim"/>
                        <xsd:enumeration value="AccountClosingPayment"/>
                        <xsd:enumeration value="LoanPayment"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="status" type="PaymentState"/>
            <xsd:element name="title" type="xsd:string"/>
            <xsd:element name="operationDate" type="Field"/>
            <xsd:element name="operationTime" type="Field"/>
            <xsd:element name="documentNumber" type="Field" minOccurs="0"/>
            <xsd:element name="billingDocumentNumber" type="Field" minOccurs="0"/>
            <xsd:choice>
                <xsd:element name="RurPayJurSBDocumentCheck" type="DocumentCheckRurPayJurSB"/>
                <xsd:element name="InternalPaymentDocumentCheck" type="DocumentCheckInternalPayment"/>
                <xsd:element name="JurPaymentDocumentCheck" type="DocumentCheckJurPayment"/>
                <xsd:element name="RurPaymentDocumentCheck" type="DocumentCheckRurPayment"/>
                <xsd:element name="AccountOpeningClaimDocumentCheck" type="DocumentCheckAccountOpeningClaim"/>
                <xsd:element name="AccountClosingPaymentDocumentCheck" type="DocumentCheckAccountClosingPayment"/>
                <xsd:element name="LoanPaymentDocumentCheck"           type="DocumentCheckLoanPayment"/>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>

