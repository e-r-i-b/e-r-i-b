<?xml version="1.0" encoding="windows-1251"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.rssl.phizicgate.rsretailV6r20.stopList" >
    <class name="JonImpl" table="DSL_JON_DBT">

        <id name="id" column="T_JONID">
	        <generator class="native">
                <param name="sequence">DSL_JON_DBT_SEQ</param>
            </generator>
        </id>

        <property name="operName"     type="string"  column="T_OPERNAME"/>
        <property name="slCliId"      type="string"  column="T_SL_CLI_ID"/>
        <property name="rsCliId"      type="integer" column="T_RS_CLI_ID"/>
        <property name="recordId"     type="string"  column="T_RECORD_ID"/>
        <property name="inn"          type="string"  column="T_INN"/>
	    <property name="operator"     type="integer" column="T_OPERATOR"/>
	    <property name="slExpDate"    type="calendar"    column="T_SL_EXPDATE"/>
	    <property name="slExpTime"    type="calendar"    column="T_SL_EXPTIME"/>
	    <property name="slName"       type="string"  column="T_SL_NAME"/>
	    <property name="operDate"     type="calendar"    column="T_OPERDATE"/>
	    <property name="operTime"     type="calendar"    column="T_OPERTIME"/>
	    <property name="absSubsys"    type="string"  column="T_ABS_SUBSYS"/>
	    <property name="operCode"     type="integer" column="T_OPERCODE"/>
	    <property name="nameIdCard"   type="string"  column="T_NAMEIDCARD"/>
	    <property name="snIdCard"     type="string"  column="T_SNIDCARD"/>
	    <property name="fio"          type="string"  column="T_FIO"/>
	    <property name="urName"       type="string"  column="T_URNAME"/>
	    <property name="urAddress"    type="string"  column="T_URADDRESS"/>
	    <property name="operId"       type="string"  column="T_OPERID"/>
	    <property name="operNote"     type="string"  column="T_OPERNOTE"/>
	    <property name="isExport"     type="integer" column="T_ISEXPORT"/>
    </class>
</hibernate-mapping>