<?xml version='1.0' encoding='windows-1251'?>
<hibernate-configuration>

    <session-factory>
        <!-- properties -->
        <property name="hibernate.connection.datasource">jdbc/PhizICMigration</property>
        <property name="hibernate.dialect">org.hibernate.dialect.Oracle9Dialect</property>
        <property name="show_sql">true</property>
        <property name="hibernate.cache.use_query_cache">true</property>

        <!-- Mapping files -->
        <mapping resource="com/rssl/phizic/business/ermb/migration/list/entity/migrator/Client.hbm.xml"/>
        <mapping resource="com/rssl/phizic/business/ermb/migration/list/operations/ListClientMigrationOperation.hbm.xml"/>
        <mapping resource="com/rssl/phizic/business/ermb/migration/list/entity/migrator/Conflict.hbm.xml"/>
        <mapping resource="com/rssl/phizic/business/ermb/migration/list/entity/migrator/ConflictedClient.hbm.xml"/>
        <mapping resource="com/rssl/phizic/business/ermb/migration/list/entity/migrator/Archive.hbm.xml"/>
        <mapping resource="com/rssl/phizic/business/ermb/migration/list/entity/migrator/MigrationInfo.hbm.xml"/>

    </session-factory>

</hibernate-configuration>
