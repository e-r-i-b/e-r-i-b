<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="unqualified">
    <xs:simpleType name="W4D_FilterType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Client"/>
            <xs:enumeration value="Contract"/>
            <xs:enumeration value="Address"/>
            <xs:enumeration value="SubContract"/>
            <xs:enumeration value="ContractList"/>
            <xs:enumeration value="Usage"/>
            <xs:enumeration value="Order"/>
            <xs:enumeration value="Statement"/>
            <xs:enumeration value="StatementAll"/>
            <xs:enumeration value="StatementAuth"/>
            <xs:enumeration value="StatementPosted"/>
            <xs:enumeration value="Product"/>
            <xs:enumeration value="ObjectSearch"/>
            <xs:enumeration value="AffiliatedSystem"/>
            <xs:enumeration value="HandBook"/>
            <xs:enumeration value="Letter"/>
            <xs:enumeration value="RelatedContract"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4C_InstitutionIDType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Branch"/>
            <xs:enumeration value="Bank"/>
            <xs:enumeration value="CB"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4C_ParmType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Tag"/>
            <xs:enumeration value="Flag"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="W4C_RefType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="type" type="W4D_Code" use="required">
                    <xs:annotation>
                        <xs:appinfo>Reference Type</xs:appinfo>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:simpleType name="W4D_Activation">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Activate"/>
            <xs:enumeration value="Deactivate"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4D_Amount">
        <xs:restriction base="xs:decimal"/>
    </xs:simpleType>
    <xs:simpleType name="W4D_CardExpire">
        <xs:annotation>
            <xs:appinfo>Max length is 4 chars</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="4"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4D_Channel">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="W4D_CheckAvailable">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Check"/>
            <xs:enumeration value="SeeMain"/>
            <xs:enumeration value="NoCheck"/>
            <xs:enumeration value="BillingLimit"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4D_CheckUsage">
        <xs:restriction base="xs:string">
            <xs:enumeration value="MainAndOwn"/>
            <xs:enumeration value="OwnOnly"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4D_Code">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="32"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4D_Comment">
        <xs:annotation>
            <xs:appinfo>Max length is 255 chars</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="W4D_ContractNumber">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="W4D_Counter">
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="W4D_Country">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="W4D_Currency">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="W4D_Date">
        <xs:restriction base="xs:date"/>
    </xs:simpleType>
    <xs:simpleType name="W4D_DateTime">
        <xs:annotation>
            <xs:appinfo>The Format can be as "YYYY-MM-DD HH:MI:SS", as "YYYY-MM-DD".</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="W4D_Embossing">
        <xs:annotation>
            <xs:appinfo>Max length is 32 chars</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    
    <xs:simpleType name="W4D_Gender">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Male"/>
            <xs:enumeration value="Female"/>
            <xs:enumeration value="Not specified"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="W4D_LongCode">
        <xs:annotation>
            <xs:appinfo>Max length is 255 chars</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="64"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4D_MemberID">
        <xs:annotation>
            <xs:appinfo>Max length is 32 chars</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    
    <xs:simpleType name="W4D_MsgId">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="36"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4D_Name">
        <xs:annotation>
            <xs:appinfo>Max length is 255 chars</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    
    <xs:simpleType name="W4D_OrderDprt">
        <xs:annotation>
            <xs:appinfo>Max length is 32 chars</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="W4D_PostalCode">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="32"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4D_RegNumber">
        <xs:annotation>
            <xs:appinfo>Max length is 32 chars</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="W4D_Relation">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="32"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="W4D_RqRs">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Rq"/>
            <xs:enumeration value="Rs"/>
            <xs:enumeration value="Notif"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4D_SearchType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ContractBase"/>
            <xs:enumeration value="CBSNumber"/>
            <xs:enumeration value="Custom"/>
            <xs:enumeration value="AlternativeID"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="W4D_SIC">
        <xs:restriction base="xs:string">
            <xs:maxLength value="4"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4D_String">
        <xs:annotation>
            <xs:appinfo>Max length is 255 chars</xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="W4D_Tag">
        <xs:annotation>
            <xs:documentation>Max length is 1 char</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    
    
    <xs:simpleType name="W4D_YesNo">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Yes"/>
            <xs:enumeration value="No"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="W4C_AccountInfo">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="AccountType" type="W4D_Code"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4C_AddInfo">
        <xs:sequence>
            <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##any" processContents="skip"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4C_Address">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="AddressType" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Address type code. Codes are defined in
the "Full \ Configuration Setup \ Client Classifiers \ Address Types" table.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="UseAddress" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Address type code of Address which should be used instead of the original Address.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="DeliveryType" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Delivery type code</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="OrderDprt" type="W4D_OrderDprt">
                <xs:annotation>
                    <xs:appinfo>Bank department accepting the application</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="FirstName" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Target's first name</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="LastName" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Target's last name</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="BirthName" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Target's birth name</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Language" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Three-symbol alphabetical ISO-639
language code. Codes are defined in the
"Code" field of the "Full \ Configuration
Setup \ Client Classifiers \ Languages"
table.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="PhoneList" type="W4C_PhoneList">
                <xs:annotation>
                    <xs:appinfo>Client telephones numbers</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="EMail" type="W4D_String">
                <xs:annotation>
                    <xs:appinfo>E-mail address</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="URL" type="W4D_String">
                <xs:annotation>
                    <xs:appinfo>URL. Reserved for future use</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Country" type="W4D_Country">
                <xs:annotation>
                    <xs:appinfo>Three-symbol alphabetical ISO-3166
country code. Codes are defined in the
"Code (3 Bytes)" field of the "Full \
Configuration Setup \ Main Tables \
Country Table" table.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="State" type="W4D_String">
                <xs:annotation>
                    <xs:appinfo>State or Province. Reserved for future use</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="City" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>City</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="PostalCode" type="W4D_PostalCode">
                <xs:annotation>
                    <xs:appinfo>Postal code</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="MunicipalityCode" type="W4D_PostalCode">
                <xs:annotation>
                    <xs:appinfo>Municipality code</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddressLocation" type="W4D_Comment">
                <xs:annotation>
                    <xs:appinfo>Address Location</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddressLine1" type="W4D_Comment">
                <xs:annotation>
                    <xs:appinfo>Address, line No. 1</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddressLine2" type="W4D_Comment">
                <xs:annotation>
                    <xs:appinfo>Address, line No. 2</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddressLine3" type="W4D_Comment">
                <xs:annotation>
                    <xs:appinfo>Address, line No. 3</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddressLine4" type="W4D_Comment">
                <xs:annotation>
                    <xs:appinfo>Address, line No. 4</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="ActivityPeriod" type="W4C_DateInterval">
                <xs:annotation>
                    <xs:appinfo>Address activity period</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4C_Affiliation">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="Type" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Affiliation type between clients</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Code" type="W4D_Code"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Client" type="W4C_ClientIDT">
                <xs:annotation>
                    <xs:appinfo>Client Data</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="UseForSubs" type="W4D_YesNo"/>
            <xs:element maxOccurs="1" minOccurs="0" name="ActivityPeriod">
                <xs:annotation>
                    <xs:appinfo>Reserved for future use</xs:appinfo>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="W4C_DateInterval">
                            <xs:sequence>
                                <xs:element maxOccurs="1" minOccurs="0" name="DateFrom" type="W4D_Date">
                                    <xs:annotation>
                                        <xs:appinfo>The initial date of the activity period</xs:appinfo>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element maxOccurs="1" minOccurs="0" name="DateTo" type="W4D_Date">
                                    <xs:annotation>
                                        <xs:appinfo>The ending date of the activity period, inclusive</xs:appinfo>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4C_Any">
        <xs:sequence>
            <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##any" processContents="skip"/>
        </xs:sequence>
    </xs:complexType>
    
    
    <xs:complexType name="W4C_BankAccountDetails">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="InstContractNumber" type="W4D_Code"/>
            <xs:sequence maxOccurs="1" minOccurs="1">
                <xs:sequence maxOccurs="1" minOccurs="1">
                    <xs:element maxOccurs="1" minOccurs="0" name="InstitutionIdType" type="W4D_Code">
                        <xs:annotation>
                            <xs:appinfo>Type of Institution identification. It is used to define type of code shown in followed Institution element. Suported value: 'B' - Branch Code; 'C' - CB Code; 'N' - Bank Code; 'RID' - Receiving Institution Identifier;
'BIN' - Bank identification number; 'BIC' - RBS Bank Identification Code.
</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="1" name="Institution" type="W4D_MemberID"/>
                </xs:sequence>
                <xs:element maxOccurs="1" minOccurs="0" name="RefSet" type="W4C_ParmSet">
                    <xs:annotation>
                        <xs:appinfo>Additional Information. Pram/ParmType is reserved for future use</xs:appinfo>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="InstName" type="W4D_Name"/>
            <xs:element maxOccurs="1" minOccurs="0" name="InstLoroId" type="W4D_Code"/>
            <xs:element maxOccurs="1" minOccurs="0" name="OrderDprt" type="W4D_Code"/>
            <xs:element maxOccurs="1" minOccurs="0" name="OrderDprtName" type="W4D_Name"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="Address" type="W4C_Address">
                <xs:annotation>
                    <xs:appinfo>Bank address information. It can be present as in local language as in English. Following fields are supported in local language (Language != "ENG"): Country, AddressZip, City, AddressLocation, AddressLine1, AddressLine2, AddressLine3, AddressLine4, and MunicipalityCode. Following fields are supported in English (Language = "ENG"): City, AddressLine1, AddressLine2, AddressLine3, and AddressLine4</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddInfo" type="W4C_AddInfo">
                <xs:annotation>
                    <xs:appinfo>Additional client information</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4C_CardDetails">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="CardExpiry" type="W4D_CardExpire">
                <xs:annotation>
                    <xs:appinfo> Card Expiration Date</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="CardSeqN" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo> Card Sequence number</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="W4C_ClientIDT">
        <xs:complexContent>
            <xs:extension base="W4O_Client">
                <xs:choice>
                    <xs:choice maxOccurs="1" minOccurs="0">
                        <xs:element maxOccurs="1" minOccurs="1" name="RefContractNumber" type="W4D_ContractNumber">
                            <xs:annotation>
                                <xs:appinfo>Contract Number for client identification.</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:sequence maxOccurs="1" minOccurs="1">
                            <xs:element maxOccurs="1" minOccurs="1" name="RefRBSNumber" type="W4D_ContractNumber">
                                <xs:annotation>
                                    <xs:appinfo>RBS Contract Number for client
identification.</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="RefRBSMemberId" type="W4D_ContractNumber">
                                <xs:annotation>
                                    <xs:appinfo>RBS Contract Member ID</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:choice>
                    <xs:sequence maxOccurs="1" minOccurs="0">
                        <xs:element maxOccurs="1" minOccurs="1" name="CustomIDT" type="W4D_Code">
                            <xs:annotation>
                                <xs:appinfo>Contract ID used in a custom search procedure</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="CustomCode" type="W4D_Name">
                            <xs:annotation>
                                <xs:appinfo>This value is used to verify the correctness of search results where a custom procedure is used.</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="CustomIDType">
                            <xs:annotation>
                                <xs:appinfo>The type of Custom identification</xs:appinfo>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:minLength value="1"/>
                                    <xs:maxLength value="1"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                </xs:choice>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4C_ContractBase">
        <xs:complexContent>
            <xs:restriction base="W4C_ContractRoot">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="IDType" type="W4D_SearchType"/>
                    <xs:sequence maxOccurs="1" minOccurs="1">
                        <xs:annotation>
                            <xs:documentation>W4 IDT</xs:documentation>
                        </xs:annotation>
                        <xs:element maxOccurs="1" minOccurs="0" name="ContractNumber" type="W4D_ContractNumber">
                            <xs:annotation>
                                <xs:appinfo>Contract number</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="Relation" type="W4D_Relation">
                            <xs:annotation>
                                <xs:appinfo>Code of type of relation between related
contracts.
Codes are defined in the Full \ Configuration
Setup \ Accounting Setup \ Contract
Relations table.</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:choice maxOccurs="1" minOccurs="1">
                        <xs:annotation>
                            <xs:documentation>Ext IDT</xs:documentation>
                        </xs:annotation>
                        <xs:sequence maxOccurs="1" minOccurs="1">
                            <xs:annotation>
                                <xs:documentation>New</xs:documentation>
                            </xs:annotation>
                            <xs:element maxOccurs="1" minOccurs="0" name="CBSNumber" type="W4D_LongCode">
                                <xs:annotation>
                                    <xs:appinfo>Core-Banking System Contract number</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="CBSCode" type="W4D_MemberID">
                                <xs:annotation>
                                    <xs:appinfo>Core-Banking System Member Identifier</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                        <xs:sequence maxOccurs="1" minOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Deprecated</xs:documentation>
                            </xs:annotation>
                            <xs:element maxOccurs="1" minOccurs="0" name="RBSNumber" type="W4D_LongCode">
                                <xs:annotation>
                                    <xs:appinfo>Contract RBS number</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="RBSMemberID" type="W4D_MemberID">
                                <xs:annotation>
                                    <xs:appinfo>Deprecated</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:choice>
                    <xs:element maxOccurs="1" minOccurs="0" name="IdentifierFor" type="W4C_IdentifierFor">
                        <xs:annotation>
                            <xs:appinfo>IdentifierFor aggregate specifies the actual object to be processed in request. Supported references are: 
'Top' - Top Object in an Objects tree,
'Main' - parent object for the object
</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="Client" type="W4C_ClientIDT"/>
                    <xs:sequence maxOccurs="1" minOccurs="0">
                        <xs:element maxOccurs="1" minOccurs="1" name="CustomIDT" type="W4D_Code">
                            <xs:annotation>
