                <xs:annotation>
                    <xs:appinfo>Additional information, line 2.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddInfo03" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Additional information, line 3.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddInfo04" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Additional information, line 4.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddInfo05" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Additional information, line 5.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddInfo06" type="W4D_Name">
                <xs:annotation>
