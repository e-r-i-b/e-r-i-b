            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddInfo07" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Additional information, line 7.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddDate01" type="W4D_DateTime">
                <xs:annotation>
                    <xs:appinfo>Additional information, date 1.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddDate02" type="W4D_DateTime">
                <xs:annotation>
                    <xs:appinfo>Additional information, date 2.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddDate03" type="W4D_DateTime">
                <xs:annotation>
                    <xs:appinfo>Additional information, date 3.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddNum01" type="W4D_Amount">
                <xs:annotation>
                    <xs:appinfo>Additional information, number 1.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddNum02" type="W4D_Amount">
                <xs:annotation>
                    <xs:appinfo>Additional information, number 2.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddNum03" type="W4D_Amount">
                <xs:annotation>
                    <xs:appinfo>Additional information, number 3.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddNum04" type="W4D_Amount">
                <xs:annotation>
                    <xs:appinfo>Additional information, number 3.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddNum05" type="W4D_Amount">
                <xs:annotation>
                    <xs:appinfo>Additional information, number 3.</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="W4C_ApplList">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="Application" type="W4T_Application">
                <xs:annotation>
                    <xs:appinfo>Application</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="W4C_BillingPeriod">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="PeriodNumber" type="W4D_Counter"/>
            <xs:element maxOccurs="1" minOccurs="0" name="StartDate" type="W4D_Date"/>
            <xs:element maxOccurs="1" minOccurs="1" name="EndDate" type="W4D_Date"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="W4C_ContractStatus">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="StatusClass">
                <xs:annotation>
                    <xs:appinfo> Contract Status Class (Valid, Invalid, Decline)</xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Valid"/>
                        <xs:enumeration value="Invalid"/>
                        <xs:enumeration value="Decline"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="1" name="StatusCode" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo> Contract Status code</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="StatusDetails" type="W4D_Comment">
                <xs:annotation>
                    <xs:appinfo> Contract Status details</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="ProductionStatus">
                <xs:annotation>
                    <xs:appinfo> Card Production Status. Supported values: 
Marked,
Sent,
Ready,
Locked,
Marked Applet,
To Mark,
Not to Mark,
To Close,
To Request,
Waiting for Response,
Waiting for Subs</xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Marked"/>
                        <xs:enumeration value="Sent"/>
                        <xs:enumeration value="Ready"/>
                        <xs:enumeration value="Locked"/>
                        <xs:enumeration value="Marked Applet"/>
                        <xs:enumeration value="To Mark"/>
                        <xs:enumeration value="Not to Mark"/>
                        <xs:enumeration value="To Close"/>
                        <xs:enumeration value="To Request"/>
                        <xs:enumeration value="Waiting for Response"/>
                        <xs:enumeration value="Waiting for Subs"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4C_DataType">
        
    </xs:complexType>
    <xs:complexType name="W4C_DataTypeRs">
        <xs:choice>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="ClientRs" type="W4O_ClientRs">
                <xs:annotation>
                    <xs:appinfo> Full client Information</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="ContractRs" type="W4O_ContractRs">
                <xs:annotation>
                    <xs:appinfo> Full Contract Information</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="1" name="ClientContractRs" type="W4O_ClientContractRs">
                <xs:annotation>
                    <xs:appinfo>Consolidated information about a Client and correspondent Contract. The aggregate is used only in extended response for an Application for ClientContract Add</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="Application" type="W4T_Application">
                <xs:annotation>
                    <xs:appinfo>Application Response</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="1" name="RefSet" type="W4C_ParmSet">
                <xs:annotation>
                    <xs:appinfo> Set of Document Identifiers. Supported values: RRN, IRN, SRN, ARN, AuthCode, TRN, ReconsRN, ReceiptRN, DRN, PrevDRN, OrigDRN. Parm/ParmType is reserved for future use.</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="W4C_DeviceConfig">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="SerialNumber" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Device serial number.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Configuration" type="W4D_Counter">
                <xs:annotation>
                    <xs:appinfo>ATM's configuration number.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Socket" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Mnemonic name of the connection between
the device and NetServer unique within the
system.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="X25Address" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>ATM's address in an X.25 or TCP/IP network.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="GlobalLimit" type="W4D_Counter">
                <xs:annotation>
                    <xs:appinfo>Maximum number of bank notes
dispensed by the ATM.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="CSService" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Deprecated since 03.33 WAY4 version</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="DeviceURL" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Merchant's electronic address.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="DenomDict" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Denomination type code. The codes are
registered in the DENOM_DICT table.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="MACStatus" type="W4D_MACStatus">
                <xs:annotation>
                    <xs:appinfo>MAC authentication of messages:
Mandatory, Optional or None.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="PBTStatus" type="W4D_MACStatus">
                <xs:annotation>
                    <xs:appinfo>PIN verification at POS terminals and
preliminary PIN verification at ATMs:
Mandatory, Optional or None.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Status" type="W4D_DeviceStatus">
                <xs:annotation>
                    <xs:appinfo>Device status:
"OK", "Information", "Warning", "Error", "Not
Configured", or "FatalError".</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4C_DeviceInfo">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="DeviceRecord" type="W4C_DeviceRecord">
                <xs:annotation>
                    <xs:appinfo>Device properties information</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="DeviceDocParms">
                <xs:annotation>
                    <xs:appinfo>Device properties information used for device contracts</xs:appinfo>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="W4C_DeviceTransDetails">
                            <xs:sequence>
                                <xs:element maxOccurs="1" minOccurs="0" name="Channel" type="W4D_Channel">
                                    <xs:annotation>
                                        <xs:appinfo> Device parameter channel</xs:appinfo>
                                    </xs:annotation>
                                </xs:element>
                            </xs:sequence>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="DeviceParms" type="W4C_AddInfo">
                <xs:annotation>
                    <xs:appinfo>Additional information on device
properties</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4C_DeviceKeys">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="KeyType" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Key type</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="KeyCode" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Key code</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="KeyName" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Key name</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Algorithm">
                <xs:annotation>
                    <xs:appinfo>Key encryption algorithm</xs:appinfo>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="DES"/>
                        <xs:enumeration value="RSA"/>
                        <xs:enumeration value="3DES ABA"/>
                        <xs:enumeration value="3DES ABC"/>
                        <xs:enumeration value="Certificate"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="1" name="Key" type="W4D_String">
                <xs:annotation>
                    <xs:appinfo>Key</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="1" name="KeyCheck" type="W4D_String">
                <xs:annotation>
                    <xs:appinfo>Key check value</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="SerialNumber" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Reserved for future use.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="ActivityPeriod" type="W4C_DateInterval">
                <xs:annotation>
                    <xs:appinfo>Reserved for future use.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="MaxUsage" type="W4D_Counter">
                <xs:annotation>
                    <xs:appinfo>Reserved for future use.</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4C_DeviceRecord">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="DeviceType" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Device type code. Codes are defined in
the "ATM/POS/Imprinter" Types table.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Location" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Device name and location.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="TransactionClass" type="W4D_TransactionClass">
                <xs:annotation>
                    <xs:appinfo>Transaction category:
"Cash" - cash withdrawal,
"Retail" - retail purchase,
"Unique" - special type.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="DefaultCurr" type="W4D_Currency">
