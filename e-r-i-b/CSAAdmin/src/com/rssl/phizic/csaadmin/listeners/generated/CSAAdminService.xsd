        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GetAllowedDepartmentsParametersType">
        <xsd:sequence>
            <xsd:element name="Employee"  type="EmployeeType"          nillable="false" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

