            <xsd:element name="Filter" type="EmployeeMailManagerFilterParametersType" nillable="false" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GetEmployeeManagerInfoParametersType">
        <xsd:sequence>
            <xsd:element name="ManagerId" type="xsd:string" nillable="false" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GetEmployeeListResultType">
        <xsd:sequence>
            <xsd:element name="Employees" type="ArrayOfEmployeeType" nillable="true" minOccurs="1" maxOccurs="1"/>
