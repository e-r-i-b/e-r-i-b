    <xsd:complexType name="UnlockEmployeeParametersType">
        <xsd:sequence>
            <xsd:element name="Login"     type="LoginType"    nillable="false" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="UnlockEmployeeResultType">
        <xsd:sequence>
            <xsd:element name="Blocks" type="ArrayOfLoginBlockType" nillable="true" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ChangeEmployeePasswordParametersType">
        <xsd:sequence>
            <xsd:element name="Login"     type="LoginType"    nillable="false" minOccurs="1" maxOccurs="1"/>
