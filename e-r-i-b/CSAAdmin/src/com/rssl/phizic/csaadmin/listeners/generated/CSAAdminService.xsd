            <xsd:element name="LockLoginParameter"  type="LoginType"      nillable="false" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="LockBlockParameter"  type="LoginBlockType" nillable="false" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LockEmployeeResultType">
        <xsd:sequence>
