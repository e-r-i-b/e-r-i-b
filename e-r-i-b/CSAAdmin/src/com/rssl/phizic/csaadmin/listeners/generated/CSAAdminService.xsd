        <xsd:complexContent>
            <xsd:extension base="GetMultiNodeListParametersType"/>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="GetRemovedMailListParametersType">
        <xsd:complexContent>
            <xsd:extension base="GetMultiNodeListParametersType"/>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="GetMailStatisticsParametersType">
        <xsd:complexContent>
            <xsd:extension base="GetMultiNodeParametersType"/>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="GetMailEmployeeStatisticsParametersType">
        <xsd:complexContent>
            <xsd:extension base="GetMultiNodeParametersType"/>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="GetMailAverageTimeParametersType">
        <xsd:complexContent>
            <xsd:extension base="GetMultiNodeParametersType"/>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="GetFirstMailDateParametersType">
        <xsd:complexContent>
            <xsd:extension base="GetMultiNodeParametersType"/>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="GetIncomeMailListResultType">
        <xsd:sequence>
            <xsd:element name="mailList"            type="IncomeMailListArrayType" nillable="true" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GetOutcomeMailListResultType">
        <xsd:sequence>
            <xsd:element name="mailList"            type="OutcomeMailListArrayType" nillable="true" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GetRemovedMailListResultType">
        <xsd:sequence>
            <xsd:element name="mailList"            type="RemovedMailListArrayType" nillable="true" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GetMailStatisticsResultType">
        <xsd:sequence>
            <xsd:element name="statisticsData"      type="MailStatisticsDataArrayType" nillable="true" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GetMailEmployeeStatisticsResultType">
        <xsd:sequence>
            <xsd:element name="statisticsData"      type="MailEmployeeStatisticsDataArrayType" nillable="true" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GetMailAverageTimeResultType">
        <xsd:sequence>
            <xsd:element name="mailAverageTime"     type="MailAverageTimeType"  nillable="true"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GetFirstMailDateResultType">
        <xsd:sequence>
            <xsd:element name="firstDate"           type="DateTime"  nillable="true"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AccessSchemeType">
        <xsd:annotation>
            <xsd:documentation>Схема прав</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="externalId"        type="xsd:long"    nillable="true"/>
            <xsd:element name="name"              type="xsd:string"  nillable="false"/>
            <xsd:element name="category"          type="xsd:string"  nillable="false"/>
            <xsd:element name="CAAdminScheme"     type="xsd:boolean" nillable="false"/>
            <xsd:element name="VSPEmployeeScheme" type="xsd:boolean" nillable="false"/>
            <xsd:element name="mailManagement"    type="xsd:boolean" nillable="false"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LoginType">
        <xsd:annotation>
            <xsd:documentation>Логин</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="id"              type="xsd:long"              nillable="true"/>
            <xsd:element name="name"            type="xsd:string"            nillable="false"/>
            <xsd:element name="password"        type="xsd:string"            nillable="true"/>
            <xsd:element name="accessScheme"    type="AccessSchemeType"      nillable="true"/>
            <xsd:element name="blocks"          type="ArrayOfLoginBlockType" nillable="true"/>
            <xsd:element name="lastUpdateDate"  type="DateTime"              nillable="true"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="LoginBlockType">
        <xsd:annotation>
            <xsd:documentation>Блокировка</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="reasonType"        type="xsd:string"     nillable="false"/>
            <xsd:element name="reasonDescription" type="xsd:string"     nillable="true"/>
          	<xsd:element name="blockedFrom"       type="DateTime"       nillable="false"/>
            <xsd:element name="blockedUntil"      type="DateTime"       nillable="true"/>
            <xsd:element name="employee"          type="LoginType"      nillable="true"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="DepartmentType">
        <xsd:sequence>
            <xsd:element name="tb"  type="xsd:string" nillable="false"/>
            <xsd:element name="osb" type="xsd:string" nillable="true"/>
            <xsd:element name="vsp" type="xsd:string" nillable="true"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="EmployeeType">
        <xsd:annotation>
            <xsd:documentation>Сотрудник</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="externalId"       type="xsd:long"       nillable="true"/>
            <xsd:element name="login"            type="LoginType"      nillable="false"/>
            <xsd:element name="surname"          type="xsd:string"     nillable="false"/>
            <xsd:element name="firstName"        type="xsd:string"     nillable="false"/>
            <xsd:element name="patronymic"       type="xsd:string"     nillable="true"/>
            <xsd:element name="info"             type="xsd:string"     nillable="true"/>
            <xsd:element name="email"            type="xsd:string"     nillable="true"/>
            <xsd:element name="mobilePhone"      type="xsd:string"     nillable="true"/>
            <xsd:element name="department"       type="DepartmentType" nillable="true"/>
            <xsd:element name="caAdmin"          type="xsd:boolean"    nillable="false"/>
            <xsd:element name="vspEmployee"      type="xsd:boolean"    nillable="false"/>
            <xsd:element name="managerId"        type="xsd:string"     nillable="true"/>
            <xsd:element name="managerPhone"     type="xsd:string"     nillable="true"/>
            <xsd:element name="managerEMail"     type="xsd:string"     nillable="true"/>
            <xsd:element name="managerLeadEMail" type="xsd:string"     nillable="true"/>
            <xsd:element name="managerChannel"   type="xsd:string"     nillable="true"/>
            <xsd:element name="sudirLogin"       type="xsd:string"     nillable="true"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ManagerInfoType">
        <xsd:annotation>
            <xsd:documentation>Информация о менеджере</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="name"    type="xsd:string"   nillable="false"/>
            <xsd:element name="email"   type="xsd:string"   nillable="true"/>
            <xsd:element name="phone"   type="xsd:string"   nillable="true"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ContactCenterEmployeeType">
        <xsd:annotation>
            <xsd:documentation>Сотрудник контактного центра</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="id"               type="xsd:long"       nillable="true"/>
            <xsd:element name="externalId"       type="xsd:string"     nillable="false"/>
            <xsd:element name="name"             type="xsd:string"     nillable="false"/>
            <xsd:element name="department"       type="xsd:string"     nillable="false"/>
            <xsd:element name="area"             type="xsd:string"     nillable="false"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ArrayOfEmployeeType">
        <xsd:annotation>
            <xsd:documentation>Список сотрудников</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="item" type="EmployeeType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ArrayOfContactCenterEmployeeType">
        <xsd:annotation>
            <xsd:documentation>Список сотрудников контактного центра</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="item" type="ContactCenterEmployeeType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ArrayOfLoginBlockType">
        <xsd:annotation>
            <xsd:documentation>Список блокировок</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="item" type="LoginBlockType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ArrayOfAccessSchemeType">
        <xsd:annotation>
            <xsd:documentation>Список схем прав</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="item" type="AccessSchemeType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ArrayOfCategoriesType">
        <xsd:annotation>
            <xsd:documentation>Список категорий</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="item" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ArrayOfDepartmentType">
        <xsd:annotation>
            <xsd:documentation>Список категорий</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="item" type="DepartmentType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MapEntryType">
        <xsd:annotation>
            <xsd:documentation>Элемент списка мапы</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="key"     type="xsd:string"/>
            <xsd:element name="value"   type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="IncomeMailListArrayType">
        <xsd:annotation>
            <xsd:documentation>Список писем</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="item" type="IncomeMailListDataType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="OutcomeMailListArrayType">
        <xsd:annotation>
            <xsd:documentation>Список писем</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="item" type="OutcomeMailListDataType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RemovedMailListArrayType">
        <xsd:annotation>
            <xsd:documentation>Список писем</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="item" type="RemovedMailListDataType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MailStatisticsDataArrayType">
        <xsd:annotation>
            <xsd:documentation>Список данных со статистикой писем</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="item" type="MailStatisticsDataType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MailEmployeeStatisticsDataArrayType">
        <xsd:annotation>
            <xsd:documentation>Список данных со статистикой писем</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="item" type="MailEmployeeStatisticsDataType"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MailListDataTypeBase" abstract="true">
        <xsd:annotation>
            <xsd:documentation>Общие данные письма</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="nodeId"              type="xsd:long"/>
            <xsd:element name="id"                  type="xsd:long"/>
            <xsd:element name="number"              type="xsd:long"     minOccurs="0"/>
            <xsd:element name="creationDate"        type="DateTime"     minOccurs="0"/>
            <xsd:element name="responseMethod"      type="xsd:string"   minOccurs="0"/>
            <xsd:element name="state"               type="xsd:string"   minOccurs="0"/>
            <xsd:element name="stateDescription"    type="xsd:string"   minOccurs="0"/>
            <xsd:element name="type"                type="xsd:string"   minOccurs="0"/>
            <xsd:element name="typeDescription"     type="xsd:string"   minOccurs="0"/>
            <xsd:element name="theme"               type="xsd:string"   minOccurs="0"/>
            <xsd:element name="subject"             type="xsd:string"   minOccurs="0"/>
            <xsd:element name="employeeFIO"         type="xsd:string"   minOccurs="0"/>
            <xsd:element name="employeeUserId"      type="xsd:string"   minOccurs="0"/>
            <xsd:element name="tb"                  type="xsd:string"   minOccurs="0"/>
            <xsd:element name="area"                type="xsd:string"   minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="IncomeMailListDataType" xml:base="MailListDataTypeBase">
        <xsd:annotation>
            <xsd:documentation>Данные входящего письма</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="MailListDataTypeBase">
                <xsd:sequence>
                    <xsd:element name="senderFIO"           type="xsd:string"   minOccurs="0"/>
                    <xsd:element name="senderId"            type="xsd:long"     minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="OutcomeMailListDataType" xml:base="MailListDataTypeBase">
        <xsd:annotation>
            <xsd:documentation>Данные исходящего письма</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="MailListDataTypeBase">
                <xsd:sequence>
                    <xsd:element name="recipientFIO"       type="xsd:string"   minOccurs="0"/>
                    <xsd:element name="recipientId"        type="xsd:long"     minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="RemovedMailListDataType" xml:base="MailListDataTypeBase">
        <xsd:annotation>
            <xsd:documentation>Данные удаленного письма</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="MailListDataTypeBase">
                <xsd:sequence>
                    <xsd:element name="recipientName"           type="xsd:string"   minOccurs="0"/>
                    <xsd:element name="directionDescription"    type="xsd:string"   minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="MailStatisticsDataType">
        <xsd:annotation>
            <xsd:documentation>Статистика для писем</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="state"               type="xsd:string"/>
            <xsd:element name="counter"             type="xsd:long"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MailEmployeeStatisticsDataType">
        <xsd:annotation>
            <xsd:documentation>Статистика для писем в разрезе сотрудников</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="id"                  type="xsd:long"/>
            <xsd:element name="nodeId"              type="xsd:long"/>
            <xsd:element name="arriveTime"          type="DateTime"     minOccurs="0"/>
            <xsd:element name="processingTime"      type="DateTime"     minOccurs="0"/>
            <xsd:element name="state"               type="xsd:string"   minOccurs="0"/>
            <xsd:element name="employeeFIO"         type="xsd:string"   minOccurs="0"/>
            <xsd:element name="areaName"            type="xsd:string"   minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="MailAverageTimeType">
        <xsd:annotation>
            <xsd:documentation>Среднее время обработки письма сотрудником</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="milliseconds"          type="xsd:long"/>
            <xsd:element name="count"                 type="xsd:long"/>
        </xsd:sequence>
    </xsd:complexType>

</xsd:schema>