<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">

    <xsd:element name="Request" type="RequestType"/>
    <xsd:element name="Response" type="ResponseType"/>

    <xsd:complexType name="RequestType">
        <xsd:sequence>
            <xsd:element name="RqUID"       type="UUIDType"     minOccurs="1" maxOccurs="1"/>
            <xsd:element name="RqTm"        type="DateTime"     minOccurs="1" maxOccurs="1"/>
            <xsd:element name="OperUID"     type="OUUIDType"    minOccurs="1" maxOccurs="1"/>
            <xsd:element name="SessionID"   type="UUIDType"     minOccurs="0" maxOccurs="1"/>
            <xsd:element name="Data"        type="RequestData"  minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ResponseType">
        <xsd:sequence>
            <xsd:element name="RqUID"   type="UUIDType"     minOccurs="1" maxOccurs="1"/>
            <xsd:element name="RqTm"    type="DateTime"     minOccurs="1" maxOccurs="1"/>
            <xsd:element name="OperUID" type="OUUIDType"    minOccurs="1" maxOccurs="1"/>
            <xsd:element name="Status"  type="StatusType"   minOccurs="1" maxOccurs="1"/>
            <xsd:element name="Data"    type="ResponseData" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:simpleType name="DateTime">
		<xsd:annotation>
			<xsd:documentation>Тип данных для представления дат</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>

    <xsd:simpleType name="UUIDType">
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="32"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="OUUIDType">
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="32"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="StatusCodeType">
        <xsd:restriction base="xsd:long"/>
    </xsd:simpleType>

    <xsd:simpleType name="StatusDescType">
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="255"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="StatusType">
        <xsd:sequence>
            <xsd:element name="StatusCode" type="StatusCodeType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="StatusDesc" type="StatusDescType" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RequestData">
        <xsd:choice>
            <xsd:element name="GetEmployeeListRq"            type="GetEmployeeListParametersType"            minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetEmployeeMailManagerListRq" type="GetEmployeeMailManagerListParametersType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetEmployeeManagerInfoRq"     type="GetEmployeeManagerInfoParametersType"     minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetEmployeeByIdRq"            type="GetEmployeeByIdParametersType"            minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetCurrentEmployeeRq"         type="GetCurrentEmployeeParametersType"         minOccurs="1" maxOccurs="1"/>
            <xsd:element name="SaveEmployeeRq"               type="SaveEmployeeParametersType"               minOccurs="1" maxOccurs="1"/>
            <xsd:element name="AssignAccessSchemeEmployeeRq" type="AssignAccessSchemeEmployeeParametersType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="DeleteEmployeeRq"             type="DeleteEmployeeParametersType"             minOccurs="1" maxOccurs="1"/>
            <xsd:element name="LockEmployeeRq"               type="LockEmployeeParametersType"               minOccurs="1" maxOccurs="1"/>
            <xsd:element name="UnlockEmployeeRq"             type="UnlockEmployeeParametersType"             minOccurs="1" maxOccurs="1"/>
            <xsd:element name="ChangeEmployeePasswordRq"     type="ChangeEmployeePasswordParametersType"     minOccurs="1" maxOccurs="1"/>
            <xsd:element name="SelfChangePasswordRq"         type="SelfChangePasswordParametersType"         minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetAllowedDepartmentsRq"      type="GetAllowedDepartmentsParametersType"      minOccurs="1" maxOccurs="1"/>
            <xsd:element name="SaveAllowedDepartmentsRq"     type="SaveAllowedDepartmentsParametersType"     minOccurs="1" maxOccurs="1"/>

            <xsd:element name="GetAccessSchemeListRq" type="GetAccessSchemeListParametersType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetAccessSchemeByIdRq" type="GetAccessSchemeByIdParametersType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="SaveAccessSchemeRq"    type="SaveAccessSchemeParametersType"    minOccurs="1" maxOccurs="1"/>
            <xsd:element name="DeleteAccessSchemeRq"  type="DeleteAccessSchemeParametersType"  minOccurs="1" maxOccurs="1"/>

            <xsd:element name="GetAuthenticationParametersRq"       type="AuthenticationParametersRequestType"          minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetEmployeeSynchronizationDataRq"    type="GetEmployeeSynchronizationDataRequestType"    minOccurs="1" maxOccurs="1"/>
            <xsd:element name="CloseSessionRq"                      type="CloseSessionParametersRequestType"            minOccurs="1" maxOccurs="1"/>
            <xsd:element name="ChangeNodeRq"                        type="ChangeNodeRequestType"                        minOccurs="1" maxOccurs="1"/>
            <xsd:element name="SaveOperationContextRq"              type="SaveOperationContextRequestType"              minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetOperationContextRq"               type="GetOperationContextRequestType"               minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetCurrentEmployeeContextRq"         type="GetCurrentEmployeeContextParametersType"      minOccurs="1" maxOccurs="1"/>

            <xsd:element name="GetIncomeMailListRq"                 type="GetIncomeMailListParametersType"      minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetOutcomeMailListRq"                type="GetOutcomeMailListParametersType"     minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetRemovedMailListRq"                type="GetRemovedMailListParametersType"     minOccurs="1" maxOccurs="1"/>

            <xsd:element name="GetMailStatisticsRq"                 type="GetMailStatisticsParametersType"              minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetMailEmployeeStatisticsRq"         type="GetMailEmployeeStatisticsParametersType"      minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetMailAverageTimeRq"                type="GetMailAverageTimeParametersType"             minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetFirstMailDateRq"                  type="GetFirstMailDateParametersType"               minOccurs="1" maxOccurs="1"/>
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="ResponseData">
        <xsd:choice>
            <xsd:element name="GetEmployeeListRs"            type="GetEmployeeListResultType"            minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetEmployeeMailManagerListRs" type="GetEmployeeMailManagerListResultType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetEmployeeManagerInfoRs"     type="GetEmployeeManagerInfoResultType"     minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetEmployeeByIdRs"            type="GetEmployeeByIdResultType"            minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetCurrentEmployeeRs"         type="GetCurrentEmployeeResultType"         minOccurs="1" maxOccurs="1"/>
            <xsd:element name="SaveEmployeeRs"               type="SaveEmployeeResultType"               minOccurs="1" maxOccurs="1"/>
            <xsd:element name="AssignAccessSchemeEmployeeRs" type="AssignAccessSchemeEmployeeResultType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="DeleteEmployeeRs"             type="DeleteEmployeeResultType"             minOccurs="1" maxOccurs="1"/>
            <xsd:element name="LockEmployeeRs"               type="LockEmployeeResultType"               minOccurs="1" maxOccurs="1"/>
            <xsd:element name="UnlockEmployeeRs"             type="UnlockEmployeeResultType"             minOccurs="1" maxOccurs="1"/>
            <xsd:element name="ChangeEmployeePasswordRs"     type="ChangeEmployeePasswordResultType"     minOccurs="1" maxOccurs="1"/>
            <xsd:element name="SelfChangePasswordRs"         type="VoidResultType"                       minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetAllowedDepartmentsRs"      type="GetAllowedDepartmentsResultType"      minOccurs="1" maxOccurs="1"/>
            <xsd:element name="SaveAllowedDepartmentsRs"     type="VoidResultType"                       minOccurs="1" maxOccurs="1"/>

            <xsd:element name="GetAccessSchemeListRs" type="GetAccessSchemeListResultType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetAccessSchemeByIdRs" type="GetAccessSchemeByIdResultType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="SaveAccessSchemeRs"    type="SaveAccessSchemeResultType"    minOccurs="1" maxOccurs="1"/>
            <xsd:element name="DeleteAccessSchemeRs"  type="DeleteAccessSchemeResultType"  minOccurs="1" maxOccurs="1"/>

            <xsd:element name="GetAuthenticationParametersRs"       type="AuthenticationParametersResultType"       minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetEmployeeSynchronizationDataRs"    type="GetEmployeeSynchronizationDataResultType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="CloseSessionRs"                      type="VoidResultType"                           minOccurs="1" maxOccurs="1"/>
            <xsd:element name="ChangeNodeRs"                        type="ChangeNodeResultType"                     minOccurs="1" maxOccurs="1"/>
            <xsd:element name="SaveOperationContextRs"              type="VoidResultType"                           minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetOperationContextRs"               type="GetOperationContextResultType"            minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetCurrentEmployeeContextRs"         type="GetCurrentEmployeeContextResultType"      minOccurs="1" maxOccurs="1"/>

            <xsd:element name="GetIncomeMailListRs"                 type="GetIncomeMailListResultType"              minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetOutcomeMailListRs"                type="GetOutcomeMailListResultType"             minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetRemovedMailListRs"                type="GetRemovedMailListResultType"             minOccurs="1" maxOccurs="1"/>

            <xsd:element name="GetMailStatisticsRs"                 type="GetMailStatisticsResultType"              minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetMailEmployeeStatisticsRs"         type="GetMailEmployeeStatisticsResultType"      minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetMailAverageTimeRs"                type="GetMailAverageTimeResultType"             minOccurs="1" maxOccurs="1"/>
            <xsd:element name="GetFirstMailDateRs"                  type="GetFirstMailDateResultType"               minOccurs="1" maxOccurs="1"/>
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="GetEmployeeListParametersType">
