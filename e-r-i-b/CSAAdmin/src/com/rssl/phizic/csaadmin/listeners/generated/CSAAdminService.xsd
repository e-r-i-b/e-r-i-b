            <xsd:element name="Employee" type="EmployeeType" nillable="true" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SaveEmployeeParametersType">
        <xsd:sequence>
            <xsd:element name="Employee"  type="EmployeeType" nillable="false" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SaveEmployeeResultType">
        <xsd:sequence>
            <xsd:element name="Employee" type="EmployeeType" nillable="true" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AssignAccessSchemeEmployeeParametersType">
        <xsd:sequence>
            <xsd:element name="SchemeId"  type="xsd:long"     nillable="true"  minOccurs="0" maxOccurs="1"/>
            <xsd:element name="Employee"  type="EmployeeType" nillable="false" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AssignAccessSchemeEmployeeResultType">
        <xsd:sequence>
            <xsd:element name="AccessScheme" type="AccessSchemeType" nillable="true" minOccurs="1" maxOccurs="1"/>
