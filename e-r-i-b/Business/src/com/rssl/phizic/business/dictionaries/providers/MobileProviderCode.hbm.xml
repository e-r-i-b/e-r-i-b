<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rssl.phizic.business.dictionaries.providers.MobileProviderCode" table="MOBILE_PROVIDER_CODES">
        <id name="id" type="long">
            <generator class="native">
                <param name="sequence">S_MOBILE_PROVIDER_CODES</param>
            </generator>
        </id>
        <property name="code"                type="string"          column="CODE"               not-null="true"/>
        <property name="name"                type="string"          column="NAME"               not-null="true"/>
        <property name="externalId"          type="string"          column="EXTERNAL_ID"        not-null="true"/>
    </class>
</hibernate-mapping>