<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rssl.phizic.business.dictionaries.pfp.period.InvestmentPeriod"
           table="PFP_INVESTMENT_PERIODS">
        <id name="id" type="long" column="ID">
            <generator class="native">
                <param name="sequence">S_PFP_INVESTMENT_PERIODS</param>
            </generator>
        </id>
        <property name="uuid"   type="string" column="UUID"   not-null="true" update="false"/>
        <property name="period" type="string" column="PERIOD" not-null="true"/>
    </class>
</hibernate-mapping>