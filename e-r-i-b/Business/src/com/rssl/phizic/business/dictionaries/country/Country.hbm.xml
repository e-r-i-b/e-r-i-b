<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rssl.phizic.business.dictionaries.country.CountryCode" table="COUNTRY_CODES">
        <id name="iso3" type="string" column="ISO3"/>
        <property name="iso2" type="string" column="ISO2" not-null="true"/>
	    <property name="name" type="string" column="NAME" not-null="true"/>
    </class>

</hibernate-mapping>
