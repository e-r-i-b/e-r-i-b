<?xml version="1.0" encoding="windows-1251" ?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rssl.phizic.person.Address" table="ADDRESS">

        <id name="id" type="long">
            <generator class="native">
                <param name="sequence">S_ADDRESS</param>
            </generator>
        </id>


        <property name="postalCode"     type="string"     column="POSTALCODE"       not-null="false"/>
        <property name="province"       type="string"     column="PROVINCE"         not-null="false"/>
        <property name="district"       type="string"     column="DISTRICT"         not-null="false"/>
        <property name="city"           type="string"     column="CITY"             not-null="false"/>
        <property name="settlement"     type="string"     column="SETTLEMENT"       not-null="false"/>
        <property name="street"         type="string"     column="STREET"           not-null="false"/>
        <property name="house"          type="string"     column="HOUSE"            not-null="false"/>
        <property name="building"       type="string"     column="BUILDING"         not-null="false"/>
        <property name="flat"           type="string"     column="FLAT"             not-null="false"/>
        <property name="unparseableAddress" type="string" column="UNPARSEABLE"      not-null="false"/>

    </class>

    </hibernate-mapping>