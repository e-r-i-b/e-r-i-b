<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.rssl.phizic.business.resources">
    <class name="ResourceType" table="RESOURCETYPES">
        <id name="id" type="long">
            <generator class="native">
             <param name="sequence">S_RESOURCETYPES</param>
            </generator>
        </id>

        <property name="className" type="string" column="CLASS" not-null="true"/>
        <property name="name" type="string" column="NAME" not-null="true"/>

    </class>
</hibernate-mapping>