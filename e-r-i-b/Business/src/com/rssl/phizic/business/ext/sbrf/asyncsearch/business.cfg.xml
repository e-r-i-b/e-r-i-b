<?xml version='1.0' encoding='windows-1251'?>
<hibernate-configuration>
	<session-factory>
		<!-- справочник поставщиков услуг -->
        <mapping resource="com/rssl/phizic/business/dictionaries/providers/ServiceProvider.hbm.xml"/>
        <!--Обновляемый в БД "живого" поиска поставщик-->
        <mapping resource="com/rssl/phizic/business/dictionaries/asyncSearch/AsynchSearchServiceProvider.hbm.xml"/>
        <!--справочник регионов поставщиков-->
        <mapping resource="com/rssl/phizic/business/dictionaries/providers/ServiceProviderRegion.hbm.xml"/>
        <!--Группы риска-->
        <mapping resource="com/rssl/phizic/business/limits/GroupRisk.hbm.xml"/>
        <!-- Подразделения банка -->
		<mapping resource="com/rssl/phizic/business/departments/Departments.hbm.xml"/>
        <mapping resource="com/rssl/phizic/business/dictionaries/offices/extended/ExtendedDepartments.hbm.xml"/>
        <!--Справочник регионов-->
        <mapping resource="com/rssl/phizic/business/dictionaries/regions/Regions.hbm.xml"/>
        <!-- справочник биллинговых систем -->
        <mapping resource="com/rssl/phizic/business/dictionaries/billing/Billing.hbm.xml"/>
        <!-- справочник услуг-->
		<mapping resource="com/rssl/phizic/business/dictionaries/payment/services/PaymentService.hbm.xml"/>
        <!-- справочник категорий услуг-->
		<mapping resource="com/rssl/phizic/business/dictionaries/payment/services/PaymentServiceCategory.hbm.xml"/>
        <!-- дополнительные поля -->
        <mapping resource="com/rssl/phizic/business/fields/FieldDescription.hbm.xml"/>

        <mapping resource="com/rssl/phizic/auth/Login.hbm.xml"/>
        <mapping resource="com/rssl/phizic/business/employees/Employees.hbm.xml"/>
        <mapping resource="com/rssl/phizicgate/manager/routing/Adapter.hbm.xml"/>
	</session-factory>
</hibernate-configuration>