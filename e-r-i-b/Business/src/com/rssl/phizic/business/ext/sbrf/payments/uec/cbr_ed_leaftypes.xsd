	<xs:simpleType name="PaytPriorityCodeType">
		<xs:annotation>
			<xs:documentation>Код. Очередность платежа. [Очередность платежа]. (0 - очередность не указана, 1-6).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="bt:CodeCategory">
			<xs:enumeration value="0"/>
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PurposeCodeType">
