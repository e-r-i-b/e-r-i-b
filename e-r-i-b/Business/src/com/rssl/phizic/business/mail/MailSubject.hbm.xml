<?xml version="1.0" encoding="windows-1251" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.rssl.phizic.business.mail.MailSubject" table="MAIL_SUBJECTS">
        <id name="id" type="long">
         <generator class="native">
          <param name="sequence">S_MAIL_SUBJECTS</param>
         </generator>
        </id>
        <property name="uuid"              type="string"           column="UUID"             not-null="true"/>
        <property name="description"       type="string"           column="DESCRIPTION"      not-null="true"/>
    </class>
</hibernate-mapping>
