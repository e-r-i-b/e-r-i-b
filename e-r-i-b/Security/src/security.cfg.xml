<?xml version='1.0' encoding='windows-1251'?>
<hibernate-configuration>
	<session-factory>
        <!-- Security -->
		<mapping resource="com/rssl/phizic/auth/Login.hbm.xml"/>
		<mapping resource="com/rssl/phizic/auth/LoginBlock.hbm.xml"/>
        <mapping resource="com/rssl/phizic/auth/imsi/LoginIMSIError.hbm.xml"/>
        <mapping resource="com/rssl/phizic/security/password/Passwords.hbm.xml"/>
        <mapping resource="com/rssl/phizic/auth/passwordcards/PasswordCards.hbm.xml"/>
