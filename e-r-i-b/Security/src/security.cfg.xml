		<mapping resource="com/rssl/phizic/auth/modes/AccessPolicySettings.hbm.xml"/>
		<mapping resource="com/rssl/phizic/security/certification/CertOwn.hbm.xml"/>
        <mapping resource="com/rssl/phizic/security/confirm.hbm.xml"/>
