<?xml version="1.0" encoding="utf-8"?>
<messaging-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="./../../../../../../../Gate/src/com/rssl/phizic/gate/messaging/configuration/messaging.xsd">
    <request endpoint="iqwave" name="CardToCardRequest">
        <response name="CardToCardResponse"/>
    </request>
    <request endpoint="iqwave" name="SimplePaymentRequest">
        <response name="OfflineTicket"/>
    </request>

    <request endpoint="iqwave" name="PaymentFnsRequest">
        <response name="OfflineTicket"/>
    </request>

    <request endpoint="iqwave" name="PaymentMgtsRequest">
        <response name="OfflineTicket"/>
    </request>

    <request endpoint="iqwave" name="PaymentGkhRequest">
        <response name="OfflineTicket"/>
    </request>

    <request endpoint="iqwave" name="PaymentMosenergoRequest">
        <response name="OfflineTicket"/>
    </request>

    <request endpoint="iqwave" name="PaymentDebtsFnsRequest">
        <response name="PaymentDebtsFnsResponse"/>
    </request>

    <request endpoint="iqwave" name="PaymentDebtsMgtsRequest">
        <response name="PaymentDebtsMgtsResponse"/>
    </request>

    <request endpoint="iqwave" name="PaymentDebtsGkhRequest">
        <response name="PaymentDebtsGkhResponse"/>
    </request>

    <request endpoint="iqwave" name="PaymentRostelecomRequest">
        <response name="OfflineTicket"/>
    </request>

    <request endpoint="iqwave" name="PaymentDebtsRostelecomRequest">
        <response name="PaymentDebtsRostelecomResponse"/>
    </request>

    <request endpoint="iqwave" name="AutoPayListRequest">
        <response name="AutoPayListResponse"/>
    </request>

    <request endpoint="iqwave" name="AutoPayReportRequest">
        <response name="AutoPayReportResponse"/>
    </request>

    <request endpoint="iqwave" name="AutoPayStateRequest" >
        <response name="AutoPayStateResponse"/>
    </request>

    <request endpoint="iqwave" name="FreePaymentRequest">
        <response name="OfflineTicket"/>
    </request>

    <request endpoint="iqwave" name="FNSFreePaymentRequest">
        <response name="OfflineTicket"/>
    </request>

    <!-- запрос на регистрацию автоплатежа-->
     <request endpoint="iqwave" name="AutoPayRegisterRequest">
        <response name="OfflineTicket"/>
    </request>

    <!-- запрос на редактирование автоплатежа-->
     <request endpoint="iqwave" name="AutoPayCorrectionRequest">
        <response name="OfflineTicket"/>
    </request>

    <!-- запрос на отмену автоплатежа-->
    <request endpoint="iqwave" name="AutoPayCancelRequest">
        <response name="OfflineTicket"/>
    </request>

    <!-- запрос на получение доступных типов автоплатежей -->
    <request endpoint="iqwave" name="AutoPayGetClientTypeRequest">
        <response name="AutoPayGetClientTypeResponse"/>
    </request>
    
    <request name="OperStatusRequest" endpoint="iqwave">
        <response name="OperStatusResponse"/>
    </request>

    <!-- запрос информации о броне (аэрофлот)-->
    <request name="PaymentCheckRESRequest" endpoint="iqwave">
        <response name="PaymentCheckRESResponse"/>
    </request>

    <!-- запрос на исполнение платежа об оплате брони (аэрофлот)-->
    <request name="PaymentRESRequest" endpoint="iqwave">
        <response name="OfflineTicket"/>
    </request>

    <!-- запрос коммисии для перевода на карту в другой банк -->
    <request name="CardToCardInfoRequest" endpoint="iqwave">
        <response name="CardToCardInfoResponse"/>
    </request>

    <!--Программа лояльности. Запрос информации по накопленным баллам-->
    <request name="LoyaltyGetBalanceRequest" endpoint="iqwave">
        <response name="LoyaltyGetBalanceResponse"/>
    </request>

    <!--Программа лояльности. Запрос информации по последним операциям-->
    <request name="LoyaltyGetStatementReportRequest" endpoint="iqwave">
        <response name="LoyaltyGetStatementReportResponse"/>
    </request>

    <!--Программа лояльности. Запрос информации по спецпредложениям-->
    <request name="LoyaltyGetOffersRequest" endpoint="iqwave">
        <response name="LoyaltyGetOffersResponse"/>
    </request>

    <!--Программа лояльности. заявка на регистрацию в программе лояльности-->
    <request name="LoyaltyRegisterRequest" endpoint="iqwave">
        <response name="LoyaltyRegisterResponse"/>
    </request>

    <!-- Запрос расписания рейсов Аэроэкспресс -->
    <request name="AeroexpressScheduleRequest" endpoint="iqwave">
        <response name="AeroexpressScheduleResponse"/>
    </request>

    <!-- Запрос прайс-листа Аэроэкспресс -->
    <request name="AeroexpressPricelistRequest" endpoint="iqwave">
        <response name="AeroexpressPricelistResponse"/>
    </request>

    <!-- Запрос на получение рейсов -->
    <request name="AeroexpressFreeSeatsGlobalRequest" endpoint="iqwave">
        <response name="AeroexpressFreeSeatsGlobalResponse"/>
    </request>

    <!-- Запрос на получение статусов мест по выбранному рейсу -->
    <request name="AeroexpressFreeSeatsDetailRequest" endpoint="iqwave">
        <response name="AeroexpressFreeSeatsDetailResponse"/>
    </request>

    <!-- Запрос на регистрацию получения билетов -->
    <request name="AeroexpressGetTicketRequest" endpoint="iqwave">
        <response name="AeroexpressGetTicketResponse"/>
    </request>
    
    <!-- Запрос на оплату билетов -->
    <request name="AeroexpressPayTicketRequest" endpoint="iqwave">
        <response name="AeroexpressPayTicketResponse"/>
    </request>

    <!--Отмена операции-->
    <request name="ReversalSimplePaymentEcommerceRequest" endpoint="iqwave">
        <response name="OfflineTicket"/>
    </request>

    <!--Возврат товара (билетов)-->
    <request name="RefundSimplePaymentEcommerceRequest" endpoint="iqwave">
        <response name="OfflineTicket"/>
    </request>

    <!--оплата интернет заказа-->
    <request endpoint="iqwave" name="SimplePaymentEcommerceRequest">
        <response name="OfflineTicket"/>
    </request>
</messaging-config>

