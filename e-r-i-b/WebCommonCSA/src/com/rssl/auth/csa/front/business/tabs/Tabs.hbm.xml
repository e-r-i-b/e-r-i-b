<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rssl.auth.csa.front.business.tabs.Tabs" table="CSA_TABS">
        <id name="id" type="long" column="ID">
            <generator class="native">
                <param name="sequence">S_CSA_TABS</param>
            </generator>
        </id>

        <property name="tabName"    type="string"   column="NAME"   not-null="false"/>
        <property name="tabText"    type="string"   column="TEXT"   not-null="false"/>
    </class>
</hibernate-mapping>