					<xs:documentation>Значение поля дополнения</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Extent">
							<xs:annotation>
								<xs:documentation>Поле дополнения</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute ref="id" use="required">
									<xs:annotation>
										<xs:documentation>Идентификатор объекта Extent</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
