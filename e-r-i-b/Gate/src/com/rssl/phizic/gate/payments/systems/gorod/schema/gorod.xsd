				<xs:element ref="lic" minOccurs="0"/>
				<xs:element ref="Cache" minOccurs="0"/>
				<xs:element name="ListOfMoreInfo" type="listOfMoreInfoType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Запрос дополнительной информации объекта Global</xs:documentation>
