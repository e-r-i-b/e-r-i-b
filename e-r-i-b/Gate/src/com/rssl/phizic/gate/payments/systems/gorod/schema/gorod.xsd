	<xs:complexType name="newEOrderType">
		<xs:annotation>
			<xs:documentation>Тип для нового (несохраненного) документа на оплату услуги</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PayOrder">
				<xs:annotation>
					<xs:documentation>Платеж</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:choice>
							<xs:element name="Abonent">
								<xs:annotation>
									<xs:documentation>Абонент оплачиваемой услуги</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute ref="id" use="required">
										<xs:annotation>
											<xs:documentation>Идентификатор объекта Abonent</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:sequence>
								<xs:annotation>
									<xs:documentation>Дополнительная услуга</xs:documentation>
								</xs:annotation>
								<xs:element name="AddService">
									<xs:annotation>
										<xs:documentation>* Оплачиваемая дополнительная услуга</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:attribute ref="id" use="required">
											<xs:annotation>
												<xs:documentation>Идентификатор объекта AddService</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:complexType>
								</xs:element>
								<xs:element name="Payer" type="newPersonType" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Плательщик (если не заполнен, берется из вносителя, если он не заполнен, то абонента инфоуслуги)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:sequence minOccurs="0">
									<xs:annotation>
										<xs:documentation>Разовый платеж</xs:documentation>
									</xs:annotation>
									<xs:element name="Payee" type="newPayeeType">
										<xs:annotation>
											<xs:documentation>Получатель платежа</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="info" type="xs:string">
										<xs:annotation>
											<xs:documentation>Назначение платежа</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:sequence>
						</xs:choice>
						<xs:element name="Person" type="newPersonType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Вноситель денег (если не заполнен, берется из плательщика, если и он не заполнен, то из абонента инфоуслуги)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:choice minOccurs="0">
							<xs:element name="MainOrder" type="newMainOrderType">
								<xs:annotation>
									<xs:documentation>Документ по простой услуге</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="OrderElectro">
								<xs:annotation>
									<xs:documentation>* Документ по электроэнергии</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="MainOrder" type="newMainOrderType">
											<xs:annotation>
												<xs:documentation>Документ по простой услуге</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="startCounter" type="xs:integer" use="optional">
										<xs:annotation>
											<xs:documentation>Начальные показания счетчика</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="endCounter" type="xs:integer" use="optional">
										<xs:annotation>
											<xs:documentation>Конечные показания счетчика</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="consumption" type="xs:integer" use="optional">
										<xs:annotation>
											<xs:documentation>Расход в кВт</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="extraAmount" type="xs:decimal" use="optional">
										<xs:annotation>
											<xs:documentation>Сумма доплаты</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="actAmount" type="xs:decimal" use="optional">
										<xs:annotation>
											<xs:documentation>Сумма контрольного акта</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="ListOfSubOrder">
								<xs:annotation>
									<xs:documentation>Список сумм по подуслугам</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence maxOccurs="unbounded">
										<xs:element name="SubOrder">
											<xs:annotation>
												<xs:documentation>Документ по подуслуге</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="SubService">
														<xs:annotation>
															<xs:documentation>Подуслуга</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute ref="id" use="required">
																<xs:annotation>
																	<xs:documentation>Идентификатор объекта SubService</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
													<xs:element name="MainOrder" type="newMainOrderType">
														<xs:annotation>
															<xs:documentation>Документ на оплату подуслуги</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:choice>
						<xs:element name="PointPay" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Пункт оплаты услуг, для которого создавать документ (по умолчанию пункт пользователя, к которому привязана карта, либо пункт пользователя XML-сервера)</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute ref="id" use="required">
									<xs:annotation>
										<xs:documentation>Идентификатор пункта оплаты услуг</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="UserSys" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Пользователь, для которого создается документ (по умолчанию пользователь, к которому привязана карта, либо пользователь XML-сервера)</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute ref="id" use="required">
									<xs:annotation>
										<xs:documentation>Идентификатор объекта UserSys</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="Tariff" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Тариф, по которому производилась оплата</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute ref="id" use="required">
									<xs:annotation>
										<xs:documentation>Идентификатор тарифа</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="PayPeriod" type="payPeriodType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Период оплаты</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PayForm" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Форма оплаты</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="type" type="payFormEnumType" default="cashless">
									<xs:annotation>
										<xs:documentation>Тип оплаты (наличная/безналичная)</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="short" type="xs:string" use="optional">
									<xs:annotation>
										<xs:documentation>* Форма оплаты (наличные/безналичные/по карте/автоматическое списание/и т.д.)</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="amount" type="xs:decimal" use="required">
						<xs:annotation>
							<xs:documentation>Сумма платежа</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="cashnum" type="xs:string">
						<xs:annotation>
							<xs:documentation>Номер кассы/терминала</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="calcmode" type="xs:decimal" use="optional">
						<xs:annotation>
							<xs:documentation>Режим расчета формул: 1 - перед поднятием формы, 2 - перерасчет, в зависимости от введеных значений, 3 - перерасчет при сохранении документа. Для режима 1 выполняются формулы типа FORMULAI, для режима 2 - FORMULA, для режима 3 - FORMULAS</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="EPay" minOccurs="0">
				<xs:annotation>
					<xs:documentation>* Пачка для документа</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="id" type="idType" use="required">
						<xs:annotation>
							<xs:documentation>Идентификатор объекта EPay (для добавления документа в существующую пачку). Если id="new" документ всегда создается в новой пачке</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="id" type="idType" use="optional">
			<xs:annotation>
				<xs:documentation>* Идентификатор объекта EOrder (для пересоздания старого документа)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
