						<xs:documentation>Запрос дополнительной информации о Card</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="oper" type="cardOperEnumType" use="required">
				<xs:annotation>
					<xs:documentation>Тип операции</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ReqCloseAbonent">
		<xs:annotation>
			<xs:documentation>Запрос на закрытие счета абонента</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element ref="Credential" minOccurs="0"/>
					<xs:element ref="Subject"/>
				</xs:choice>
				<xs:element ref="lic" minOccurs="0"/>
				<xs:element name="Abonent">
					<xs:annotation>
