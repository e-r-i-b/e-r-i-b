		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element ref="Credential" minOccurs="0"/>
					<xs:element ref="Subject"/>
				</xs:choice>
				<xs:element ref="lic" minOccurs="0"/>
				<xs:element name="Card" type="card4OperType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дополнительная информация о карте для совершения операции</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ListOfMoreInfo" type="listOfMoreInfoType" minOccurs="0">
