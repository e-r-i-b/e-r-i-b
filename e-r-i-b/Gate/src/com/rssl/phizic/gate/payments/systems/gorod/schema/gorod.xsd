	<xs:complexType name="newListOfOrderExtentType">
		<xs:annotation>
			<xs:documentation>Тип для значений полей дополнений при расчете/сохранении документа</xs:documentation>
		</xs:annotation>
		<xs:sequence maxOccurs="unbounded">
			<xs:element name="OrderExtent">
