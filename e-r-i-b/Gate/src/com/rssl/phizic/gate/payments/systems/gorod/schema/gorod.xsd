				<xs:element ref="rDesc"/>
				<xs:element ref="RGroup" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="lifeTime" type="xs:string">
				<xs:annotation>
					<xs:documentation>Время достоверности данных</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="RGroup">
		<xs:annotation>
			<xs:documentation>Группа данных</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ListOfGVal" minOccurs="0"/>
				<xs:choice minOccurs="0">
					<xs:element ref="ListOfLine"/>
					<xs:sequence>
						<xs:element ref="RGroup" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:choice>
				<xs:element ref="ListOfIVal" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RParam">
		<xs:annotation>
			<xs:documentation>Параметры отчета</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Sorting" minOccurs="0"/>
				<xs:element ref="Filtering" minOccurs="0"/>
				<xs:element ref="Grouping" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="RType">
		<xs:annotation>
			<xs:documentation>Описание отчета и его допустимые атрибуты</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="rname"/>
				<xs:element ref="ListOfAtr"/>
			</xs:sequence>
			<xs:attribute name="rcode" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Код отчета в системе Город</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="Sorting">
		<xs:annotation>
			<xs:documentation>Перечисление атрибутов отчета, по которым должна производится сортировка, с указанием порядока сортировки (прямой, обратный). Сортировка выполняется в порядке следования элементов sortAtr</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="sortAtr" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Subject">
		<xs:annotation>
			<xs:documentation>Идентификатор клиента, выполняющего запрос к системе "Город"</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="pan" type="xs:string">
					<xs:annotation>
						<xs:documentation>ПАН карты "Город"</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TransIDs">
		<xs:annotation>
			<xs:documentation>* Идентификатор транзакции</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="transID" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Идентификатор транзакции</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="RSASMsg">
		<xs:annotation>
			<xs:documentation>Корневая структура, содержит все запросы и ответы</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:annotation>
					<xs:documentation>Req/Ans</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:annotation>
						<xs:documentation>Req</xs:documentation>
					</xs:annotation>
					<xs:choice maxOccurs="unbounded">
						<xs:element ref="ReqAuth"/>
						<xs:element ref="ReqCalcEOrder"/>
						<xs:element ref="ReqCard"/>
						<xs:element ref="ReqCardOper"/>
						<xs:element ref="ReqCloseAbonent"/>
						<xs:element ref="ReqCursor"/>
						<xs:element ref="ReqDelEOrder"/>
						<xs:element ref="ReqDelEPay"/>
						<xs:element ref="ReqGlobal"/>
						<xs:element ref="ReqLinkCard"/>
						<xs:element ref="ReqListOfPay"/>
						<xs:element ref="ReqMassPay"/>
						<xs:element ref="ReqMessage"/>
						<xs:element ref="ReqOrder4Pay"/>
						<xs:element ref="ReqPayAdvice"/>
						<xs:element ref="ReqPayInform"/>
						<xs:element ref="ReqPrintCheque"/>
						<xs:element ref="ReqReeIn"/>
						<xs:element ref="ReqReport"/>
						<xs:element ref="ReqSample"/>
						<xs:element ref="ReqSaveAbonent"/>
						<xs:element ref="ReqSaveEOrder"/>
						<xs:element ref="ReqSaveEPay"/>
						<xs:element ref="ReqSendEPay"/>
						<xs:element ref="ReqUnlinkCard"/>
						<xs:element ref="ReqXObject"/>
					</xs:choice>
				</xs:sequence>
				<xs:choice>
					<xs:annotation>
						<xs:documentation>Ans</xs:documentation>
					</xs:annotation>
					<xs:choice maxOccurs="unbounded">
						<xs:element ref="AnsAuth"/>
						<xs:element ref="AnsCalcEOrder"/>
						<xs:element ref="AnsCard"/>
						<xs:element ref="AnsCardOper"/>
						<xs:element ref="AnsCloseAbonent"/>
						<xs:element ref="AnsCursor"/>
						<xs:element ref="AnsDelEOrder"/>
						<xs:element ref="AnsDelEPay"/>
						<xs:element ref="AnsGlobal"/>
						<xs:element ref="AnsLinkCard"/>
						<xs:element ref="AnsListOfPay"/>
						<xs:element ref="AnsMassPay"/>
						<xs:element ref="AnsMessage"/>
						<xs:element ref="AnsOrder4Pay"/>
						<xs:element ref="AnsPayAdvice"/>
						<xs:element ref="AnsPayInform"/>
						<xs:element ref="AnsPrintCheque"/>
						<xs:element ref="AnsReeIn"/>
						<xs:element ref="AnsReport"/>
						<xs:element ref="AnsSample"/>
						<xs:element ref="AnsSaveAbonent"/>
						<xs:element ref="AnsSaveEOrder"/>
						<xs:element ref="AnsSendEPay"/>
						<xs:element ref="AnsSaveEPay"/>
						<xs:element ref="AnsUnlinkCard"/>
						<xs:element ref="AnsXObject"/>
					</xs:choice>
					<xs:element ref="Code"/>
				</xs:choice>
			</xs:choice>
			<xs:attribute name="version" type="xs:string" fixed="2.02.001.26">
				<xs:annotation>
					<xs:documentation>Версия схемы</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="ReqAuth">
		<xs:annotation>
			<xs:documentation>Запрос на аутентификацию клиента</xs:documentation>
