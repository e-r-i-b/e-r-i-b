					<xs:element ref="Subject"/>
				</xs:choice>
				<xs:element ref="lic" minOccurs="0"/>
				<xs:element ref="Cache" minOccurs="0"/>
				<xs:element name="Cursor">
					<xs:annotation>
						<xs:documentation>Выполняемый курсор</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:choice>
							<xs:element name="id" type="idType">
								<xs:annotation>
									<xs:documentation>Идентификатор объекта Prog</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="short" type="xs:string">
