								<xs:documentation>Поставщик услуг</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute ref="id" use="required">
									<xs:annotation>
										<xs:documentation>Идентификатор Provider</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:sequence>
				<xs:element name="ListOfMoreInfo" type="listOfMoreInfoType" minOccurs="0">
