								<xs:documentation>Агент</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute ref="id" use="required">
									<xs:annotation>
										<xs:documentation>Идентификатор Agent</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
