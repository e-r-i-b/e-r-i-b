										<xsd:documentation>8-ный код территориального банка</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:restriction>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Status">
				<xsd:annotation>
					<xsd:documentation>Статусное сообщение (результат выполнения операции)</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:restriction base="Status_Type">
							<xsd:sequence>
								<xsd:element name="StatusCode" type="StatusCode_Type">
									<xsd:annotation>
										<xsd:documentation>Статусный код возврата</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="StatusDesc" type="StatusDesc_Type" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Описание статуса</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ServerStatusDesc" type="ServerStatusDesc_Type" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Описание статуса системы источника</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:restriction>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
