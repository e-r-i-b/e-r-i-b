			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status" minOccurs="0"/>
			<xsd:element name="DepoAcctDeptInfoRec">
				<xsd:complexType>
					<xsd:annotation>
						<xsd:documentation>Запись о счете депо</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element ref="DepoAcctId"/>
						<xsd:element name="DepoDeptRes" type="DepoDeptResZad_Type" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoDeptRes_Type">
		<xsd:annotation>
			<xsd:documentation>Запись о счете депо</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="DepoAcctId"/>
			<xsd:element ref="AcctBal" minOccurs="0"/>
			<xsd:element name="DepoAcctBalRec" type="DepoAcctBalRec_Type" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoDeptResZad_Type">
		<xsd:annotation>
			<xsd:documentation>Запись о задолженности на счете ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RecNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер выставленного счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EffDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата выставления счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="DeptRec" minOccurs="0"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoDeptsInfoRsType">
		<xsd:annotation>
			<xsd:documentation>Расшифровка задолженности на счете ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status" minOccurs="0"/>
			<xsd:element name="DepoAcctDeptInfoRec">
				<xsd:complexType>
					<xsd:annotation>
						<xsd:documentation>Запись о счете депо</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="DepoDeptRes" type="DepoDeptRes_Type"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoDetailOperationReason_Type">
		<xsd:annotation>
			<xsd:documentation>Основание операции - описание договора (ДЕПО).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AgreementDetail" type="DepoAgreement_Type">
				<xsd:annotation>
					<xsd:documentation>описание договора (ДЕПО)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoRecMethodr_Type">
		<xsd:annotation>
			<xsd:documentation>Метод обработки информации (ДЕПО)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Method" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Номер рублевого счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AgreementDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Вид пластиковой карты. Обязательно, если карт счет.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AgreementNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер рублевого счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoRevokeDocRqType">
		<xsd:annotation>
			<xsd:documentation>Отзыв документа ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="DocNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер отзываемого  документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RevokePurpose" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Причина отзыва документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RevokeDate" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Дата отзыва документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OperInfo" type="OperInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация об операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoRevokeDocRsType">
		<xsd:annotation>
			<xsd:documentation>Отзыв документа ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoSecurityDictInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Запись о ценной бумаге в справочнике (ДЕПО).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Issuer" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Эмитент ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecurityName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecurityNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Регистрационный номер ценной бумаги </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecurityType" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Тип ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecurityNominal" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Минимальный номинал выпуска ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecurityNominalCur" type="AcctCur_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Валюта минимального номинала ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InsideCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Депозитарный  код выпуска ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsDelete" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак была ли запись удалена</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoSecurityOperationList_Type">
		<xsd:annotation>
			<xsd:documentation>Планируемая операция над ценной бумагой</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OperName" type="DepoSecurityOperationType_Type" minOccurs="0" maxOccurs="5">
				<xsd:annotation>
					<xsd:documentation>Наименование операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CustomOperName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование операции (в виде текста)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoSecurityOperationInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Параметры операции с ценной бумагой (ДЕПО).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DocumentDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentNumber" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Номер поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecurityName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecurityNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Регистрационный номер ценной бумаги </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Issuer" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Эмитент ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Depositary" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation/>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InsideCode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Operations" type="DepoSecurityOperationList_Type" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Планируемые операции над ценной бумагой</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DivisionNumber_Type">
		<xsd:annotation>
			<xsd:documentation>Тип и номер раздела</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Type" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Тип раздела. Должен представлять собой текст на русском языке начинающийся с большой буквы, готовый для показа пользователю.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Number" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер раздела.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoSecurityRec_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о разделе счета (ДЕПО).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DivisionNumber" type="DivisionNumber_Type">
				<xsd:annotation>
					<xsd:documentation>Тип и номер раздела</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SectionInfo" type="DepoSecuritySectionInfo_Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Информация о ценной бумаге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoSecuritySectionInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о ценной бумаге (ДЕПО).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SecurityMarker" type="SecurityMarker_Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Маркер ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InsideCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Депозитарный код выпуска ценной бумаги (партии неэмисионной ЦБ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Remainder" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Остаток, кол-во ценных бумаг на счете(шт.)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StorageMethod" type="DepoStorageMethod_Type">
				<xsd:annotation>
					<xsd:documentation>Метод хранения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DeptId_Type">
		<xsd:annotation>
			<xsd:documentation>Идентификатор долга.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RecNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер выставленного счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EffDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата выставления счета </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DeptRec_Type">
		<xsd:annotation>
			<xsd:documentation>Реквизиты для оплаты задолженности ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="BIC" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>БИК банка получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование банка получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CorBankAccount" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Корсчет банка получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecipientName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxId" type="TaxId_Type">
				<xsd:annotation>
					<xsd:documentation>ИНН получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="KPP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>КПП получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecipientAccount" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Расчетный счет получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DetailAcctInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Детальная информация по вкладу</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="DepAcctId"/>
			<xsd:element ref="DepAccInfo"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DocInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о переводе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="DepAcctId"/>
			<xsd:element name="Activity" type="String">
				<xsd:annotation>
					<xsd:documentation>Действия с вкладом: 1 – расторгнуть договор с закрытием счета по вкладу, 2 – выдать дубликат сберкнижки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecAcc" type="AcctIdType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер счета, для перечисления средств со вклада</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsByCash" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Выдать наличными</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DocStateUpdateRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип запроса на оповещение об изменении статуса подписки</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID" minOccurs="0"/>
			<xsd:element ref="SPName"/>
			<xsd:element name="Document" type="Document_Type" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DocStateUpdateRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа на запрос на оповещение об изменении статуса подписки</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID" minOccurs="0"/>
			<xsd:element name="Document" type="Document_Type" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Информация об обновлении статуса документ на стороне получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Document_Type">
		<xsd:annotation>
			<xsd:documentation>Тип документа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AutoSubscriptionId" type="AutoSubscriptionId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификационная информация о подписке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocNumber">
				<xsd:annotation>
					<xsd:documentation>Номер документа, по которому поменялся статус</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="C">
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EDBOContract_Type">
		<xsd:annotation>
			<xsd:documentation>Данные депозитного счета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="BankInfo" type="BankInfo_Type"/>
			<xsd:element name="MobilePhone" type="xsd:string"/>
			<xsd:element name="MobileOperator" type="xsd:string"/>
			<xsd:element name="Email" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EmployeeOfTheVSP_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных сотрудника ВСП, выполнившего действие</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Login">
				<xsd:annotation>
					<xsd:documentation>Логин</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="C">
						<xsd:maxLength value="120"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element ref="PersonName">
				<xsd:annotation>
					<xsd:documentation>ФИО Сотрудника ВСП</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EmploymentHistoryType">
		<xsd:annotation>
			<xsd:documentation>Тип &lt;Информация о месте работы&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OrgInfo" type="OrgInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о работодателе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="JobTitle" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Должность</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ExecStatus_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация об исполнении автоплатежа в АС AutoPay</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ExecPaymentDate" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата исполнения платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NonExecReasonCode" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код причины неисполнения платежа. Присутствует только для платежей со статусом Отменен. (PaymentStatus = Canceled)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NonExecReasonDesc" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Причина неисполнения. Присутствует только для платежей со статусом Отменен. (PaymentStatus = Canceled)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ExctractLine_Type">
		<xsd:sequence>
			<xsd:element name="PayDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Summa" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SummaCur" type="AcctCur_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Валюта суммы платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MonthOfPay" type="Date">
