			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="BankAcctRec" type="ImsRec_Type" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ImaAcctInRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса IMA_IS_s получения детальной информации по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status" minOccurs="0"/>
			<xsd:element ref="ImsRec" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMAAcctId_Type">
		<xsd:annotation>
			<xsd:documentation>Тип информация об ОМС счете (для переводов)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="AcctId">
				<xsd:annotation>
					<xsd:documentation>Номер ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CustInfo"/>
			<xsd:element ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMAInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип информация об ОМС договоре</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="CustRec"/>
			<xsd:element ref="AcctId" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AcctCur">
				<xsd:annotation>
					<xsd:documentation>Код металла</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctCode" type="Long">
				<xsd:annotation>
					<xsd:documentation>Вид вклада</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctSubCode" type="Long">
				<xsd:annotation>
					<xsd:documentation>Подвид вклада</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankInfo" type="MinBankInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о номере ОСБ и ВСП, в котором открывается ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMAInfoResponse_Type">
		<xsd:annotation>
			<xsd:documentation>Тип информация об ОМС договоре (ответ)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="AcctId">
				<xsd:annotation>
					<xsd:documentation>Номер ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMAOperConvInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация о конверсии при операциях с ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CurType" type="IMAOperCurType_Type">
				<xsd:annotation>
					<xsd:documentation>Признак: операция с металлом или денежными средствами</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CurAmt">
				<xsd:annotation>
					<xsd:documentation>Сумма операции в рублях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AcctCur" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Валюта</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMAToCardAddRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип запроса для интерфейса SrvIMAToCardAdd (TIС). Продажа обезличенного металла с зачислением денежных средств на МБК того же клиента. Операция ОМС-Карта</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="XferInfo" type="XferIMAInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о реквизитах совершаемой операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMAToCardAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа для интерфейса SrvIMAToCardAdd (TIC). Продажа обезличенного металла с зачислением денежных средств на МБК того же клиента. Операция ОМС-Карта</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ImsAcctId_Type">
		<xsd:sequence>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="AcctId"/>
			<xsd:element ref="BankInfo" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ImsAcctInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="AcctCur" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Вид металла</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Краткое наименование. На русском языке готовое для отображения пользователю</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Amount" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Остаток в граммах(Значение дробной части должно быть указано с точностью до второго знака.)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StartDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата открытия счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата закрытия счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Status" type="IMSStatusEnum_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Статус счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AgreementNumber" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер договора счета ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MaxSumWrite" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Максимальная сумма списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BankInfo" minOccurs="0"/>
			<xsd:element ref="CustRec" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMSAcctRec_Type">
		<xsd:annotation>
			<xsd:documentation>Выписка по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="BankAcctId"/>
			<xsd:element ref="BankAcctFullStmtInfo" minOccurs="0"/>
			<xsd:element ref="BankAcctFullStmtRec" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="BankAcctStmtRec" minOccurs="0" maxOccurs="10"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMSBalance_Type">
		<xsd:annotation>
			<xsd:documentation>Тип баланса по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Amt" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Остаток в граммах</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AmtRub" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Остаток в рублях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ImsRec_Type">
		<xsd:annotation>
			<xsd:documentation>Информация по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="ImsAcctId"/>
			<xsd:element ref="ImsAcctInfo" minOccurs="0"/>
			<xsd:element ref="Status" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InsuranceApp_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о страховом/НПФ продукте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Reference" type="String">
				<xsd:annotation>
					<xsd:documentation>ID (референс) страховки, по которой запрашиваются данные</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BusinessProcess" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование бизнес-процесса, в рамках которого оформлена страховка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProductType" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование типа продукта, в рамках которого оформлена страховка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Status" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Статус страховки для отображения. Для страховых продуктов (Действует, Срок действия истекает, Срок действия истек, Аннулирован). Для НПФ продуктов (Договор заключен, Действующий клиент НПФ Сбербанка, Договор расторгнут).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Company" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Страховая компания для отображения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Program" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Страховая программа для отображения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StartDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата начала действия страховки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата окончания действия страховки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SNILS" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер СНИЛС для пенсионного страхования</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Amount" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Страховая сумма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AmountCur" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Валюта страховай суммы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PolicyDetails" type="PolicyDetails_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание реквизитов договора страхования: Серия, номер и дата выдачи полиса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Risks" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание страховых рисков</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdditionalInfo" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дополнительная информация (№ и дата кредитного договора (при условии оформления страховки в рамках кредитной сделки)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InsuranceAppList_Type">
		<xsd:annotation>
			<xsd:documentation>Список страховых/НПФ продуктов</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="InsuranceApp" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IntegrationInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Идентификаторы клиента в ИС &lt;IntegrationInfo&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IntegrationId" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ISCode" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Код системы, в которой будут проведены изменения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ISCustId" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Идентификатор клиента</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InterestOnDeposit_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для "Информации, предназначенная для перечисления процентов"</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="AcctId"/>
			<xsd:element ref="CardNum"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IntRate_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для процентов</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Rate" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Величина процентой ставки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IssueCardRq_Type">
		<xsd:annotation>
			<xsd:documentation>  Запрос  на  первичеый  выпуск  карты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RqUID" type="RqUID_Type">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RqTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время передачи сообщения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OperUID" type="OperUID_Type">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SPName" type="SPName_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы, инициировавший запрос</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SystemId" type="SystemId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы, предоставляющей услуги </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UserName" type="Identifier">
				<xsd:annotation>
					<xsd:documentation>Идентификатор пользователя, инициировавший заявку на выпуск карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OperName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование операции (PersonalCardESB)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankInfo" type="BankInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация  о  банке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CardAcctId" type="CardAcctId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор карточного счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CardAcctInfo" type="CardAcctInfoType">
				<xsd:annotation>
					<xsd:documentation>Информация по карте</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IssueCardRs_Type">
		<xsd:annotation>
			<xsd:documentation>  Ответ  на    запрос  первичный  выпуск  карты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RqUID" type="RqUID_Type">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RqTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время передачи сообщения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SPName" type="SystemId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы, инициировавший запрос</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SystemId" type="SystemId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы, предоставляющей услуги </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Status" type="Status_way_Type"/>
			<xsd:element name="CardAcctId" type="CardAcctId_Type" minOccurs="0"/>
			<xsd:element name="CardAcctInfo" type="CardAcctInfoType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация по карте</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Issuer_Type">
		<xsd:annotation>
			<xsd:documentation>ВСП, осуществляющий выдачу ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="BankInfo" type="BankInfoExt_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о банке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankOwnerInfo" type="Owner_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о банке - текущем правоприемнике ВСП, осуществившего выдачу ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanAcctId_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о кредите</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="SystemId" minOccurs="0"/>
			<xsd:element ref="AcctId" minOccurs="0"/>
			<xsd:element ref="AgreemtNum" minOccurs="0"/>
			<xsd:element ref="ProdType" minOccurs="0"/>
			<xsd:element ref="LoanType" minOccurs="0"/>
			<xsd:element name="DateCalc" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата, на которую нужно рассчитать задолженность</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CurAmt" minOccurs="0"/>
			<xsd:element ref="AcctCur" minOccurs="0"/>
			<xsd:element ref="OrigAmt" minOccurs="0"/>
			<xsd:element ref="BankInfo" minOccurs="0"/>
			<xsd:element name="ProdId" type="AgreemtNum_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор кредитного договора в ЕКП</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanAcctIdDepo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о кредите</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SystemId" type="SystemId_Type"/>
			<xsd:element ref="AcctId"/>
			<xsd:element ref="AcctCur"/>
			<xsd:element name="LoanStatus" type="LoanStatus_Type"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanAcctRec_Type">
		<xsd:annotation>
			<xsd:documentation>Список кредитов</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="LoanAcctId"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="BankAcctInfo"/>
			<xsd:element ref="CustRec"/>
			<xsd:element name="Period" type="Period_type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Срок договора в днях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CreditigRate" type="CreditingRate_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Процентная ставка по кредиту</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RegNextPayDate" type="RegNextPayDate_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Предельная дата платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NextPayDate" type="NextPayDate_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Предельная дата текущего платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NextPaySum" type="NextPaySum_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Предполагаемая сумма платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RegPayAmount" type="RegPayAmount_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Размер аннуитетного платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PaymentStatus" type="PaymentStatus_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Статус оплаты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanStatus" type="LoanStatus_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Статус кредита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrevPayDate" type="PrevPayDate_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата предыдущего платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrevPaySum" type="PrevPaySum_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма предыдущего платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanInfo_Type">
		<xsd:sequence>
			<xsd:element name="AcctBalFull" type="AcctBal_Type" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="AcctBalOnDate" type="AcctBal_Type" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="IdSpacing" type="xsd:string" minOccurs="0"/>
			<xsd:element ref="LoanAcctId" minOccurs="0"/>
			<xsd:element ref="AgreemtNum" minOccurs="0"/>
			<xsd:element ref="ProdType" minOccurs="0"/>
			<xsd:element ref="LoanType" minOccurs="0"/>
			<xsd:element ref="AcctCur" minOccurs="0"/>
			<xsd:element ref="CurAmt" minOccurs="0"/>
			<xsd:element name="Period" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Срок договора в днях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CreditingRate" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Процентная ставка по кредиту</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RegNextPayDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Предельная дата платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NextPayDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Предельная дата текущего периода</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NextPaySum" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Предполагаемая сумма платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RegPayAmount" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Размер аннуитетного платежа, поле обязательно, если признак аннуитетности равен true</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PaymentStatus" type="PaymentStatus_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Статус оплаты, 1 – не просрочен, 2 – просрочен</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanStatus" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Статус кредита, 1 – открыт, 2 – закрыт</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrevPayDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата предыдущего платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrevPaySum" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма предыдущего платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PayCard" type="CardNumType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Карта оплаты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CustRec" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanInfoRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса LN_CI получения детальной информации по кредиту</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="LoanAcctId"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanInfoRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса LN_CI получения детальной информации по кредиту</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status" minOccurs="0"/>
			<xsd:element ref="LoanRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса IIC получения информации о задолженности по кредиту</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="LoanAcctId"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса IIC получения информации о задолженности по кредиту</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element ref="LoanRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanPaymentRec_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о платеже по кредиту</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="LoanPaymentNumber" type="Long">
				<xsd:annotation>
					<xsd:documentation>Порядковый номер платежа в графике</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanPaymentStatus" type="String">
				<xsd:annotation>
					<xsd:documentation>Статус: 1 – оплачен, 2 – текущая оплата, 3 – к оплате</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanPaymentDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата погашения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RemainDebt" type="Amt_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Остаток основного долга после внесения платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctBalOnDate" type="AcctBal_Type" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanPaymentRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса LN_PSC получения графика платежей</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="LoanAcctId"/>
			<xsd:element name="StartNumber" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер платежа, с которого необходимо отобразить график</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MaxForwardCount" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Максимальное количество будущих платежей, которое может быть возвращено</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MaxRewardCount" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Макисмальное количество платежей, которое может быть возвращено</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanPaymentRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса LN_PSC получения графика платежей</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="Status"/>
			<xsd:element ref="AcctId" minOccurs="0"/>
			<xsd:element name="DoneAmount" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма всех выплат, которые были произведены, с учетом штрафов и проч.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RemainAmount" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма платежа для закрытия кредита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FineAmount" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма всех штрафов и пеней</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MaxSize" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Общее количество платежей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="LoanPaymentRec" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanRec_Type">
		<xsd:sequence>
			<xsd:element ref="LoanInfo" minOccurs="0"/>
			<xsd:element ref="BankInfo" minOccurs="0"/>
			<xsd:element name="BankAccInfo" type="BankAcctInfo_Type" minOccurs="0"/>
			<xsd:element ref="Status" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MBCInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация об услуге мобильный банк</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Status" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Подключена услуга или нет</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ContractType" type="xsd:long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип пакета (0 - полный, 1 - экономический)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PhoneNum" type="PhoneNum_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация об операторе и моб.телефоне, к которому подключена услуга &lt; Мобильный банк &gt;</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MDMClientInfoUpdateRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса MDM_CLT регистрации и обновления данных о клиенте в MDM и ЕРИБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="OperType" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CustRec"/>
			<xsd:element name="UserInfo" type="UserInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о пользователе, осуществившим изменения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MDMClientInfoUpdateRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса MDM_CLT регистрации и обновления данных о клиенте в MDM и ЕРИБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element name="IntegrationInfo" type="IntegrationInfo_Type" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Menu_Type">
		<xsd:annotation>
			<xsd:documentation>Список значений для выбора клиентом</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="MenuItem" type="C" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Одно из значений для списка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MessageRecvRqType">
		<xsd:annotation>
			<xsd:documentation>Оповещение о приеме сообщения ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element name="DocNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер сформированного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MessageRecvRsType">
		<xsd:annotation>
			<xsd:documentation>Оповещение о приеме сообщения ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element name="DocNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер сформированного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OperInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип &lt;Информация об операции&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DocumentDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentNumber" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Номер поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OrgInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип &lt;Информация об организации&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="String">
				<xsd:annotation>
					<xsd:documentation>Название организации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxId" type="TaxId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ИНН</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OTPRestrictionModRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфейса OTPR - изменение ограничения на печать и использование одноразовых паролей по карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="CardAcctRec" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OTPRestrictionModRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-ответа для интерфейса OTPR - изменение ограничения на печать и использование одноразовых паролей по карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element name="BankAcctResult" type="BankAcctResult_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Результат изменения для каждой карты в отдельности</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OTPRestriction_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для блока "Группа ограничений для одноразовых паролей (OTP)"</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OTPGet" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак возможности печати одноразовых паролей по карте
true - установить возможность 
false - запретить возможность 
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OTPUse" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак возможности использования ранее выданных одноразовых паролей.
true – выданные ранее одноразовые пароли не блокируются
false - - выданные ранее одноразовые парли блокируются
Обязателен при OPTGet=false. 
Для сервиса GFL следующий смысл "Признак наличия по карте ранее выданных одноразовых паролей" 
true – пароли есть
false – паролей нет
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Owner_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о ВСП, на имя которого осуществляется учет операции</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="BankInfo" type="BankInfoExt_Type">
				<xsd:annotation>
					<xsd:documentation>Расширенная информация о банке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PayDay_Type">
		<xsd:annotation>
			<xsd:documentation>Расписание  исполнения АП.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Day" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>День платежа в месяце. Передается значение от 1 до количества дней в месяце создания АП (январь - 31, в апрель - 30)
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MonthInQuarter" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер месяца в квартале: от 1 до 3</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MonthInYear" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер месяца в году: от 1 до 12</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WeekDay" type="WeekDay_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>День недели платежа: –	Monday – Понедельник;–	Tuesday - Вторник;–	Wednesday  - Среда;–	Thursday - Четверг–	Friday – Пятница;–	Saturday – Суббота;Sunday – Воскресенье</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PayInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о переводе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="CardAcctId" minOccurs="0"/>
			<xsd:element name="AcctId" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="SystemId"/>
						<xsd:element name="AcctIdFrom" type="AcctIdType"/>
						<xsd:element ref="BankInfo"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element ref="DepoAcctId"/>
			<xsd:element ref="DeptRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PaymentInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о платеже</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PaymentStatus" type="PaymentStatusASAP_Type">
				<xsd:annotation>
					<xsd:documentation>Код состояния платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PaymentStatusDesc" type="String">
				<xsd:annotation>
					<xsd:documentation>Текстовое описание состояния платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Commission" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Комиссия платежа в рублях </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MadeOperationId" type="String">
				<xsd:annotation>
					<xsd:documentation>Код операции в биллинге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExecStatus" type="ExecStatus_Type">
				<xsd:annotation>
					<xsd:documentation>Информация об исполнении автоплатежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PaymentDetails_Type">
		<xsd:annotation>
			<xsd:documentation>Платежные реквизиты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="BIC" type="BIC_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>БИК</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CorrAcctNum" type="AcctId_NC_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер кор. счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctNum" type="AcctId_NC_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InterfilialAcctNum" type="AcctIdType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер счета для МФР</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PaymentList_Type">
		<xsd:annotation>
			<xsd:documentation>Список задолженностей по АП</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AutoPaymentId" type="AutoPaymentId_Type"/>
			<xsd:element name="PaymentInfo" type="PaymentInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о платеже</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecipientRec" type="RecipientRec_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о поставщике</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankAcctRec">
				<xsd:annotation>
					<xsd:documentation>Информация о платёжном средстве, с которого производилась оплата</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:restriction base="BankAcctRec_Type">
							<xsd:sequence>
								<xsd:element name="CardAcctId">
									<xsd:complexType>
										<xsd:complexContent>
											<xsd:restriction base="CardAcctId_Type">
												<xsd:sequence>
													<xsd:element name="SystemId" type="SystemId_Type"/>
													<xsd:element name="CardNum" type="CardNumType"/>
													<xsd:element name="AcctId" type="AcctIdType" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Номер карточного счета для карт (из ЦОД)</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="CardType" type="C">
														<xsd:annotation>
															<xsd:documentation>Тип пластиковой карты. CC- кредитная, DC - дебетовая, OV - с овердрафтом. Для АС Филиала используется для услуги МБК (0 - основная или 1- активная)</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="CardLevel" type="CardLevel_Type">
														<xsd:annotation>
															<xsd:documentation>Вид карты в рамках МПС</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="CardBonusSign" type="CardBonusSign_Type" minOccurs="0">
														<xsd:annotation>
															<xsd:documentation>Признак принадлежности карты к бонусной программе</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="AcctCur" type="AcctCur_Type" minOccurs="0"/>
													<xsd:element name="EndDt" type="EndDt_Type"/>
													<xsd:element name="CustInfo">
														<xsd:complexType>
															<xsd:complexContent>
																<xsd:restriction base="CustInfo_Type">
																	<xsd:sequence>
																		<xsd:element name="PersonInfo">
																			<xsd:annotation>
																				<xsd:documentation>Информация о физическом лице.</xsd:documentation>
																			</xsd:annotation>
																			<xsd:complexType>
																				<xsd:complexContent>
																					<xsd:restriction base="PersonInfo_Type">
																						<xsd:sequence>
																							<xsd:element name="Birthday" type="Date" minOccurs="0">
																								<xsd:annotation>
																									<xsd:documentation>Дата рождения</xsd:documentation>
																								</xsd:annotation>
																							</xsd:element>
																							<xsd:element name="PersonName" type="PersonName_Type" minOccurs="0">
																								<xsd:annotation>
																									<xsd:documentation>ФИО клиента</xsd:documentation>
																								</xsd:annotation>
																							</xsd:element>
																							<xsd:element name="IdentityCard" minOccurs="0">
																								<xsd:annotation>
																									<xsd:documentation>Удостоверение личности.</xsd:documentation>
																								</xsd:annotation>
																								<xsd:complexType>
																									<xsd:complexContent>
																										<xsd:restriction base="IdentityCard_Type">
																											<xsd:sequence>
																												<xsd:element name="IdType" type="IdType_Type" minOccurs="0">
																													<xsd:annotation>
																														<xsd:documentation>Тип документа удостоверяющего личность</xsd:documentation>
																													</xsd:annotation>
																												</xsd:element>
																												<xsd:element name="IdSeries" type="IdSeries_Type" minOccurs="0">
																													<xsd:annotation>
																														<xsd:documentation>Серия документа удостоверяющего личность</xsd:documentation>
																													</xsd:annotation>
																												</xsd:element>
																												<xsd:element name="IdNum" type="IdNum_Type" minOccurs="0">
																													<xsd:annotation>
																														<xsd:documentation>Номер документа удостоверяющего личность</xsd:documentation>
																													</xsd:annotation>
																												</xsd:element>
																											</xsd:sequence>
																										</xsd:restriction>
																									</xsd:complexContent>
																								</xsd:complexType>
																							</xsd:element>
																							<xsd:element name="ContactInfo">
																								<xsd:annotation>
																									<xsd:documentation>Контактная информация</xsd:documentation>
																								</xsd:annotation>
																								<xsd:complexType>
																									<xsd:complexContent>
																										<xsd:restriction base="ContactInfo_Type">
																											<xsd:sequence>
																												<xsd:element name="PostAddr">
																													<xsd:annotation>
																														<xsd:documentation>Полный адрес</xsd:documentation>
																													</xsd:annotation>
																													<xsd:complexType>
																														<xsd:complexContent>
																															<xsd:restriction base="FullAddress_Type">
																																<xsd:sequence>
																																	<xsd:element name="AddrType" type="AddressType_Type">
																																		<xsd:annotation>
																																			<xsd:documentation>Тип адреса</xsd:documentation>
																																		</xsd:annotation>
																																	</xsd:element>
																																	<xsd:element name="Addr3" type="C">
																																		<xsd:annotation>
																																			<xsd:documentation>Ненормализованный адрес</xsd:documentation>
																																		</xsd:annotation>
																																	</xsd:element>
																																</xsd:sequence>
																															</xsd:restriction>
																														</xsd:complexContent>
																													</xsd:complexType>
																												</xsd:element>
																											</xsd:sequence>
																										</xsd:restriction>
																									</xsd:complexContent>
																								</xsd:complexType>
																							</xsd:element>
																						</xsd:sequence>
																					</xsd:restriction>
																				</xsd:complexContent>
																			</xsd:complexType>
																		</xsd:element>
																	</xsd:sequence>
																</xsd:restriction>
															</xsd:complexContent>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="BankInfo">
														<xsd:complexType>
															<xsd:complexContent>
																<xsd:restriction base="BankInfo_Type">
																	<xsd:sequence>
																		<xsd:element name="RbBrchId" type="RbBrchType">
																			<xsd:annotation>
																				<xsd:documentation>6-ный код территориального банка, в котором ведется продукт клиента (кредит, вклад, ОМС, карты)</xsd:documentation>
																			</xsd:annotation>
																		</xsd:element>
																	</xsd:sequence>
																</xsd:restriction>
															</xsd:complexContent>
														</xsd:complexType>
													</xsd:element>
												</xsd:sequence>
											</xsd:restriction>
										</xsd:complexContent>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:restriction>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="CardAuthorization" type="CardAuthorization_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация об авторизации карты в Way4, если операция прошла успешно</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PaymentStatusInfoList_Type">
