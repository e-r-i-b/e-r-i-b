	<xsd:complexType name="DepToNewIMAAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа для интерфейса TDIO. Открытие ОМС с переводом на него денежных средств со вклада</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element name="AgreemtInfo" type="AgreemtInfoResponse_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация об открытом счете ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="DstCurAmt" minOccurs="0"/>
			<xsd:element name="SrcLayoutInfo" type="SrcLayoutInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Разбивка микроопераций списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAccInfoRqType">
		<xsd:annotation>
			<xsd:documentation>Получение информации по счетам ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="DepoAcctId" maxOccurs="unbounded"/>
			<xsd:element name="OperInfo" type="OperInfo_Type" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAccInfoRsType">
		<xsd:annotation>
			<xsd:documentation>Получение информации по счетам ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status" minOccurs="0"/>
			<xsd:element name="DepoAcctInfoRec">
				<xsd:complexType>
					<xsd:annotation>
						<xsd:documentation>Результат выполнения запроса. Список записей о счете депо</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="DepoAcctRes" type="DepoAcctRes_Type" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
