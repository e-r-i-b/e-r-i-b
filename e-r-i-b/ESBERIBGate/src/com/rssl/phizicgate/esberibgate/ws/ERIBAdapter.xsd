	<xsd:simpleType name="OpenEnum">
		<xsd:annotation>
			<xsd:documentation>Открытое перечисление. Перечисление содержит конечный набор рекомендуемых значений. Допустимы значения, не входящие в рекомендуемый набор. Интепретация корректности значения осуществляется автором и конечным получателем. Промежуточные узлы не осуществляют проверку и считают значение всегда корректным.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="NC">
			<xsd:maxLength value="80"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OperName_Type">
		<xsd:annotation>
			<xsd:documentation>Тип операции</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TCC">
				<xsd:annotation>
					<xsd:documentation>Погашение кредита с карты</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TCD">
				<xsd:annotation>
					<xsd:documentation>Перевод с карты на вклад</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDC">
				<xsd:annotation>
					<xsd:documentation>Перевод со вклада на карту</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDD">
				<xsd:annotation>
					<xsd:documentation>Перевод со вклада на вклад</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TCP">
				<xsd:annotation>
					<xsd:documentation>Оплата товаров и услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SDP">
				<xsd:annotation>
					<xsd:documentation>Создание длительного поручения на перевод денежных средств с вклада физическому лицу</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CreateForm190ForLoanRequest">
				<xsd:annotation>
					<xsd:documentation>Создание длительного поручения на погашение кредита с вкладного счета</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDDC">
				<xsd:annotation>
					<xsd:documentation>Закрытие вклада с переводом денежных средств со счета закрываемого вклада на другой счет по вкладу</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDCC">
				<xsd:annotation>
					<xsd:documentation>Закрытие вклада с переводом денежных средств со счета закрываемого вклада на счет МБК</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDI">
				<xsd:annotation>
					<xsd:documentation>Перевод с вклада на ОМС (тоже самое что и TID, и ТDD)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TID">
				<xsd:annotation>
					<xsd:documentation>Перевод с ОМС на вклад (тоже самое что и TDI, и ТDD)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDIO">
				<xsd:annotation>
					<xsd:documentation>Открытие ОМС переводом с вклада</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TCIO">
				<xsd:annotation>
					<xsd:documentation>Открытие ОМС переводом с карты</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TCI">
				<xsd:annotation>
					<xsd:documentation>Перевод с карты на ОМС (покупка металла)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TIC">
				<xsd:annotation>
					<xsd:documentation>Перевод с ОМС на карту (продажа металла)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDDO">
				<xsd:annotation>
					<xsd:documentation>Открытие вклада переводом с другого вклада</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TCDO">
				<xsd:annotation>
					<xsd:documentation>Открытие вклада переводом с карты</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SrvChangeAccountInfo">
				<xsd:annotation>
					<xsd:documentation>Изменение информации по счету</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OperNameSec_Type">
		<xsd:annotation>
			<xsd:documentation>Наименование операции</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OperUID_Type">
		<xsd:annotation>
			<xsd:documentation>Уникальный идентификатор операции</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="UUID"/>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentStatus_Type">
		<xsd:annotation>
			<xsd:documentation>Статус оплаты, 1 – не просрочен, 2 – просрочен</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentStatusASAP_Type">
		<xsd:annotation>
			<xsd:documentation>Код состояния платежа в АС AutoPay</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="New">
				<xsd:annotation>
					<xsd:documentation>Новый</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Canceled">
				<xsd:annotation>
					<xsd:documentation>Отменён</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Done">
				<xsd:annotation>
					<xsd:documentation>Исполнен</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Reg_Debt"/>
			<xsd:enumeration value="Wait_Approve"/>
			<xsd:enumeration value="In_Exec"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ParticipantNumber_Type">
		<xsd:annotation>
			<xsd:documentation>Тип счета</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="10"/>
			<xsd:maxLength value="10"/>
			<xsd:pattern value="[0-9]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PermissType_Type">
		<xsd:annotation>
			<xsd:documentation>Типы прав доступа</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="View">
				<xsd:annotation>
					<xsd:documentation>Просмотр (при отсутствии этого права для сущности, она не должна отображаться в системе)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PhoneNumber_Type">
		<xsd:annotation>
			<xsd:documentation>Телефонный номер</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PfrAccount_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для Номера СНИЛС ПФР</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:pattern value="\d{3}-\d{3}-\d{3} \d{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PlaceCode_Type">
		<xsd:annotation>
			<xsd:documentation>Код типа населенного пункта</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Place_Type">
		<xsd:annotation>
			<xsd:documentation>Населенный пункт</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PmtKind_Type">
		<xsd:annotation>
			<xsd:documentation>Код вида платежа</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PostalCode_Type">
		<xsd:annotation>
			<xsd:documentation>Тип "Почтовый индекс"</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ProdType_Type">
		<xsd:annotation>
			<xsd:documentation>Идентификатор банковского продукта (ИД в ИАСК)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SummaKindCodeASAP_Type">
		<xsd:annotation>
			<xsd:documentation>Алгоритм расчета суммы очередного автоплатежа</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="FIXED_SUMMA_IN_RECIP_CURR">
				<xsd:annotation>
					<xsd:documentation>Фиксированная сумма</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BY_BILLING">
				<xsd:annotation>
					<xsd:documentation>По выставленному счету</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PERCENT_BY_ANY_RECEIPT">
				<xsd:annotation>
					<xsd:documentation>Заданный клиентом процент от любого зачисления на счет списания</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PERCENT_BY_DEBIT">
				<xsd:annotation>
					<xsd:documentation>Заданный клиентом процент от любого списания со счета</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RUR_SUMMA">
				<xsd:annotation>
					<xsd:documentation>Заданная клиентом сумма  в рублях</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FIXED_SUMMA">
				<xsd:annotation>
					<xsd:documentation>Заданная клиентом сумма  в валюте счета списания</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BY_BILLING_BASKET"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Rate_Type">
		<xsd:annotation>
			<xsd:documentation>Величина процентной ставки. Ставка налога в процентах. Доля участия в уставном капитале в процентах.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="Decimal"/>
	</xsd:simpleType>
	<xsd:simpleType name="RbBrchType">
		<xsd:annotation>
			<xsd:documentation>Тип данных для 6-значного кода территориального банка. Используется для передачи кода ТБ, в котором обслуживается продукт клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:length value="6"/>
			<xsd:pattern value="[0-9]{6}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RbTbBrchType">
		<xsd:annotation>
			<xsd:documentation>Тип данных для 8-значного кода территориального банка. Используется для передачи кода ТБ, в котором открыт счет МБК, по которой клиент произвел идентификацию на УС</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:length value="8"/>
			<xsd:pattern value="[0-9]{8}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RcptKind_Type">
		<xsd:annotation>
			<xsd:documentation>Код вида получателя</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="ReasonCode_Type">
		<xsd:annotation>
			<xsd:documentation>Код причины перевыпуска карты</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RegionId_Type">
		<xsd:annotation>
			<xsd:documentation>Код региона банка</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Resident_Type">
		<xsd:annotation>
			<xsd:documentation>Признак резидента</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:boolean"/>
	</xsd:simpleType>
	<xsd:simpleType name="RequestId_Type">
		<xsd:annotation>
			<xsd:documentation>Уникальный идентификатор заявки на создание/изменение подписки</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="NC">
			<xsd:pattern value="[0-9a-zA-Z]{32}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RqUID_Type">
		<xsd:annotation>
			<xsd:documentation>Уникальный идентификатор запроса</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="UUID"/>
	</xsd:simpleType>
	<xsd:simpleType name="RRN_Type">
		<xsd:annotation>
			<xsd:documentation>Тип регистрационного номера документа</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:length value="12"/>
			<xsd:pattern value="([0-9]|[a-f]|[A-F]){12}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SegmentCodeType">
		<xsd:annotation>
			<xsd:documentation>Код сегмента, к которому относят клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:enumeration value="0">
				<xsd:annotation>
					<xsd:documentation>отсутствует</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation>потенциально МВС (массовый высокодоходный сегмент)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3">
				<xsd:annotation>
					<xsd:documentation>ВИП</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="4">
				<xsd:annotation>
					<xsd:documentation>МВС (массовый высокодоходный сегмент)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ServerStatusDesc_Type">
		<xsd:annotation>
			<xsd:documentation>Исходное описание ошибки из внешней системы (заполняется только при значении StatusCode, отличном от 0.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String"/>
	</xsd:simpleType>
	<xsd:simpleType name="SPName_Type">
		<xsd:annotation>
			<xsd:documentation>Провайдер услуг(название системы, которая предоставляет услугу или запрашивает)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BP_ES">
				<xsd:annotation>
					<xsd:documentation>Устройство самообслуживания (СмартВиста)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_VSP">
				<xsd:annotation>
					<xsd:documentation>АС ВСП</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_ERIB">
				<xsd:annotation>
					<xsd:documentation>АС ЕРИБ для Базового продукта</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_COD">
				<xsd:annotation>
					<xsd:documentation>АС ЦОД</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_ESK">
				<xsd:annotation>
					<xsd:documentation>АС ЭСК</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_IASK">
				<xsd:annotation>
					<xsd:documentation>АС ИАСК</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_WAY">
				<xsd:annotation>
					<xsd:documentation>АС WAY4U</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_BS">
				<xsd:annotation>
					<xsd:documentation>АС БС</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_INFOBANK"/>
			<xsd:enumeration value="urn:sbrfsystems:99-autopay"/>
			<xsd:enumeration value="urn:sbrfsystems:99-crm"/>
			<xsd:enumeration value="urn:sbrfsystem:99-way"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SPNum_Type">
		<xsd:annotation>
			<xsd:documentation>Номер банкомата</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="8"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StatusCode_Type">
		<xsd:annotation>
			<xsd:documentation>Код ответа. Если все хорошо, то значение равно 0</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="Long"/>
	</xsd:simpleType>
	<xsd:simpleType name="StatusDesc_Type">
		<xsd:annotation>
			<xsd:documentation>Описание ответа - причина ошибки (заполняется только при значении StatusCode, отличном от 0.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StatusType_Type">
		<xsd:annotation>
			<xsd:documentation>Описание ответа - тип ошибки (заполняется только при значении StatusCode, отличном от 0.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="22"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StmtSummType_Type">
		<xsd:restriction base="String"/>
	</xsd:simpleType>
	<xsd:simpleType name="StmtType_Type">
		<xsd:annotation>
			<xsd:documentation>Тип выписки</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Short10">
				<xsd:annotation>
					<xsd:documentation>Короткая выписка, 10 последних операций</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Full">
				<xsd:annotation>
					<xsd:documentation>Полная выписка за период</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StreetCode_Type">
		<xsd:annotation>
			<xsd:documentation>Код улицы</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="15"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Street_Type">
		<xsd:annotation>
			<xsd:documentation>Наименование улицы</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="200"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Boolean">
		<xsd:restriction base="xsd:boolean"/>
	</xsd:simpleType>
	<xsd:simpleType name="Integer">
		<xsd:restriction base="xsd:integer"/>
	</xsd:simpleType>
	<xsd:simpleType name="String">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="String255Type">
		<xsd:restriction base="String">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="String50Type">
		<xsd:restriction base="String">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SystemId_Type">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TarifPlanCodeType">
		<xsd:annotation>
			<xsd:documentation>Тип Код тарифного плана</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:enumeration value="0">
				<xsd:annotation>
					<xsd:documentation>отсутствует</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>Сбербанк Премьер</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation>Сбербанк Золотой</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3">
				<xsd:annotation>
					<xsd:documentation>Сбербанк Первый</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TaxId_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных "ИНН".Элемент может содержать 10 или 12 символов</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="40"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TINType_Type">
		<xsd:annotation>
			<xsd:documentation>Тип налогового идентификатора</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="OpenEnum">
			<xsd:enumeration value="KIO"/>
			<xsd:enumeration value="INN"/>
			<xsd:enumeration value="EIN"/>
			<xsd:enumeration value="SSN"/>
			<xsd:enumeration value="NRABusiness"/>
			<xsd:enumeration value="NRAPersonal"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TransDirection_Type">
		<xsd:annotation>
			<xsd:documentation>Направление перевода</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TCC_OWN">
				<xsd:annotation>
					<xsd:documentation>перевод между своими картами</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TCC_P2P">
				<xsd:annotation>
					<xsd:documentation>перевод стороннему лицу на карту СБ РФ внутри одного ТБ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TCC_P2P_FOREIGN">
				<xsd:annotation>
					<xsd:documentation>перевод стороннему лицу на карту СБ РФ в другой ТБ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="UserCategoryType">
		<xsd:annotation>
