			<xsd:element name="Tariff" type="TarifUnionType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Тариф за обслуживание</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DLCode" type="FNC10" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>10-ный код (2 цифры – код рег.банка+4 цифры – код ОСБ+4 цифры – код ВСП). Используется только для СПООБК для передачи по кредитным картам</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TAGCode" type="YesNoType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Возможные значения - Y или N. Используется только для СПООБК для передачи по кредитным картам</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndDtForWay" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Срок действия карты в формате процессинга YYMM (передается для ЕРИБ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CardOrderNum" type="OrderNum_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер заявления-анкеты на получение карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CardOrderDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата заявления-анкеты на получение карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CardSubType" type="ClassifierCode_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Субтип контракта для карт с индивидуальным дизайном</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PersonalDesignRefNum" type="CardId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Ссылочный номер карты индивидуального дизайна</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RiskFactor" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Риск-фактор контракта. Рисковый параметр, который используется чиповой схемой.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BonusInfo" type="BonusInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Участие  в  программе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ContactInfoIssue_Type">
		<xsd:annotation>
			<xsd:documentation source="WMQI_DOCUMENTATION">Контактная информация</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PhoneNum" type="PhoneNumber_Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Телефонные номера</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmailAddr" type="EMailAddr_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="WMQI_DOCUMENTATION">Адрес электронной почты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="URL" type="URL" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Адрес Web сайта</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FullAddress" type="FullAddress_Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Список почтовых адресов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MessageDeliveryType" type="MessageDeliveryType_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Способ отправки отчетов по счету карты Тип доставки сообщений. Возможные значения:
E - e-mail
N - no e-mail\mail
S - sbrf e-mail
P – mail
I - e-mail/mail
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Contract_Type">
		<xsd:annotation>
			<xsd:documentation>Информация по счетовому контракту в WAY4</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ContractNumber" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер счетового контракта</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OpenDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата открытия счетового контракта YYYY-MM-DD</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProductCode" type="ProductCode_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код продукта счетового контракта 
CODEWAY4SHORT справочника CARD_MDM</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TariffClassifier_Type">
		<xsd:annotation>
			<xsd:documentation>Классификаторы тарифного плана</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Code" type="ClassifierCode_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код классификатора  тарифного   плана</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Value" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Значение классификатора  тарифного   плана</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Vid" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак  отнесения  классификатора  к контракту (счетовому карточному) Используется для  определения   классификаторов для счетовой  и   карточной   аппликации  для  WAYU.  Возможные   значения  Card - для  карточного  классификатора  Account-для  счетовoго  классификатора </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CustAddRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип: Запрос создания нового клиента в прцоессинговой системе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="CustInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CustAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип: Ответ на запрос создания нового клиента в процессинговой системе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TarifUnionType">
		<xsd:annotation>
			<xsd:documentation>  Информация   по  Тарифному    модулю</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TariffType" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип тарифа платы за обслуживание: 1- индивидуальный тариф, 2 – тариф банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FeeByFirstYear" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма тарифа за первый год обслуживания в валюте счета (в целых рублях/долларах/евро)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FeeByOtherYears" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма тарифа за каждый год со второго года обслуживания в валюте счета (в целых рублях/долларах/евро)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CodePlanTariff" type="TarifPlanCodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код  тарифа   за  обслуживание</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TariffClassifier" type="TariffClassifier_Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Классификатор тарифного плана</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RequestPrivateEarlyRepaymentRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос на досрочное погашение кредита ФЛ(запрос)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="RepaymentRequest" type="RepaymentRequest_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RepaymentRequest_Type">
		<xsd:sequence>
			<xsd:element name="TerminationRequestId" type="xsd:long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор заявки на досрочное погашение. Передается в случае отмены заявки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ERIBRequestID" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор заявки на ДП в ЕРИБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProdId" type="AgreemtNum_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор кредитного договора в ЕКП</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PayAccount" type="AcctIdType">
				<xsd:annotation>
					<xsd:documentation>Счет для погашения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Type" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип
Card – номер карты
Account – номер счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RepaymentDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата досрочного погашения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Amount" type="Amt_Type">
				<xsd:annotation>
					<xsd:documentation>Сумма для досрочного погашения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Currency" type="AcctCur_Type"/>
			<xsd:element name="FullRepayment" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак полного досрочного погашения.
1 – ПДП
0 или не передается – ЧДП
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DateOfSign" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата подписания клиентом заявления на досрочное погашение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RequestPrivateEarlyRepaymentRs_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос на досрочное погашение кредита ФЛ(ответ)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="Status"/>
			<xsd:element name="PrivateEarlyTerminationResult" type="PrivateEarlyTerminationResult_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PrivateEarlyTerminationResult_Type">
		<xsd:sequence>
			<xsd:element name="ProdId" type="AgreemtNum_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanAcct_Type">
		<xsd:sequence>
			<xsd:element ref="SystemId"/>
			<xsd:element name="ProdId" type="AgreemtId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор кредитного договора в ЕКП </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetPrivateLoanListRq_Type">
		<xsd:annotation>
			<xsd:documentation>Получение краткой информации о кредитах(запрос)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="LoanAcctId" type="LoanAcct_Type" maxOccurs="unbounded"/>
			<xsd:element name="OnDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата, на которую нужно рассчитать задолженность</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PrivateLoanInfo_Type">
		<xsd:sequence>
			<xsd:element name="ProdId" type="AgreemtNum_Type"/>
			<xsd:element name="RecPayment" type="Amt_Type"/>
			<xsd:element name="PaymentDate" type="Date"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetPrivateLoanListRs_Type">
		<xsd:annotation>
			<xsd:documentation>Получение краткой информации о кредитах(ответ)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="Status"/>
			<xsd:element name="LoanRec" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="LoanInfo" type="PrivateLoanInfo_Type"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetPrivateLoanDetailsRq_Type">
		<xsd:annotation>
			<xsd:documentation>Получение детальной информации по кредиту(запрос)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="LoanAcctId" type="LoanAcct_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetPrivateLoanDetailsRs_Type">
		<xsd:annotation>
			<xsd:documentation>Получение детальной информации по кредиту(ответ)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID" minOccurs="0"/>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="Status"/>
			<xsd:element name="LoanRec" type="PrivateLoanDetails_Type" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EarlyRepayment_Type">
		<xsd:sequence>
			<xsd:element name="Amount" type="Decimal"/>
			<xsd:element name="Date" type="Date"/>
			<xsd:element name="Status" type="String"/>
			<xsd:element name="Account" type="AcctIdType"/>
			<xsd:element name="RepaymentChannel" type="String">
				<xsd:annotation>
					<xsd:documentation>Канал, по которому зарегистрировано ДП.
Возможные значения:
1 – канал УКО
2 – канал не УКО(ВСП)
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TerminationRequestId" type="String">
				<xsd:annotation>
					<xsd:documentation>Идентификатор заявки на досрочное погашение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ERIBRequestID" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор заявки на ДП в ЕРИБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PrivateLoanDetails_Type">
		<xsd:sequence>
			<xsd:element name="ProdId" type="AgreemtNum_Type">
				<xsd:annotation>
					<xsd:documentation>идентификатор кредитного договора в ЕКП</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="LoanType"/>
			<xsd:element name="Period" type="Long">
				<xsd:annotation>
					<xsd:documentation>Срок договора в месяцах</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CreditingRate" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Процентная ставка по кредиту</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanStatus" type="Long">
				<xsd:annotation>
					<xsd:documentation>Статус кредита,
1 – открыт
2 – закрыт</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PrincipalBalance" type="Decimal"/>
			<xsd:element name="FullRepaymentAmount" type="Decimal"/>
			<xsd:element name="Overdue" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак просрочки ближайшего платежа:
1 – просрочен
0 или отсутствие поля – не просрочен
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Accounts" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Card" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="Element" maxOccurs="unbounded">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="Id" type="AcctIdType"/>
												<xsd:element name="Priority" type="Integer"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Account" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="Element" maxOccurs="unbounded">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="Id" type="AcctIdType"/>
												<xsd:element name="Priority" type="Integer"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="BAKS" type="Integer"/>
			<xsd:element name="AutoGrantion" type="Integer">
				<xsd:annotation>
					<xsd:documentation>Признак автоматической выдачи
0 – ручная выдача
2 – автоматическая выдача
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ApplicationNumberCA" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер заявки на получение КД</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CustRec" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="AgencyAddress" type="xsd:string"/>
			<xsd:element name="EarlyRepayment" type="EarlyRepayment_Type" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="AcctBalOnDate" type="AcctBal_Type" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DocStatNotRqType">
