	<xsd:complexType name="PersonInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о физическом лице &lt;PersonInfo&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PFRAccount" type="PfrAccount_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер СНИЛС клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Birthday" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата рождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BirthPlace" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Место рождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxId" type="TaxId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ИНН</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Citizenship" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Гражданство</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdditionalInfo" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Гражданство</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PersonRole" type="CreditRole_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Роль клиента (используется в кредитном договоре)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Gender" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Пол. Значения:Male – мужской Female – женский Unknown – неизвестен</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Resident" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Резидент или нет</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="PersonName" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ФИО клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="IdentityCard" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Удостоверение личности.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="ContactInfo" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Контактная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Insider" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак инсайдера</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentHistory" type="EmploymentHistoryType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о месте работы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ControlWord" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Контрольная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PersonInfoSec_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о физическом лице формат МДО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="PersonName"/>
			<xsd:element name="ContactInfo" type="ContactInfoSec_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Контактная информация</xsd:documentation>
				</xsd:annotation>
				<!--xsd:element ref="ContactInfo" minOccurs="1"-->
			</xsd:element>
			<xsd:element name="TINInfo" type="TINInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Налоговый идентификатор</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BirthDt" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата рождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Gender" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Пол</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdentityCards" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Перечень ДУЛ</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="IdentityCard" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="BirthPlace" type="BirthPlace_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Место рождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Resident" type="Resident_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак резидента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Citizenship" type="CitizenShip_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Гражданство</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ClientId" type="ClientId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PersonName_Type">
		<xsd:annotation>
			<xsd:documentation>Имя физического лица &lt;PersonName&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="LastName" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Фамилия.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="FirstName" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Имя.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="MiddleName" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Отчество.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CompanyName" type="String" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PrivateBlock_Type">
		<xsd:annotation>
			<xsd:documentation>Дополнительная информация</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AddInfo1" type="AddInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дополнительная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AddInfo2" type="AddInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дополнительная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AddInfo3" type="AddInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дополнительная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PfrGetInfoInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для запроса интерфейса ESK_GET_PFRINFO получения выписки клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="OperationId" type="UUID">
				<xsd:annotation>
					<xsd:documentation>Идентификатор запроса PfrHasInfoInqRequest(PfrHasInfoInqRequest/RqUID), выполненного ранее</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PfrGetInfoInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для ответа интерфейса ESK_GET_PFRINFO получения выписки клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element ref="Result" minOccurs="0"/>
			<xsd:element name="PfrInfo" type="xsd:anyType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Выписка из ПФР. Это поле присутствует и обязательно только в случае, если ResponseExist = true</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PfrHasInfoInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для запроса интерфейса ESK_HAS_PFRINFO проверки наличия выписки клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="CustInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PfrHasInfoInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для ответа интерфейса ESK_HAS_PFRINFO проверки наличия выписки клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element ref="Result" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PfrResult_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для возврата результата о состоянии выписки при запросе ПФР</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ResponseExists" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак наличия выписки в ПФР</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SendTime" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время отправки запроса в ПФР</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReceiveTime" type="DateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата и время ответа из ПФР.Обязателен в случае, если ResponseExists = true</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PhoneNum_Type">
		<xsd:sequence>
			<xsd:element name="PhoneType" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип телефона. Если не задан, то считается Mobile</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PhoneCode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код оператора связи</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PhoneOperName" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование оператора связи</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Phone" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер телефона</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PlasticInfoType">
		<xsd:annotation>
			<xsd:documentation>Информация о клиенте, которая печатается на карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PersonTitle" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Обращение к клиенту</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LastName" type="LastNameForPlastic_Type">
				<xsd:annotation>
					<xsd:documentation>Фамилия клиента для записи на пластиковую карту (только латинские буквы до 32 символов)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FirstName" type="FirstNameForPlastic_Type">
				<xsd:annotation>
					<xsd:documentation>Имя клиента для записи на пластиковую карту (только латинские буквы до 32 символов)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CompanyName" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Название компании</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="32"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PolicyDetails_Type">
		<xsd:annotation>
			<xsd:documentation>Описание реквизитов договора страхования: Серия, номер и дата выдачи полиса</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Series" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Серия</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Num" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IssueDt" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PostAddr_Type">
		<xsd:annotation>
			<xsd:documentation source="WMQI_DOCUMENTATION">Почтовый адрес</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AddrType" type="AddrType_Type">
				<xsd:annotation>
					<xsd:documentation>Тип адреса. Предопределенные значения:
Seasonal – сезонный
Primary – основной
Secondary – второй
Business – рабочий
Home – домашний
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Addr1" type="Addr1_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Строка адреса 1</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="City" type="City_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Город</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AreaCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код района</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="C">
						<xsd:maxLength value="11"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PostalCode" type="PostalCode_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Индекс</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Country" type="Country_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Страна</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CountryCode" type="CountryCode_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код страны согласно ISO</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DistrictCode" type="DistrictCode_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код района города.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="District" type="District_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Район города</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PlaceCode" type="PlaceCode_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код населенног пункта</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Place" type="Place_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Населенный пункт</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StreetCode" type="StreetCode_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код улицы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Street" type="Street_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование улицы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="House" type="House_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер дома</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Corpus" type="Corpus_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Корпус</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Building" type="Building_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Стоение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Flat" type="Flat_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер квартиры</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RecipientRec_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о поставщике</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CodeRecipientBS" type="NC" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор организации получателя платежей.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Name" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование получателя, готовое для отображения пользователю</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="GroupService" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код группы услуг. Параметр должен быть определен в ЕРИБ по ПУ и услуге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CodeService" type="NC" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор услуги, к которой относится получатель</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NameService" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование услуги, к которой относится получатель, готовое для отображения пользователю</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxId" type="TaxId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ИНН получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CorrAccount" type="AcctIdType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Корр. счет получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="KPP" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>КПП получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BIC" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>БИК банка получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctId" type="AcctIdType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Расчетный счет получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NameOnBill" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование поставщика, выводимое в чеке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NotVisibleBankDetails" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак «не отображать банковские реквизиты для поставщика» (в том числе и на странице подтверждения платежа). Если указано «false» или тэг отсутствует, то банковские реквизиты отображаются. Если указано «true» - не отображаются.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PhoneToClient" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер телефона поставщика услуг для обращений клиентов банка, которые совершили платежи в адрес поставщика для печати в чеке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsAutoPayAccessible" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак, определяющий возможность регистрации подписки на автоплатёж в пользу данного ПУ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AutoPayDetails" type="AutopayDetails_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Параметры подписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BankInfo" minOccurs="0"/>
			<xsd:element name="Requisites" type="Requisites_Type" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RepayLoan_Type">
		<xsd:annotation>
			<xsd:documentation>Погашаемый по Top-Up договор</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IdSource" type="IdSource_Type">
				<xsd:annotation>
					<xsd:documentation>ИД источника формирования кредитных историй</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IdContract" type="ContractNumber_Type">
				<xsd:annotation>
					<xsd:documentation>ИД Договора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanAccount_Number" type="AcctIdType">
				<xsd:annotation>
					<xsd:documentation>АС СД: Номер ссудного счёта</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AgreementNumber" type="AgreemtNum_Type">
				<xsd:annotation>
					<xsd:documentation>Номер кредитного договора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StartDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата выдачи кредита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MaturityDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Плановая дата погашения кредита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TotalAmount" type="Amt_Type">
				<xsd:annotation>
					<xsd:documentation>Сумма кредита (начальная)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Currency" type="AcctCur_Type">
				<xsd:annotation>
					<xsd:documentation>Валюта кредита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RegExp_Type">
		<xsd:annotation>
			<xsd:documentation>Регулярное выражение</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Exp" type="String">
				<xsd:annotation>
					<xsd:documentation>Регулярное выражение, для проверки введенных данных</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Mess" type="String">
				<xsd:annotation>
					<xsd:documentation>Сообщение для отображения пользавателю, в случае не выполнения рег. Выражения.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Regular_Type">
		<xsd:annotation>
			<xsd:documentation>Параметры регулярного платежа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DateBegin" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата начала действия поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DateEnd" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата окончания действия поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PayDay">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Day" type="Long" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Priority" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Приоритет исполнения поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExeEventCode" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код события исполнения формы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Summa" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SummaKindCode" type="String">
				<xsd:annotation>
					<xsd:documentation>Алгоритм расчета суммы платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Percent" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Процент от суммы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Requisites_Type">
		<xsd:annotation>
			<xsd:documentation>Дополнительныe реквизиты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Requisite" type="Requisite_Type" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RequisiteTypes_Type">
		<xsd:sequence>
			<xsd:element name="RequisiteType" type="NC" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Бизнес-смысл реквизита.
Область допустимых значений реквизита:
– IsListOfCharges (реквизит является списком начислений);
– IsPeriod (реквизит является периодом платежа).
Данные должны передаваться в формате mm.yyyy;
– IsSubscriberID (реквизит является идентификатором абонента у ПУ).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Requisite_Type">
		<xsd:annotation>
			<xsd:documentation>Дополнительный реквизит</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="NameVisible" type="C">
				<xsd:annotation>
					<xsd:documentation>Наименование дополнительного реквизита для отображения пользователю</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NameBS" type="NC">
				<xsd:annotation>
					<xsd:documentation>Идентификатор поля состоящий из цифр и букв английского алфавита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Description" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Правила заполнения поля.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Comment" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Комментарий к полю</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Type" type="C">
				<xsd:annotation>
					<xsd:documentation>Тип поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberPrecision" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Точность числового значения (количество знаков после запятой).
Если = 0, то допустимы только целые числа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsRequired" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак «Дополнительный реквизит обязательно должен быть указан при формировании платежа»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsSum" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак «Общая сумма платежа». В том случае, когда БС ТБ возвращает не только общую сумму задолженности, но и расшифровку этой суммы (в разбивке по видам начислений), то этот признак указывает, какое поле следует считать общей суммой задолженности. В реквизитах, полученных от БС ТБ, может быть только одно поле с таким признаком.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsKey" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак «Данный реквизит является одним из ключевых, т.е. индетифицирующий абонента».
Реквизиты с таким признаками будут отображаться в чеках, отправляться в подтверждающих смс вне зависимости от значения признаков isForBill и includeInSMS.
Признак не может быть передан одновременно с  IsVisible = false, IsSum
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsEditable" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак «Редактируемого поля». Поля предоставляет возможность ввода данных</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsVisible" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак видимости поля клиентом. Признак не может быть равен false одновременно с передачей  IsSum для всех случаев, кроме оформления подписки на автоплатежи</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsForBill" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак «Включать в счет»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IncludeInSMS" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак «Включать в SMS»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SaveInTemplate" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак «Сохранять в шаблоне платежа»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RequisiteTypes" type="RequisiteTypes_Type" minOccurs="0"/>
			<xsd:element name="HideInConfirmation" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак «Скрывать реквизит на форме подтверждения»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AttributeLength" type="AttributeLength_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание длины поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Validators" type="Validators_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Валидаторы (контроли), используемые для дополнительного атрибута. Неприменимо для типов set и list.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Menu" type="Menu_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Список значений для выбора клиентом при для выбора клиентом при Type=list и set</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EnteredData" type="EnteredData_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Введенные клиентом данные.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DefaultValue" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Значение по умолчанию для поля.
Клиент передает в этом поле, тоже значение, что пришло от сервера. Не используется для типа set.
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Error" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Текст ошибки на русском языке для пользователя, в случае нахождения ошибки на сервере</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EnteredData_Type">
		<xsd:annotation>
			<xsd:documentation>Введенные клиентом данные. В данном запросе группа не возвращается. Если БС ТБ выполняет пересчет или изменение введенных клиентом данных, то пересчитанные значения помещаются в данную группу.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DataItem" type="C" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Введенное или выбранное  пользователем из списка значение. В случае Type=set может быть указано несколько элементов DataItem</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AttributeLength_Type">
		<xsd:annotation>
			<xsd:documentation>Описание длины поля</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="MaxLength" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Максимальная длина поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MinLength" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Минимальная длина поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Validators_Type">
		<xsd:annotation>
			<xsd:documentation>Валидаторы (контроли), используемые для дополнительного атрибута. Неприменимо для типов set и list.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Validator" type="Validator_Type" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Validator_Type">
		<xsd:annotation>
			<xsd:documentation>Валидаторы (контроли), используемые для дополнительного атрибута. Неприменимо для типов set и list.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Type" type="NC">
				<xsd:annotation>
					<xsd:documentation>Тип валидатора (контроля). На текущий момент используется значение: regexp</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Message" type="C">
				<xsd:annotation>
					<xsd:documentation>Сообщение, выводимое на экран клиенту при срабатывании валидатора (контроля).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Parameter" type="C">
				<xsd:annotation>
					<xsd:documentation>Выражение для валидатора (контроля). Список значений для выбора клиентом при для выбора клиентом при Type=list и set
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SecuritiesAcctInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о сертификатах</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Holder" type="CustInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Держатель ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecuritiesRec" type="SecuritiesRecShort_Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Информационная запись по ценной бумаге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SecuritiesClient_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о клиенте по договору хранения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PersonInfo" type="PersonInfoSec_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификационные данные клиента физ. лица, которому выдана ценная бумага</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SecuritiesDocument_Type">
		<xsd:annotation>
			<xsd:documentation>Связанный документ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DocType">
				<xsd:annotation>
					<xsd:documentation>Тип документа</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="C">
						<xsd:maxLength value="36"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DocNum">
				<xsd:annotation>
					<xsd:documentation>Номер документа</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="C">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DocDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Annotation" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Примечание</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="C">
						<xsd:maxLength value="255"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Status" type="Identifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Состояние документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Client" type="SecuritiesClient_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о клиенте по договору хранения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Registrar" type="Owner_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ВСП, осуществляющее учет договора хранения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SecuritiesHolder_Type">
		<xsd:annotation>
			<xsd:documentation>Держатель ЦБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PersonInfo" type="PersonInfoSec_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификационные данные клиента - физ лица, которыму выдана ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SecuritiesInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Базовая информация ценной бумаги</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="NominalAmt" type="Amt_Type">
				<xsd:annotation>
					<xsd:documentation>Сумма номинала ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NominalCurCode" type="CurCode_Type">
				<xsd:annotation>
					<xsd:documentation>Код валюты номинала ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IncomeRate" type="Rate_Type">
				<xsd:annotation>
					<xsd:documentation>Процентная ставка дохода по ценной бумаге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IncomeAmt" type="Amt_Type">
				<xsd:annotation>
					<xsd:documentation>Сумма дохода по ценной бумаге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ComposeDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата составления (валютирования) ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TermType" type="Identifier" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип срока платежа по ценной бумаге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TermDays" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Срок оплаты ценной бумаги в днях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TermStartDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата начала срока оплаты ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TermFinishDt" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата окончания срока оплаты ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TermLimitDt" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата наступления срока давности об оплате</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SecuritiesInfoInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Запроса реквизитов ценных бумаг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RqUID" type="RqUID_Type">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RqTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время передачи сообщения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SPName" type="SPName_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы инициирующей запрос</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SystemId" type="SystemId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы назначения запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankInfo" type="BankInfoESB_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о банке для целей маршрутизации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OperName" type="OperNameSec_Type">
				<xsd:annotation>
					<xsd:documentation>Наименование операции
"SrvBSGetInfo"</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="BlankInfo" type="BlankInfo_Type">
					<xsd:annotation>
						<xsd:documentation>Бланк ценной бумаги</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="BlankPackage" type="BlankPackage_Type">
					<xsd:annotation>
						<xsd:documentation>Пакет бланков ценных бумаг</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SecuritiesInfoInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Ответ на запрос реквизитов ценных бумаг</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RqUID" type="RqUID_Type">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RqTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время передачи сообщения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SPName" type="SPName_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы, предоставляющей услуги (провайдер сервиса)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SystemId" type="SystemId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы, инициировавший запрос</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankInfo" type="BankInfoESB_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о банке для целей маршрутизации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Status" type="Status_Type">
				<xsd:annotation>
					<xsd:documentation>Статус ответа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecuritiesRec" type="SecuritiesRec_Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Информационная запись по ценной бумаге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SecuritiesRec_Type">
		<xsd:annotation>
			<xsd:documentation>Информационная запись по ценной бумаге</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Issuer" type="Issuer_Type">
				<xsd:annotation>
					<xsd:documentation>ВСП, осуществившее выдачу ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Holder" type="SecuritiesHolder_Type">
				<xsd:annotation>
					<xsd:documentation>Держатель ЦБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BlankInfo" type="BlankInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Бланк ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FormDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата оформления ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecuritiesInfo" type="SecuritiesInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Базовая информация ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecuritiesDocument" type="SecuritiesDocument_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Информация о договоре хранения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SecuritiesRecShort_Type">
		<xsd:annotation>
			<xsd:documentation>Информациооная запись по ценной бумаге</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="BlankInfo" type="BlankInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Бланк ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecuritiesInfo" type="SecuritiesInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Базовая информация ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OnStorageInBank" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак нахождения на хранении</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SecurityMarker_Type">
		<xsd:annotation>
			<xsd:documentation>Маркер ценной бумаги</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Remainder" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Остаток, кол-во ценных бумаг данного вида</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MarkerDescription" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Информация о ценной бумаге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SelRangeDt_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для временного периода</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="StartDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Начальная дата периода</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Конечная дата периода</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SelRangeDtTm_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для временного периода (дата и время)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="StartDtTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Начальная дата и время периода</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndDtTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Последняя дата и время периода</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SetAccountStateRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса SACS изменение состояния счета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="DepAcctId"/>
			<xsd:element ref="CustInfo"/>
			<xsd:element name="AccountStateAction" type="AccountStateAction_Type">
				<xsd:annotation>
					<xsd:documentation>Действие метода. Возможна передача значения: STATE_SAVE_BOOK_LOST - признак утери сберкнижки по счету</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SetAccountStateRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса SACS изменение состояния счета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ServiceInfo_Type">
		<xsd:sequence>
			<xsd:element name="ProdType" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип продукта</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RegionNum" type="String" minOccurs="0"/>
			<xsd:element name="BranchNum" type="String" minOccurs="0"/>
			<xsd:element name="AgencyNum" type="String" minOccurs="0"/>
			<xsd:element name="AgreementNum" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер заявления на подключение к услуге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StartDt" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата подключения к услуге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndDt" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата отключения от услуги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BankInfo" minOccurs="0"/>
			<xsd:element name="Status" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ServiceStmtRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса ESK_SAS получения графика исполнения длительного поручения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="SvcAcctId"/>
			<xsd:element name="DtBegin" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата начала периода выписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DtEnd" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата конца периода выписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ServiceStmtRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса ESK_SAS получения графика исполнения длительного поручения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element ref="ExctractLine" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Source_Type">
		<xsd:annotation>
			<xsd:documentation>Источник запроса</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ContractNumber" type="NC">
				<xsd:annotation>
					<xsd:documentation>Идентификатор источника запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SourceId_Type">
		<xsd:sequence>
			<xsd:element name="SourceIdType">
				<xsd:annotation>
					<xsd:documentation>Тип идентификатора канала ('BP_VSP', 'BP_ERIB')</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="32"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="VSPInfo" type="BankInfoLeadZero_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ВСП из которго идет запрос. Заполняется в случае SourceIdType = 'BP_VSP'</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SPDefField_Type">
		<xsd:annotation>
			<xsd:documentation>Поля, определяемые ПУ &lt;SPDefField&gt;</xsd:documentation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="FieldName">
				<xsd:annotation>
					<xsd:documentation>Название признака</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="FieldValue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Значение признака</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FieldDt" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата открытия УДБО</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FieldNum" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FieldDt1" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата закрытия УДБО</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FieldData1" type="AgencyId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код ТБ+индекс ОСБ (информация используется ЭСК)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="State" type="State_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Статус УДБО</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SPDefFieldShort_Type">
		<xsd:annotation>
			<xsd:documentation>Поля, определяемые ПУ &lt;SPDefField&gt;</xsd:documentation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FieldData1" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Код ТБ+индекс ОСБ (информация используется ЭСК)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FieldNum" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Номер</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Status_Type">
		<xsd:annotation>
			<xsd:documentation>Статус ответа &lt;Status&gt;. Агрегат &lt;Status&gt; используется для возврата статуса сообщения. </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="StatusCode">
				<xsd:annotation>
					<xsd:documentation>Статусный код возврата.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="StatusDesc" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание статуса.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="StatusType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип ошибки.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="ServerStatusDesc" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание статуса системы источника</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Status_way_Type">
		<xsd:annotation>
			<xsd:documentation>Статус ответа &lt;Status&gt;. Агрегат &lt;Status&gt; используется для возврата статуса сообщения. </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="StatusCode">
				<xsd:annotation>
					<xsd:documentation>Статусный код возврата.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Severity" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Серьезность ошибки. Значения: Error, Warn, Info</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="StatusDesc" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание статуса.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="StmtSummAmt_Type">
		<xsd:sequence>
			<xsd:element ref="StmtSummType"/>
			<xsd:element ref="CurAmt"/>
			<xsd:choice>
				<xsd:element name="CurAmtCur" type="AcctCur_Type">
					<xsd:annotation>
						<xsd:documentation>Валюта операции</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="AcctCur" type="AcctCur_Type">
					<xsd:annotation>
						<xsd:documentation>Валюта операции</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="ContrAccount" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер счета контрагента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAcctAudRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса COD_GSI получения информации по длительному поручению</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="SvcAcct" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="SvcAcctId"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAcctAudRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса COD_GSI получения информации по длительному поручению</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status" minOccurs="0"/>
			<xsd:element ref="SvcActInfo" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAcctDelRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса COD_ASD отмены длительного поручения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="SvcAcct">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="SvcAcctId"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAcctDelRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса COD_ASD отмены длительного поручения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAcctId_Type">
		<xsd:annotation>
			<xsd:documentation>Данные длительного поручения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="SystemId" minOccurs="0"/>
			<xsd:element name="SvcAcctNum" type="Long">
				<xsd:annotation>
					<xsd:documentation>Номер длительного поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="SvcType" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак "Длительное поручение на перевод"</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcActInfo_Type">
		<xsd:sequence>
			<xsd:element ref="Status"/>
			<xsd:element name="SvcAct" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="SvcAcctId"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element ref="DepAcctIdFrom" minOccurs="0"/>
			<xsd:element ref="CardAcctIdFrom" minOccurs="0"/>
			<xsd:element name="RecBIC" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>БИК получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecCorrAccount" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Корсчет получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecCalcAccount" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Расчетный счет получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecAcctCur" type="AcctCur_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Валюта счета получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecINN" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ИНН получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="KPPTo" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>КПП получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Purpose" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Назначение платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecipientName" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование получателя юр. Лица, либо ФИО физ.лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PmtKind" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Regular" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAddRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейсов (SDP, SDС) создания длительного поручения на перевод денежных средств</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element name="OperName" type="OperName_Type" minOccurs="0"/>
			<xsd:element ref="BankInfo" minOccurs="0"/>
			<xsd:element ref="CustId" minOccurs="0"/>
			<xsd:element ref="XferInfo"/>
			<xsd:element ref="Regular" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейсов (SDP, SDС ) создания длительного поручения на перевод денежных средств</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element ref="SvcAcctId" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcsAcct_Type">
		<xsd:annotation>
			<xsd:documentation>Список длительных поручений</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="SystemId"/>
			<xsd:element name="SvcAcctId">
				<xsd:annotation>
					<xsd:documentation>Идентификатор длительного поручения</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="SvcAcctNum" type="Long">
							<xsd:annotation>
								<xsd:documentation>Номер длительного поручения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="BankInfo"/>
						<xsd:element name="SvcType" type="xsd:boolean">
							<xsd:annotation>
								<xsd:documentation>Признак «Длительное поручение на перевод». Если значение false, то значит не перевод, а коммунальные платежи</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="DtStart" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата начала действия длительного поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DtEnd" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата окончания действия длительного поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurAmt" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма длительного поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CardNum" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер карты, с которой выполнится списание</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AcctId" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер счета, с которого выполнится списание</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PmtKind" type="PmtKind_Type">
				<xsd:annotation>
					<xsd:documentation>Код вида платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RcptKind" type="RcptKind_Type">
				<xsd:annotation>
					<xsd:documentation>Код вида получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TarifPlanInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация о тарифном плане и сегменте клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Code" type="TarifPlanCodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код тарифного плана</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ConnectionDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата подключения к тарифному плану</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SegmentCode" type="SegmentCodeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код сегмента, к которому относят клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ManagerId" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер персонального менеджера</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ManagerOsb" type="AgencyId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ОСБ КМ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ManagerTb" type="RegionId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ТБ КМ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ManagerFilial" type="BranchId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ВСП КМ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TaxColl_Type">
		<xsd:annotation>
			<xsd:documentation>Налоговые реквизиты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SRCstatus" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Статус составителя документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxRegCodeTo" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>КПП получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BudgetCode" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код бюджетной классификации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OKATO" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код ОКАТО</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxBase" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Основание налогового платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxPeriod" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Налоговый период</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxNum" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер налогового документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxDt" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата налогового документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxType" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Тип налогового платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TopUp_Type">
		<xsd:annotation>
			<xsd:documentation>Блок данных по TopUp кредитам</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TopUpLoanListCount" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>Количество блоков погашаемых договоров</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TotalRepaymentSum" type="Amt_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма полного досрочного погашения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RepayLoan" type="RepayLoan_Type" maxOccurs="5">
				<xsd:annotation>
					<xsd:documentation>Погашаемый договор</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TINInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Налоговый идентификатор (напр.ИНН)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TaxId" type="TaxId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор налогоплательщика (ИНН/КИО).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TransferInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Параметры операции (ДЕПО).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DocumentDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentNumber" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Номер поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OperType" type="DepoOperType_Type">
				<xsd:annotation>
					<xsd:documentation>Тип операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OperationSubType" type="DepoOperationSubType_Type">
				<xsd:annotation>
					<xsd:documentation>Содержание операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OperationDesc" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Комментарий к операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DivisionNumber" type="DivisionNumber_Type">
				<xsd:annotation>
					<xsd:documentation>Тип и номер раздела</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InsideCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Депозитарный  код выпуска ценной бумаги (партии неэмисионной ЦБ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecurityCount" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Кол-во ценных бумаг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OperationReason" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Основание операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DetailOperationReason" type="DepoDetailOperationReason_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Описание договора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransferRcpInfo" type="TransferRcpInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о противоположной стороне операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TransferRcpInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Основание операции - описание договора (ДЕПО).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CorrDepositary" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование депозитария получателя (ДЕПО)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CorrDepoAcctId" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер счета депо на который на осуществить перевод или с которого необходимо принять ценные бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CorrOwner" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Владелец счета депо</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CorrOwnerDetail" type="CorrOwnerDetail_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Детальная информации о владельце счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdditionalInfo" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дополнительные реквизиты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeliveryType" type="DepoDeliveryType_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Способ доставки сертификатов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="UserInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация о пользователе осуществившим изменения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="UserCategory" type="UserCategoryType">
				<xsd:annotation>
					<xsd:documentation>Тип пользователя, осуществившего изменения: Employee - пользователь системы, TechnicalUser - технологический, Client - клиент</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Login" type="C" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код/логин пользователя внесшего изменения. Передается только для сотрудника банка.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Fio" type="C">
				<xsd:annotation>
					<xsd:documentation>ФИО пользователя внесшего изменения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankInfo" type="BankInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Учреждение, к которому привязан пользователь, осуществивший изменения (в кодировке subbranch)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="UsageInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация о транзакционных ограничителях</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="UsageCode" type="C">
				<xsd:annotation>
					<xsd:documentation>Код ограничителя:
P2P_CUM_LIM – индивидуальный суточный лимит по счету карты для операций перевода с карточного счета клиента на карточный счет другого клиента через УС
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MaxCurAmt" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Максимальная сумма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AcctCur"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="VSPOperation_Type">
		<xsd:annotation>
			<xsd:documentation>Реквизиты операции в ВСП</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ClientName" type="FullName_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ФИО клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ClientAccount" type="AcctIdType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsDebit" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак приходной или расходной операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Amount" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма операции в валюте  счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PayDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата совершения операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationCode" type="Long">
				<xsd:annotation>
					<xsd:documentation>Код авторизации </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="XferAddRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфрейсов  TDD, TDC,TCD,TCP,TCC и т.д.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element name="SPNum" type="SPNum_Type" minOccurs="0"/>
			<xsd:element name="OperName" type="OperName_Type" minOccurs="0"/>
			<xsd:element ref="BankInfo" minOccurs="0"/>
			<xsd:element ref="CustId" minOccurs="0"/>
			<xsd:element ref="XferInfo"/>
			<xsd:element ref="Regular" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="XferAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-ответа для интерфрейсов  TDD, TDC,TCD,TCP,TCC и т.д.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element ref="SvcAcctId" minOccurs="0"/>
			<xsd:element ref="AgreemtInfo" minOccurs="0"/>
			<xsd:element ref="DstCurAmt" minOccurs="0"/>
			<xsd:element ref="SrcCurAmt" minOccurs="0"/>
			<xsd:element name="SrcLayoutInfo" type="SrcLayoutInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Разбивка микроопераций списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="XferInfo_Type">
		<xsd:sequence>
			<xsd:element name="XferMethod" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Перечисление, куда отправляется перевод</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ClientDocumentNumber" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер клиентского поручения (Используется для оплаты товаров и услуг и создании ДП оплаты в Московском банке)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ClientDocumentDate" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата клиентского поручения. (Используется для оплаты товаров и услуг и создании ДП оплаты в Москоском банке)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PayerInfo" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентифицирующая информация о плательщике. (Используется для оплаты товаров и услуг и создании ДП оплаты товаров и услуг  в Москоском банке)</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="PersonInfo" type="PersonInfo_Type"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="TaxIdTo" type="TaxId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ИНН получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="KPPTo" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>КПП получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxIdFrom" type="TaxId_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ИНН плательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecipientName" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование получателя юр. Лица, либо ФИО физ.лица, склеенные через пробел. Пробелы внутри фамилии, имени и отчества не допускаются</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CustInfo" minOccurs="0"/>
			<xsd:element ref="TaxColl" minOccurs="0"/>
			<xsd:sequence minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Для интерфейса TDD - перевод денежных средств со счета по вкладу (первые тег DepAcctIdFrom) на счет по вкладу (если второй тег DepAcctIdTo)  или Интерфейс TDС - перевод денежных средств со счета по вкладу (первые тег DepAcctIdFrom) на счет МБК (если второй тег CardAcctIdTo). Интерфейс TCD - перевод денежных средств со счета МБК (первый тег CardAcctIdFrom) на счет по вкладу (если второй тег DepAcctIdTo) или Интерфейс TCC - перевод денежных средств со счета МБК (первый тег CardAcctIdFrom) на погашение кредита (если второй тег LoanAcctIdTo). Для интерфейса TDDO - открытие вклада с переводом на него денежных средств с другого вклада (первый тег - DepAcctIdFrom, второй тег (для запрос в ЦОД и xBank) - DepAcctIdTo, третий тег - AgreemtInfo), для интерфейса TCDO - открытие вклада с переводом на него денежных средств с МБК (первый тег CardAcctIdFrom, второй тег (для запрос в ЦОД и xBank) - DepAcctIdTo, третий тег - AgreemtInfo), для интерфейса TDO - открытие бессрочного вклада без первоначального взноса (первый тег (для запрос в ЦОД и xBank) - DepAcctIdTo, второй тег - AgreemtInfo)</xsd:documentation>
				</xsd:annotation>
				<xsd:element ref="CardAcctIdFrom" minOccurs="0"/>
				<xsd:element ref="DepAcctIdFrom" minOccurs="0"/>
				<xsd:element ref="DepAcctIdTo" minOccurs="0"/>
				<xsd:element ref="CardAcctIdTo" minOccurs="0"/>
				<xsd:element ref="LoanAcctIdTo" minOccurs="0"/>
				<xsd:element ref="AgreemtInfo" minOccurs="0"/>
			</xsd:sequence>
			<xsd:element name="IdSpacing" type="String" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор разбивки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Purpose" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Назначение платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NonResCode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код операции по счетам нерезидентов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CurAmt" minOccurs="0"/>
			<xsd:element ref="AcctCur" minOccurs="0"/>
			<xsd:element ref="CardAcctId" minOccurs="0"/>
			<xsd:element ref="DepAcctId" minOccurs="0"/>
			<xsd:element name="CurAmt1" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctCur1" type="AcctCur_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Валюта операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurAmt2" type="Decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Сумма комиссии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctCur2" type="AcctCur_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Валюта комиссии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DateCalc" type="Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата, на которую получена разбивка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CurAmtConv" minOccurs="0"/>
			<xsd:element ref="BankInfo" minOccurs="0"/>
			<xsd:element name="ParentAuthorizationCode" type="Long" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код авторизации прямой транзакции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ParentTransDtTm" type="DateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Время прямой транзакции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WithClose" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак операции открытия с закрытием. На первом этапе внедрения - фиксированное значение «false»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Execute" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак проведения операции. Для операции DepToNewDepAdd и CardToNewDepAdd передается фиксированное значение «true».</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Comission" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Значение данного элемента должно совпадать со значением элемента preparedPayment_a.comission, полученного из АС ЦПФЛ. Передается для московского банка при оплате товаров и услуг </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Requisites" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Значение данного элемента должно совпадать со значением элемента preparedPayment_a.nplatRequisites.requisites, полученного из АС ЦПФЛ. Передается для московского банка при оплате товаров и услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DataSpec" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Значение данного элемента должно совпадать со значением элемента preparedPayment_a.nplatRequisites.requisites, полученного из АС ЦПФЛ. Передается для московского банка при создании длительных поручений на оплату товаров и услуг 
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UniqueNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор организации получателя платежей (Передается в случае оплаты договорного получателя для Московского банка )</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecClientCode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код спецклиента (Передается в случае оплаты товаров и услуг и создания ДП для Московского банка)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurMonth" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак платежа за текущий месяц. Передается в случае создания ДП для оплаты товаров и услуг для Московского банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CommissionLabel" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Значение данного элемента должно совпадать со значением элемента preparedPayment_a.CommissionLabel, полученного из АС ЦПФЛ.Передается в случае создания ДП для оплаты товаров и услуг для Московского банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServiceCode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Значение данного элемента должно совпадать со значением элемента preparedPayment_a.ServiceCode, полученного из АС ЦПФЛ. Передается в случае создания ДП для оплаты товаров и услуг для Московского банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServiceKind" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код тарифа 
Значение данного элемента должно совпадать со значением элемента preparedPayment_a.tarif.tarifCode, выбранного клиентом из коллекции  тарифов  полученной из АС ЦПФЛ в узле preparedPayment_a.tarif. Передается в случае создания ДП для оплаты товаров и услуг для Московского банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Tarif" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Значение данного элемента должно совпадать со значением элемента preparedPayment_a.tarif.tarifValue, соотвествующего выбранному клиентом  preparedPayment_a.tarif.tarifCode из коллекции тарифов preparedPayment_a.tarif полученной из АС ЦПФЛ. Передается в случае создания ДП для 
оплаты товаров и услуг для Московского банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CanChangeTarif" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак разрешения изменения суммы платежа по тарифам. 
Значение данного элемента устанавливается клиентом, если значение узла preparedPayment_a.ackClientBankCanChangeSumm  в сообщении из АС ЦПФЛ равно true. Иначе значение элемента должно совпадать со значением элемента preparedPayment_a.ackClientBankCanChangeSumm. Передается в случае создания ДП для 
оплаты товаров и услуг для Московского банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SrcLayoutInfo" type="SrcLayoutInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Разбивка микроопераций списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="XferIMAInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация об операции со счетом ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element ref="DepAcctIdFrom"/>
				<xsd:element ref="CardAcctIdFrom"/>
				<xsd:element name="IMAAcctIdFrom" type="IMAAcctId_Type">
					<xsd:annotation>
						<xsd:documentation>Идентификатор счета ОМС (для операции TIC)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:choice>
				<xsd:element ref="AgreemtInfo"/>
				<xsd:element name="IMAAcctIdTo" type="IMAAcctId_Type">
					<xsd:annotation>
						<xsd:documentation>Идентификатор счета ОМС (для операции TCI)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="CardAcctIdTo"/>
			</xsd:choice>
			<xsd:element name="Purpose" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Назначение платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CurAmt">
				<xsd:annotation>
					<xsd:documentation>Сумма операции (в граммах)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AcctCur"/>
			<xsd:element ref="CurAmtConv"/>
			<xsd:element name="Execute" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак проведения операции. Для операций DepToNewIMAAdd, CardToNewIMAAdd, CardToIMAAdd, IMAToCardAdd передается фиксированное значение «true».</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SrcLayoutInfo" type="SrcLayoutInfo_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Разбивка микроопераций списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="XferOperStatusInfoRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип запроса для интерфейса SrvXferOperStatusInfo. Запрос на уточнение статуса операции из ЕРИБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element name="OriginalRqTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время оригинальной операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="SPName"/>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="OperName" type="OperName_Type">
				<xsd:annotation>
					<xsd:documentation>Операция, по которой необходимо уточнить информацию в АБС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="XferOperStatusInfoRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа для интерфейса SrvXferOperStatusInfo. Запрос на уточнение статуса операции из ЕРИБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element name="StatusOriginalRequest" type="Status_Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Результат выполнения оригинального запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice minOccurs="0">
				<xsd:element name="TDDO">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="AgreemtInfo" type="AgreemtInfoResponse_Type"/>
							<xsd:element ref="AgreemtInfoClose" minOccurs="0"/>
							<xsd:element ref="DstCurAmt" minOccurs="0"/>
							<xsd:element ref="SrcCurAmt" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="TCDO">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="CardAuthorization"/>
							<xsd:element name="AgreemtInfo" type="AgreemtInfoResponse_Type"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="TDIO">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="AgreemtInfo" type="AgreemtInfoResponse_Type"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="TCIO">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="CardAuthorization"/>
							<xsd:element name="AgreemtInfo" type="AgreemtInfoResponse_Type"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SrcLayoutInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для разбивки микроопераций списания</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IsCalcOperation" type="xsd:boolean" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Признак расчёта операции в депозитной системе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WriteDownOperation" type="WriteDownOperation_Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Микрооперация списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="WriteDownOperation_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для микрооперации списания</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OperationName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Краткое наименование микрооперации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CurAmt">
				<xsd:annotation>
					<xsd:documentation>Сумма микрооперации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Turnover" type="Turnover_Type">
				<xsd:annotation>
					<xsd:documentation>Признак списания/зачисления</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="VariantInterestPayment_Type">
		<xsd:annotation>
			<xsd:documentation>Элементы для передачи информации о порядке уплаты процентов</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IsInterestToCard" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Вариант уплаты процентов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CardNumber" type="CardNumType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер банковской карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AlterMinRemainder_Type">
		<xsd:annotation>
			<xsd:documentation>Элементы для изменения неснижаемого остатка</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="NewMinRemainder" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Новый неснижаемый остаток счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NewInterestRate" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Новая процентная ставка по вкладу</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
