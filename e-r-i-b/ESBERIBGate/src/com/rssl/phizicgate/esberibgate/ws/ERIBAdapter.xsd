			<xsd:element ref="Status" minOccurs="0"/>
			<xsd:element name="DepoAccSecInfoRec">
				<xsd:complexType>
					<xsd:annotation>
						<xsd:documentation>Счета депо</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element ref="DepoAcctId"/>
						<xsd:element name="DepoRec" type="DepoSecurityRec_Type" minOccurs="0" maxOccurs="unbounded"/>
						<xsd:element ref="Status"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAccSecRegRqType">
		<xsd:annotation>
			<xsd:documentation>Подача заявки на регистрацию новой ценной бумаги ДЕПО</xsd:documentation>
		</xsd:annotation>
