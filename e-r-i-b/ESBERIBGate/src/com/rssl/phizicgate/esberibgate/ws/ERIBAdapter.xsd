<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:simpleType name="AccountStateAction_Type">
		<xsd:annotation>
			<xsd:documentation>Действие метода по изменению состояния счета</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:enumeration value="STATE_SAVE_BOOK_LOST">
				<xsd:annotation>
					<xsd:documentation>Признак утери сберкнижки по счету</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AccountStatusEnum_Type">
		<xsd:annotation>
			<xsd:documentation>Статус счета</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Opened">
				<xsd:annotation>
					<xsd:documentation>Открыт</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Closed">
				<xsd:annotation>
					<xsd:documentation>Закрыт</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Arrested">
				<xsd:annotation>
					<xsd:documentation>Арестован</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Lost-passbook">
				<xsd:annotation>
					<xsd:documentation>Утеряна сберкнижка</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AcctIdType">
		<xsd:annotation>
			<xsd:documentation>Номер счета (карточного, кредитного, депозитного)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String"/>
	</xsd:simpleType>
	<xsd:simpleType name="AcctId_NC_Type">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:restriction base="NC">
			<xsd:maxLength value="32"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AcctCur_Type">
		<xsd:annotation>
			<xsd:documentation>Валюта счета</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:length value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AcctType_Type">
		<xsd:annotation>
			<xsd:documentation>
				Типы продуктов клиента, перечень которых должен
				вернуться.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Deposit">
				<xsd:annotation>
					<xsd:documentation>Вклады</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="IMA">
				<xsd:annotation>
					<xsd:documentation>ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Card">
				<xsd:annotation>
					<xsd:documentation>
						Пластиковые карты
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Credit">
				<xsd:annotation>
					<xsd:documentation>Кредиты</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LongOrd">
				<xsd:annotation>
					<xsd:documentation>
						Длительные поручения
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DepoAcc">
				<xsd:annotation>
					<xsd:documentation>Cчета депо</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CardWay"/>
			<xsd:enumeration value="Securities"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Addr1_Type">
		<xsd:annotation>
			<xsd:documentation>Строка адреса 1.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AdditionalCard_Type">
		<xsd:annotation>
			<xsd:documentation>Тип дополнительной карты</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Client2Client">
				<xsd:annotation>
					<xsd:documentation>Карта, выпущенная на имя клиента и к его собственному счету</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Client2Other">
				<xsd:annotation>
					<xsd:documentation>Карта, выпущенная к счету клиента на имя другого лица</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other2Client">
				<xsd:annotation>
					<xsd:documentation>Карта на имя клиента, выпущенная к счету другого лица</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AddressType_Type">
		<xsd:annotation>
			<xsd:documentation>Тип адреса</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AddrType_Type">
		<xsd:annotation>
			<xsd:documentation>Тип адреса МДО</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="OpenEnum">
			<xsd:enumeration value="Seasonal">
				<xsd:annotation>
					<xsd:documentation>Сезонный</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Primary">
				<xsd:annotation>
					<xsd:documentation>Основной (фактический)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Secondary">
				<xsd:annotation>
					<xsd:documentation>Второй адрес (дополнительный)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Business">
				<xsd:annotation>
					<xsd:documentation>Бизнес (юридический) адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Home">
				<xsd:annotation>
					<xsd:documentation>Домашний адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Work">
				<xsd:annotation>
					<xsd:documentation>Рабочий адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Registration">
				<xsd:annotation>
					<xsd:documentation>Адрес регистрации</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>Другой адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PostAddress">
				<xsd:annotation>
					<xsd:documentation>Почтовый адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LegalAddress">
				<xsd:annotation>
					<xsd:documentation>Юридический адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ActualAddress">
				<xsd:annotation>
					<xsd:documentation>Фактический адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RegAddress">
				<xsd:annotation>
					<xsd:documentation>Адрес регистрации</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value=""/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AgencyId_Type">
		<xsd:annotation>
			<xsd:documentation>Номер отделения, в котором открыт или ведется продукт. С лидирующими нулями.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="5"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AgreemtNum_Type">
		<xsd:annotation>
			<xsd:documentation>Номер кредитного договора</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="30"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Amt_Type">
		<xsd:annotation>
			<xsd:documentation>Сумма</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="Decimal"/>
	</xsd:simpleType>
	<xsd:simpleType name="AutopayStatus_Type">
		<xsd:annotation>
			<xsd:documentation>Состояние подписки АС АП</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="New">
				<xsd:annotation>
					<xsd:documentation>Новая</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="OnRegistration">
				<xsd:annotation>
					<xsd:documentation>Подписка на регистрации у ПУ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="WaitingForActivation">
				<xsd:annotation>
					<xsd:documentation>Ожидание активации</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Confirmed">
				<xsd:annotation>
					<xsd:documentation>Прошла регистрацию у поставщика услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Active">
				<xsd:annotation>
					<xsd:documentation>Активна и готова к исполнению</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="WaitForAccept">
				<xsd:annotation>
					<xsd:documentation>Ожидание подтверждения клиентом</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ErrorRegistration">
				<xsd:annotation>
					<xsd:documentation>Не зарегистрирована</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Paused">
				<xsd:annotation>
					<xsd:documentation>Приостановлена</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Closed">
				<xsd:annotation>
					<xsd:documentation>Закрыта</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AutopayType_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для Типа подписки в АС AutoPay</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="R">
				<xsd:annotation>
					<xsd:documentation>Регулярный автоплатеж</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="P">
				<xsd:annotation>
					<xsd:documentation>Пороговый автоплатеж</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A">
				<xsd:annotation>
					<xsd:documentation>Автоплатеж по счету</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BalType_Type">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:restriction base="String"/>
	</xsd:simpleType>
	<xsd:simpleType name="BankAcctStatusCode_Type">
		<xsd:annotation>
			<xsd:documentation>Код статуса счета</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String"/>
	</xsd:simpleType>
	<xsd:simpleType name="BankId_Type">
		<xsd:annotation>
			<xsd:documentation>Идентификатор банка</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="NC">
			<xsd:maxLength value="34"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BIC_Type">
		<xsd:annotation>
			<xsd:documentation>БИК</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BirthPlace_Type">
		<xsd:annotation>
			<xsd:documentation>Место рождения</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BlockReasonType">
		<xsd:annotation>
			<xsd:documentation>Причина блокировки карты</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="LOST">
				<xsd:annotation>
					<xsd:documentation>Карта потеряна</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="STOLEN">
				<xsd:annotation>
					<xsd:documentation>Карта украдена</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HOLDER">
				<xsd:annotation>
					<xsd:documentation>По инициативе держателя</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ATM">
				<xsd:annotation>
					<xsd:documentation>Изъята банкоматом</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BonusProgram_Type">
		<xsd:annotation>
			<xsd:documentation>Бонус-программа</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:enumeration value="BPT_AE">
				<xsd:annotation>
					<xsd:documentation>Аэрофлот-бонус</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BPT_PG">
				<xsd:annotation>
					<xsd:documentation>Подари Жизнь</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BPT_GM">
				<xsd:annotation>
					<xsd:documentation>Золотая Маска</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BPT_OB">
				<xsd:annotation>
					<xsd:documentation>Олимпийская</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BPT_MT">
				<xsd:annotation>
					<xsd:documentation>МТС</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BranchId_Type">
		<xsd:annotation>
			<xsd:documentation>Номер филиала, в котором открыт или ведется продукт. Без лидирующих нулей.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="5"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Building_Type">
		<xsd:annotation>
			<xsd:documentation>Строение</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="C">
		<xsd:annotation>
			<xsd:documentation>Базовый символьный тип, элемент данного типа может содержать количество символов до определенного максимума</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="CardBonusSign_Type">
		<xsd:annotation>
			<xsd:documentation>Признак принадлежности карты к бонусной программе:
A – Аэрофлот-бонус
G – Золотая маска
M – МТС
Y – Молодёжная
Z – Подари жизнь
O – Олимпийская благотворительная</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CardLevel_Type">
		<xsd:annotation>
			<xsd:documentation>Вид карты:
A1 – AMEX Black
A3 – AMEX Platinum (КФ)
A4 – AMEX Platinum
CM – Сбербанк-Maestro
CT – MasterCard транспортная
MB – MasterCard Business
MG – Gold MasterCard
MP – MasterCard Platinum
MS – MasterCard Standard
MV – MasterCard Virtual
MQ – MasterCard payPass
MR – MasterCard WBE PR
MW – MasterCard WBE
PC – ПРО100
PO – ПРО100
PP – ПРО100
PR – ПРО100
PS – ПРО100
UE – УЭК
VB – Visa Business
VC – Visa Classic
VE – Сбербанк-Visa Electron
VG – Visa Gold
VI – Visa Infinite
VP – Visa Platinum
VT – Visa транспортная
VV – Visa Virtual
VR – Visa Platinum PR
VQ – Visa Classic payWave</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CardName_Type">
		<xsd:annotation>
			<xsd:documentation>Наименование карты</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String"/>
	</xsd:simpleType>
	<xsd:simpleType name="CardNumType">
		<xsd:annotation>
			<xsd:documentation>Номер карты</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:minLength value="13"/>
			<xsd:maxLength value="19"/>
			<xsd:pattern value="[0-9]+"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ContractNumber_Type">
		<xsd:annotation>
			<xsd:documentation>Номер контракта</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="64"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CountryCode_Type">
		<xsd:annotation>
			<xsd:documentation>Код страны. ISO-3176</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="NC">
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Country_Type">
		<xsd:annotation>
			<xsd:documentation>Страна.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="NC">
			<xsd:maxLength value="150"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Commission_Type">
		<xsd:annotation>
			<xsd:documentation>Признак взимания коммиссии (True/False)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:boolean"/>
	</xsd:simpleType>
	<xsd:simpleType name="Corpus_Type">
		<xsd:annotation>
			<xsd:documentation>Корпус</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CreditRole_Type">
		<xsd:annotation>
			<xsd:documentation>Тип роль клиента в кредитном договоре</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="Long">
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>Заёмщик или созаёмщик</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation>Поручитель или залогодатель</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ChangeStatusAction_Type">
		<xsd:annotation>
			<xsd:documentation>Простой тип Действие с подпиской АС АП</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="PAUSE">
				<xsd:annotation>
					<xsd:documentation>Приостановка подписки</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UNPAUSE">
				<xsd:annotation>
					<xsd:documentation>Возобновление подписки</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CLOSE">
				<xsd:annotation>
					<xsd:documentation>Закрытие подписки</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Channel_Type">
		<xsd:annotation>
			<xsd:documentation>Простой тип Канал создания подписки</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="IB">
				<xsd:annotation>
					<xsd:documentation>Интернет-банк</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VSP">
				<xsd:annotation>
					<xsd:documentation>ВСП (филиал)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="US">
				<xsd:annotation>
					<xsd:documentation>Устройство самообслуживания</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SMSSender">
				<xsd:annotation>
					<xsd:documentation>Автоматически</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CitizenShip_Type">
		<xsd:annotation>
			<xsd:documentation>Гражданство.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C"/>
	</xsd:simpleType>
	<xsd:simpleType name="City_Type">
		<xsd:annotation>
			<xsd:documentation>Населенный пункт</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="60"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CurCode_Type">
		<xsd:annotation>
			<xsd:documentation>Код валюты. ISO-4217.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="NC">
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CustPermId_Type">
		<xsd:annotation>
