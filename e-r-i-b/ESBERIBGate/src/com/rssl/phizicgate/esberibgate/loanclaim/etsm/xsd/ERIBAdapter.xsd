	<xsd:complexType name="DepoSecurityRec_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о разделе счета (ДЕПО).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DivisionNumber" type="DivisionNumber_Type">
				<xsd:annotation>
					<xsd:documentation>Тип и номер раздела</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="SectionInfo" type="DepoSecuritySectionInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о ценной бумаге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoSecuritySectionInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о ценной бумаге (ДЕПО).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="SecurityMarker" type="SecurityMarker_Type">
				<xsd:annotation>
					<xsd:documentation>Маркер ценной бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InsideCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Депозитарный код выпуска ценной бумаги (партии неэмисионной ЦБ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Remainder" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Остаток, кол-во ценных бумаг на счете(шт.)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StorageMethod" type="DepoStorageMethod_Type">
				<xsd:annotation>
					<xsd:documentation>Метод хранения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DeptId_Type">
		<xsd:annotation>
			<xsd:documentation>Идентификатор долга.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RecNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер выставленного счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EffDt" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата выставления счета </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DeptRec_Type">
		<xsd:annotation>
			<xsd:documentation>Реквизиты для оплаты задолженности ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="BIC" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>БИК банка получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование банка получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CorBankAccount" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Корсчет банка получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecipientName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TaxId" type="TaxId_Type">
				<xsd:annotation>
					<xsd:documentation>ИНН получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="KPP" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>КПП получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecipientAccount" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Расчетный счет получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DetailAcctInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Детальная информация по вкладу</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="DepAcctId"/>
			<xsd:element ref="DepAccInfo"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DocInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о переводе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="DepAcctId"/>
			<xsd:element name="Activity" type="String">
				<xsd:annotation>
					<xsd:documentation>Действия с вкладом: 1 – расторгнуть договор с закрытием счета по вкладу, 2 – выдать дубликат сберкнижки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RecAcc" type="AcctIdType">
				<xsd:annotation>
					<xsd:documentation>Номер счета, для перечисления средств со вклада</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsByCash" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Выдать наличными</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DocStateUpdateRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип запроса на оповещение об изменении статуса подписки</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element maxOccurs="unbounded" name="Document" type="Document_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DocStateUpdateRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа на запрос на оповещение об изменении статуса подписки</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element maxOccurs="unbounded" name="Document" type="Document_Type">
				<xsd:annotation>
					<xsd:documentation>Информация об обновлении статуса документ на стороне получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Document_Type">
		<xsd:annotation>
			<xsd:documentation>Тип документа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DocNumber">
				<xsd:annotation>
					<xsd:documentation>Номер документа, по которому поменялся статус</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="C">
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EmployeeOfTheVSP_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных сотрудника ВСП, выполнившего действие</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Login">
				<xsd:annotation>
					<xsd:documentation>Логин</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="C">
						<xsd:maxLength value="120"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element ref="PersonName">
				<xsd:annotation>
					<xsd:documentation>ФИО Сотрудника ВСП</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EmploymentHistoryType">
		<xsd:annotation>
			<xsd:documentation>Тип &lt;Информация о месте работы&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="OrgInfo" type="OrgInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о работодателе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="JobTitle" type="String">
				<xsd:annotation>
					<xsd:documentation>Должность</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ExecStatus_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация об исполнении автоплатежа в АС AutoPay</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ExecPaymentDate" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата исполнения платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NonExecReasonCode" type="String">
				<xsd:annotation>
					<xsd:documentation>Код причины неисполнения платежа. Присутствует только для платежей со статусом Отменен. (PaymentStatus = Canceled)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NonExecReasonDesc" type="String">
				<xsd:annotation>
					<xsd:documentation>Причина неисполнения. Присутствует только для платежей со статусом Отменен. (PaymentStatus = Canceled)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ExctractLine_Type">
		<xsd:sequence>
			<xsd:element name="PayDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Summa" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="SummaCur" type="AcctCur_Type">
				<xsd:annotation>
					<xsd:documentation>Валюта суммы платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MonthOfPay" type="Date">
				<xsd:annotation>
					<xsd:documentation>Месяц платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecBIC" type="String">
				<xsd:annotation>
					<xsd:documentation>БИК получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RecCorrAccount" type="String">
				<xsd:annotation>
					<xsd:documentation>Корсчет получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecCalcAccount" type="String">
				<xsd:annotation>
					<xsd:documentation>Расчетный счет получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Status" type="String">
				<xsd:annotation>
					<xsd:documentation>Статус платежа: 1 – исполнен, 2 – не выполнен</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Field_Type">
		<xsd:annotation>
			<xsd:documentation>Поле дополнительного реквизита</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SysFieldName" type="String">
				<xsd:annotation>
					<xsd:documentation>Идентификатор поля состоящий из цифр и букв английского алфавита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TemplateType" type="String">
				<xsd:annotation>
					<xsd:documentation>Шаблон для заполнения поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="RegExp"/>
			<xsd:element minOccurs="0" name="Length" type="Long">
				<xsd:annotation>
					<xsd:documentation>Точная длина значения поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Prefix" type="String">
				<xsd:annotation>
					<xsd:documentation>Символы на экране до поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Postfix" type="String">
				<xsd:annotation>
					<xsd:documentation>Символы на экране после поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="Menu"/>
			<xsd:element minOccurs="0" name="EnteredData" type="String">
				<xsd:annotation>
					<xsd:documentation>Введенные клиентом данные</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DefaultData" type="String">
				<xsd:annotation>
					<xsd:documentation>Значение по умолчанию для поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Error" type="String">
				<xsd:annotation>
					<xsd:documentation>Текст ошибки на русском языке для пользователя, в случае нахождения ошибки на сервере</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FullAddress_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о полном адресе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AddrType" type="AddressType_Type">
				<xsd:annotation>
					<xsd:documentation>Тип адреса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Addr3" type="String">
				<xsd:annotation>
					<xsd:documentation>Ненормализованный адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Country" type="String">
				<xsd:annotation>
					<xsd:documentation>Страна</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PostalCode" type="PostalCode_Type">
				<xsd:annotation>
					<xsd:documentation>Почтовый индекс</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Region" type="String">
				<xsd:annotation>
					<xsd:documentation>Регион(республика,область,край)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Area" type="String">
				<xsd:annotation>
					<xsd:documentation>Район</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="City" type="String">
				<xsd:annotation>
					<xsd:documentation>Город</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Settlement" type="String">
				<xsd:annotation>
					<xsd:documentation>Населенный пункт</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Addr1" type="String">
				<xsd:annotation>
					<xsd:documentation>Улица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="HouseNum" type="String">
				<xsd:annotation>
					<xsd:documentation>Дом (владение)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="HouseExt" type="String">
				<xsd:annotation>
					<xsd:documentation>Корпус</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Unit" type="String">
				<xsd:annotation>
					<xsd:documentation>Строение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="UnitNum" type="String">
				<xsd:annotation>
					<xsd:documentation>Квартира/офис</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetAutoPaymentDetailInfoRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфейса GAPDI - получение детальной информации по платежам</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element maxOccurs="unbounded" name="AutoPaymentId" type="AutoPaymentId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификационная информация об автоплатеже</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetAutoPaymentDetailInfoRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-ответа для интерфейса GAPDI - получение детальной информации по платежам</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="AutoPaymentRec" type="AutoPaymentRec_Type">
				<xsd:annotation>
					<xsd:documentation>Список найденных автоплатежей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetAutoPaymentListRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфейса GAPL - получение списка платежей по подписке</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="AutoSubscriptionId" type="AutoSubscriptionId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификационная информация о подписке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PaymentStatusList" type="PaymentStatusList_Type">
				<xsd:annotation>
					<xsd:documentation>Список возможных состояний автоплатежа. Если не передано, то фильтрация по состояниям  не осуществляется</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="SelRangeDtTm" type="SelRangeDtTm_Type">
				<xsd:annotation>
					<xsd:documentation>Период времени за который были исполнены платежи.
Если элемент отсутствует, то фильтра по дате исполнения нет и возвращается 10 последних платежей по подписке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetAutoPaymentListRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-ответа для интерфейса GAPL - получение списка платежей по подписке</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="AutoPaymentRec" type="AutoPaymentRec_Type">
				<xsd:annotation>
					<xsd:documentation>Список найденных автоплатежей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetAutoSubscriptionDetailInfoRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфейса GASDI - получение детальной информации по подписке</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element maxOccurs="unbounded" name="AutoSubscriptionId" type="AutoSubscriptionId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификационная информация о подписке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetAutoSubscriptionDetailInfoRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-ответа для интерфейса GASDI - получение детальной информации по подписке</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="AutoSubscriptionRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="GetAutoSubscriptionListRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфейса GASL - получение списка подписок по платежным инструментам</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element maxOccurs="unbounded" ref="BankAcctRec"/>
			<xsd:element minOccurs="0" name="AutopayStatusList" type="AutopayStatusList_Type">
				<xsd:annotation>
