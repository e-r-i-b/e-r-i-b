			<xsd:element maxOccurs="unbounded" name="BankAcctRec" type="ImsRec_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ImaAcctInRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса IMA_IS_s получения детальной информации по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element minOccurs="0" ref="Status"/>
			<xsd:element maxOccurs="unbounded" ref="ImsRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMAAcctId_Type">
		<xsd:annotation>
			<xsd:documentation>Тип информация об ОМС счете (для переводов)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="AcctId">
				<xsd:annotation>
					<xsd:documentation>Номер ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CustInfo"/>
			<xsd:element ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMAInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип информация об ОМС договоре</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="CustRec"/>
			<xsd:element minOccurs="0" ref="AcctId">
				<xsd:annotation>
					<xsd:documentation>Номер ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AcctCur">
				<xsd:annotation>
					<xsd:documentation>Код металла</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctCode" type="Long">
				<xsd:annotation>
					<xsd:documentation>Вид вклада</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctSubCode" type="Long">
				<xsd:annotation>
					<xsd:documentation>Подвид вклада</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BankInfo" type="MinBankInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о номере ОСБ и ВСП, в котором открывается ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMAInfoResponse_Type">
		<xsd:annotation>
			<xsd:documentation>Тип информация об ОМС договоре (ответ)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="AcctId">
				<xsd:annotation>
					<xsd:documentation>Номер ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMAOperConvInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация о конверсии при операциях с ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CurType" type="IMAOperCurType_Type">
				<xsd:annotation>
					<xsd:documentation>Признак: операция с металлом или денежными средствами</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CurAmt">
				<xsd:annotation>
					<xsd:documentation>Сумма операции в рублях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="AcctCur">
				<xsd:annotation>
					<xsd:documentation>Валюта</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMAToCardAddRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип запроса для интерфейса SrvIMAToCardAdd (TIС). Продажа обезличенного металла с зачислением денежных средств на МБК того же клиента. Операция ОМС-Карта</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="XferInfo" type="XferIMAInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о реквизитах совершаемой операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMAToCardAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа для интерфейса SrvIMAToCardAdd (TIC). Продажа обезличенного металла с зачислением денежных средств на МБК того же клиента. Операция ОМС-Карта</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ImsAcctId_Type">
		<xsd:sequence>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="AcctId"/>
			<xsd:element minOccurs="0" ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ImsAcctInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="AcctCur">
				<xsd:annotation>
					<xsd:documentation>Вид металла</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AcctName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Краткое наименование. На русском языке готовое для отображения пользователю</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Amount" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Остаток в граммах(Значение дробной части должно быть указано с точностью до второго знака.)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="StartDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата открытия счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="EndDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата закрытия счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Status" type="IMSStatusEnum_Type">
				<xsd:annotation>
					<xsd:documentation>Статус счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AgreementNumber" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер договора счета ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MaxSumWrite" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Максимальная сумма списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element minOccurs="0" ref="CustRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMSAcctRec_Type">
		<xsd:annotation>
			<xsd:documentation>Выписка по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="BankAcctId"/>
			<xsd:element minOccurs="0" ref="BankAcctFullStmtInfo"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="BankAcctFullStmtRec"/>
			<xsd:element maxOccurs="10" minOccurs="0" ref="BankAcctStmtRec"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IMSBalance_Type">
		<xsd:annotation>
			<xsd:documentation>Тип баланса по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Amt" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Остаток в граммах</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AmtRub" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Остаток в рублях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ImsRec_Type">
		<xsd:annotation>
			<xsd:documentation>Информация по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="ImsAcctId"/>
			<xsd:element minOccurs="0" ref="ImsAcctInfo"/>
			<xsd:element minOccurs="0" ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InsuranceApp_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о страховом/НПФ продукте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Reference" type="String">
				<xsd:annotation>
					<xsd:documentation>ID (референс) страховки, по которой запрашиваются данные</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BusinessProcess" type="String">
				<xsd:annotation>
					<xsd:documentation>Наименование бизнес-процесса, в рамках которого оформлена страховка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ProductType" type="String">
				<xsd:annotation>
					<xsd:documentation>Наименование типа продукта, в рамках которого оформлена страховка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Status" type="String">
				<xsd:annotation>
					<xsd:documentation>Статус страховки для отображения. Для страховых продуктов (Действует, Срок действия истекает, Срок действия истек, Аннулирован). Для НПФ продуктов (Договор заключен, Действующий клиент НПФ Сбербанка, Договор расторгнут).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Company" type="String">
				<xsd:annotation>
					<xsd:documentation>Страховая компания для отображения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Program" type="String">
				<xsd:annotation>
					<xsd:documentation>Страховая программа для отображения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="StartDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата начала действия страховки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="EndDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата окончания действия страховки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="SNILS" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер СНИЛС для пенсионного страхования</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Amount" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Страховая сумма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AmountCur" type="String">
				<xsd:annotation>
					<xsd:documentation>Валюта страховай суммы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PolicyDetails" type="PolicyDetails_Type">
				<xsd:annotation>
					<xsd:documentation>Описание реквизитов договора страхования: Серия, номер и дата выдачи полиса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Risks" type="String">
				<xsd:annotation>
					<xsd:documentation>Описание страховых рисков</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AdditionalInfo" type="String">
				<xsd:annotation>
					<xsd:documentation>Дополнительная информация (№ и дата кредитного договора (при условии оформления страховки в рамках кредитной сделки)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InsuranceAppList_Type">
		<xsd:annotation>
			<xsd:documentation>Список страховых/НПФ продуктов</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" ref="InsuranceApp"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IntegrationInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Идентификаторы клиента в ИС &lt;IntegrationInfo&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" name="IntegrationId">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ISCode" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Код системы, в которой будут проведены изменения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ISCustId" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Идентификатор клиента</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InterestOnDeposit_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для "Информации, предназначенная для перечисления процентов"</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="AcctId"/>
			<xsd:element ref="CardNum"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IntRate_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для процентов</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Rate" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Величина процентой ставки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanAcctId_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о кредите</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="SystemId"/>
			<xsd:element minOccurs="0" ref="AcctId"/>
			<xsd:element minOccurs="0" ref="AgreemtNum"/>
			<xsd:element minOccurs="0" ref="ProdType"/>
			<xsd:element minOccurs="0" ref="LoanType"/>
			<xsd:element minOccurs="0" name="DateCalc" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата, на которую нужно рассчитать задолженность</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CurAmt"/>
			<xsd:element minOccurs="0" ref="AcctCur"/>
			<xsd:element minOccurs="0" ref="OrigAmt"/>
			<xsd:element minOccurs="0" ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanAcctIdDepo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о кредите</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SystemId" type="SystemId_Type"/>
			<xsd:element ref="AcctId"/>
			<xsd:element ref="AcctCur"/>
			<xsd:element name="LoanStatus" type="LoanStatus_Type"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanAcctRec_Type">
		<xsd:annotation>
			<xsd:documentation>Список кредитов</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="LoanAcctId"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="BankAcctInfo"/>
			<xsd:element ref="CustRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanInfo_Type">
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="AcctBalFull" type="AcctBal_Type"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="AcctBalOnDate" type="AcctBal_Type"/>
			<xsd:element minOccurs="0" name="IdSpacing" type="xsd:string"/>
			<xsd:element minOccurs="0" ref="LoanAcctId"/>
			<xsd:element minOccurs="0" ref="AgreemtNum"/>
			<xsd:element minOccurs="0" ref="ProdType"/>
			<xsd:element minOccurs="0" ref="LoanType"/>
			<xsd:element minOccurs="0" ref="AcctCur"/>
			<xsd:element minOccurs="0" ref="CurAmt"/>
			<xsd:element minOccurs="0" name="Period" type="Long">
				<xsd:annotation>
					<xsd:documentation>Срок договора в днях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CreditingRate" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Процентная ставка по кредиту</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RegNextPayDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Предельная дата платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NextPayDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Предельная дата текущего периода</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NextPaySum" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Предполагаемая сумма платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RegPayAmount" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Размер аннуитетного платежа, поле обязательно, если признак аннуитетности равен true</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PaymentStatus" type="PaymentStatus_Type">
				<xsd:annotation>
					<xsd:documentation>Статус оплаты, 1 – не просрочен, 2 – просрочен</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="LoanStatus" type="String">
				<xsd:annotation>
					<xsd:documentation>Статус кредита, 1 – открыт, 2 – закрыт</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PrevPayDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата предыдущего платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PrevPaySum" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма предыдущего платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PayCard" type="CardNumType">
				<xsd:annotation>
					<xsd:documentation>Карта оплаты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="CustRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanInfoRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса LN_CI получения детальной информации по кредиту</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="LoanAcctId"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanInfoRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса LN_CI получения детальной информации по кредиту</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element minOccurs="0" ref="Status"/>
			<xsd:element ref="LoanRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса IIC получения информации о задолженности по кредиту</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="LoanAcctId"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса IIC получения информации о задолженности по кредиту</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element ref="LoanRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanPaymentRec_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о платеже по кредиту</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="LoanPaymentNumber" type="Long">
				<xsd:annotation>
					<xsd:documentation>Порядковый номер платежа в графике</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanPaymentStatus" type="String">
				<xsd:annotation>
					<xsd:documentation>Статус: 1 – оплачен, 2 – текущая оплата, 3 – к оплате</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LoanPaymentDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата погашения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="AcctBalOnDate" type="AcctBal_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanPaymentRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса LN_PSC получения графика платежей</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="LoanAcctId"/>
			<xsd:element minOccurs="0" name="StartNumber" type="Long">
				<xsd:annotation>
					<xsd:documentation>Номер платежа, с которого необходимо отобразить график</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MaxForwardCount" type="Long">
				<xsd:annotation>
					<xsd:documentation>Максимальное количество будущих платежей, которое может быть возвращено</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MaxRewardCount" type="Long">
				<xsd:annotation>
					<xsd:documentation>Макисмальное количество платежей, которое может быть возвращено</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanPaymentRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса LN_PSC получения графика платежей</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="AcctId"/>
			<xsd:element minOccurs="0" name="DoneAmount" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма всех выплат, которые были произведены, с учетом штрафов и проч.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RemainAmount" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма платежа для закрытия кредита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="FineAmount" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма всех штрафов и пеней</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MaxSize" type="Long">
				<xsd:annotation>
					<xsd:documentation>Общее количество платежей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="LoanPaymentRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanRec_Type">
		<xsd:sequence>
			<xsd:element ref="LoanInfo"/>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element minOccurs="0" name="BankAccInfo" type="BankAcctInfo_Type"/>
			<xsd:element minOccurs="0" ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MBCInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация об услуге мобильный банк</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Status" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Подключена услуга или нет</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ContractType" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Тип пакета (0 - полный, 1 - экономический)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PhoneNum" type="PhoneNum_Type">
				<xsd:annotation>
					<xsd:documentation>Информация об операторе и моб.телефоне, к которому подключена услуга &lt; Мобильный банк &gt;</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MDMClientInfoUpdateRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса MDM_CLT регистрации и обновления данных о клиенте в MDM и ЕРИБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="OperType" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CustRec"/>
			<xsd:element minOccurs="0" name="UserInfo" type="UserInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о пользователе, осуществившим изменения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MDMClientInfoUpdateRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса MDM_CLT регистрации и обновления данных о клиенте в MDM и ЕРИБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" name="IntegrationInfo" type="IntegrationInfo_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Menu_Type">
		<xsd:annotation>
			<xsd:documentation>Список значений для выбора клиентом</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="MenuItem" type="String">
				<xsd:annotation>
					<xsd:documentation>Одно из значений для списка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MessageRecvRqType">
		<xsd:annotation>
			<xsd:documentation>Оповещение о приеме сообщения ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element name="DocNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер сформированного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MessageRecvRsType">
		<xsd:annotation>
			<xsd:documentation>Оповещение о приеме сообщения ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" name="DocNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер сформированного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OperInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип &lt;Информация об операции&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DocumentDate" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentNumber" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Номер поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OrgInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип &lt;Информация об организации&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="EmployerCode" type="String">
				<xsd:annotation>
					<xsd:documentation>Организационна-правовая форма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OTPRestrictionModRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфейса OTPR - изменение ограничения на печать и использование одноразовых паролей по карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element maxOccurs="unbounded" ref="CardAcctRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OTPRestrictionModRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-ответа для интерфейса OTPR - изменение ограничения на печать и использование одноразовых паролей по карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" name="BankAcctResult" type="BankAcctResult_Type">
				<xsd:annotation>
					<xsd:documentation>Результат изменения для каждой карты в отдельности</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OTPRestriction_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для блока "Группа ограничений для одноразовых паролей (OTP)"</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OTPGet" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак возможности печати одноразовых паролей по карте
true - установить возможность 
false - запретить возможность 
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="OTPUse" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак возможности использования ранее выданных одноразовых паролей.
true – выданные ранее одноразовые пароли не блокируются
false - - выданные ранее одноразовые парли блокируются
Обязателен при OPTGet=false. 
Для сервиса GFL следующий смысл "Признак наличия по карте ранее выданных одноразовых паролей" 
true – пароли есть
false – паролей нет
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PayInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о переводе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="CardAcctId"/>
			<xsd:element minOccurs="0" name="AcctId">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="SystemId"/>
						<xsd:element name="AcctIdFrom" type="AcctIdType"/>
						<xsd:element ref="BankInfo"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element ref="DepoAcctId"/>
			<xsd:element ref="DeptRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PaymentStatusList_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Список возможных состояний автоплатежа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" name="PaymentStatus" type="PaymentStatusASAP_Type">
				<xsd:annotation>
					<xsd:documentation>Код состояния автоплатежа в АС АП</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PaymentTemp_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о шаблоне платежа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RecipientID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Идентификатор организации получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecipientName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование организации получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PaymentsID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Идентификатор платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PersonInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о физическом лице &lt;PersonInfo&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="PFRAccount" type="PfrAccount_Type">
				<xsd:annotation>
					<xsd:documentation>Номер СНИЛС клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="Birthday">
				<xsd:annotation>
					<xsd:documentation>Дата рождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BirthPlace" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Место рождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TaxId" type="TaxId_Type">
				<xsd:annotation>
					<xsd:documentation>ИНН</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Citizenship" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Гражданство</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AdditionalInfo" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Гражданство</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PersonRole" type="CreditRole_Type">
				<xsd:annotation>
					<xsd:documentation>Роль клиента (используется в кредитном договоре)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="Gender">
				<xsd:annotation>
					<xsd:documentation>Пол. Значения:Male – мужской Female – женский Unknown – неизвестен</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Resident" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Резидент или нет</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="PersonName">
				<xsd:annotation>
					<xsd:documentation>ФИО клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="IdentityCard">
				<xsd:annotation>
					<xsd:documentation>Удостоверение личности.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="ContactInfo">
				<xsd:annotation>
					<xsd:documentation>Контактная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PersonName_Type">
		<xsd:annotation>
			<xsd:documentation>Имя физического лица &lt;PersonName&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="LastName">
				<xsd:annotation>
					<xsd:documentation>Фамилия.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="FirstName">
				<xsd:annotation>
					<xsd:documentation>Имя.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="MiddleName">
				<xsd:annotation>
					<xsd:documentation>Отчество.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PfrGetInfoInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для запроса интерфейса ESK_GET_PFRINFO получения выписки клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="OperationId" type="UUID">
				<xsd:annotation>
					<xsd:documentation>Идентификатор запроса PfrHasInfoInqRequest(PfrHasInfoInqRequest/RqUID), выполненного ранее</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PfrGetInfoInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для ответа интерфейса ESK_GET_PFRINFO получения выписки клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" ref="Result"/>
			<xsd:element minOccurs="0" name="PfrInfo" type="xsd:anyType">
				<xsd:annotation>
					<xsd:documentation>Выписка из ПФР. Это поле присутствует и обязательно только в случае, если ResponseExist = true</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PfrHasInfoInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для запроса интерфейса ESK_HAS_PFRINFO проверки наличия выписки клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="CustInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PfrHasInfoInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для ответа интерфейса ESK_HAS_PFRINFO проверки наличия выписки клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" ref="Result"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PfrResult_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для возврата результата о состоянии выписки при запросе ПФР</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ResponseExists" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак наличия выписки в ПФР</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SendTime" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время отправки запроса в ПФР</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ReceiveTime" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время ответа из ПФР.Обязателен в случае, если ResponseExists = true</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PhoneNum_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="PhoneType" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Тип телефона. Если не задан, то считается Mobile</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PhoneCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код оператора связи</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PhoneOperName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование оператора связи</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Phone" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер телефона</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PlasticInfoType">
		<xsd:annotation>
			<xsd:documentation>Информация о клиенте, которая печатается на карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="PersonTitle" type="String">
				<xsd:annotation>
					<xsd:documentation>Обращение к клиенту</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LastName" type="LastNameForPlastic_Type">
				<xsd:annotation>
					<xsd:documentation>Фамилия клиента для записи на пластиковую карту (только латинские буквы до 32 символов)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FirstName" type="FirstNameForPlastic_Type">
				<xsd:annotation>
					<xsd:documentation>Имя клиента для записи на пластиковую карту (только латинские буквы до 32 символов)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PolicyDetails_Type">
		<xsd:annotation>
			<xsd:documentation>Описание реквизитов договора страхования: Серия, номер и дата выдачи полиса</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="Series" type="String">
				<xsd:annotation>
					<xsd:documentation>Серия</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Num" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IssueDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RecipientRec_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о поставщике</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="CodeRecipientBS" type="String">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор организации получателя платежей.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Name" type="String">
				<xsd:annotation>
					<xsd:documentation>Наименование получателя, готовое для отображения пользователю</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="GroupService" type="String">
				<xsd:annotation>
					<xsd:documentation>Код группы услуг. Параметр должен быть определен в ЕРИБ по ПУ и услуге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CodeService" type="String">
				<xsd:annotation>
					<xsd:documentation>Идентификатор услуги, к которой относится получатель</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NameService" type="String">
				<xsd:annotation>
					<xsd:documentation>Наименование услуги, к которой относится получатель, готовое для отображения пользователю</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TaxId" type="TaxId_Type">
				<xsd:annotation>
					<xsd:documentation>ИНН получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CorrAccount" type="AcctIdType">
				<xsd:annotation>
					<xsd:documentation>Корр. счет получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="KPP" type="String">
				<xsd:annotation>
					<xsd:documentation>КПП получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BIC" type="String">
				<xsd:annotation>
					<xsd:documentation>БИК банка получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AcctId" type="AcctIdType">
				<xsd:annotation>
					<xsd:documentation>Расчетный счет получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NameOnBill" type="String">
				<xsd:annotation>
					<xsd:documentation>Наименование поставщика, выводимое в чеке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NotVisibleBankDetails" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак «не отображать банковские реквизиты для поставщика» (в том числе и на странице подтверждения платежа). Если указано «false» или тэг отсутствует, то банковские реквизиты отображаются. Если указано «true» - не отображаются.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PhoneToClient" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер телефона поставщика услуг для обращений клиентов банка, которые совершили платежи в адрес поставщика для печати в чеке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsAutoPayAccessible" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак, определяющий возможность регистрации подписки на автоплатёж в пользу данного ПУ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AutoPayDetails" type="AutopayDetails_Type">
				<xsd:annotation>
					<xsd:documentation>Параметры подписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element minOccurs="0" name="Requisites" type="Requisites_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RegExp_Type">
		<xsd:annotation>
			<xsd:documentation>Регулярное выражение</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Exp" type="String">
				<xsd:annotation>
					<xsd:documentation>Регулярное выражение, для проверки введенных данных</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Mess" type="String">
				<xsd:annotation>
					<xsd:documentation>Сообщение для отображения пользавателю, в случае не выполнения рег. Выражения.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Regular_Type">
		<xsd:annotation>
			<xsd:documentation>Параметры регулярного платежа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DateBegin" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата начала действия поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DateEnd" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата окончания действия поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PayDay">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element minOccurs="0" name="Day" type="Long"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element minOccurs="0" name="Priority" type="Long">
				<xsd:annotation>
					<xsd:documentation>Приоритет исполнения поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ExeEventCode" type="String">
				<xsd:annotation>
					<xsd:documentation>Код события исполнения формы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Summa" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SummaKindCode" type="String">
				<xsd:annotation>
					<xsd:documentation>Алгоритм расчета суммы платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Percent" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Процент от суммы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Requisites_Type">
		<xsd:annotation>
			<xsd:documentation>Дополнительныe реквизиты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" name="Requisite" type="Requisite_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RequisiteTypes_Type">
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" name="RequisiteType" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Бизнес-смысл реквизита.
Область допустимых значений реквизита:
– IsListOfCharges (реквизит является списком начислений);
– IsPeriod (реквизит является периодом платежа).
Данные должны передаваться в формате mm.yyyy;
– IsSubscriberID (реквизит является идентификатором абонента у ПУ).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Requisite_Type">
		<xsd:annotation>
			<xsd:documentation>Дополнительный реквизит</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="NameVisible" type="String">
				<xsd:annotation>
					<xsd:documentation>Наименование дополнительного реквизита для отображения пользователю</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NameBS" type="String">
				<xsd:annotation>
					<xsd:documentation>Идентификатор поля состоящий из цифр и букв английского алфавита</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Description" type="String">
				<xsd:annotation>
					<xsd:documentation>Правила заполнения поля.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Comment" type="String">
				<xsd:annotation>
					<xsd:documentation>Комментарий к полю</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Type" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NumberPrecision" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Точность числового значения (количество знаков после запятой).
Если = 0, то допустимы только целые числа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsRequired" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак «Дополнительный реквизит обязательно должен быть указан при формировании платежа»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsSum" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак «Общая сумма платежа». В том случае, когда БС ТБ возвращает не только общую сумму задолженности, но и расшифровку этой суммы (в разбивке по видам начислений), то этот признак указывает, какое поле следует считать общей суммой задолженности. В реквизитах, полученных от БС ТБ, может быть только одно поле с таким признаком.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsKey" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак «Данный реквизит является одним из ключевых, т.е. индетифицирующий абонента».
Реквизиты с таким признаками будут отображаться в чеках, отправляться в подтверждающих смс вне зависимости от значения признаков isForBill и includeInSMS.
Признак не может быть передан одновременно с  IsVisible = false, IsSum
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsEditable" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак «Редактируемого поля». Поля предоставляет возможность ввода данных</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsVisible" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак видимости поля клиентом. Признак не может быть равен false одновременно с передачей  IsSum для всех случаев, кроме оформления подписки на автоплатежи</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsForBill" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак «Включать в счет»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IncludeInSMS" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак «Включать в SMS»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="SaveInTemplate" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак «Сохранять в шаблоне платежа»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RequisiteTypes" type="RequisiteTypes_Type"/>
			<xsd:element minOccurs="0" name="HideInConfirmation" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак «Скрывать реквизит на форме подтверждения»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AttributeLength" type="AttributeLength_Type">
				<xsd:annotation>
					<xsd:documentation>Описание длины поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Validators" type="Validators_Type">
				<xsd:annotation>
					<xsd:documentation>Валидаторы (контроли), используемые для дополнительного атрибута. Неприменимо для типов set и list.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Menu" type="Menu_Type">
				<xsd:annotation>
					<xsd:documentation>Список значений для выбора клиентом при для выбора клиентом при Type=list и set</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="EnteredData" type="EnteredData_Type">
				<xsd:annotation>
					<xsd:documentation>Введенные клиентом данные.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DefaultValue" type="String">
				<xsd:annotation>
					<xsd:documentation>Значение по умолчанию для поля.
Клиент передает в этом поле, тоже значение, что пришло от сервера. Не используется для типа set.
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Error" type="String">
				<xsd:annotation>
					<xsd:documentation>Текст ошибки на русском языке для пользователя, в случае нахождения ошибки на сервере</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EnteredData_Type">
		<xsd:annotation>
			<xsd:documentation>Введенные клиентом данные. В данном запросе группа не возвращается. Если БС ТБ выполняет пересчет или изменение введенных клиентом данных, то пересчитанные значения помещаются в данную группу.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" name="DataItem" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Введенное или выбранное  пользователем из списка значение. В случае Type=set может быть указано несколько элементов DataItem</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AttributeLength_Type">
		<xsd:annotation>
			<xsd:documentation>Описание длины поля</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="MaxLength" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Максимальная длина поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MinLength" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Минимальная длина поля</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Validators_Type">
		<xsd:annotation>
			<xsd:documentation>Валидаторы (контроли), используемые для дополнительного атрибута. Неприменимо для типов set и list.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" name="Validator" type="Validator_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Validator_Type">
		<xsd:annotation>
			<xsd:documentation>Валидаторы (контроли), используемые для дополнительного атрибута. Неприменимо для типов set и list.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Type" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Тип валидатора (контроля). На текущий момент используется значение: regexp</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Message" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Сообщение, выводимое на экран клиенту при срабатывании валидатора (контроля).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Parameter" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Выражение для валидатора (контроля). Список значений для выбора клиентом при для выбора клиентом при Type=list и set
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SecurityMarker_Type">
		<xsd:annotation>
			<xsd:documentation>Маркер ценной бумаги</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Remainder" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Остаток, кол-во ценных бумаг данного вида</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MarkerDescription" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Информация о ценной бумаге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SelRangeDt_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для временного периода</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="StartDate" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Начальная дата периода</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="EndDate" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Конечная дата периода</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SelRangeDtTm_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для временного периода (дата и время)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="StartDtTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Начальная дата и время периода</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndDtTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Последняя дата и время периода</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SetAccountStateRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса SACS изменение состояния счета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="DepAcctId"/>
			<xsd:element ref="CustInfo"/>
			<xsd:element name="AccountStateAction" type="AccountStateAction_Type">
				<xsd:annotation>
					<xsd:documentation>Действие метода. Возможна передача значения: STATE_SAVE_BOOK_LOST - признак утери сберкнижки по счету</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SetAccountStateRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса SACS изменение состояния счета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ServiceInfo_Type">
		<xsd:sequence>
			<xsd:element name="ProdType" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип продукта</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RegionNum" type="String"/>
			<xsd:element minOccurs="0" name="BranchNum" type="String"/>
			<xsd:element minOccurs="0" name="AgencyNum" type="String"/>
			<xsd:element minOccurs="0" name="AgreementNum" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер заявления на подключение к услуге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="StartDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата подключения к услуге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="EndDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата отключения от услуги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element minOccurs="0" name="Status" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ServiceStmtRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса ESK_SAS получения графика исполнения длительного поручения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="SvcAcctId"/>
			<xsd:element name="DtBegin" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата начала периода выписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DtEnd" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата конца периода выписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ServiceStmtRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса ESK_SAS получения графика исполнения длительного поручения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="ExctractLine"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SourceId_Type">
		<xsd:sequence>
			<xsd:element name="SourceIdType">
				<xsd:annotation>
					<xsd:documentation>Тип идентификатора канала ('BP_VSP', 'BP_ERIB')</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="32"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="VSPInfo" type="BankInfoLeadZero_Type">
				<xsd:annotation>
					<xsd:documentation>ВСП из которго идет запрос. Заполняется в случае SourceIdType = 'BP_VSP'</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SPDefField_Type">
		<xsd:annotation>
			<xsd:documentation>Поля, определяемые ПУ &lt;SPDefField&gt;</xsd:documentation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="FieldName">
				<xsd:annotation>
					<xsd:documentation>Название признака</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="FieldValue">
				<xsd:annotation>
					<xsd:documentation>Значение признака</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="FieldDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата открытия УДБО</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="FieldNum" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="FieldDt1" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата закрытия УДБО</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="FieldData1" type="AgencyId_Type">
				<xsd:annotation>
					<xsd:documentation>Код ТБ+индекс ОСБ (информация используется ЭСК)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Status_Type">
		<xsd:annotation>
			<xsd:documentation>Статус ответа &lt;Status&gt;. Агрегат &lt;Status&gt; используется для возврата статуса сообщения. </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="StatusCode">
				<xsd:annotation>
					<xsd:documentation>Статусный код возврата.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="StatusDesc">
				<xsd:annotation>
					<xsd:documentation>Описание статуса.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="StatusType">
				<xsd:annotation>
					<xsd:documentation>Тип ошибки.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="ServerStatusDesc">
				<xsd:annotation>
					<xsd:documentation>Описание статуса системы источника</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="StmtSummAmt_Type">
		<xsd:sequence>
			<xsd:element ref="StmtSummType"/>
			<xsd:element ref="CurAmt"/>
			<xsd:choice>
				<xsd:element name="CurAmtCur" type="AcctCur_Type">
					<xsd:annotation>
						<xsd:documentation>Валюта операции</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="AcctCur" type="AcctCur_Type">
					<xsd:annotation>
						<xsd:documentation>Валюта операции</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element minOccurs="0" name="ContrAccount" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер счета контрагента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAcctAudRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса COD_GSI получения информации по длительному поручению</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element maxOccurs="unbounded" name="SvcAcct">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="SvcAcctId"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAcctAudRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса COD_GSI получения информации по длительному поручению</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element minOccurs="0" ref="Status"/>
			<xsd:element maxOccurs="unbounded" ref="SvcActInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAcctDelRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса COD_ASD отмены длительного поручения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="SvcAcct">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="SvcAcctId"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAcctDelRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса COD_ASD отмены длительного поручения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAcctId_Type">
		<xsd:annotation>
			<xsd:documentation>Данные длительного поручения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="SystemId"/>
			<xsd:element name="SvcAcctNum" type="Long">
				<xsd:annotation>
					<xsd:documentation>Номер длительного поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="SvcType" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак "Длительное поручение на перевод"</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcActInfo_Type">
		<xsd:sequence>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" name="SvcAct">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="SvcAcctId"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element minOccurs="0" ref="DepAcctIdFrom"/>
			<xsd:element minOccurs="0" ref="CardAcctIdFrom"/>
			<xsd:element minOccurs="0" name="RecBIC" type="String">
				<xsd:annotation>
					<xsd:documentation>БИК получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RecCorrAccount" type="String">
				<xsd:annotation>
					<xsd:documentation>Корсчет получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RecCalcAccount" type="String">
				<xsd:annotation>
					<xsd:documentation>Расчетный счет получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RecAcctCur" type="AcctCur_Type">
				<xsd:annotation>
					<xsd:documentation>Валюта счета получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RecINN" type="String">
				<xsd:annotation>
					<xsd:documentation>ИНН получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="KPPTo" type="String">
				<xsd:annotation>
					<xsd:documentation>КПП получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Purpose" type="String">
				<xsd:annotation>
					<xsd:documentation>Назначение платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RecipientName" type="String">
				<xsd:annotation>
					<xsd:documentation>Наименование получателя юр. Лица, либо ФИО физ.лица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PmtKind" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="Regular"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAddRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейсов (SDP, SDС) создания длительного поручения на перевод денежных средств</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element minOccurs="0" name="OperName" type="OperName_Type"/>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element minOccurs="0" ref="CustId"/>
			<xsd:element ref="XferInfo"/>
			<xsd:element minOccurs="0" ref="Regular"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейсов (SDP, SDС ) создания длительного поручения на перевод денежных средств</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" ref="SvcAcctId"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SvcsAcct_Type">
		<xsd:annotation>
			<xsd:documentation>Список длительных поручений</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="SystemId"/>
			<xsd:element name="SvcAcctId">
				<xsd:annotation>
					<xsd:documentation>Идентификатор длительного поручения</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="SvcAcctNum" type="Long">
							<xsd:annotation>
								<xsd:documentation>Номер длительного поручения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="BankInfo"/>
						<xsd:element name="SvcType" type="xsd:boolean">
							<xsd:annotation>
								<xsd:documentation>Признак «Длительное поручение на перевод». Если значение false, то значит не перевод, а коммунальные платежи</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="DtStart" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата начала действия длительного поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DtEnd" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата окончания действия длительного поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurAmt" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма длительного поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CardNum">
				<xsd:annotation>
					<xsd:documentation>Номер карты, с которой выполнится списание</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="AcctId">
				<xsd:annotation>
					<xsd:documentation>Номер счета, с которого выполнится списание</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PmtKind" type="PmtKind_Type">
				<xsd:annotation>
					<xsd:documentation>Код вида платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RcptKind" type="RcptKind_Type">
				<xsd:annotation>
					<xsd:documentation>Код вида получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TarifPlanInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация о тарифном плане и сегменте клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Code" type="TarifPlanCodeType">
				<xsd:annotation>
					<xsd:documentation>Код тарифного плана</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ConnectionDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата подключения к тарифному плану</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SegmentCode" type="SegmentCodeType">
				<xsd:annotation>
					<xsd:documentation>Код сегмента, к которому относят клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ManagerId" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер персонального менеджера</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TaxColl_Type">
		<xsd:annotation>
			<xsd:documentation>Налоговые реквизиты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="SRCstatus" type="String">
				<xsd:annotation>
					<xsd:documentation>Статус составителя документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TaxRegCodeTo" type="String">
				<xsd:annotation>
					<xsd:documentation>КПП получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BudgetCode" type="String">
				<xsd:annotation>
					<xsd:documentation>Код бюджетной классификации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="OKATO" type="String">
				<xsd:annotation>
					<xsd:documentation>Код ОКАТО</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TaxBase" type="String">
				<xsd:annotation>
					<xsd:documentation>Основание налогового платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TaxPeriod" type="String">
				<xsd:annotation>
					<xsd:documentation>Налоговый период</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TaxNum" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер налогового документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TaxDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата налогового документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TaxType" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип налогового платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TransferInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Параметры операции (ДЕПО).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DocumentDate" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentNumber" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Номер поручения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OperType" type="DepoOperType_Type">
				<xsd:annotation>
					<xsd:documentation>Тип операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OperationSubType" type="DepoOperationSubType_Type">
				<xsd:annotation>
					<xsd:documentation>Содержание операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="OperationDesc" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Комментарий к операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DivisionNumber" type="DivisionNumber_Type">
				<xsd:annotation>
					<xsd:documentation>Тип и номер раздела</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InsideCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Депозитарный  код выпуска ценной бумаги (партии неэмисионной ЦБ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecurityCount" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Кол-во ценных бумаг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="OperationReason" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Основание операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DetailOperationReason" type="DepoDetailOperationReason_Type">
				<xsd:annotation>
					<xsd:documentation>Описание договора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransferRcpInfo" type="TransferRcpInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о противоположной стороне операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TransferRcpInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Основание операции - описание договора (ДЕПО).</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="CorrDepositary" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование депозитария получателя (ДЕПО)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CorrDepoAcctId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер счета депо на который на осуществить перевод или с которого необходимо принять ценные бумаги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CorrOwner" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Владелец счета депо</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CorrOwnerDetail" type="CorrOwnerDetail_Type">
				<xsd:annotation>
					<xsd:documentation>Детальная информации о владельце счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AdditionalInfo" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Дополнительные реквизиты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DeliveryType" type="DepoDeliveryType_Type">
				<xsd:annotation>
					<xsd:documentation>Способ доставки сертификатов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="UserInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация о пользователе осуществившим изменения</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="UserCategory" type="UserCategoryType">
				<xsd:annotation>
					<xsd:documentation>Тип пользователя, осуществившего изменения: Employee - пользователь системы, TechnicalUser - технологический, Client - клиент</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Login" type="C">
				<xsd:annotation>
					<xsd:documentation>Код/логин пользователя внесшего изменения. Передается только для сотрудника банка.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Fio" type="C">
				<xsd:annotation>
					<xsd:documentation>ФИО пользователя внесшего изменения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BankInfo" type="BankInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Учреждение, к которому привязан пользователь, осуществивший изменения (в кодировке subbranch)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="UsageInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация о транзакционных ограничителях</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="UsageCode" type="C">
				<xsd:annotation>
					<xsd:documentation>Код ограничителя:
P2P_CUM_LIM – индивидуальный суточный лимит по счету карты для операций перевода с карточного счета клиента на карточный счет другого клиента через УС
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MaxCurAmt" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Максимальная сумма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AcctCur"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="XferAddRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфрейсов  TDD, TDC,TCD,TCP,TCC и т.д.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element minOccurs="0" name="SPNum" type="SPNum_Type"/>
			<xsd:element minOccurs="0" name="OperName" type="OperName_Type"/>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element minOccurs="0" ref="CustId"/>
			<xsd:element ref="XferInfo"/>
			<xsd:element minOccurs="0" ref="Regular"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="XferAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-ответа для интерфрейсов  TDD, TDC,TCD,TCP,TCC и т.д.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" ref="SvcAcctId"/>
			<xsd:element minOccurs="0" ref="AgreemtInfo"/>
			<xsd:element minOccurs="0" ref="DstCurAmt"/>
			<xsd:element minOccurs="0" ref="SrcCurAmt"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="XferInfo_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="XferMethod" type="String">
				<xsd:annotation>
					<xsd:documentation>Перечисление, куда отправляется перевод</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ClientDocumentNumber" type="Long">
				<xsd:annotation>
					<xsd:documentation>Номер клиентского поручения (Используется для оплаты товаров и услуг и создании ДП оплаты в Московском банке)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ClientDocumentDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата клиентского поручения. (Используется для оплаты товаров и услуг и создании ДП оплаты в Москоском банке)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PayerInfo">
				<xsd:annotation>
					<xsd:documentation>Идентифицирующая информация о плательщике. (Используется для оплаты товаров и услуг и создании ДП оплаты товаров и услуг  в Москоском банке)</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="PersonInfo" type="PersonInfo_Type"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element minOccurs="0" name="TaxIdTo" type="TaxId_Type">
				<xsd:annotation>
					<xsd:documentation>ИНН получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="KPPTo" type="String">
				<xsd:annotation>
					<xsd:documentation>КПП получателя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TaxIdFrom" type="TaxId_Type">
				<xsd:annotation>
					<xsd:documentation>ИНН плательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RecipientName" type="String">
				<xsd:annotation>
					<xsd:documentation>Наименование получателя юр. Лица, либо ФИО физ.лица, склеенные через пробел. Пробелы внутри фамилии, имени и отчества не допускаются</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CustInfo"/>
			<xsd:element minOccurs="0" ref="TaxColl"/>
			<xsd:sequence minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Для интерфейса TDD - перевод денежных средств со счета по вкладу (первые тег DepAcctIdFrom) на счет по вкладу (если второй тег DepAcctIdTo)  или Интерфейс TDС - перевод денежных средств со счета по вкладу (первые тег DepAcctIdFrom) на счет МБК (если второй тег CardAcctIdTo). Интерфейс TCD - перевод денежных средств со счета МБК (первый тег CardAcctIdFrom) на счет по вкладу (если второй тег DepAcctIdTo) или Интерфейс TCC - перевод денежных средств со счета МБК (первый тег CardAcctIdFrom) на погашение кредита (если второй тег LoanAcctIdTo). Для интерфейса TDDO - открытие вклада с переводом на него денежных средств с другого вклада (первый тег - DepAcctIdFrom, второй тег - AgreemtInfo), для интерфейса TCDO - открытие вклада с переводом на него денежных средств с МБК (первый тег CardAcctIdFrom, второй тег - AgreemtInfo )</xsd:documentation>
				</xsd:annotation>
				<xsd:element minOccurs="0" ref="CardAcctIdFrom"/>
				<xsd:element minOccurs="0" ref="DepAcctIdFrom"/>
				<xsd:element minOccurs="0" ref="DepAcctIdTo"/>
				<xsd:element minOccurs="0" ref="CardAcctIdTo"/>
				<xsd:element minOccurs="0" ref="LoanAcctIdTo"/>
				<xsd:element minOccurs="0" ref="AgreemtInfo"/>
			</xsd:sequence>
			<xsd:element minOccurs="0" name="IdSpacing" type="String">
				<xsd:annotation>
					<xsd:documentation>Идентификатор разбивки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Purpose" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Назначение платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NonResCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код операции по счетам нерезидентов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CurAmt"/>
			<xsd:element minOccurs="0" ref="AcctCur"/>
			<xsd:element minOccurs="0" ref="CardAcctId"/>
			<xsd:element minOccurs="0" ref="DepAcctId"/>
			<xsd:element minOccurs="0" name="CurAmt1" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AcctCur1" type="AcctCur_Type">
				<xsd:annotation>
					<xsd:documentation>Валюта операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CurAmt2" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма комиссии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AcctCur2" type="AcctCur_Type">
				<xsd:annotation>
					<xsd:documentation>Валюта комиссии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DateCalc" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата, на которую получена разбивка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CurAmtConv"/>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element minOccurs="0" name="ParentAuthorizationCode" type="Long">
				<xsd:annotation>
					<xsd:documentation>Код авторизации прямой транзакции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ParentTransDtTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Время прямой транзакции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="WithClose" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак операции открытия с закрытием. На первом этапе внедрения - фиксированное значение «false»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Execute" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак проведения операции. Для операции DepToNewDepAdd и CardToNewDepAdd передается фиксированное значение «true».</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Comission" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Значение данного элемента должно совпадать со значением элемента preparedPayment_a.comission, полученного из АС ЦПФЛ. Передается для московского банка при оплате товаров и услуг </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Requisites" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Значение данного элемента должно совпадать со значением элемента preparedPayment_a.nplatRequisites.requisites, полученного из АС ЦПФЛ. Передается для московского банка при оплате товаров и услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DataSpec" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Значение данного элемента должно совпадать со значением элемента preparedPayment_a.nplatRequisites.requisites, полученного из АС ЦПФЛ. Передается для московского банка при создании длительных поручений на оплату товаров и услуг 
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="UniqueNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор организации получателя платежей (Передается в случае оплаты договорного получателя для Московского банка )</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="SpecClientCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код спецклиента (Передается в случае оплаты товаров и услуг и создания ДП для Московского банка)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CurMonth" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак платежа за текущий месяц. Передается в случае создания ДП для оплаты товаров и услуг для Московского банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CommissionLabel" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Значение данного элемента должно совпадать со значением элемента preparedPayment_a.CommissionLabel, полученного из АС ЦПФЛ.Передается в случае создания ДП для оплаты товаров и услуг для Московского банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ServiceCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Значение данного элемента должно совпадать со значением элемента preparedPayment_a.ServiceCode, полученного из АС ЦПФЛ. Передается в случае создания ДП для оплаты товаров и услуг для Московского банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ServiceKind" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код тарифа 
Значение данного элемента должно совпадать со значением элемента preparedPayment_a.tarif.tarifCode, выбранного клиентом из коллекции  тарифов  полученной из АС ЦПФЛ в узле preparedPayment_a.tarif. Передается в случае создания ДП для оплаты товаров и услуг для Московского банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Tarif" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Значение данного элемента должно совпадать со значением элемента preparedPayment_a.tarif.tarifValue, соотвествующего выбранному клиентом  preparedPayment_a.tarif.tarifCode из коллекции тарифов preparedPayment_a.tarif полученной из АС ЦПФЛ. Передается в случае создания ДП для 
оплаты товаров и услуг для Московского банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CanChangeTarif" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Признак разрешения изменения суммы платежа по тарифам. 
Значение данного элемента устанавливается клиентом, если значение узла preparedPayment_a.ackClientBankCanChangeSumm  в сообщении из АС ЦПФЛ равно true. Иначе значение элемента должно совпадать со значением элемента preparedPayment_a.ackClientBankCanChangeSumm. Передается в случае создания ДП для 
оплаты товаров и услуг для Московского банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="XferIMAInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация об операции со счетом ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element ref="DepAcctIdFrom"/>
				<xsd:element ref="CardAcctIdFrom"/>
				<xsd:element name="IMAAcctIdFrom" type="IMAAcctId_Type">
					<xsd:annotation>
						<xsd:documentation>Идентификатор счета ОМС (для операции TIC)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:choice>
				<xsd:element ref="AgreemtInfo"/>
				<xsd:element name="IMAAcctIdTo" type="IMAAcctId_Type">
					<xsd:annotation>
						<xsd:documentation>Идентификатор счета ОМС (для операции TCI)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="CardAcctIdTo"/>
			</xsd:choice>
			<xsd:element minOccurs="0" name="Purpose" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Назначение платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CurAmt">
				<xsd:annotation>
					<xsd:documentation>Сумма операции (в граммах)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AcctCur"/>
			<xsd:element ref="CurAmtConv"/>
			<xsd:element name="Execute" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак проведения операции. Для операций DepToNewIMAAdd, CardToNewIMAAdd, CardToIMAAdd, IMAToCardAdd передается фиксированное значение «true».</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="XferOperStatusInfoRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип запроса для интерфейса SrvXferOperStatusInfo. Запрос на уточнение статуса операции из ЕРИБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element name="OriginalRqTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время оригинальной операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="SPName"/>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="OperName" type="OperName_Type">
				<xsd:annotation>
					<xsd:documentation>Операция, по которой необходимо уточнить информацию в АБС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="XferOperStatusInfoRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа для интерфейса SrvXferOperStatusInfo. Запрос на уточнение статуса операции из ЕРИБ</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" name="StatusOriginalRequest" type="Status_Type">
				<xsd:annotation>
					<xsd:documentation>Результат выполнения оригинального запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice minOccurs="0">
				<xsd:element name="TDDO">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="AgreemtInfo" type="AgreemtInfoResponse_Type"/>
							<xsd:element minOccurs="0" ref="AgreemtInfoClose"/>
							<xsd:element minOccurs="0" ref="DstCurAmt"/>
							<xsd:element minOccurs="0" ref="SrcCurAmt"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="TCDO">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="CardAuthorization"/>
							<xsd:element name="AgreemtInfo" type="AgreemtInfoResponse_Type"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="TDIO">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="AgreemtInfo" type="AgreemtInfoResponse_Type"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="TCIO">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="CardAuthorization"/>
							<xsd:element name="AgreemtInfo" type="AgreemtInfoResponse_Type"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="AccStopDocRq" type="AccStopDocRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос для интерфейса ACC_STP. Приостановление операций по вкладу</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AccStopDocRs" type="AccStopDocRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ для интерфейса ACC_STP. Приостановление операций по вкладу</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AutoSubscriptionModRq" type="AutoSubscriptionModRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос для интерфейса ASM. Создание/изменение подписки</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AutoSubscriptionModRs" type="AutoSubscriptionModRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ для интерфейса ASM. Создание/изменение подписки</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AutoSubscriptionStatusModRq" type="AutoSubscriptionStatusModRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос для интерфейса ASSM. Приостановка/возобновление/закрытие подписки</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AutoSubscriptionStatusModRs" type="AutoSubscriptionStatusModRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ для интерфейса ASSM. Приостановка/возобновление/закрытие подписки</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AcctId" type="AcctIdType">
		<xsd:annotation>
			<xsd:documentation>Номер счета</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AcctBal" type="AcctBal_Type">
		<xsd:annotation>
			<xsd:documentation>Вид остатка, вид платежа</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AcctCur" type="AcctCur_Type">
		<xsd:annotation>
			<xsd:documentation>Валюта счета</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AcctInfoRq" type="AcctInfoRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос детальной информации по вкладу (ACC_DI)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AcctInfoRs" type="AcctInfoRs_Type">
		<xsd:annotation>
			<xsd:documentation>Детальная информация по вкладу (ACC_DI)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AcctInqRec" type="AcctInqRec_Type"/>
	<xsd:element name="AcctInqRq" type="AcctInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос для интерфейса IRB - Получение остатка по вкладу, по ОМС и по карте</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AcctInqRs" type="AcctInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ для интерфейса IRB - Получение остатка по вкладу, по ОМС и по карте</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AcctType" type="AcctType_Type">
		<xsd:annotation>
			<xsd:documentation>Тип счета (краткое наименование)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AdditionalCardInfo" type="AdditionalCardInfo_Type"/>
	<xsd:element name="AgencyId" type="AgencyId_Type">
		<xsd:annotation>
			<xsd:documentation>Номер отделения(ОСБ)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AgreemtInfo" type="AgreemtInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о договоре (используется при открытии)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AgreemtInfoClose" type="AgreemtInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о договоре (используется для передачи информации о нарушении условий при закрытии договора с нулевой суммой TDDC(WithClose=true))</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AutopayStatus" type="AutopayStatus_Type">
		<xsd:annotation>
			<xsd:documentation>Код состояния подписки в АС АП</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AutoSubscriptionRec" type="AutoSubscriptionRec_Type">
		<xsd:annotation>
			<xsd:documentation>Запись с информацией о подписке автоплатежа</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AgreemtNum" type="AgreemtNum_Type">
		<xsd:annotation>
			<xsd:documentation>Номер договора</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BalType" type="BalType_Type">
		<xsd:annotation>
			<xsd:documentation>Тип остатка</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BankAcctFullStmtInfo" type="BankAcctFullStmtInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о запросе полной выписки по ОМС</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BankAcctFullStmtInqRq" type="BankAcctFullStmtInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса IAS_F получения полной выписки по ОМС</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BankAcctFullStmtInqRs" type="BankAcctFullStmtInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса IAS_F получения полной выписки по ОМС</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BankAcctFullStmtRec" type="BankAcctFullStmtRec_Type">
		<xsd:annotation>
			<xsd:documentation>Запись о выписке</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BankAcctId" type="BankAcctId_Type">
		<xsd:annotation>
			<xsd:documentation>Информация по ОМС</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BankAcctInfo" type="BankAcctInfo_Type"/>
	<xsd:element name="BankAcctInqRq" type="BankAcctInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос для интерфейса GFL - Получение списка продуктов клиента</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BankAcctInqRs" type="BankAcctInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ для интерфейса GFL - Получение списка продуктов клиента</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BankAcctPermiss" type="BankAcctPermiss_Type">
		<xsd:annotation>
			<xsd:documentation>Права доступа к счету или карте (кредитному, депозитному,карточному и т.д.)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BankAcctPermissModRq" type="BankAcctPermissModRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос для интерфейса IIC  - Изменение прав доступа к счету/карте</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
