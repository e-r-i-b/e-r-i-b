					<xsd:documentation>Денежные средства</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CS_METAL">
				<xsd:annotation>
					<xsd:documentation>Слиток</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="IMSStatusEnum_Type">
		<xsd:annotation>
			<xsd:documentation>Статус счета</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:enumeration value="Opened">
				<xsd:annotation>
					<xsd:documentation>Открыт</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Closed">
				<xsd:annotation>
					<xsd:documentation>Закрыт</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Lost-passbook">
				<xsd:annotation>
					<xsd:documentation>Утеряна сберкнижка</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="IssuedBy_Type">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LastName_Type">
		<xsd:annotation>
			<xsd:documentation>Фамилия клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="160"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LastNameForPlastic_Type">
		<xsd:annotation>
			<xsd:documentation>Фамилия клиента для записи на пластиковую карту (только латинские буквы до 32 символов)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="LastName_Type">
			<xsd:maxLength value="32"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LoanStatus_Type">
		<xsd:annotation>
			<xsd:documentation>Статус кредита</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>Открыт</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation>Закрыт</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LoanType_Type">
		<xsd:annotation>
			<xsd:documentation>Краткое наименование кредита</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="128"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Long">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:restriction base="xsd:long"/>
	</xsd:simpleType>
	<xsd:simpleType name="MessageDeliveryType_Type">
		<xsd:annotation>
			<xsd:documentation>Тип доставки сообщения</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:enumeration value="E">
				<xsd:annotation>
					<xsd:documentation>e-mail</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="N">
				<xsd:annotation>
					<xsd:documentation>no e-mail/mail</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="S">
				<xsd:annotation>
					<xsd:documentation>sbrf e-mail</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="P">
				<xsd:annotation>
					<xsd:documentation>mail</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="I">
				<xsd:annotation>
					<xsd:documentation>e-mail/mail</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MiddleName_Type">
		<xsd:annotation>
			<xsd:documentation>Отчество клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="160"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OpCount_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных "Количество последних операций, которые включаются в выписку по счету". От 1 до 10 последних операций</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="Long">
			<xsd:minInclusive value="1"/>
			<xsd:maxInclusive value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OperName_Type">
		<xsd:annotation>
			<xsd:documentation>Тип операции</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TCC">
				<xsd:annotation>
					<xsd:documentation>Погашение кредита с карты</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TCD">
				<xsd:annotation>
					<xsd:documentation>Перевод с карты на вклад</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDC">
				<xsd:annotation>
					<xsd:documentation>Перевод со вклада на карту</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDD">
				<xsd:annotation>
					<xsd:documentation>Перевод со вклада на вклад</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TCP">
				<xsd:annotation>
					<xsd:documentation>Оплата товаров и услуг</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SDP">
				<xsd:annotation>
					<xsd:documentation>Создание длительного поручения на перевод денежных средств с вклада физическому лицу</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="CreateForm190ForLoanRequest">
				<xsd:annotation>
					<xsd:documentation>Создание длительного поручения на погашение кредита с вкладного счета</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDDC">
				<xsd:annotation>
					<xsd:documentation>Закрытие вклада с переводом денежных средств со счета закрываемого вклада на другой счет по вкладу</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDCC">
				<xsd:annotation>
					<xsd:documentation>Закрытие вклада с переводом денежных средств со счета закрываемого вклада на счет МБК</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDI">
				<xsd:annotation>
					<xsd:documentation>Перевод с вклада на ОМС (тоже самое что и TID, и ТDD)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TID">
				<xsd:annotation>
					<xsd:documentation>Перевод с ОМС на вклад (тоже самое что и TDI, и ТDD)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDIO">
				<xsd:annotation>
					<xsd:documentation>Открытие ОМС переводом с вклада</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TCIO">
				<xsd:annotation>
					<xsd:documentation>Открытие ОМС переводом с карты</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TCI">
				<xsd:annotation>
					<xsd:documentation>Перевод с карты на ОМС (покупка металла)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TIC">
				<xsd:annotation>
					<xsd:documentation>Перевод с ОМС на карту (продажа металла)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TDDO">
				<xsd:annotation>
					<xsd:documentation>Открытие вклада переводом с другого вклада</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TCDO">
				<xsd:annotation>
					<xsd:documentation>Открытие вклада переводом с карты</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OperUID_Type">
		<xsd:annotation>
			<xsd:documentation>Уникальный идентификатор операции</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="UUID"/>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentStatus_Type">
		<xsd:annotation>
			<xsd:documentation>Статус оплаты, 1 – не просрочен, 2 – просрочен</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentStatusASAP_Type">
		<xsd:annotation>
			<xsd:documentation>Код состояния платежа в АС AutoPay</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:enumeration value="New">
				<xsd:annotation>
					<xsd:documentation>Новый</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Canceled">
				<xsd:annotation>
					<xsd:documentation>Отменён</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Done">
				<xsd:annotation>
					<xsd:documentation>Исполнен</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PermissType_Type">
		<xsd:annotation>
			<xsd:documentation>Типы прав доступа</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:enumeration value="View">
				<xsd:annotation>
					<xsd:documentation>Просмотр (при отсутствии этого права для сущности, она не должна отображаться в системе)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PfrAccount_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для Номера СНИЛС ПФР</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:pattern value="\d{3}-\d{3}-\d{3} \d{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PmtKind_Type">
		<xsd:annotation>
			<xsd:documentation>Код вида платежа</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PostalCode_Type">
		<xsd:annotation>
			<xsd:documentation>Тип "Почтовый индекс"</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ProdType_Type">
		<xsd:annotation>
			<xsd:documentation>Идентификатор банковского продукта (ИД в ИАСК)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SummaKindCodeASAP_Type">
		<xsd:annotation>
			<xsd:documentation>Алгоритм расчета суммы очередного автоплатежа</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:enumeration value="FIXED_SUMMA_IN_RECIP_CURR">
				<xsd:annotation>
					<xsd:documentation>Фиксированная сумма</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BY_BILLING">
				<xsd:annotation>
					<xsd:documentation>По выставленному счету</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RbBrchType">
		<xsd:annotation>
			<xsd:documentation>Тип данных для 6-значного кода территориального банка. Используется для передачи кода ТБ, в котором обслуживается продукт клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:length value="6"/>
			<xsd:pattern value="[0-9]{6}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RbTbBrchType">
		<xsd:annotation>
			<xsd:documentation>Тип данных для 8-значного кода территориального банка. Используется для передачи кода ТБ, в котором открыт счет МБК, по которой клиент произвел идентификацию на УС</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:length value="8"/>
			<xsd:pattern value="[0-9]{8}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RcptKind_Type">
		<xsd:annotation>
			<xsd:documentation>Код вида получателя</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="ReasonCode_Type">
		<xsd:annotation>
			<xsd:documentation>Код причины перевыпуска карты</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:maxLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RegionId_Type">
		<xsd:annotation>
			<xsd:documentation>Код региона банка</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="99"/>
			<xsd:enumeration value="18"/>
			<xsd:enumeration value="42"/>
			<xsd:enumeration value="31"/>
			<xsd:enumeration value="70"/>
			<xsd:enumeration value="67"/>
			<xsd:enumeration value="49"/>
			<xsd:enumeration value="38"/>
			<xsd:enumeration value="54"/>
			<xsd:enumeration value="77"/>
			<xsd:enumeration value="36"/>
			<xsd:enumeration value="55"/>
			<xsd:enumeration value="60"/>
			<xsd:enumeration value="44"/>
			<xsd:enumeration value="40"/>
			<xsd:enumeration value="16"/>
			<xsd:enumeration value="13"/>
			<xsd:enumeration value="52"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RqUID_Type">
		<xsd:annotation>
			<xsd:documentation>Уникальный идентификатор запроса</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="UUID"/>
	</xsd:simpleType>
	<xsd:simpleType name="HeaderInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Контейнер для атрибутного состава заголовка</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="UUID"/>
	</xsd:simpleType>
	<xsd:simpleType name="SegmentCodeType">
		<xsd:annotation>
			<xsd:documentation>Код сегмента, к которому относят клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:enumeration value="0">
				<xsd:annotation>
					<xsd:documentation>отсутствует</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation>потенциально МВС (массовый высокодоходный сегмент)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3">
				<xsd:annotation>
					<xsd:documentation>ВИП</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="4">
				<xsd:annotation>
					<xsd:documentation>МВС (массовый высокодоходный сегмент)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ServerStatusDesc_Type">
		<xsd:annotation>
			<xsd:documentation>Исходное описание ошибки из внешней системы (заполняется только при значении StatusCode, отличном от 0.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String"/>
	</xsd:simpleType>
	<xsd:simpleType name="SPName_Type">
		<xsd:annotation>
			<xsd:documentation>Провайдер услуг(название системы, которая предоставляет услугу или запрашивает)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="BP_ES">
				<xsd:annotation>
					<xsd:documentation>Устройство самообслуживания (СмартВиста)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_VSP">
				<xsd:annotation>
					<xsd:documentation>АС ВСП</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_ERIB">
				<xsd:annotation>
					<xsd:documentation>АС ЕРИБ для Базового продукта</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_COD">
				<xsd:annotation>
					<xsd:documentation>АС ЦОД</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_ESK">
				<xsd:annotation>
					<xsd:documentation>АС ЭСК</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_IASK">
				<xsd:annotation>
					<xsd:documentation>АС ИАСК</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_WAY">
				<xsd:annotation>
					<xsd:documentation>АС WAY4U</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="BP_BS">
				<xsd:annotation>
					<xsd:documentation>АС БС</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
				<xsd:enumeration value="CRM">
				<xsd:annotation>
					<xsd:documentation>АС CRM</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ETSM_Rtl">
				<xsd:annotation>
					<xsd:documentation>Розничный контур ETSM</xsd:documentation>
				</xsd:annotation>		
			</xsd:enumeration>
			<xsd:enumeration value="urn:sbrfsystems:99-autopay"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SPNum_Type">
		<xsd:annotation>
			<xsd:documentation>Номер банкомата</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="8"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StatusCode_Type">
		<xsd:annotation>
			<xsd:documentation>Код ответа. Если все хорошо, то значение равно 0</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="Long"/>
	</xsd:simpleType>
	<xsd:simpleType name="StatusDesc_Type">
		<xsd:annotation>
			<xsd:documentation>Описание ответа - причина ошибки (заполняется только при значении StatusCode, отличном от 0.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StatusType_Type">
		<xsd:annotation>
			<xsd:documentation>Описание ответа - тип ошибки (заполняется только при значении StatusCode, отличном от 0.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="22"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StmtSummType_Type">
		<xsd:restriction base="String"/>
	</xsd:simpleType>
	<xsd:simpleType name="StmtType_Type">
		<xsd:annotation>
			<xsd:documentation>Тип выписки</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:enumeration value="Short10">
				<xsd:annotation>
					<xsd:documentation>Короткая выписка, 10 последних операций</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Full">
				<xsd:annotation>
					<xsd:documentation>Полная выписка за период</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="String">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="SystemId_Type">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Number_Type">
		<xsd:annotation>
			<xsd:documentation>Уникальный  номер заявки во внешней системе (номер/идентификатор заявки в ЕРИБ)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Source_Type">
		<xsd:annotation>
			<xsd:documentation>Уникальный  номер заявки во внешней системе (номер/идентификатор заявки в ЕРИБ)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="30"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Type_Type">
		<xsd:annotation>
			<xsd:documentation>Уникальный  номер заявки во внешней системе (номер/идентификатор заявки в ЕРИБ)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CreatedByLogin_Type">
		<xsd:annotation>
			<xsd:documentation>Логин сотрудника, оформившего заявку в ЕРИБ</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CreatedByFullName_Type">
		<xsd:annotation>
			<xsd:documentation>ФИО сотрудника, оформившего заявку ЕРИБ</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CRMDate_Type">
		<xsd:annotation>
			<xsd:documentation>Тип время для CRM</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:pattern value="([0][1-9]|[1][0-2])[/]([0][1-9]|[1-2][0-9]|[3][0-1])[/]([0-9]){4}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CRMDateTime_Type">
		<xsd:annotation>
			<xsd:documentation>Тип дата-время для CRM</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="([0][1-9]|[1][0-2])[/]([0][1-9]|[1-2][0-9]|[3][0-1])[/]([0-9]{4})\s([0-1][0-9]|[2][0-3])(:[0-5][0-9]){2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PassportNum_Type">
		<xsd:annotation>
			<xsd:documentation>Номер паспорта</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="32"/>
			<xsd:pattern value="[0-9]{4}\s[0-9]{6}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="WayCardId_Type">
		<xsd:annotation>
			<xsd:documentation>Номер карты в Way (идентификатор карты в карточной системе). Для случая,  если клиент откликнулся в канале «УС».</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="30"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CampaingMemberId_Type">
		<xsd:annotation>
			<xsd:documentation>Идентификатор участника кампании.  Заполняется в случае участия клиента в кампании.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="WorkPhone_Type">
		<xsd:annotation>
			<xsd:documentation>Рабочий телефон</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:pattern value="[+]7([0-9]){10}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CellPhone_Type">
		<xsd:annotation>
			<xsd:documentation>Мобильный телефон</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:pattern value="[+]7([0-9]){10}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AddPhone_Type">
		<xsd:annotation>
			<xsd:documentation>Дополнительный телефон</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:pattern value="[+]7([0-9]){10}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ProductName_Type">
		<xsd:annotation>
			<xsd:documentation>Наименование продукта продаваемого</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="250"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TargetProductType_Type">
		<xsd:annotation>
			<xsd:documentation>Тип продукта в продуктовой системе получателе заявки (применяется для кредитных заявок в TSM)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="250"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TargetProduct_Type">
		<xsd:annotation>
			<xsd:documentation>Продукт  в продуктовой системе получателе заявки (применяется для кредитных заявок в TSM)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="250"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TargetProductSub_Type">
		<xsd:annotation>
			<xsd:documentation>Субпродукт в продуктовой системе получателе заявки (применяется для кредитных заявок в TSM)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="250"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CreditCurrency_Type">
		<xsd:annotation>
			<xsd:documentation>Символьный ISO код валюты,   из справочника «Валюта»</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CreditAmount_Type">
		<xsd:annotation>
			<xsd:documentation>Запрашиваемая сумма кредита/лимита</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:long"/>
	</xsd:simpleType>
	<xsd:simpleType name="CreditPeriod_Type">
		<xsd:annotation>
			<xsd:documentation>Срок кредитования.  Кол-во месяцев. Целое число в интервале 1-360.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:long">
			<xsd:minInclusive value="1"/>
			<xsd:maxInclusive value="360"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CreditRate_Type">
		<xsd:annotation>
			<xsd:documentation>Процентная ставка, %.  Число с точностью до двух знаков после запятой в интервале от 0 до 100.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:fractionDigits value="2"/>
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Comments_Type">
		<xsd:annotation>
			<xsd:documentation>Комментарии</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="500"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DivisionId_Type">
		<xsd:annotation>
			<xsd:documentation>Подразделение Банка из справочника «Подразделения банка» (Код ВСП) .
 В формате АС Статотчетность, 11 цифр без пробелов: 2-код ТБ, 4-код ОСБ, 5-код ВСП.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="50"/>
			<xsd:pattern value="[0-9]{11}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StatusCRM_Type">
		<xsd:annotation>
			<xsd:documentation>Статус создания заявки во внешней системе</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StatusDescCRM_Type">
		<xsd:annotation>
			<xsd:documentation>Описание статуса (причина возникновения ошибки)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PrefferedCallTime_Type">
		<xsd:annotation>
			<xsd:documentation>1	с 09:00 до 11:00
3	с 11:00 до 13:00
4	с 13:00 до 15:00
5	с 15:00 до 17:00
6	с 17:00 до 19:00
7	с 19:00 до 20:00</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="1"/>
			<xsd:enumeration value="2"/>
			<xsd:enumeration value="3"/>
			<xsd:enumeration value="4"/>
			<xsd:enumeration value="5"/>
			<xsd:enumeration value="6"/>
			<xsd:enumeration value="7"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TarifPlanCodeType">
		<xsd:annotation>
			<xsd:documentation>Тип Код тарифного плана</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:enumeration value="0">
				<xsd:annotation>
					<xsd:documentation>отсутствует</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation>Сбербанк Премьер</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation>Сбербанк Золотой</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3">
				<xsd:annotation>
					<xsd:documentation>Сбербанк Первый</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TaxId_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных "ИНН".Элемент может содержать 10 или 12 символов</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:maxLength value="40"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="UserCategoryType">
		<xsd:annotation>
			<xsd:documentation>Тип пользователя, осуществившего изменения Employee - пользователь системы, TechnicalUser - технологический, Client - клиент</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="C">
			<xsd:enumeration value="Employee"/>
			<xsd:enumeration value="TechnicalUser"/>
			<xsd:enumeration value="Client"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="UUID">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:restriction base="String">
			<xsd:pattern value="[0-9a-zA-Z]{32}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="AccStopDocRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса ACC_STP приостановления операций по вкладу</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element minOccurs="0" ref="CustId"/>
			<xsd:element ref="DocInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AccStopDocRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса ACC_STP приостановления операций по вкладу</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AcctBal_Type">
		<xsd:annotation>
			<xsd:documentation>Остаток.Предопределенные значения для BalType: Avail -остаток по счету(доступный расходный лимит по карте), AvailCash - доступный лимит для получения наличных, AvailPmt - доступный лимит для оплаты товаров/услуг, CashAvail - макс.сумма списания без нарушения условий договора, Outstanding - сумма задолженности, Pmt - величина ближайшего платежа,MinAvail - величина неснижаемого остатка.Если BalType пустой- остаток после операции</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="BalType"/>
			<xsd:element minOccurs="0" name="BalName" type="xsd:string"/>
			<xsd:element minOccurs="0" ref="CurAmt"/>
			<xsd:element minOccurs="0" ref="AcctCur"/>
			<xsd:element minOccurs="0" name="Priority" type="xsd:long"/>
			<xsd:element minOccurs="0" name="AcctCount" type="xsd:string"/>
			<xsd:element minOccurs="0" name="EffDt" type="xsd:date"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AcctInfoRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос детальной информации по вкладу (ACC_DI)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element maxOccurs="unbounded" ref="DepAcctRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AcctInfoRs_Type">
		<xsd:annotation>
			<xsd:documentation>Детальная информация по вкладу (ACC_DI)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element minOccurs="0" ref="Status"/>
			<xsd:element maxOccurs="unbounded" ref="DetailAcctInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AcctInqRec_Type">
		<xsd:annotation>
			<xsd:documentation>Запись об остатке по счету</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:annotation>
				<xsd:documentation>Информация по счету( кредитному, депозитному, ОМС, карточному).Передается только один из тегов BankAcctId, DepAcctId,CardAcctId</xsd:documentation>
			</xsd:annotation>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="BankAcctRes"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="DepAcctRes"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="CardAcctRes"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AcctInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Получение остатка по вкладу, по ОМС или по номеру карты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:choice>
				<xsd:element ref="BankAcctId"/>
				<xsd:element ref="DepAcctId"/>
				<xsd:element ref="CardAcctId"/>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AcctInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Справка об остатках</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" ref="AcctInqRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AdditionalCardInfo_Type">
		<xsd:sequence>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="CardNum"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="Cards">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="CardNum"/>
						<xsd:element ref="SystemId"/>
						<xsd:element name="AdditionalCard" type="String">
							<xsd:annotation>
								<xsd:documentation>Тип доп. карты</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="BankInfo"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AgreemtInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Инфромация о договоре</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="AgreemtType" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип договора. При создании вклада используется фиксированное значение «Dep». При создании ОМС используется фиксированное значение "IMA"</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:element ref="DepInfo">
					<xsd:annotation>
						<xsd:documentation>Информация о депозитном договоре</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element ref="IMAInfo">
					<xsd:annotation>
						<xsd:documentation>Информация об ОМС</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AgreemtInfoResponse_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация о договоре (ответ)</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="DepInfo" type="DepInfoResponse_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о вкладе (ответ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IMAInfo" type="IMAInfoResponse_Type">
				<xsd:annotation>
					<xsd:documentation>Информация об ОМС (ответ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="AutopayDetails_Type">
		<xsd:annotation>
			<xsd:documentation>Тип для передачи Параметров подписки при подготовке или исполнении</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="3" name="AutoPayType" type="AutopayType_Type">
				<xsd:annotation>
					<xsd:documentation>Тип подписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Limit" type="String">
				<xsd:annotation>
					<xsd:documentation>Лимит (допустимое пороговое значение или список допустимых пороговых значений) для пороговых автоплатежей </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AutopayStatusList_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Список возможных состояний подписки</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" ref="AutopayStatus"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AutoPaymentId_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Идентификационная информация об автоплатеже</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="SystemId"/>
			<xsd:element name="PaymentId" type="String">
				<xsd:annotation>
					<xsd:documentation>Идентификатор платежа в АС AutoPay</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AutoPaymentInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация об автоплатеже</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PaymentStatus" type="PaymentStatusASAP_Type">
				<xsd:annotation>
					<xsd:documentation>Код состояния платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PaymentStatusDesc" type="String">
				<xsd:annotation>
					<xsd:documentation>Текстовое описание состояния</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CurAmt">
				<xsd:annotation>
					<xsd:documentation>Сумма платежа в рублях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Commission" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Комиссия платежа в рублях. Не заполняется, если платеж не был проведен</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MadeOperationId" type="String">
				<xsd:annotation>
					<xsd:documentation>Код операции в биллинге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExecStatus" type="ExecStatus_Type">
				<xsd:annotation>
					<xsd:documentation>Информация об исполнении автоплатежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AutoPaymentRec_Type">
		<xsd:annotation>
			<xsd:documentation>Запись с информацией об автоплатеже</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AutoPaymentId" type="AutoPaymentId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификационная информация об автоплатеже</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AutoPaymentInfo" type="AutoPaymentInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация об автоплатеже</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="RecipientRec"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="BankAcctRec">
				<xsd:annotation>
					<xsd:documentation>Информация о платёжном средстве, с которого производилась оплата</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CardAuthorization">
				<xsd:annotation>
					<xsd:documentation>Информация об авторизации карты в Way4, если операция прошла успешно</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AutoSubscriptionId" type="AutoSubscriptionId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификационная информация о подписке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AutoPaymentTemplate_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация о шаблоне автоплатежа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="SystemId"/>
			<xsd:element minOccurs="0" ref="RecipientRec"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="BankAcctRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AutoSubscriptionId_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Идентификационная информация о подписке.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="SystemId"/>
			<xsd:element name="AutopayId" type="Long">
				<xsd:annotation>
					<xsd:documentation>Идентификатор подписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AutoSubscriptionInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация о подписке</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="RequestId" type="String">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор заявки на создание подписки. Заполняет ЕРИБ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AutopayName" type="String">
				<xsd:annotation>
					<xsd:documentation>Наименование подписки, введенное клиентом</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ExeEventCode" type="ExeEventCodeASAP_Type">
				<xsd:annotation>
					<xsd:documentation>Код события, который определяет исполнение подписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="SummaKindCode" type="SummaKindCodeASAP_Type">
				<xsd:annotation>
					<xsd:documentation>Алгоритм расчета суммы очередного автоплатежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="AutopayStatus"/>
			<xsd:element minOccurs="0" name="AutopayStatusDesc" type="String">
				<xsd:annotation>
					<xsd:documentation>Текстовое описание состояния подписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="StartDate" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время создания подписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="UpdateDate" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время изменения подписки. Обязательно при изменении</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MaxSumWritePerMonth" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Максимальная сумма платежей по подписке в месяц. 
В случае если при исполнении очередного платежа в течение 1 месяца общая сумма платежей превышает значение этого лимита – платеж исполнен не будет</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IrreducibleAmt" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Пороговый лимит проведения платежа. Если на счете сумма меньше лимита, то производится пополнение счета. Обязателен для пороговых подписок</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CurAmt" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Фиксированная сумма платежа (всегда в рублях). Обязателен для пороговых и регулярных подписок</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NextPayDate" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата ближайшего исполнения подписки. При создании указывается дата первого платежа. Обязателен для регулярных подписок и подписок по счету</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ChangeStatus" type="ChangeStatus_Type">
				<xsd:annotation>
					<xsd:documentation>Информация об изменении состояния подписки. Передается, если было какое-то изменение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ChannelType" type="Channel_Type">
				<xsd:annotation>
					<xsd:documentation>Канал создания Подписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="SPNum"/>
			<xsd:element minOccurs="0" ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AutoSubscriptionModRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфейса ASM - cоздание/изменение подписки</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element minOccurs="0" name="EmployeeOfTheVSP" type="EmployeeOfTheVSP_Type">
				<xsd:annotation>
					<xsd:documentation>Данные сотрудника ВСП, выполнившего действие</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AutoSubscriptionRec"/>
			<xsd:element name="Execute" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>true - необходимо создать подписку и немедленно исполнить платёж,
false - необходимо только создать подписку</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MBCInfo" type="MBCInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация об услуге мобильный банк</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NeedConfirmation" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Необходимость подтверждения операции клиентом.
true - нужно подтверждение
false - не нужно подтверждение (операция уже была подтверждена клиентом)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AutoSubscriptionModRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-ответа для интерфейса ASM - cоздание/изменение подписки</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AutoSubscriptionStatusModRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфейса ASSM - приостановка/возобновление/закрытие подписки</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="AutoSubscriptionRec"/>
			<xsd:element name="ActionType" type="ChangeStatusAction_Type">
				<xsd:annotation>
					<xsd:documentation>Действие с подпиской</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ChannelType" type="Channel_Type">
				<xsd:annotation>
					<xsd:documentation>Канал изменения состояния Подписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="EmployeeOfTheVSP" type="EmployeeOfTheVSP_Type">
				<xsd:annotation>
					<xsd:documentation>Данные сотрудника ВСП, выполнившего действие</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MBCInfo" type="MBCInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация об услуге мобильный банк</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NeedConfirmation" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Необходимость подтверждения операции клиентом.
true - нужно подтверждение
false - не нужно подтверждение (операция уже была подтверждена клиентом)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AutoSubscriptionStatusModRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-ответа для интерфейса ASSM - приостановка/возобновление/закрытие подписки</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AutoSubscriptionRec_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Запись о подписке автоплатежа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="AutoSubscriptionId" type="AutoSubscriptionId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификационная информация о подписке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AutoSubscriptionInfo" type="AutoSubscriptionInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о подписке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AutoPaymentTemplate" type="AutoPaymentTemplate_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о шаблоне автоплатежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctFullStmtInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о запросе полной выписки по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FromDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата начала периода выписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ToDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата окончания периода выписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LastDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата предыдущей операции по счету</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurAmtCur" type="AcctCur_Type"/>
			<xsd:element name="StartBalance" type="IMSBalance_Type">
				<xsd:annotation>
					<xsd:documentation>Входящий остаток</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndBalance" type="IMSBalance_Type">
				<xsd:annotation>
					<xsd:documentation>Исходящий остаток</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Rate" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Учетная цена Банка России (руб/гр) на дату получения выписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctFullStmtInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса IAS_F получения полной выписки по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="BankAcctRec"/>
			<xsd:element name="FromDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата начала периода выписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ToDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата окончания периода выписки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctFullStmtInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса IAS_F получения полной выписки по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element maxOccurs="unbounded" ref="IMSAcctRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctFullStmtRec_Type">
		<xsd:annotation>
			<xsd:documentation>Запись о выписке</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="EffDate">
				<xsd:annotation>
					<xsd:documentation>Дата операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Number" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Code" type="String">
				<xsd:annotation>
					<xsd:documentation>Шифр операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentNumber" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер документа, на основании которого была выполнена операция, с указанием обоснования.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CorAcc" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер корреспондирующего счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="StmtSummType"/>
			<xsd:element name="Amt" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма операции в граммах</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AmtPhiz" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма операции в физической форме</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Balance" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Остаток в граммах</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BalancePhiz" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Остаток в физической форме</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctId_Type">
		<xsd:annotation>
			<xsd:documentation>Идентификатор ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="SystemId"/>
			<xsd:element ref="AcctId">
				<xsd:annotation>
					<xsd:documentation>Номер счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="AcctCur">
				<xsd:annotation>
					<xsd:documentation>Валюта счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AcctName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Краткое наименование счета. Краткое наименование счета из поля qsname справочника qvb ЦАС НСИ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element minOccurs="0" name="StartDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата открытия счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Дополнительные параметры для счета (договора)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element name="Ann" type="FlagAnn_Type">
					<xsd:annotation>
						<xsd:documentation>Признак аннуитентности договора</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="IsAnn" type="FlagAnn_Type">
					<xsd:annotation>
						<xsd:documentation>Признак аннуитентности договора</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element ref="StartDt"/>
			<xsd:element ref="ExpDt"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос о незаблокированных счетах, ОМС, картах и кредитах</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element minOccurs="0" ref="CustInfo"/>
			<xsd:element maxOccurs="6" minOccurs="0" ref="AcctType">
				<xsd:annotation>
					<xsd:documentation>Типы продуктов, перечень которых должен вернуться</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CardAcctId"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Получение списка продуктов</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="DepAcctRec"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="BankAcctRec"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="CardAcctRec"/>
			<xsd:element minOccurs="0" name="DepoAccounts" type="DepoAccounts_Type"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="SvcsAcct"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="LoanAcctRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctPermiss_Type">
		<xsd:annotation>
			<xsd:documentation>Тип "Права доступа к счету или карте"</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PermissType" type="PermissType_Type">
				<xsd:annotation>
					<xsd:documentation>Тип доступа (сейчас только на просмотр - значение View)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PermissValue" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак отсутствия указанного типа доступа (false - доступ есть)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SPName" type="SPName_Type">
				<xsd:annotation>
					<xsd:documentation>Код системы, для которой проставляется право. Используется два значения BP_ES (для УС),BP_ERIB (для СББОЛ)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctPermissModRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос на изменение прав доступа (видимости) потребителей к счету</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="CustInfo"/>
			<xsd:element maxOccurs="unbounded" ref="DepAcctRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctPermissModRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ на изменение прав доступа (видимости) потребителей к счету</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" name="BankAcctResult" type="BankAcctResult_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctRec_Type">
		<xsd:annotation>
			<xsd:documentation>Список депозитов,ОМС, кредитов, банковских карт, длительных поручений, услуг мобильный банк</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="BankInfo">
				<xsd:annotation>
					<xsd:documentation>Доп.информация о банке используется для DepAcctId и BankAcctId и CardAcctId</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="BankAcctId"/>
			<xsd:element minOccurs="0" ref="ImsAcctInfo"/>
			<xsd:element minOccurs="0" ref="CardAcctId"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="AcctBal"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctRes_Type">
		<xsd:sequence>
			<xsd:element ref="BankAcctId"/>
			<xsd:element ref="AcctBal"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctResult_Type">
		<xsd:annotation>
			<xsd:documentation>Результат изменения прав доступа для каждого счета в отдельности</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice>
				<xsd:element maxOccurs="unbounded" name="DepAcctRec" type="DepAcctRec_Type"/>
				<xsd:element maxOccurs="unbounded" name="CardAcctRec" type="CardAcctRec_Type"/>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctStatus_Type">
		<xsd:annotation>
			<xsd:documentation>Статус счета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="BankAcctStatusCode">
				<xsd:annotation>
					<xsd:documentation>Код статуса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="StatusDesc">
				<xsd:annotation>
					<xsd:documentation>Описание статуса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctStmtImgInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных входящего сообщения для интерфейса SCREP отправки отчета по счету карты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element ref="SelRangeDt"/>
			<xsd:element ref="CardAcctId"/>
			<xsd:element ref="ContactInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctStmtImgInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных исходящего сообщения для интерфейса SCREP отправки отчета по счету карты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctStmtInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос для интерфейса IAS - Получение выписки по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="BankAcctRec"/>
			<xsd:element ref="StmtType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctStmtInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ для интерфейса IAS - Получение выписки по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element minOccurs="0" ref="Status"/>
			<xsd:element maxOccurs="unbounded" ref="IMSAcctRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankAcctStmtRec_Type">
		<xsd:annotation>
			<xsd:documentation>Информация по выписке по ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="EffDate"/>
			<xsd:element ref="StmtSummAmt"/>
			<xsd:element ref="AcctBal">
				<xsd:annotation>
					<xsd:documentation>Тип остатков BalType для ОМС: BalanceAfterOperation</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IMAOperConvInfo" type="IMAOperConvInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о конверсии при операциях с ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о банке &lt;BankInfo&gt;</xsd:documentation>
			<xsd:documentation>Этот агрегат содержит дополнительную информацию для идентификации банка.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="BranchId">
				<xsd:annotation>
					<xsd:documentation>Номер филиала.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="AgencyId">
				<xsd:annotation>
					<xsd:documentation>Номер отделения.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="RegionId">
				<xsd:annotation>
					<xsd:documentation>Номер террбанка.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="RbTbBrchId">
				<xsd:annotation>
					<xsd:documentation>8-ный код территориального банка, в котором открыт счет МБК, по которой клиент произвел идентификацию на УС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="RbBrchId">
				<xsd:annotation>
					<xsd:documentation>6-ный код территориального банка, в котором ведется продукт клиента (кредит, вклад, ОМС, карты)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankInfoMod1_Type">
		<xsd:annotation>
			<xsd:documentation>Реквизиты банков. Тип используется для идентификации банка</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RegionId">
				<xsd:annotation>
					<xsd:documentation>Код тербанка = 
'99'</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="C">
						<xsd:maxLength value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="AgencyId">
				<xsd:annotation>
					<xsd:documentation>Код отделения</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="C">
						<xsd:maxLength value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="BranchId">
				<xsd:annotation>
					<xsd:documentation>Код филиала банка</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="BranchId_Type">
						<xsd:maxLength value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BankInfoLeadZero_Type">
		<xsd:annotation>
			<xsd:documentation>Реквизиты банков. Тип используется для идентификации банка</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RegionId">
				<xsd:annotation>
					<xsd:documentation>Код тербанка</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="C">
						<xsd:maxLength value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="AgencyId">
				<xsd:annotation>
					<xsd:documentation>Код отделения</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="C">
						<xsd:maxLength value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="BranchId">
				<xsd:annotation>
					<xsd:documentation>Код филиала банка</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="BranchId_Type">
						<xsd:maxLength value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MinBankInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о банке &lt;BankInfo&gt;. Этот агрегат используется для передачи только ОСБ и ВСП, ТБ (опционально)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="BranchId">
				<xsd:annotation>
					<xsd:documentation>Номер филиала</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AgencyId">
				<xsd:annotation>
					<xsd:documentation>Номер отделения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="RegionId">
				<xsd:annotation>
					<xsd:documentation>Номер террбанка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BillingPayExecRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса TBP_PAY проводки билингового платежа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element name="SBOLUID" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Идентификатор операции в СБОЛ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SBOLTm" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время регистрации операции в СБОЛ</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="CardAcctId"/>
			<xsd:element ref="RecipientRec"/>
			<xsd:element name="Commission" type="xsd:decimal"/>
			<xsd:element name="CommissionCur" type="AcctCur_Type"/>
			<xsd:element name="MadeOperationId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Код операции в биллинге</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AuthorizationCode" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Код авторизации транзакции WAY4 для повторного запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AuthorizationDtTm" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>Время авторизации транзакции WAY4 для повторного запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BillingPayExecRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса TBP_PAY проводки билингового платежа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" name="AuthorizationCode" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Код авторизации операции пополнения, если операция прошла успешно.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AuthorizationDtTm" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>Время авторизации запроса в Way4</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BillingPayInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса TBP_RI поиска информации о биллинговом получателе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element minOccurs="0" name="IsAutoPayment" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак «Запрос на подготовку подписки на автоплатёж»</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="RecipientRec"/>
			<xsd:element minOccurs="0" name="IsTemplate" type="xsd:boolean"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BillingPayInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса TBP_RI поиска информации о биллинговом получателе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element ref="SystemId"/>
			<xsd:element name="RecipientRec" type="RecipientRec_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BillingPayPrepRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос интерфейса TBP_PR подготовки билингового платежа к отправке</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element minOccurs="0" ref="CardAcctId"/>
			<xsd:element ref="SystemId"/>
			<xsd:element minOccurs="0" name="IsTemplate" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак Запрос выполняется для подготовки шаблона платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsAutoPayment" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак Запрос на подготовку подписки на автоплатеж</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="RecipientRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BillingPayPrepRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ интерфейса TBP_PR подготовки билингового платежа к отправке</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" ref="SystemId"/>
			<xsd:element minOccurs="0" ref="RecipientRec"/>
			<xsd:element minOccurs="0" name="WithCommision" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак наличия комиссии при осуществлении платежей по подписке на автоплатеж в соответствии с тарифами банка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Commission" type="xsd:decimal"/>
			<xsd:element minOccurs="0" name="CommissionCur" type="AcctCur_Type"/>
			<xsd:element minOccurs="0" name="MadeOperationId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер операции в БС ТБ. Наличие данного поля и поля Commission в документе является признаком того, что переданных ранее реквизитов достаточно для исполнения операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardAccount_Type">
		<xsd:annotation>
			<xsd:documentation>Номер картсчета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="SystemId"/>
			<xsd:element ref="AcctId"/>
			<xsd:element ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardAcctDepoInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Дополнительная информация по карточному договору (ДЕПО)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CardName" type="CardName_Type">
				<xsd:annotation>
					<xsd:documentation>Наименование карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AcctCur" type="AcctCur_Type">
				<xsd:annotation>
					<xsd:documentation>Валюиа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndDt" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Срок действия карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Status" type="AccountStatusEnum_Type">
				<xsd:annotation>
					<xsd:documentation>Статус карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardAcctDInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных входящего сообщения интерфейса CRDWI получение детализированной информации о карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element maxOccurs="unbounded" ref="CardInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardAcctDInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных исходящего сообщения интерфейса CRDWI получение детализированной информации о карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element minOccurs="0" ref="Status"/>
			<xsd:element maxOccurs="unbounded" name="BankAcctRec" type="CardAcctRec_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardAcctId_Type">
		<xsd:annotation>
			<xsd:documentation>Идентификатор карточного счета &lt;CardAcctId&gt; &lt;CardAcctIdFrom&gt; &lt;CardAcctIdTo&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="SystemId"/>
			<xsd:element minOccurs="0" ref="SPName"/>
			<xsd:element minOccurs="0" ref="CardNum"/>
			<xsd:element minOccurs="0" ref="AcctId">
				<xsd:annotation>
					<xsd:documentation>Номер карточного счета для карт (из ЦОД)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AcctCode" type="Long">
				<xsd:annotation>
					<xsd:documentation>Тип счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AcctSubCode" type="Long">
				<xsd:annotation>
					<xsd:documentation>Подтип счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CardType" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип пластиковой карты. CC- кредитная, DC - дебетовая, OV - с овердрафтом. Для АС Филиала используется для услуги МБК (0 - основная или 1- активная)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CardName"/>
			<xsd:element minOccurs="0" ref="AcctCur"/>
			<xsd:element minOccurs="0" ref="EndDt"/>
			<xsd:element minOccurs="0" name="PmtDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата платежа ( для кредитных карт)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IssDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата выпуска карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CardHolder" type="String">
				<xsd:annotation>
					<xsd:documentation>Имя на карте</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="UNICardType" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="UNIAcctType" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CardAccount">
				<xsd:annotation>
					<xsd:documentation>Номер картсчета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CustInfo"/>
			<xsd:element minOccurs="0" ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardAcctInfo_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="AdditionalCard" type="String">
				<xsd:annotation>
					<xsd:documentation>Тип дополнительной карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MainCard" type="CardNumType">
				<xsd:annotation>
					<xsd:documentation>Номер основной карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="NextReportDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата очередного отчета по счету карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="EndDtForWay" type="String">
				<xsd:annotation>
					<xsd:documentation>Срок действия карты в формате процессинга YYMM</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardAcctRec_Type">
		<xsd:annotation>
			<xsd:documentation>Информация по счету</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element minOccurs="0" ref="BankAcctStatus"/>
			<xsd:element minOccurs="0" ref="CardAcctId"/>
			<xsd:element minOccurs="0" ref="CardAcctInfo"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="AcctBal"/>
			<xsd:element minOccurs="0" ref="Status">
				<xsd:annotation>
					<xsd:documentation>Статус обработки запроса для данной карты (возвращается при групповых операциях по картам, которые не поддерживаются бек-офисными системами)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="OTPRestriction"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="UsageInfo" type="UsageInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о транзакционных ограничителях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardAcctRes_Type">
		<xsd:sequence>
			<xsd:element ref="CardAcctId"/>
			<xsd:element maxOccurs="unbounded" ref="AcctBal"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardAdditionalInfoRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос получения списка доп. карт к карте клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element maxOccurs="unbounded" ref="CardInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardAdditionalInfoRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ получения списка доп. карт к карте клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element maxOccurs="unbounded" ref="AdditionalCardInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardBlockRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных входящего сообщения для интерфейса CRBLOCK блокировка карты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element minOccurs="0" ref="CustId"/>
			<xsd:element ref="CardAcctId"/>
			<xsd:element name="BlockReason" type="BlockReasonType">
				<xsd:annotation>
					<xsd:documentation>Причина блокировки карты LOST – карта потеряна, STOLEN – карта украдена, HOLDER – по инициативе держателя, ATM – изъята банкоматом.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardBlockRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных для исходящего сообщения для интерфейса CRBLOCK блокировка карты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardInfo_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element ref="CardAcctId"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardAuthorization_Type">
		<xsd:annotation>
			<xsd:documentation>Тип информация об авторизации карты</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AuthorizationCode" type="Long">
				<xsd:annotation>
					<xsd:documentation>Код авторизации, если транзакция прошла успешно</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AuthorizationDtTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время транзакции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardReissuePlaceRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип запроса для перевыпуска карт (в т.ч. не по месту ведения счета)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RqUID">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор запроса</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="RqUID_Type">
						<xsd:maxLength value="36"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="RqTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время передачи сообщения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SPName">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы, инициирующей запрос - 
'BP_ERIB', 'BP_SOFIA', 'BP_XBANK', 'BP_ASFS'</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="SPName_Type">
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="SystemId" type="SystemId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы назначения запроса </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankInfo" type="BankInfoMod1_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о банке, куда посылаем запрос</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CardNum" type="CardNumType">
				<xsd:annotation>
					<xsd:documentation>Номер пластиковой карты.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CardType">
				<xsd:annotation>
					<xsd:documentation>Тип пластиковой карты</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="16"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Destination" type="BankInfoLeadZero_Type">
				<xsd:annotation>
					<xsd:documentation>Код отделения доставки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Source" type="SourceId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор канала обслуживания </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReasonCode" type="ReasonCode_Type">
				<xsd:annotation>
					<xsd:documentation>Код причины перевыпуска карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Commission" type="Commission_Type">
				<xsd:annotation>
					<xsd:documentation>Признак взимания комиссии за перевыпуск карты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardReissuePlaceRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа для перевыпуска карт (в т.ч. не по месту ведения счета)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RqUID">
				<xsd:annotation>
					<xsd:documentation>Уникальный идентификатор запроса</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="RqUID_Type">
						<xsd:maxLength value="36"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="RqTm" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время передачи сообщения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="SPName">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы, инициировавший запрос</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="SPName_Type">
						<xsd:maxLength value="50"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element minOccurs="0" name="SystemId" type="SystemId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы назначения запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Status" type="Status_Type">
				<xsd:annotation>
					<xsd:documentation>Статус операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardToIMAAddRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип запроса для интерфейса SrvCardToIMAAdd (TCI). Покупка обезличенного металла за счет денежных средств на МБК того же клиента. Операция Карта-ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="XferInfo" type="XferIMAInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о реквизитах совершаемой операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardToIMAAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа для интерфейса SrvCardToIMAAdd (TCI). Покупка обезличенного металла за счет денежных средств на МБК того же клиента. Операция Карта-ОМС</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardToNewDepAddRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип запроса для интерфейса TCDO. Открытие вклада с переводом на него денежных средств с МБК</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="XferInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardToNewDepAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа для нтерфейса TCDO. Открытие вклада с переводом на него денежных средств с МБК</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" ref="CardAuthorization"/>
			<xsd:element minOccurs="0" ref="AgreemtInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardToNewIMAAddRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип запроса для интерфейса SrvCardToNewIMAAdd (TCIO). Открытие ОМС с переводом на него денежных средств с МБК того же клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="XferInfo" type="XferIMAInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о реквизитах совершаемой операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardToNewIMAAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа для интерфейса SrvCardToNewIMAAdd (TCIO). Открытие ОМС с переводом на него денежных средств с МБК того же клиента</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" ref="CardAuthorization"/>
			<xsd:element minOccurs="0" name="AgreemtInfo" type="AgreemtInfoResponse_Type">
				<xsd:annotation>
					<xsd:documentation>Информация об открытом счете ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardUsageInfoLimitModRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфейса CUILM - изменение лимитов транзакционных ограничителей по карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element maxOccurs="unbounded" ref="CardAcctRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CardUsageInfoLimitModRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-ответа для интерфейса CUILM - изменение лимитов транзакционных ограничителей по карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" name="BankAcctResult" type="BankAcctResult_Type">
				<xsd:annotation>
					<xsd:documentation>Результат изменения для каждой карты в отдельности</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CCAcctExtStmtInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных входящего сообщения для интерфейса CREXT получения расширенной выписки по карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element maxOccurs="unbounded" ref="CardInfo"/>
			<xsd:element name="OpCount" type="OpCount_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CCAcctExtStmtInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип данных ответного сообщения для интерфейса CREXT получения расширенной выписки по карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element minOccurs="0" ref="Status"/>
			<xsd:element maxOccurs="unbounded" name="CardAcctRec">
				<xsd:annotation>
					<xsd:documentation>Возвращаемый массив данных по каждой карте</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="CardAcctId"/>
						<xsd:element maxOccurs="2" minOccurs="0" ref="AcctBal"/>
						<xsd:element maxOccurs="10" minOccurs="0" ref="CCAcctStmtRec"/>
						<xsd:element ref="Status"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CCAcctFullStmtInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Запрос получения полной выписки по карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element maxOccurs="unbounded" ref="CardInfo"/>
			<xsd:element name="FromDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата начала периода выписки.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ToDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата окончания периода выписки.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CCAcctFullStmtInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Ответ получения полной выписки по карте</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element maxOccurs="unbounded" name="CardAcctRec">
				<xsd:annotation>
					<xsd:documentation>Возвращаемый массив данных по каждой карте</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="FromDate" type="Date">
							<xsd:annotation>
								<xsd:documentation>Дата начала периода выписки.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ToDate" type="Date">
							<xsd:annotation>
								<xsd:documentation>Дата окончания периода выписки.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element ref="CardAcctId"/>
						<xsd:element maxOccurs="2" minOccurs="0" ref="AcctBal"/>
						<xsd:element minOccurs="0" name="StartBalance" type="AcctBal_Type">
							<xsd:annotation>
								<xsd:documentation>Входящий остаток по счету карты клиента на дату начала периода выписки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element minOccurs="0" name="EndBalance" type="AcctBal_Type">
							<xsd:annotation>
								<xsd:documentation>Исходящий остаток по счету карты на дату окончания периода выписки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element maxOccurs="10" minOccurs="0" ref="CCAcctStmtRec"/>
						<xsd:element ref="Status"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CCAcctStmtRec_Type">
		<xsd:sequence>
			<xsd:element name="EffDate" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DiscDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата списания или зачисления</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Number" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Code" type="String">
				<xsd:annotation>
					<xsd:documentation>Шифр операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CorAccountNumber" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер корреспондирующего счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="StmtSummAmt"/>
			<xsd:choice>
				<xsd:element name="IsDebit" type="xsd:boolean">
					<xsd:annotation>
						<xsd:documentation>True- операция зачисления, False – операция списания</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="TrnType" type="xsd:boolean">
					<xsd:annotation>
						<xsd:documentation>Признак, что операция списания со счета карты. True- операция зачисления, False – операция списания</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element minOccurs="0" name="TrnDest" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование операции. Готовое для отображения пользователю.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TrnSrc" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Информация о торговой точке, совершившей операцию</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TrnDesc" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Описание транзакции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OrigCurAmt">
				<xsd:annotation>
					<xsd:documentation>Сумма в валюте счета карты</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="CurAmt"/>
						<xsd:element ref="AcctCur"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element minOccurs="0" name="OperationAmt">
				<xsd:annotation>
					<xsd:documentation>Сумма в валюте операции</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="CurAmt"/>
						<xsd:element ref="AcctCur"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element minOccurs="0" name="RemaindAmt">
				<xsd:annotation>
					<xsd:documentation>Остаток на счете после выполнения операции</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="CurAmt"/>
						<xsd:element ref="AcctCur"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ContactData_Type">
		<xsd:sequence>
			<xsd:element name="ContactType" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Тип контактной информации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ContactNum" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Значение контактной информации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="PhoneOperName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Наименование мобильного оператора</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ChangeStatus_Type">
		<xsd:annotation>
			<xsd:documentation>Тип Информация об изменении состояния подписки</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ReasonCode" type="String">
				<xsd:annotation>
					<xsd:documentation>Код причины нахождения подписки в данном состоянии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReasonDesc" type="String">
				<xsd:annotation>
					<xsd:documentation>Текстовое описание причины нахождения подписки в данном состоянии</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="ChangeDate" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время изменения статуса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ContactInfo_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="EmailAddr" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Адрес электронной почты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MessageDeliveryType" type="MessageDeliveryType_Type">
				<xsd:annotation>
					<xsd:documentation>Тип доставки сообщений</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="6" minOccurs="0" ref="ContactData">
				<xsd:annotation>
					<xsd:documentation>Блок нетипизированных контактных данных</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="5" minOccurs="0" name="PostAddr" type="FullAddress_Type">
				<xsd:annotation>
					<xsd:documentation>Полный адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CorrOwnerDetail_Type">
		<xsd:annotation>
			<xsd:documentation>Детальная информации о владельце счета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="PersonName"/>
			<xsd:element ref="IdentityCard"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CurAmtConv_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="debet_sale" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Курс продажи банком клиенту для валюты списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="debet_buy" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Курс покупки банком у клиента для валюты списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="debet_cb" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Курс ЦБ РФ для валюты списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="credit_sale" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Курс продажи банком клиенту для валюты зачисления</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="credit_buy" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Курс покупки банком у клиента для валюты зачисления</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="credit_cb" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Курс ЦБ РФ для валюты зачисления</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CustId_Type">
		<xsd:annotation>
			<xsd:documentation>Идентификация потребителя &lt;CustId&gt;</xsd:documentation>
			<xsd:documentation>Агрегат &lt;CustId&gt; используется для уникальной идентификации потребителя, пославшего запрос. Элемент &lt;SPName&gt; указывает на ППУ, который присвоил потребителю постоянный идентификатор.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="SPName">
				<xsd:annotation>
					<xsd:documentation>Название ПУ, присвоившего &lt;CustPermId&gt;.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CustPermId">
				<xsd:annotation>
					<xsd:documentation>Постоянный идентификатор потребителя. Используется как ключ БД для уникальной идентификации потребителя. Не может быть изменен потребителем.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CustInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация о потребителе &lt;CustInfo&gt;</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="CustId" type="EribCustId_Type"/>
			<xsd:element minOccurs="0" name="EffDt" type="DateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время последнего изменения данных</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="PersonInfo">
				<xsd:annotation>
					<xsd:documentation>Информация о физическом лице.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element minOccurs="0" name="SPDefField" type="SPDefField_Type">
				<xsd:annotation>
					<xsd:documentation>Поле, опеределяющее  заключение клиентом заявления на подключение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IntegrationInfo" type="IntegrationInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификаторы клиента в ИС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CustInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфейса CEDBO - Проверка заключения УДБО клиентом</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element minOccurs="0" ref="OperUID"/>
			<xsd:element name="SPName" type="SPName_Type"/>
			<xsd:element name="BankInfo" type="BankInfo_Type"/>
			<xsd:sequence>
				<xsd:element minOccurs="0" name="CustInfo" type="CustInfo_Type"/>
				<xsd:element minOccurs="0" ref="CardAcctId"/>
			</xsd:sequence>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CustInqRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответного сообщения для интерфейса CEDBO - Проверка заключения УДБО клиентом</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" ref="CustRec"/>
			<xsd:element ref="SPDefField"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CustRec_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="CustId" type="EribCustId_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор потребителя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="CustInfo"/>
			<xsd:element minOccurs="0" ref="ServiceInfo"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="BnkAccRub" type="DepoBankAccount_Type">
				<xsd:annotation>
					<xsd:documentation>Информации о рублевом счете</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" name="BnkAccCur" type="DepoBankAccountCur_Type">
				<xsd:annotation>
					<xsd:documentation>Информации о валютном счете</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AdditionalInfo" type="DepoBankAccountAdditionalInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Доп. информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="StartDate" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата заполнения.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TaxIdFrom" type="TaxId_Type">
				<xsd:annotation>
					<xsd:documentation>ИНН плательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="TarifPlanInfo" type="TarifPlanInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о тарифном плане и сегменте</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CustStatus_Type">
		<xsd:annotation>
			<xsd:documentation>Состояние потребителя</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="SPName" type="SPName_Type">
				<xsd:annotation>
					<xsd:documentation>Идентификатор системы (может использоваться опционально)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CustStatusCode" type="StatusCode_Type">
				<xsd:annotation>
					<xsd:documentation>Код статуса потребителя в системе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="StatusDesc" type="StatusDesc_Type">
				<xsd:annotation>
					<xsd:documentation>Описание статуса потребителя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepAccInfo_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="AcctCur"/>
			<xsd:element minOccurs="0" name="AcctName" type="xsd:string"/>
			<xsd:element minOccurs="0" name="AcctCode" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Тип счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AcctSubCode" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Подтип счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CurAmt"/>
			<xsd:element minOccurs="0" name="MaxSumWrite" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Максимальная сумма списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="IrreducibleAmt"/>
			<xsd:element minOccurs="0" name="InterestOnDeposit" type="InterestOnDeposit_Type">
				<xsd:annotation>
					<xsd:documentation>Информация, предназначенная для перечисления процентов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Period" type="Long">
				<xsd:annotation>
					<xsd:documentation>Срок вклада в днях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Rate" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Процентная ставка по вкладу, для текущих счетов не возвращается.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="OpenDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата открытия счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="EndDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата закрытия счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Status" type="AccountStatusEnum_Type">
				<xsd:annotation>
					<xsd:documentation>Статус счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsCreditAllowed" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Разрешено ли списание со счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsDebitAllowed" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Разрешено ли зачисление на счет</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsProlongationAllowed" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Разрешена ли пролонгация на следующий срок</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsCreditCrossAgencyAllowed" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Разрешено ли списание со счета в других ОСБ ( признак «Зеленая улица»)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsDebitCrossAgencyAllowed" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Разрешено ли зачисление на счет в других ОСБ (признак «Зеленая улица»)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsPassBook" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак наличия сберкнижки. True – есть, False – нет.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element minOccurs="0" ref="CustRec"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepAcct_Type">
		<xsd:sequence>
			<xsd:element ref="DepAcctId"/>
			<xsd:element minOccurs="0" ref="DepAcctStmtGen"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="DepAcctStmtRec"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepAcctExtRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответного сообщения для интерфейса DAS - Запрос выписки по вкладному счету</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element minOccurs="0" ref="Status"/>
			<xsd:element maxOccurs="unbounded" ref="DepAcct"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepAcctId_Type">
		<xsd:annotation>
			<xsd:documentation>Идентификатор депозитного счета</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="SystemId"/>
			<xsd:element ref="AcctId"/>
			<xsd:element minOccurs="0" name="BIC" type="String">
				<xsd:annotation>
					<xsd:documentation>БИК</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="CorrAcctId" type="AcctIdType">
				<xsd:annotation>
					<xsd:documentation>Корсчет</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="AcctCur"/>
			<xsd:element minOccurs="0" name="AcctName" type="xsd:string"/>
			<xsd:element minOccurs="0" name="AcctCode" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Тип счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AcctSubCode" type="xsd:long">
				<xsd:annotation>
					<xsd:documentation>Подтип счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="MaxSumWrite" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Максимальная сумма списания</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="OpenDate" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата открытия счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Status" type="AccountStatusEnum_Type">
				<xsd:annotation>
					<xsd:documentation>Статус счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="CustInfo"/>
			<xsd:element minOccurs="0" ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepAcctInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация по счету</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="AcctCur"/>
			<xsd:element name="Status" type="LoanStatus_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepAcctRec_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="BankInfo"/>
			<xsd:element ref="DepAcctId"/>
			<xsd:element minOccurs="0" ref="DepAccInfo"/>
			<xsd:element maxOccurs="2" minOccurs="0" ref="BankAcctPermiss"/>
			<xsd:element maxOccurs="3" minOccurs="0" ref="AcctBal"/>
			<xsd:element minOccurs="0" ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepAcctRes_Type">
		<xsd:sequence>
			<xsd:element ref="DepAcctId"/>
			<xsd:element ref="AcctBal"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepAcctStmtGen_Type">
		<xsd:sequence>
			<xsd:element minOccurs="0" name="DateFrom" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата с которой необходимо получать выписку, включая эту дату</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DateTo" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата по которую необходимо получать выписку, включая эту дату</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BalanceFrom" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Входящий остаток (остаток на дату «С»)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BalanceFromCur" type="AcctCur_Type">
				<xsd:annotation>
					<xsd:documentation>Валюта входящего остатка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BalanceTo" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Исходящий остаток (остаток на дату «ПО»)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="BalanceToCur" type="AcctCur_Type">
				<xsd:annotation>
					<xsd:documentation>Валюта исходящего остатка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepAcctStmtInqRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип сообщения-запроса для интерфейса DAS - Запрос выписки по вкладному счету</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="DepAcctRec">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element ref="DepAcctId"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element ref="StmtType"/>
			<xsd:element minOccurs="0" name="DateFrom" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата с которой необходимо получать выписку, включая эту дату</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DateTo" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата по которую необходимо получать выписку, включая эту дату</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepAcctStmtRec_Type">
		<xsd:annotation>
			<xsd:documentation>Запись о выписке</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="EffDate"/>
			<xsd:element minOccurs="0" name="DocumentNumber" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Name" type="String">
				<xsd:annotation>
					<xsd:documentation>Наименование операции. На русском языке, готовое для показал пользователю.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Aspect" type="String">
				<xsd:annotation>
					<xsd:documentation>Вид операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="IsDebit" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Расходная операция. True – операция зачисления, False – операция списания.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Amount" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AmountCur" type="AcctCur_Type">
				<xsd:annotation>
					<xsd:documentation>Валюта суммы операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Correspondent" type="String">
				<xsd:annotation>
					<xsd:documentation>Корреспондирующий счет</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Balance" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Остаток после операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BalanceCur" type="AcctCur_Type">
				<xsd:annotation>
					<xsd:documentation>Валюта остатка после операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Recipient" type="String">
				<xsd:annotation>
					<xsd:documentation>Текстовое наименование получателя или плательщика. Текст на русском языке, готовый для отобображения пользователю.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RecipientAccount" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер счета получателя или плательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="RecipientBIC" type="String">
				<xsd:annotation>
					<xsd:documentation>БИК банка получателя или плательщика</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="Destination" type="String">
				<xsd:annotation>
					<xsd:documentation>Назначение платежа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Тип информация о депозитном договоре</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:choice minOccurs="0">
				<xsd:element name="MatDt" type="Date">
					<xsd:annotation>
						<xsd:documentation>Дата окончания срока вклада</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Term" type="Long">
					<xsd:annotation>
						<xsd:documentation>Срок вклада в днях</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element minOccurs="0" ref="IrreducibleAmt"/>
			<xsd:element minOccurs="0" ref="CustRec"/>
			<xsd:element minOccurs="0" ref="AcctId">
				<xsd:annotation>
					<xsd:documentation>Номер вкладного счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="AcctCur"/>
			<xsd:element minOccurs="0" name="AcctCode" type="Long">
				<xsd:annotation>
					<xsd:documentation>Вид вклада</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="AcctSubCode" type="Long">
				<xsd:annotation>
					<xsd:documentation>Подвид вклада</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DaysToEndOfSaving" type="Long">
				<xsd:annotation>
					<xsd:documentation>Кол-во дней до окончания хранения закрываемого вклада. Значение обязательно заполняется для операций с закрытием вкладов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="DateToEndOfSaving" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата окончания хранения закрываемого вклада.  Значение обязательно заполняется для операций с закрытием вкладов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="EarlyTermRate" type="IntRate_Type">
				<xsd:annotation>
					<xsd:documentation>Процентная ставка при досрочном погашении (IntRateType = Fixed).  Значение обязательно заполняется для операций с закрытием вкладов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="HaveForm190" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Признак наличия длительного поручения.  Значение обязательно заполняется для операций с закрытием вкладов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepInfoResponse_Type">
		<xsd:annotation>
			<xsd:documentation>Тип информация о депозитном договоре (ответ)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="AcctId">
				<xsd:annotation>
					<xsd:documentation>Номер вкладного счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepToNewDepAddRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип запроса для интерфейса TDDO и TDO. Открытие вклада с переводом на него денежных средств с другого вклада и открытие бессрочного вклада без первоначального взноса</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="XferInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepToNewDepAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа для интерфейса TDDO и TDO. Открытие вклада с переводом на него денежных средств с другого вклада (возможно с закрытием этого вклада) и открытие бессрочного вклада без первоначального взноса</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" ref="AgreemtInfo"/>
			<xsd:element minOccurs="0" ref="AgreemtInfoClose"/>
			<xsd:element minOccurs="0" ref="DstCurAmt"/>
			<xsd:element minOccurs="0" ref="SrcCurAmt"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepToNewIMAAddRq_Type">
		<xsd:annotation>
			<xsd:documentation>Тип запроса для интерфейса TDIO. Открытие ОМС с переводом на него денежных средств со вклада</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element name="XferInfo" type="XferIMAInfo_Type">
				<xsd:annotation>
					<xsd:documentation>Информация о реквизитах совершаемой операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepToNewIMAAddRs_Type">
		<xsd:annotation>
			<xsd:documentation>Тип ответа для интерфейса TDIO. Открытие ОМС с переводом на него денежных средств со вклада</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" name="AgreemtInfo" type="AgreemtInfoResponse_Type">
				<xsd:annotation>
					<xsd:documentation>Информация об открытом счете ОМС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAccInfoRqType">
		<xsd:annotation>
			<xsd:documentation>Получение информации по счетам ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element maxOccurs="unbounded" ref="DepoAcctId"/>
			<xsd:element minOccurs="0" name="OperInfo" type="OperInfo_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAccInfoRsType">
		<xsd:annotation>
			<xsd:documentation>Получение информации по счетам ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element minOccurs="0" ref="Status"/>
			<xsd:element name="DepoAcctInfoRec">
				<xsd:complexType>
					<xsd:annotation>
						<xsd:documentation>Результат выполнения запроса. Список записей о счете депо</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element maxOccurs="unbounded" name="DepoAcctRes" type="DepoAcctRes_Type"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAccount_Type">
		<xsd:annotation>
			<xsd:documentation>Информация по счету</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="BankInfo"/>
			<xsd:element ref="DepoAcctId"/>
			<xsd:element minOccurs="0" name="DepoAccInfo" type="DepAcctInfo_Type"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAccounts_Type">
		<xsd:annotation>
			<xsd:documentation>Список счетов депо.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="Status"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="DepoAccount"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAccSecInfoRsType">
		<xsd:annotation>
			<xsd:documentation>Получение информации о содержании счета ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element minOccurs="0" ref="Status"/>
			<xsd:element name="DepoAccSecInfoRec">
				<xsd:complexType>
					<xsd:annotation>
						<xsd:documentation>Счета депо</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element ref="DepoAcctId"/>
						<xsd:element maxOccurs="unbounded" minOccurs="0" name="DepoRec" type="DepoSecurityRec_Type"/>
						<xsd:element ref="Status"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAccSecRegRqType">
		<xsd:annotation>
			<xsd:documentation>Подача заявки на регистрацию новой ценной бумаги ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="DepoAccInfoRqType">
				<xsd:sequence>
					<xsd:element name="OperationInfo" type="DepoSecurityOperationInfo_Type"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DepoAccSecRegRsType">
		<xsd:annotation>
			<xsd:documentation>Подача заявки на регистрацию новой ценной бумаги ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" name="DocNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер сформированного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAcctBalRec_Type">
		<xsd:annotation>
			<xsd:documentation>Разбивка задолженности ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RecNumber" type="String">
				<xsd:annotation>
					<xsd:documentation>Номер выставленного счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EffDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Дата выставления счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurAmt" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма без ндс</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurAmtNDS" type="Decimal">
				<xsd:annotation>
					<xsd:documentation>Сумма ндс</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="AcctCur"/>
			<xsd:element name="StrDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Начало периода, за который выставлен счет</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndDt" type="Date">
				<xsd:annotation>
					<xsd:documentation>Конец периода, за который выставлен счет</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAcctId_Type">
		<xsd:annotation>
			<xsd:documentation>Данные по счету депо</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" ref="SystemId"/>
			<xsd:element name="AcctId" type="xsd:string"/>
			<xsd:element minOccurs="0" ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAcctInfo_Type">
		<xsd:annotation>
			<xsd:documentation>Информация по счету депо</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element minOccurs="0" name="Status" type="String">
				<xsd:annotation>
					<xsd:documentation>Статус счета депо: 1 – открыт, 2 – закрыт</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" name="isOperationAllowed" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Разрешены ли операции со счетом</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="AgreemtNum"/>
			<xsd:element minOccurs="0" ref="StartDt"/>
			<xsd:element ref="BankInfo"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAccTranRqType">
		<xsd:annotation>
			<xsd:documentation>Поручения на перевод/прием перевода ценных бумаг ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="SPName"/>
			<xsd:element ref="BankInfo"/>
			<xsd:element minOccurs="0" ref="CustId"/>
			<xsd:element ref="DepoAcctId"/>
			<xsd:element name="TransferInfo" type="TransferInfo_Type"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAccTranRsType">
		<xsd:annotation>
			<xsd:documentation>Поручения на перевод/прием перевода ценных бумаг ДЕПО</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="RqUID"/>
			<xsd:element ref="RqTm"/>
			<xsd:element ref="OperUID"/>
			<xsd:element ref="Status"/>
			<xsd:element minOccurs="0" name="DocNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер сформированного документа</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAcctRes_Type">
		<xsd:annotation>
			<xsd:documentation>Запись о счете депо</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="DepoAcctId"/>
			<xsd:element ref="AcctBal"/>
			<xsd:element name="AcctInfo" type="DepoAcctInfo_Type"/>
			<xsd:element ref="CustRec"/>
			<xsd:element ref="Status"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DepoAgreement_Type">
		<xsd:annotation>
			<xsd:documentation>Описание договора (ДЕПО).</xsd:documentation>
