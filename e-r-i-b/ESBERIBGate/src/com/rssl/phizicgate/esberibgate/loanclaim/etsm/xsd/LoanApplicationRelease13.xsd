				</xs:annotation>
			</xs:element>
			<xs:element ref="PersonName"/>
			<xs:element ref="Birthday"/>
			<xs:element ref="DependentFlag"/>
			<xs:element ref="SBCreditFlag"/>
			<xs:element ref="SBEmployeeFlag"/>
