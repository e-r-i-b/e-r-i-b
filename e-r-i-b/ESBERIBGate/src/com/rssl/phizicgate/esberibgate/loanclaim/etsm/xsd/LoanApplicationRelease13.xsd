			<xs:documentation>Тип - Зарплатныедепозит</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Type">
				<xs:annotation>
					<xs:documentation>Тип депозита
0 - зарплатный депозит
1 - пенсионный</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="1"/>
						<xs:enumeration value="0"/>
						<xs:enumeration value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="AcctId"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Passport_Type">
		<xs:annotation>
			<xs:documentation>Тип - Паспорт</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="IdSeries">
				<xs:annotation>
					<xs:documentation>Серия документа удостоверяющего личность</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="IdNum">
				<xs:annotation>
					<xs:documentation>Номер документа удостоверяющего личность</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="IssuedBy">
				<xs:annotation>
					<xs:documentation>Кем выдан.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IssuedCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>Кем выдан. Код подразделения</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="IssueDt">
