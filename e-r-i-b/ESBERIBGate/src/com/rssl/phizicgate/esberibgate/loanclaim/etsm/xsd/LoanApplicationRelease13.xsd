	<xs:complexType name="RealEstate_Type">
		<xs:annotation>
			<xs:documentation>Тип - объект недвижимости</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Type" type="RealEstateType_Type">
				<xs:annotation>
					<xs:documentation>Вид недвижимости
