					<xs:restriction base="EmploymentStatus_Type"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OrgInfo" type="OrgInfo_Type" minOccurs="0"/>
			<xs:element name="OrgInfo_Ext" type="OrgInfoExt_Type" minOccurs="0"/>
