	<xs:complexType name="SBEmployee_Type">
		<xs:annotation>
			<xs:documentation>Тип - блок данных о сотруднике СБ</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="RegionId"/>
			<xs:element name="FullName">
				<xs:annotation>
					<xs:documentation>Полное наименование подразделения</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="250"/>
