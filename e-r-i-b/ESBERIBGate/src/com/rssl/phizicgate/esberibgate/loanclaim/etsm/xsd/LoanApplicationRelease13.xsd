	<xs:complexType name="OrgInfoExt_Type">
		<xs:annotation>
			<xs:documentation>Тип - Описание организацции</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FullName">
				<xs:annotation>
					<xs:documentation>Полное наименование компании/ организации</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="120"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TaxId">
				<xs:annotation>
					<xs:documentation>ИНН</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="IndustSector" type="IndustSector_Type">
				<xs:annotation>
					<xs:documentation>Вид деятельности компании</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NumEmployeesCode" type="NumEmployeesCode_Type">
				<xs:annotation>
					<xs:documentation>Код.Количество сотрудников в компании работодателе
1 - До 10
2 - 11-30
3 - 31-50
4 - 51-100
5 - Более 100
6 - Затрудняюсь ответить</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Address_Type">
		<xs:annotation>
			<xs:documentation>Тип - адрес заемщика</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ManualInputFlag" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Флаг ручного ввода: true - да, false - нет</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AddrType">
				<xs:annotation>
					<xs:documentation>Тип адреса
1 - постоянной регистрации
2 - фактического проживания
3 - временной регистрации</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
						<xs:enumeration value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PostalCode" type="PostalCode_Type">
				<xs:annotation>
					<xs:documentation>Почтовый индекс</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RegionCode">
				<xs:annotation>
					<xs:documentation>Числовой ко региона, см справочник</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="4"/>
						<xs:pattern value="\d\d\d\d"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AreaType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Числовой код, описывающий тип района/округа, см. справочник</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="3"/>
						<xs:pattern value="\d\d\d"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Area" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Район/Округ</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Числовой код, описывающий тип города, см. справочник возможных значений</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="3"/>
						<xs:pattern value="\d\d\d"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="City" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Город</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SettlementType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Числовой код - тип населенного пункта</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="3"/>
						<xs:pattern value="\d\d\d"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Settlement" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Населенный пункт</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="StreetType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Числовой код - тип улицы</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="3"/>
						<xs:pattern value="\d\d\d"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Street">
				<xs:annotation>
					<xs:documentation>Улица</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="HouseNum">
				<xs:annotation>
					<xs:documentation>Дом (владение)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="HouseExt" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Корпус</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Unit" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Строение</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="UnitNum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Квартира/офис</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RelativeInfo_Type">
		<xs:annotation>
			<xs:documentation>Тип - блок данных о родственнике</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Type" type="RelativeType_Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Степень родства
