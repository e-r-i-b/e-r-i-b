				<xs:simpleType>
					<xs:restriction base="xs:unsignedInt">
						<xs:totalDigits value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Cost">
				<xs:annotation>
					<xs:documentation>Рыночная стоимость в долларах США</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="10"/>
