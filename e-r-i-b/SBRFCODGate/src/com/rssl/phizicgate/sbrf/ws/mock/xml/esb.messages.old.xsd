<?xml version="1.0" encoding="Windows-1251" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
<!-- <xs:schema targetNamespace="urn:esb.messages" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="urn:esb.messages" elementFormDefault="qualified" attributeFormDefault="unqualified"> -->

<xs:element name="message" type="MessageType" />

<xs:complexType name="MessageType">
  <xs:annotation>
    <xs:documentation>Сообщения</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="messageId" type="GUIDType" />
    <xs:element name="messageDate" type="xs:date" />
    <xs:element name="fromAbonent" type="AbonentEnum" />
    <xs:choice>
      <xs:element name="agreementRegistration_q" type="AgreementRegistrationType" />
      <xs:element name="agreementModification_q" type="AgreementModificationType" />
      <xs:element name="agreementCancellation_q" type="AgreementCancellationType" />
      <xs:element name="agreementCancellationWithoutCharge" type="AgreementCancellationWithoutChargeType" />
      <xs:element name="agreementCancellationMandatory" type="AgreementCancellationMandatoryType" />
      <xs:element name="agreementInfoDemand" type="AgreementInfoDemandType" />
      <xs:element name="agreementInfo" type="AgreementInfoType" />
      <xs:element name="restoreAccess" type="RestoreAccessType" />
      <xs:element name="restoreKey" type="RestoreKeyType" />
      <xs:element name="accountInfoDemand_q" type="AccountInfoDemandType" />
      <xs:element name="accountInfo_a" type="AccountInfoType" />
      <xs:element name="accountBalanceDemand_q" type="AccountBalanceDemandType" />
      <xs:element name="accountBalance_a" type="AccountBalanceType" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="billingDemand_q" type="BillingDemandType" />
      <xs:element name="billing_a" type="BillingType" />
      <xs:element name="form190ResultDemand_q" type="Form190ResultDemandType" />
      <xs:element name="form190Result_a" type="Form190ResultType" />
      <xs:element name="lossingPassbook_q" type="LossingPassbookType" />
      <xs:element name="transferAccountToAccountDemand_q" type="TransferAccountToAccountDemandType" />
      <xs:element name="transferForm143Demand_q" type="TransferForm143DemandType" />
      <xs:element name="saleCurrencyDemand_q" type="SaleCurrencyDemandType" />
      <xs:element name="purchaseCurrencyDemand_q" type="PurchaseCurrencyDemandType" />
      <xs:element name="convertCurrencyDemand_q" type="ConvertCurrencyDemandType" />
      <xs:element name="executePayment_q" type="ExecutePaymentType" />
      <xs:element name="transferOtherBank_q" type="TransferOtherBankType" />
      <xs:element name="acknowledge_t" type="ConfirmationType" />
      <xs:element name="refusal_t" type="ErrorType" />
      <xs:element name="confirmation_a" type="ConfirmationType" />
      <xs:element name="error_a" type="ErrorType" />
      <xs:sequence>
        <xs:choice>
          <xs:element name="error_offline_a" type="OperationRefusalType" />
          <xs:element name="confirmation_offline_a" type="OperationResultType" />
          <xs:element name="cancellationConfirmation" type="CancellationConfirmationType" />
        </xs:choice>
        <xs:element name="parentId" minOccurs="0" maxOccurs="1" >
          <xs:complexType>
            <xs:sequence>
              <xs:element name="messageId" type="GUIDType" />
              <xs:element name="messageDate" type="xs:date" />
              <xs:element name="fromAbonent" type="AbonentEnum" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:element name="lockClient_esk_q" type="LockAgreementType" />
      <xs:element name="unlockClient_esk_q" type="UnlockAgreementType" />
      <xs:element name="flowback" type="FlowbackType" />
      <xs:element name="revokeOperation" type="RevokeOperationType" />
      <xs:element name="requisitesChange" type="RequisitesChangeType" />
      <xs:element name="operationTraceDemand_q" type="OperationTraceDemandType" />
      <xs:element name="operationTrace_a" type="OperationTraceType" />
      <xs:element name="agreementLock" type="AgreementLockType" />
      <xs:element name="agreementUnlock" type="AgreementUnlockType" />
    </xs:choice>
  </xs:sequence>
</xs:complexType>

<xs:complexType name="ConfirmationType" >
  <xs:annotation>
    <xs:documentation>Подтверждение приема сообщения</xs:documentation>
  </xs:annotation>
  <xs:sequence>
  </xs:sequence>
</xs:complexType>

<xs:complexType name="ErrorType" >
  <xs:annotation>
    <xs:documentation>Ошибка приема сообщения</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="code" type="ErrorCodeEnum" />
    <xs:element name="message">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="256" />
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
  </xs:sequence>
</xs:complexType>

<xs:complexType name="AgreementRegistrationType">
  <xs:annotation>
    <xs:documentation>Регистрация договора на обслуживание</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="agreementNumber">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="16" />
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="agreementDate" type="xs:date" />
    <xs:element name="insertionOfService" type="xs:date" />
    <xs:element name="agreementOwner" type="BranchType" />
    <xs:element name="deposit" minOccurs="1" maxOccurs="unbounded">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="account" type="AccountType" />
        </xs:sequence>

      </xs:complexType>
    </xs:element>
    <xs:element name="owner" type="ClientType" />
    <xs:choice>
      <xs:sequence>
        <xs:element name="innerRecipient" type="InnerRecipientType" minOccurs="0" maxOccurs="unbounded" />
        <xs:element name="outerRecipient" type="OuterRecipientType" minOccurs="0" maxOccurs="unbounded" />
        <xs:element name="feeAccount" type="AccountType" minOccurs="1" maxOccurs="unbounded" />
        <xs:element name="feeValue" type="MoneyType" minOccurs="0" maxOccurs="1" />
        <xs:element name="addendum" type="AddendumType" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
      <xs:element name="trustingClient" type="GUIDType" minOccurs="0" maxOccurs="1" />
    </xs:choice>
  </xs:sequence>
</xs:complexType>

<xs:complexType name="AgreementCancellationType">
  <xs:annotation>
    <xs:documentation>Отмена договора об обслуживании</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="stopDate" type="xs:date" />
    <xs:element name="id" type="GUIDType" />
    <xs:element name="trustingClient" type="GUIDType" minOccurs="0" maxOccurs="1" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="AgreementCancellationWithoutChargeType">
  <xs:annotation>
    <xs:documentation>Отмена договора об обслуживании без списания платы</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="stopDate" type="xs:date" minOccurs="0" maxOccurs="1"/>
    <xs:element name="id" type="GUIDType" />
    <xs:element name="trustingClient" type="GUIDType" minOccurs="0" maxOccurs="1" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="AgreementCancellationMandatoryType">
  <xs:annotation>
    <xs:documentation>Отмена договора об обслуживании по иницитиве банка</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="stopDate" type="xs:date" minOccurs="0" maxOccurs="1" />
    <xs:element name="id" type="GUIDType" />
    <xs:element name="trustingClient" type="GUIDType" minOccurs="0" maxOccurs="1" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="CancellationConfirmationType">
  <xs:annotation>
    <xs:documentation>Подтверждение отмены договора</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="ownerId" type="GUIDType" />
    <xs:element name="outstandingDebt" type="MoneyType" minOccurs="0" maxOccurs="1" />
    <xs:element name="parentId" minOccurs="0" maxOccurs="1" >
      <xs:complexType>
        <xs:sequence>
          <xs:element name="messageId" type="GUIDType" />
          <xs:element name="messageDate" type="xs:date" />
          <xs:element name="fromAbonent" type="AbonentEnum" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>
  </xs:sequence>
</xs:complexType>

<xs:complexType name="AgreementModificationType">
  <xs:annotation>
    <xs:documentation>Изменение договора об обслуживании</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="clientId" type="GUIDType" />
    <xs:element name="owner" minOccurs="0" maxOccurs="1">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="tokenId" type="TokenIdType" minOccurs="0" maxOccurs="1" />
          <xs:element name="fullName" minOccurs="0" maxOccurs="1">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="1" />
                <xs:maxLength value="128" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="gender" type="GenderEnum" minOccurs="0" maxOccurs="1" />
          <xs:element name="birthdate" type="xs:date" minOccurs="0" maxOccurs="1" />
          <xs:element name="citizen" minOccurs="0" maxOccurs="1">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="1" />
                <xs:maxLength value="70" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="identityCard" type="IdentityCardType" minOccurs="0" maxOccurs="1" />
          <xs:element name="login" type="LoginType" minOccurs="0" maxOccurs="1" />
          <xs:element name="allowedOperations" type="AllowedOperationType" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="removed" minOccurs="0" maxOccurs="1" >
      <xs:complexType>
        <xs:sequence>
          <xs:element name="deposit" type="AccountType" minOccurs="0" maxOccurs="unbounded" />
          <xs:element name="feeAccount" type="AccountType" minOccurs="0" maxOccurs="unbounded" />
          <xs:element name="innerRecipient" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="account" type="AccountType" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="outerRecipient" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="account" type="AccountType" />
                <xs:element name="bankBIC" type="BICType" />
                <xs:element name="bankAccount" type="AccountType" minOccurs="0" maxOccurs="1" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="addendum" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="fullName">
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:minLength value="1" />
                      <xs:maxLength value="128" />
                    </xs:restriction>
                  </xs:simpleType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="added" minOccurs="0" maxOccurs="1" >
      <xs:complexType>
        <xs:sequence>
          <xs:element name="deposit" type="AccountType" minOccurs="0" maxOccurs="unbounded" />
          <xs:element name="feeAccount" type="AccountType" minOccurs="0" maxOccurs="unbounded" />
          <xs:element name="innerRecipient" type="InnerRecipientType" minOccurs="0" maxOccurs="unbounded" />
          <xs:element name="outerRecipient" type="OuterRecipientType" minOccurs="0" maxOccurs="unbounded" />
          <xs:element name="addendum" type="AddendumType" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="trustingClient" type="GUIDType" minOccurs="0" maxOccurs="1" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="AgreementInfoDemandType">
  <xs:annotation>
    <xs:documentation>Запрос информации о договоре ЭСК</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="ownerId" type="GUIDType" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="AgreementInfoType">
  <xs:annotation>
    <xs:documentation>Запрос информации о договоре ЭСК</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="ownerId" type="GUIDType" />
    <xs:element name="state">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="ACTIVE" />
          <xs:enumeration value="CANCELLED" />
          <xs:enumeration value="LOCKED" />
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="mode">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="RESTRICTED" />
          <xs:enumeration value="FULL" />
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="agreementNumber">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="16" />
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="agreementDate" type="xs:date" />
    <xs:element name="agreementOwner" type="BranchType" />
    <xs:element name="insertionOfService" type="xs:date" />
    <xs:element name="serviceRate" type="MoneyType" minOccurs="0" maxOccurs="1" />
    <xs:element name="restoreAccessRate" type="MoneyType" minOccurs="0" maxOccurs="1" />
    <xs:element name="restoreKeyRate" type="MoneyType" minOccurs="0" maxOccurs="1" />
    <xs:element name="lastInvoiceDate" type="xs:date" minOccurs="0" maxOccurs="1" />
    <xs:element name="nextInvoiceDate" type="xs:date" minOccurs="0" maxOccurs="1" />
    <xs:element name="outstandingDebt" type="MoneyType" minOccurs="0" maxOccurs="1" />
    <xs:element name="invoice" minOccurs="0" maxOccurs="unbounded">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="invoiceDate" type="xs:date" />
          <xs:element name="reason">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="1" />
                <xs:maxLength value="256" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="totalSum" type="MoneyType" />
          <xs:element name="insufficientSum" type="MoneyType" minOccurs="0" maxOccurs="1" />
          <xs:element name="lastAttemptDate" type="xs:date" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>
  </xs:sequence>
</xs:complexType>

<xs:complexType name="RestoreAccessType">
  <xs:annotation>
    <xs:documentation>Запрос на списание платы за восстановление доступа к ЭСК</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="clientId" type="GUIDType" />
    <xs:element name="login" type="LoginType" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="RestoreKeyType">
  <xs:annotation>
    <xs:documentation>Запрос на списание платы за секретного ключа ЭСК</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="clientId" type="GUIDType" />
    <xs:element name="tokenId" type="TokenIdType" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="ProxyAgreementModificationType">
  <xs:annotation>
    <xs:documentation>Изменение договора об обслуживании доверенного лица</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="clientId" type="GUIDType" />
    <xs:element name="owner">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="tokenId" type="TokenIdType" minOccurs="0" maxOccurs="1" />
          <xs:element name="fullName" minOccurs="0" maxOccurs="1">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="1" />
                <xs:maxLength value="128" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="gender" type="GenderEnum" minOccurs="0" maxOccurs="1" />
          <xs:element name="birthdate" type="xs:date" minOccurs="0" maxOccurs="1" />
          <xs:element name="citizen" minOccurs="0" maxOccurs="1">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:minLength value="1" />
                <xs:maxLength value="70" />
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="identityCard" type="IdentityCardType" minOccurs="0" maxOccurs="1" />
          <xs:element name="allowedOperations" type="AllowedOperationType" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="removed">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="deposit" type="AccountType" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <xs:element name="added">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="deposit" type="AccountType" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
      </xs:complexType>
    </xs:element>
  </xs:sequence>
</xs:complexType>

<xs:complexType name="ProxyAgreementRegistrationType">
  <xs:annotation>
    <xs:documentation>Регистрация договора на обслуживание доверенного лица</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="agreementNumber">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="16" />
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="agreementDate" type="xs:date" />
    <xs:element name="insertionOfService" type="xs:date" />
    <xs:element name="deposit" type="AccountType" minOccurs="1" maxOccurs="unbounded" />
    <xs:element name="proxy" type="ClientType" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="AccountBalanceDemandType">
  <xs:annotation>
    <xs:documentation>Запрос остатка по счету</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="clientId" type="GUIDType" />
    <xs:element name="signature" type="SignatureType" minOccurs="0" maxOccurs="1" />
    <xs:element name="account" type="AccountType" minOccurs="1" maxOccurs="unbounded" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="AccountBalanceType">
  <xs:annotation>
    <xs:documentation>Остаток по счету</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="branch" type="BranchType" />
    <xs:element name="office" type="OfficeType" />
    <xs:element name="account" type="AccountType" />
    <xs:element name="status" type="AccountStatusEnum" />
    <xs:element name="balance" type="MoneyType" />
    <xs:element name="availableBalance" type="MoneyType" minOccurs="0" maxOccurs="1" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="LossingPassbookType">
  <xs:annotation>
    <xs:documentation>Сообщение об утере сберкнижки</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="clientId" type="GUIDType" />
    <xs:element name="signature" type="SignatureType" minOccurs="0" maxOccurs="1" />
    <xs:element name="account" type="AccountType" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="AccountInfoDemandType">
  <xs:annotation>
    <xs:documentation>Запрос информации по счету</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="account" type="AccountType" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="AccountInfoType">
  <xs:annotation>
    <xs:documentation>Информация по счету</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="account" type="AccountType" />
    <xs:element name="branch" type="BranchType" />
    <xs:element name="office" type="OfficeType" />
    <xs:element name="status" type="AccountStatusEnum" />
    <xs:element name="depositor" type="DepositorType" />
    <xs:element name="balance" type="MoneyType" />
    <xs:element name="isDeposit" type="xs:boolean" />
    <xs:element name="isSpecial" type="xs:boolean" />
    <xs:element name="accName">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="128" />
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="openDate" type="xs:date" />
    <xs:element name="isCreditAllowed" type="xs:boolean" />
    <xs:element name="isDebitAllowed" type="xs:boolean" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="BillingDemandType">
  <xs:annotation>
    <xs:documentation>Запрос выписки по счету</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="clientId" type="GUIDType" />
    <xs:element name="signature" type="SignatureType" minOccurs="0" maxOccurs="1" />
    <xs:element name="copying" type="xs:boolean" />
    <xs:element name="account" type="AccountType" />
    <xs:element name="startDate" type="xs:date" />
    <xs:element name="endDate" type="xs:date" minOccurs="0" maxOccurs="1" />
  </xs:sequence>
</xs:complexType>

<xs:complexType name="BillingType">
  <xs:annotation>
    <xs:documentation>Выписка по счету</xs:documentation>
  </xs:annotation>
  <xs:sequence>
    <xs:element name="formType">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:length value="3" fixed="true" />
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="bankName">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="128" />
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="branch" type="BranchType" />
    <xs:element name="office" type="OfficeType" />
    <xs:element name="account" type="AccountType" />
    <xs:element name="startDate" type="xs:date" />
    <xs:element name="endDate" type="xs:date" />
    <xs:element name="depositorName">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="128" />
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="depositType">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:minLength value="1" />
          <xs:maxLength value="128" />
        </xs:restriction>
