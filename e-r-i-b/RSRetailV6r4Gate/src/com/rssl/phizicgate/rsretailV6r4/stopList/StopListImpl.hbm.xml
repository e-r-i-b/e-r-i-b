<?xml version="1.0" encoding="windows-1251"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.rssl.phizicgate.rsretailV6r4.stopList" >
    <class name="StopListImpl" table="DSL_FL_DBT">

        <id name="id" column="T_RECORD_ID"/>

        <property name="cliId"      type="string"  column="T_CLI_ID"/>
        <property name="family"     type="string"  column="T_FAMILY"/>
        <property name="name"       type="string"  column="T_NAME"/>
        <property name="patrName"   type="string"  column="T_SNAME"/>
        <property name="inn"        type="string"  column="T_INN"/>
	    <property name="docType"    type="integer" column="T_TYPE_DOC"/>
	    <property name="series"     type="string"  column="T_SERIES"/>
	    <property name="number"     type="string"  column="T_NUMBER"/>
	    <property name="birthDate"  type="calendar"    column="T_BIRTHDATE"/>
	    <property name="slType"     type="integer" column="T_SLTYPE"/>
	    <property name="isTer"      type="integer" column="T_ISTER"/>
    </class>
</hibernate-mapping>