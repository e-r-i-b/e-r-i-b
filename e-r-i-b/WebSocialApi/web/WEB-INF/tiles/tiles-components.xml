<?xml version="1.0" encoding="Windows-1251" ?>

<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 1.1//EN"
       "http://jakarta.apache.org/struts/dtds/tiles-config_1_1.dtd"

	[ <!ENTITY nbsp "&#160;"> ]
>

<tiles-definitions>
    <!-- Компонента для передачи денег мобильном протоколе -->
    <definition name="mobileMoneyType" path="/WEB-INF/jsp/common/socialApi/types/mobileMoneyType.jsp">
		<put name="name" value=""/>
        <put name="sign" value=""/>
        <put name="money" value=""/>
        <put name="decimal" value=""/>
        <put name="currency" value=""/>
        <put name="currencyCode" value=""/>
        <put name="currencySign" value=""/>
        <put name="noCents" value="false"/>
	</definition>

    <definition name="currencyType" path="/WEB-INF/jsp/common/socialApi/types/currencyType.jsp">
        <put name="currency"/>
        <put name="name" value=""/>
    </definition>

    <!--Компонента для передачи дат типа DateTime-->
    <definition name="mobileDateTimeType" path="/WEB-INF/jsp/common/socialApi/types/mobileDateTimeType.jsp">
        <put name="name" value=""/>
        <put name="calendar"/>
        <put name="pattern" value="dd.MM.yyyy'T'HH:mm:ss"/>
    </definition>

    <!--Компонента для передачи дат типа Date-->
    <definition name="mobileDateType" path="/WEB-INF/jsp/common/socialApi/types/mobileDateTimeType.jsp">
        <put name="name" value=""/>
        <put name="calendar"/>
        <put name="pattern" value="dd.MM.yyyy"/>
    </definition>

    <!-- Компонента для курсов в мобильном протоколе -->
    <definition name="mobileRate" path="/WEB-INF/jsp/common/socialApi/types/rate.jsp">
		<put name="mainCode" value=""/>
        <put name="code" value=""/>
        <put name="department"/>
	</definition>

    <!-- Компонента для описание Person в мобильном протоколе -->
    <definition name="personType" path="/WEB-INF/jsp/common/socialApi/types/personType.jsp">
        <put name="person"/>
        <put name="name" value=""/>
	</definition>

    <!-- Компонента для описания Image в мобильном протоколе -->
    <definition name="imageType" path="/WEB-INF/jsp/common/socialApi/types/imageType.jsp">
        <put name="name" value=""/>
        <put name="id" value=""/>
        <!--workaround: задаем _стринговое_ дефолтное значение для объекта типа java.util.Date - updateTime,-->
        <!--иначе если updateTime будет null, то tiles обнулит значения остальных параметров (не всех)-->
        <put name="updateTime" value=""/>
        <put name="url" value=""/>
        <put name="imageData" value=""/>
	</definition>

    <!-- Компонента для confirmInfo -->
    <definition name="confirmInfo" path="/WEB-INF/jsp/common/socialApi/types/confirmInfo.jsp">
        <put name="confirmRequest"/>
	</definition>

    <!-- Компонента для confirmType -->
    <definition name="confirmType" path="/WEB-INF/jsp/common/socialApi/types/confirmType.jsp">
        <put name="confirmRequest"/>
	</definition>

    <!--Комопнента для confirmStage-->
    <definition name="confirmStage" path="/WEB-INF/jsp/common/socialApi/types/confirmStage.jsp">
        <put name="confirmRequest"/>
        <put name="autoStart" value="false"/>
    </definition>

     <!-- Шаблон для отображения информации по операциям -->
    <definition name="historyTemplateFrom" path="/WEB-INF/jsp/common/socialApi/types/historyTemplateFrom.jsp">
    </definition>

    <!-- Шаблон для отображения информации по операциям -->
    <definition name="historyTemplateTo" path="/WEB-INF/jsp/common/socialApi/types/historyTemplateTo.jsp">
    </definition>

    <!--Компонента для описания билингового поставщика-->
    <definition name="billingPayment" page="/WEB-INF/jsp/common/socialApi/types/billingPayment.jsp">
        <put name="serviceProvider"/>
    </definition>
</tiles-definitions>
