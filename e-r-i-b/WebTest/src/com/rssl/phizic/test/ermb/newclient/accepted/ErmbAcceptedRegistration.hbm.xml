<?xml version="1.0" encoding="windows-1251" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rssl.phizic.test.ermb.newclient.accepted.ErmbAcceptedRegistration" table="ERMB_ACCEPTED_REGISTRATIONS">

        <id name="id" type="long">
            <generator class="identity"/>
        </id>

        <property name="connectedDate" type="calendar" column="ConnectedDate" not-null="true"/>
        <property name="phone" type="string" column="PhoneNumber" not-null="true"/>
    </class>
</hibernate-mapping>
