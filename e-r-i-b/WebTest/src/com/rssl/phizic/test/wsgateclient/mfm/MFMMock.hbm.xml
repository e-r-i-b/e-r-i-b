<?xml version="1.0" encoding="windows-1251" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.rssl.phizic.test.wsgateclient.mfm.outmessageconsumer.OutcomeMessage" table="OUTCOME_MESSAGES">

        <id name="guid" type="string" column="GUID"/>
        <property name="address" type="string" column="ADDRESS" not-null="true"/>
        <property name="priority" type="string" column="PRIORITY" not-null="true"/>
        <property name="text" type="string" column="TEXT" not-null="true"/>
    </class>
</hibernate-mapping>